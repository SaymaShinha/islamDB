[{"id": 498180, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5713, "text": "Eebe wuxuu ku dhaartay Malaa'igta Nafta si daran uga siibta (gaalada)."}, {"id": 498181, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5714, "text": "Iyo tan si fudud uga qaadda (mu'miniinta)."}, {"id": 498182, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5715, "text": "Iyo Malaa'igta Hawada Dabbaalata (si amar fulin)."}, {"id": 498183, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5716, "text": "Iyo tan u orotanta wanaagga."}, {"id": 498184, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5717, "text": "Iyo kuwa Maamula amarka (Eebe)."}, {"id": 498185, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5718, "text": "Maalinta ay gilgilan afuufka (hore)."}, {"id": 498186, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5719, "text": "Oyna raaci tii dambe (waa sugantahay)."}, {"id": 498187, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5720, "text": "Qalbiyaduna Maalintaa way cabsan."}, {"id": 498188, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5721, "text": "Araggeeduna wuu dullaysnaan."}, {"id": 498189, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5722, "text": "Waxayna dhihi (Gaaladu) Ma raadkiibaa naloo celin (Adduunkii)."}, {"id": 498190, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5723, "text": "Markaan noqonno Lafo duugoobay."}, {"id": 498191, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5724, "text": "Waxay dheheen taasi waa noqosho khasaara ah."}, {"id": 498192, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5725, "text": "Waa uun Hal Qaylo kaliya."}, {"id": 498193, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5726, "text": "Dadkuna markaas ay Dhulka kor iman."}, {"id": 498194, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5727, "text": "Ma ku soo gaadhay warkii (Nabi) Muuse."}, {"id": 498195, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5728, "text": "Markii Eebe uga yeedahy Toggii barakaysnaa ee Dhuwa."}, {"id": 498196, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5729, "text": "Kuna yidhi u tag Fircoon wuu xadgudbaye."}, {"id": 498197, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5730, "text": "Oo ku dheh ma rabtaa inaad daahir noqotid."}, {"id": 498198, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5731, "text": "Oon Eebe Jidkiisa kugu hanuuniyo kana yaabtid."}, {"id": 498199, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5732, "text": "Wuxuuna tusiyay (Nabi-Muuse) Aayad wayn."}, {"id": 498200, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5733, "text": "Wuuse beeniyay oo caasiyay (Fircoon)."}, {"id": 498201, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5734, "text": "Wuuna jeedsaday isagoo xumaan la socda."}, {"id": 498202, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5735, "text": "Wuxuuna kulmiyay (Askartii) una Qaylyay."}, {"id": 498203, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5736, "text": "Isagoo dhihi waxaan ahay Eebihiinna sare."}, {"id": 498204, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5737, "text": "Markaasaa Eebe Ciqaabay Aakhiro iyo Adduunba."}, {"id": 498205, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5738, "text": "Arrintaasna waano yaa ugu sugan Ruuxii Eebe ka yaabi."}, {"id": 498206, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5739, "text": "Ma idinkaa daran abuuridda masc Samada Eebaa dhisay."}, {"id": 498207, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5740, "text": "Korna yeelay jurmigeeda ekeeyayna."}, {"id": 498208, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5741, "text": "Habeenkeedana madoobeeyay Maalinteedana soo bixiyay (Ifiyay)."}, {"id": 498209, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5742, "text": "Dhulkana markaas ka dib fidiyay."}, {"id": 498210, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5743, "text": "Biyahiisa iyo Daaqiisana soo bixiyay."}, {"id": 498211, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5744, "text": "Buurahana sugay."}, {"id": 498212, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5745, "text": "Si aad idinka iyo Xoolihiinnu ugu nacfidaan."}, {"id": 498213, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5746, "text": "Markay timaaddo Musiibadii waynayd (qiyaamadu)."}, {"id": 498214, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5747, "text": "Maalintaas Dadku way xusuusan waxay faleen."}, {"id": 498215, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5748, "text": "Naarta Jaxiimana waxaa loo muujin Ruuxii Arki."}, {"id": 492663, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5749, "text": "Ruuxiise xadgudba."}, {"id": 492662, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5750, "text": "Nolosha Adduunyana doorta."}, {"id": 498216, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5751, "text": "Wuxuu ku dheelman Naarta Jaxiimo."}, {"id": 498217, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5752, "text": "Ruuxiise ka yaaba la kulanka Eebe Naftana ka reeba Hawada (xun)."}, {"id": 498218, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5753, "text": "Wuxuu u dheelman Janno."}, {"id": 498219, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5754, "text": "Waxay ku waydiin Saacadda (Qiyaamaha) markay sugnaan."}, {"id": 498220, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5755, "text": "Maxaa ku ogaysiiyay waqtigeeda."}, {"id": 498221, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5756, "text": "Eebe umbuu ku dhammaan ogaansheheedu."}, {"id": 498222, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Adugu waxaad tahay uun u digaha Ruuxii ka yaabi."}, {"id": 498223, "identifier": "so.abduh", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5758, "text": "Waxay la mid noqon Malintay arkaan (Qiyaamada) yagon nagaanin Galab iyo Barqin mooyeen (Addunka)."}]