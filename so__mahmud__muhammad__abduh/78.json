[{"id": 498140, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5673, "text": "Maxay iska waydiinayaan (Gaaladu)."}, {"id": 498141, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5674, "text": "Warkii waynaa xaggiisa (Qiyayaamada ama Quraanka)."}, {"id": 498142, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5675, "text": "Ay isku diiddanaayeen."}, {"id": 498143, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5676, "text": "Saas ma aha ee way ogaan donaan."}, {"id": 498144, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5677, "text": "Haddana way ogaandoonaan (wa xa ka dhaca)."}, {"id": 498145, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5678, "text": "Miyaannaan Dhulka gogol ka yeelin."}, {"id": 498146, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5679, "text": "Buurahana Rukummo (ka yeelin)."}, {"id": 498147, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5680, "text": "Miyannaan idinka Abuurina Noocyo."}, {"id": 498148, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5681, "text": "Miyannaan Hurdada Raaxo idiinka dhigin."}, {"id": 498149, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5682, "text": "Miyaannaan Habeenka asturaal idiinka dhigin."}, {"id": 498150, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5683, "text": "Miyaannaan Maalinta waqti shaqo idiinka dhigin."}, {"id": 498151, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5684, "text": "Korkiinna miyaannaan ka dhisin Tooddoba xoog leh."}, {"id": 498152, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5685, "text": "Miyaannaan dhexdeeda yeelin Sirraad aad u ifi (Qorraxda)."}, {"id": 498153, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5686, "text": "Waxaan idiinka soodajinnay daruuraha biyo badan."}, {"id": 498154, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5687, "text": "Si aan ugu soo bixinno Midho iyo Daaq."}, {"id": 498155, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5688, "text": "Iyo Beero kulansan (badan)."}, {"id": 498156, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5689, "text": "Maalinta kalabuxu waxay leedahay waqtu go'an."}, {"id": 498157, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5690, "text": "Waana Maalinta Suurka la afuufi ood u imanaysaan Kooxo."}, {"id": 498158, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5691, "text": "Samadana waa la furi waxayna yeelan Albaabo."}, {"id": 498159, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5692, "text": "Buurahana waa la kaxayn waxayna noqon Dhalanteed."}, {"id": 498160, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5693, "text": "Naarta Jahannamana waxay u darbantahay."}, {"id": 498161, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5694, "text": "(Gaalada) Xadgudbay meelay u noqdaan."}, {"id": 498162, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5695, "text": "Waxayna ku nagaan Dhexdeeda Sanooyin."}, {"id": 498163, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5696, "text": "Mana ku dhadhamiyaan dhexdeeda qabaw (ama Hurdo) ama Cabbid midna."}, {"id": 498164, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5697, "text": "Waxaan ahayn biyo kulul iyo Dhacaanka (ehlu Naarka)."}, {"id": 498165, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5698, "text": "Waxaana lagu abaalmariyay si waafaqsan Camalkoodii."}, {"id": 498166, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5699, "text": "Waxayna ahaayeen kuwa aan ka yaabaynin xisaab."}, {"id": 498167, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5700, "text": "Waxayna beeniyeen Aayaadkannaga."}, {"id": 498168, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5701, "text": "Wax walbana waxaan ku koobnay Kitaab."}, {"id": 498169, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5702, "text": "Ee dhadhamiya Caddibaad Gaalooy waxaan caddibaad ahayn idiin kordhinmaynee."}, {"id": 498170, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5703, "text": "Kuwa Eebe ka dhawrsada waxay leeyihiin liibaan."}, {"id": 498171, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5704, "text": "Beero iyo Cinabyana way heli."}, {"id": 498172, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5705, "text": "Waxayna Guursan Haween da'yar oo isle eg."}, {"id": 498173, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5706, "text": "Waxayna heli Cabbid wanaagsan."}, {"id": 498174, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5707, "text": "Jannada kuma dhexmaqlaan hadal macna la'aan ah iyo is beenin midna."}, {"id": 498175, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5708, "text": "Waa abaalmarin Eebe uu siiyay oo ku filan."}, {"id": 498176, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5709, "text": "Eebaha Samada iyo Dhulka iyo waxa u dhexeeya waa Allaha Raxmaan ah, Hadal wax ku soocelinna ma jiro (Qiyaamada)."}, {"id": 498177, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5710, "text": "Maalinta Jibriil iyo Malaa'igtu Saf istaagi wax hadli ma jiro Ruux Eebe idmo mooyee oo Hadal xaq ah ku hadla."}, {"id": 498178, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5711, "text": "Maalintaasi waa sugantahay ee Ruuxii doona Eebe xaggiisa meeluu ku noqdo ha ka samaysto."}, {"id": 498179, "identifier": "so.abduh", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5712, "text": "Waxaannu idiinka digaynaa Cadibaad dhaw Maalinta uu arki Ruux walba waxay Gacmihiisu hor marsadeen, Gaalkuna uu dhihi Shallaytadaydee maan Carro noqdo."}]