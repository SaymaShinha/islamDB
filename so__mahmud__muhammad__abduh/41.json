[{"id": 496699, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4219, "text": "Waxay ku tusin mucjisada Quraanka sida soo hormartay."}, {"id": 496700, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4220, "text": "Quraanka waa soo dajinta Eebe naxariista guud iyo mid gaaraba naxariista,"}, {"id": 496701, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4221, "text": "Waana Kitaab loo caddeeyey aayaadkiisa isagoo Quraan carabi ku qoran ah ciddii wax garan."}, {"id": 496702, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4222, "text": "Waana bishaareeye u dige ah, waase ka jeedsadeen badankoodu waxna ma maqlayaan."}, {"id": 496703, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4223, "text": "Waxayna dheheen quluubtanadu waxay kaga sugantahay dabool waxaad noogu yeedhi, dhagahanagana waxaa kaga jira culeys, dhexdanada iyo adigana waxaa ah xijaab, ee camal fal anana waancamalfaleynaaye."}, {"id": 496704, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4224, "text": "Waxaad dhahdaa Nabiyow anigu waxaan uun ahay bashar (Dad) idinkookale ah, waxaana la iiwaxyooday in Eebehenno yahay Eebe kaliya, ee u toosnaada xaggiisa, kana dambidhaaf waydiista, halaagna wuxuu u sugnaaday mushrikiinta."}, {"id": 496705, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4225, "text": "Kuwaan bixinin zakada ahna aakhiro kuwa ka gaaloobey."}, {"id": 496706, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4226, "text": "Kuwa xaqa rumeeyeyse oo camalfiican falay waxaa usugnaaday ajri aan guraynin."}, {"id": 496707, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4227, "text": "Waxaad dhahdaa Nabiyow idinku ma waxaad ka gaaloobaysaan kan (Eebaha) ku abuuray Dhulka laba maalmood, ood uyeelaysaan kuwo la mid ah, kaasi waa Eebaha Caalamka."}, {"id": 496708, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4228, "text": "Wuxuuna yeelay Dhulka dhexdiisa Buuro wuuna barakeeyey dhexdiisa una qadaray (u qiyaasay) Cunnadiisa afar maalmood iyadooy usimantahay dadka weydiin."}, {"id": 496709, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4229, "text": "Markaasuu Eebe u istawooday (qasday) Cirka oo qiiq ah kuna yidhi Cirka iyo Dhulka ahaada idinkoo adeecsan ama la ikraahi (laqasbi) waxayna dheheen waan nimid anagoo addeecsan."}, {"id": 496710, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4230, "text": "Wuxuuna Eebe ka abuuray Cirka Todobo Samo laba maalmood wuxuuna faray Samo kasta amarkeedii, waxaana ku qurxinay Cirka dhaw Misbaaxyo, waana ilaalinay, taasina waa qadaridda (maamulka) Eebaha adkaada ee wax og."}, {"id": 496711, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4231, "text": "Hadday ka jeedsadeen Gaaladu (xaqa) waxaad ku dhahdaa Nabiyow waxaan idiinka digi cadaab daran oo lamid ah kii Reer Caad iyo Thamuud."}, {"id": 496712, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4232, "text": "Markay ugaga timid Rasuuladii hor iyo gadaalba, iyagoo ku leh ha caabudina waxaan Eebe ahyn, ayna ku dhaheen hadduu doono Eebeheen wuxuu soo dajinlahaa Malaa'ig, anaguna waxa laydiin soo diray waan ka gaaloownay."}, {"id": 496713, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4233, "text": "Ree Caad iyagu way isku kibriyeen Dhulka xaq darro, waxayna dheheen yaa naga xoog badan miyeyna arag Eebaha abuuray inuu ka xoogbadanyahay waxayna ahaayeen kuwo aayaadkanaga diidan."}, {"id": 496714, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4234, "text": "Waxaana ku dirray korkooda Dabayl qaylo daran Todobo Maalmood oo uxun (iyaga) si aan u dhandhansiino cadaabka dulliga Nolosha adduun, cadaabka aakhiraana ka sii dulli badan loomana gargaaro."}, {"id": 496715, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4235, "text": "Ree thamuud waxaantusinay xaqa, waxayna ka jeeclaadeen indho la'aan (baadi) hanuunkii, waxaana qabtay qaylo cadaab wax dulleeya, waxay kasbanayeen darteed."}, {"id": 496716, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4236, "text": "Waxaana korinay kuwii rumeeyey (xaqa) oo dhawrsan jiray."}, {"id": 496717, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4237, "text": "(u sheeg gaalada) Maalinta lagu kulmin colka Eebe naarta iyagoo is cidhiidhyi."}, {"id": 496718, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4238, "text": "Markay yimaaddaana uu ku maragfuro korkooda, maqalkooda, aragoodu, iyo haragoodu (jidhkoodu) waxay falijireen."}, {"id": 496719, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4239, "text": "ayna ku dhahaan korkooda (Jidhkooda) maxaad noogu marag kacdeen, ayna ku dhahaan waxaa naga hadliyey Eebaha ka hadliya wax kasta, isagaana idin abuuray markii hore xagiisana laydiin celin."}, {"id": 496720, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4240, "text": "Mana aydaan ahayn kuwa iska astura inay ku maragkacaan jidhkiinu, araggiinu iyo haraggiinu, inuusan Eebe ogayn wax badan ood falaysaan."}, {"id": 496721, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4241, "text": "kaasina waa malihiinii aad u malayseen Eebihiin isagaana idin halaagay, waxaadna ahateen kuwo khasaaray."}, {"id": 496722, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4242, "text": "haday samraanna naarbay ku hoyan hadday cudur daar dalbaana lagama aqbalo."}, {"id": 496723, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4243, "text": "Waxaan u darabnay Gaalada saaxiibo u qurxiya waxa ka horeeya, iyo waxa ka dambeeya, waxaana ku xukunaatay hadalkii Eebe isagoo umada kale ku dhex sugan (oo la cadaabi) oo ka horeeyey Jinni iyo Insina leh waxayna ahaayeen kuwo khasaaray."}, {"id": 496724, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4244, "text": "Waxay dhaheen kuwii Gaaloobay ha maqlina Quraanka kuna qaylya (marka la akhrin) waxaad u dhawdihiin inaad adkaataane."}, {"id": 496725, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4245, "text": "Waxaan dhadhansiinaynaa kuwii Gaaloobay Cadaab daran, waxaana ka abaalmarinaynaa kii ugu xumaa waxay camal falayeen."}, {"id": 496726, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4246, "text": "Kaasi waa abaalmarinta colka Eebe, waxayna gali Naar iyadoo u noqon Curi waarid, iyagoo laga abaal marin aayaadkanaga aydiideen."}, {"id": 496727, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4247, "text": "Waxay dheheen kuwii Gaaloobay Eebow na tusi labadaas nadhumiyey oo Jinni iyo Insi ah aan yeelo Gomadahanaga hoostoodee si ay u noqdaan kuwo ku hooseeya (Naarta)."}, {"id": 496728, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4248, "text": "kuwa yidhi Eebahanno waa Alle hadana toosnaaday waxaa ku soo dagta Malaa'igta (markey dhiman iyadoo ku dhihi) ha cabsanina hana murugoonina, kuna bishaareysta Jannadii laydiin yaboohay."}, {"id": 496729, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4249, "text": "anagaa sokeeye idiin ah nolosha addunyo iyo aakhiraba, waxaana idiinsugnaaday dhexdeeda waxay doonto Naftiinnu waxaadna ka helaysaan waxaad dalabtaan."}, {"id": 496730, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4250, "text": "Waana martiqaadka Eebaha dambi dhaafa ee Naxariista."}, {"id": 496731, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4251, "text": "yaa ka fiican hadal ruux ugu yeedhay (Dadka) Eebe xagiisa, oo camalfiican sameeyey, oo dhaha anigu waxaan ka mid ahay Muslimiinta."}, {"id": 496732, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4252, "text": "Mana eka Wanaag iyo Xumaan, reebna oo (bixi) arrinta fiican markaas waxaa soobixi in midkii col idin dhexyiil uu noqon sokeeye iyo saaxiib."}, {"id": 496733, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4253, "text": "Lalamana kulmiyo arrintaas kuwii samrey mooyee, lalamana kulmiyo mid nasiib weyn leh mooyee."}, {"id": 496734, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4254, "text": "Hadduu ku waswaasiyo shaydaan xaggiisa waswaasiye ka magangal Eebe isagaa wax maqla waxna oge."}, {"id": 496736, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4255, "text": "Aayaadka Eebe (jiridiisa iyo qudradiisa) waxaa ka mid ah Habeenka, Maalinta, Qorraxda iyo Dayaxa, ha u sujuudina Qorraxda iyo Dayaxa ee u sujuuda Eebaha abuuray haddaad tihiin kuwo (dhab) u caabudi."}, {"id": 496737, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": true, "sajda_id": 12, "sajda_obligatory": true, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4256, "text": "Haday iska kibriyaan (cibaadada) kuwa Eebahaa agtiisa ah way u tasbiixsadaan Habeen iyo Dharaarba kana noogin."}, {"id": 496738, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4257, "text": "Aayaadka Eebe waxaa ka mid ah inaad aragtidDhulkoo dulleysan (abaar ah) markaan ku soo dajinno Biyaha uu gilgisho soona kor noqdo, midka nooleeyey wuxuu nooleyn wixii dhintay wax walbana wuu karaa."}, {"id": 496739, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4258, "text": "Kuwa iilaya aayaadkanaga nagama qarsoona, ruux lagu tuuray Naarta miyaa khayrroon mise mid isagoo aamin ah imaan Qiyaamada, fala waxaad doontaan, Eebe waxaad faleysaan waa arkaa."}, {"id": 496740, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4259, "text": "Kuwa ka gaaloobay Quraanka markuu u yimid (waa la abaal marin) waana Kitaab qaali ah."}, {"id": 496741, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4260, "text": "Mana timaado xumaan hortiisa iyo gadaashiisa, (waa dhawran yahay) waana soo dajinta Eebaha falka san ee lamahdiyo."}, {"id": 496742, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4261, "text": "Lagumana dhaho waxaan ahayn waxii lagu yidhi Rasuuladii ka horreeyey, Eebahaana waadambi dhaafe ciqaab daran."}, {"id": 496743, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4262, "text": "Hadaan kayeelo \u00abKitaabka\u00bb Quraanka Cajami ah waxay dhihi lahaayeen maxaa loo caddayn waayey Aayaadkiisa, ma (quraan) Cajami ah iyo Nabi (Carabi ah) waxaad dhahdaa isagu kuwa rumeeyey wuxuu u yahay Haanuun iyo Caafimaad, kuwa aan rumeynina dhagahooda waxaa ah culays, waana ka indha la' yihiin kuwaasna waxaa looga yeedhi meel fog."}, {"id": 496744, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4263, "text": "Dhab ahaanbaan u siinay Nabi Muuse Kitaabka waana laysku khilaafay, haddayan jirin kalimo ka hormartay Eebahaa waa lakala xukminlahaa dhexdooda waxayna ku suganyihiin gaaladu shaki daran."}, {"id": 496745, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4264, "text": "Ruuxi camalfiican fala wuxuu u falay Naftiisa, Ruuxii xumaan fala wuxuu dhibay Naftiisa, Eebaheena ma aha dulmiyaha Addoomada."}, {"id": 496747, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4265, "text": "Xagiisaana loo celiyaa ogida Saacadda (Qiyaame) midhana kama soobaxaan Sabuulkooda, mana uuroobo Dhadig mana dhalo ogaanshaha Eebe la'aantiiis, Maalintuu ku dhihi away kuwaad ila wadaajiseenna waxay dhihi waxaan ku ogaysiinnay Nagamidma aha Ruux marag furi arintaas."}, {"id": 496748, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4266, "text": "Waxaa ka dhumey waxay ahaayeen kuwo (baryo) caabuda horey, waxayna yaqiinsadeen inuusan u sugnaan gargaare."}, {"id": 496749, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4267, "text": "Dadku kama daalo baryada khayrka, hadduu taabto sharra wuu quusasha badanyahay."}, {"id": 496750, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4268, "text": "Haddaan dhadhansiinno naxariis xagganaga ka timid dhib taabtay kadib, wuxuu dhahaa kan anaa iska leh mana u maleeynaayo inay Saacadda (qiyaame) kici, haddii layceliyo Eebahayna waxaan ku leeyahay agtiisa wanaag, dhab ahaan baan ugu warranmeynaa kuwii Gaaloobey waxay camal faleen, waxaana dhabahaan u dhadhansiinaynaa Cadaab daran."}, {"id": 496751, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4269, "text": "Markaan unicmeyno Dadka wuu jeedsadaa oo is dadbaa (daacadduu ka tagaa) markuu taabto dhibna wuxuu soo baxaa isagoo baryo badan."}, {"id": 496752, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4270, "text": "Waxaad ku dhahdaa bal kawarrama hadduu Quraankani xaga Eebe ka yimid markaas aad ka Gaaloowdaan yaa ka baadi badan Ruux Gaalnimo iyo khilaaf fog ku sugan."}, {"id": 496753, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4271, "text": "Waxaanu ku tusin aayaadkanaga jahooyinka iyo Naftooda intey uga caddaato inuu Quraanku xaq yahay, miyuuna ku filleyn inuu waxkaste ogyahay."}, {"id": 496754, "identifier": "so.abduh", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4272, "text": "Waxayna Gaaladu kaga suganyihiin shaki lakulanka Eebe, Eebana waxkasta wuu koobay."}]