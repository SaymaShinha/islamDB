[{"id": 497286, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4847, "text": "Waxaa dhawaatay Saacaddii (Qiyaame), dayixiina wuu dillaacay."}, {"id": 497319, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Hadday Gaaladu arkaan Aayad way ka jeedsadaan waxayna dhahaan waa Sixir daran."}, {"id": 497320, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4849, "text": "Way beeniyeen (Nabiga iyo xaqa) waxayna raaceen Hawadooda, amar kastana wuxuu leeyahay meel uu ku sugnaado."}, {"id": 497321, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4850, "text": "Waxaana ugu yimid dhab ahaan warar ku dheehan waano."}, {"id": 497322, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4851, "text": "Waana Xigmad xeel dheer, maxaysa tari udigidi."}, {"id": 497323, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Ee iskaga jeedso (sugna) Maalinta mid dhawaaqaa uu ugu yeedhi wax daran."}, {"id": 497324, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4853, "text": "Aragooduna wuu dullaysnaan waxayna ka soo bixi Qubuurahooda iyagood moodid Ayax faafay."}, {"id": 497325, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4854, "text": "Iyagoo u dagdagi dhawaaqaha xaggiisa, waxayna dhihi Gaaladu kani waa Maalin daran."}, {"id": 497326, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4855, "text": "Waxaa beeniyay (xaqa) iyaga hortood (Nabi) Nuux Qoomkiisii, waxay beeniyeen Addonkanagii, waxayna dheheen wuu waalanyahay, waana la guulguulay, \u00aboo la canaantay\u00bb."}, {"id": 497327, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4856, "text": "Eebuuna tuugay inuu tabaryaryahay oo u gargaaro."}, {"id": 497328, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4857, "text": "Markaasaan ku furay Albaabbada Samada (Cirka) Biyo badanna ka keenay."}, {"id": 497329, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4858, "text": "Dhulkana Eebaa ka dillaaciyay ilo, wayna kulmeen biyihii amar la qadaray dartiis."}, {"id": 497330, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4859, "text": "Waxaanna ku xambaaray Nabi Nuux (Doon) Looxyo iyo Musbaarro leh."}, {"id": 497331, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4860, "text": "Kuna socota illalintanada si loo abaalmariyo cidda Gaalowday."}, {"id": 497332, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4861, "text": "Waxaan uga tagnay (yuu Eebe yidhi) Calaamad (lagu wantoobo) ee ma jirtaa cid wacdoomi (oo wax xusuusan)."}, {"id": 492657, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4862, "text": "Seese yahay Caddib Eebe iyo digiddiisu."}, {"id": 497333, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4863, "text": "Waan fududaynnay Quraanka xusuus darteed ee ma jirtaa cid wacdoomi."}, {"id": 497334, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4864, "text": "Caadna way beenisay (xaqii), seese noqotay caddibaaddii Eebe iyo u digitiisii."}, {"id": 497335, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4865, "text": "Waxaan ku dirray korkooda dabayl qabaw daran Maalin baasaysan dhexdeed oo Joogta ah."}, {"id": 497336, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4866, "text": "Oo dadka siibaysa sida Timir gunteedii la rujiyay."}, {"id": 497337, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4867, "text": "Seese ahaa caddibaaddii Eebe iyo u digiddiisii."}, {"id": 497338, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4868, "text": "Waxaan ufududaynay Quraanka xusuus darteed ee ma jirtaa cid ku wacdoomi."}, {"id": 497339, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4869, "text": "Waxaa beenisay (xaqii) loogu digay Thamuud."}, {"id": 497340, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4870, "text": "Waxayna dheheen ma Ruux dad ah oo naga mid ah yaan raacaynaa, markaas waxaan ku sugannahay baadi iyo dhibaato."}, {"id": 497341, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4871, "text": "Ma isagoo na dhexjoogaa waxyi lagu soo dajiyay, saas ma aha ee waa beenbadane kibirlow ah."}, {"id": 497342, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4872, "text": "Way ogaan doonaan barri cidda beenbadane kibirlow ah."}, {"id": 497343, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4873, "text": "Hashii Annagaa soo bixinnay Jirrabkooda dartiis, Ee sug kuna samir."}, {"id": 497344, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4874, "text": "Una warran in biyuhu yihiin qayb (iyaga iyo Hasha), cidbana maalin gaara leedahay."}, {"id": 497345, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4875, "text": "Waxay u dhawaaqeen saaxiibkood hashiina wuu dilay."}, {"id": 497346, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4876, "text": "Seese ahaa caddibaaddii Eebe iyo u digiddiisii."}, {"id": 497347, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4877, "text": "Dhawaaq kaliyaan ku siidaynay, waxayna la mid noqdeen jabad burburay."}, {"id": 497348, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4878, "text": "Quraankana waan u fududaynay xusuus darteed ee ma jirtaa cid ku wacdoomi."}, {"id": 497349, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Way beeniyeen Qoomkii (NabiLuudh) u digiddii."}, {"id": 497350, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4880, "text": "Waxaana ku dirray korkooda Dabayl Dhagaxyo wadaa, waxaanse ka nabadgalinnay (Luudh) ehelkiisii (rumeeyay) goor Aroor ah."}, {"id": 497351, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4881, "text": "Nicmad xaggannaga ka timid darteed saasaana u abaalmarinnaa ciddii mahadisa (Eebe)."}, {"id": 497352, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4882, "text": "Nabi-Luudh wuxuu uga digay qoomkiisii qabashadannada (daran), wayse shakiyeen digiddaas."}, {"id": 497353, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4883, "text": "Waxayna dooneen martidiisii (inay xumeeyaan) markaasaan indhaha tirray, waxaana lagu yidhi dhadhamiya caddibaadda Eebe iyo digiddiisa."}, {"id": 497354, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4884, "text": "Waxaa ku waabariistay caddibaad sugan."}, {"id": 497355, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4885, "text": "Waxaana lagu yidhi dhadhamiya caddibaadda Eebe iyo digiddiisa."}, {"id": 497356, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4886, "text": "Waan fududaynay Quraanka xusuus darteed ee ma jirtaa cid wacdoomi."}, {"id": 497357, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4887, "text": "Fircoon iyo ehelkiisii waxaa uyimid digid."}, {"id": 497358, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4888, "text": "Wayna beeniyeen Aayaadkannagii dhammaanteed, markaasaan u qabannay si xoog iyo awood leh."}, {"id": 497359, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4889, "text": "Ma Gaaladinnan baa ka khayr badan kuwaas mase dambi la'aanbaa idiin ku sugan Kutubta."}, {"id": 497360, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4890, "text": "Mase waxay dhihi koox guulaysan yaanahay."}, {"id": 497361, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4891, "text": "Waa la jabin kooxda, Dabadayna jeedin."}, {"id": 497362, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4892, "text": "Saacadda (Qiyaame) yaa ballan u ah, Saacadduna iyadaa daran oo khadhaadh."}, {"id": 497363, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4893, "text": "Kuwa dambiilayaasha ah baadi iyo fogaansho yay ku suganyihiin."}, {"id": 497364, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4894, "text": "(Waxaana lagu dhihi) Maalinta Naarta WajiWaji loogu ridi dhadhamiya taabashada Saqara."}, {"id": 492658, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4895, "text": "Wax kasta waxaan ku abuurray qadar."}, {"id": 497365, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4896, "text": "Amarkannaguna ma aha mid mooyeen sida Ilbidhiqsi."}, {"id": 497366, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4897, "text": "Dhab ahaan baan u halaagnay kuwa idin la mid ah, ee wax waantoobi ma jiraa."}, {"id": 497367, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4898, "text": "Wax kastoo dadku falo wuxuu ku suganyahay Kutubta."}, {"id": 497368, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4899, "text": "Wax kastoo yar ama waynna waa qoranyahay."}, {"id": 497369, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4900, "text": "Kuwa dhawrsadana waxay gali Jannooyin iyo wabiyaal."}, {"id": 497370, "identifier": "so.abduh", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4901, "text": "Meel la fadhiisto oo xaq ah oo Eebaha awoodda leh agtiisa ah (yaa u sugnaatay)."}]