[{"id": 495891, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3410, "text": "waxay ku tusin mucjisada Quraanka (sidii soo hormartay)."}, {"id": 495892, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3411, "text": "waxaa lagaga adkaaday Ruum."}, {"id": 495893, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3412, "text": "Dhulkii dhawaa, intii laga adkaaday ka dibna way adkaan."}, {"id": 495894, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3413, "text": "dhawr sano dhexdeed, amarkana Eebaa iska leh horey iyo diba, Maalintaasayna ku farxi Mu'miniintu."}, {"id": 495895, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3414, "text": "Gargaarka Eebe, wuxuuna u gargaari cidduu doono, waana adkaade naxariista ah."}, {"id": 495896, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3415, "text": "waa yabooha Eebe, mana baajiyo Eebe yaboohiisa, laakin Dadka badankiisu ma oga."}, {"id": 495897, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3416, "text": "waxay yaqaanaan Dadku muuqaalka nolosha addunyo Aakhirose way halmaansanyihiin (ka jaahil yihiin)."}, {"id": 495898, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3417, "text": "miyeyna ka fikirin naftooda Inuusan Eebe Samaawaadka iyo Dhulka iyo waxa u dhaxeeya u abuurin waxaan xaq iyo muddo magacaaban ahayn, wax badan oo dadka ka mid ahna waxay ka Gaaloobi la kulanka Eebahood."}, {"id": 495899, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3418, "text": "meyeyna ku soconin Dhulka oy eegan siday ahaatay cidhibtii kuwii ka horreeyey (Gaaladii Makaad) waxay ahaayeen kuwo ka daran iyaga kana xoog badan, Dhulkana waybeerteen wayna Dhiseen Dhulka intay dhiseen wax ka badan. waxayna ula timid Rasuulladoodii Xujooyin, Eebana ma aha mid wax dulmiya, laakiin iyagaa naftooda dulmiyey."}, {"id": 495900, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3419, "text": "markaasay noqotay cidhibtii kuwii xumaanta falay, xumaan iyo (Jahannamo) beenintay aayadaha Eebe beeniyeen darteed, waxayna ahaayeen kuwo aayadaha ku jees jeesa."}, {"id": 495901, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3420, "text": "Eebaa billaahay (Abuurka) Khalqiga isagaana soo celin, xaggiisaana laydiin celin."}, {"id": 495902, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3421, "text": "Maalintay kici Saacaddu (Qiyaamadu) way aamusi dambiilayaashu (ooy quusan)."}, {"id": 495903, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3422, "text": "ugumana sugnaanin kuway Eebe lawadajiyeen (Cibaadada) Shafeeco waxayna noqon kuwo diida Shurakadoodii."}, {"id": 495904, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3423, "text": "Maalintay kici Saacadu waa maalin ay kala tagi Dadku."}, {"id": 495905, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3424, "text": "kuwa rumeeyey (xaqa) oo camalfiicanfalay Beer Janno yaa lagu faraxgaliyaa."}, {"id": 495906, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3425, "text": "kuwa Gaaloobayse oo beeniyey aayaadkanaga iyo lakulanka aakhiro kuwaas waxaa lagu kulmin Cadaabka.,"}, {"id": 495907, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3426, "text": "Eebaa nasahan (Ceeb ka fog) markaad galabaysanaysaan iyo markaad waabariisanaysaan."}, {"id": 495908, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3427, "text": "mahadna isagaa ku leh Samooyinka iyo Dhulka dhexdooda iyo Habeenkii iyo Duhurkiiba."}, {"id": 495909, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3428, "text": "kana soobixiya wax nool wax dhintay wax dhintayna wax nool, nooleeyana Dhulka intuu dhintay ka dhib, saasaana laydiin soo bixin."}, {"id": 495910, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3429, "text": "waxaana ka mid ah Calaamooyinkiisa inuu idinka abuuray Carro markaasaad noqoteen Dad dhaqdhaqaaqi (Faafi)."}, {"id": 495911, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3430, "text": "waxaa ka mid ah Calaamooyinkiisa inuu idiinka abuuray naftiina Haween si aad isugu xasishaan, yeelayna dhexdiina isjacayl iyo naxariis, taasina waxaa ugu sugan Calaamad qoomkii fikiri."}, {"id": 495912, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3431, "text": "Waxaa ka mid ah Aayaadka ku tusin (Qudrada) Eebe samooyinka uu abuuray iyo Dhulka iyo kala duwanaanta Afkiina (Luuqada) iyo midabkiinaba, taasina calaamad yaa ugu sugan kuwa Cilmiga leh."}, {"id": 495913, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3432, "text": "waxaa ka mid ah Aayadka tusin (Qudrada) Eebe Hurdadiina Habeenkii iyo Maalintii iyo doonidiina Fadliga Eebe, taasna calaamad yaa ugu sugan kuwa wax maqli."}, {"id": 495914, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3433, "text": "waxaa ka mid ah Aayaadka ku tusin (qudra) Eebe Hillaaca uu idin tusiyo, Cabsi iyo Doonid darteed, iyo Samada uu idiinka soo dajiyo Biyo, kuna nooleeyo Dhulka intuu dhintay ka dib, taasina Calaamado yaa ugu sugan kuwa wax kasi."}, {"id": 495915, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3434, "text": "waxaa ka mid ah Aayaadka ku tusin (qudrada) Eebe inay Samada iyo Dhulku ku taaganyihiin amarka Eebe, markuu Dhulka idiinka yeedhana (soobixinta) aad durba soo baxdaan."}, {"id": 495916, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3435, "text": "Eebaa iska leh waxa Samooyinka iyo Dhulkaba ku sugan, isagayna u khushuucaan (u hogaansamaan)."}, {"id": 495917, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3436, "text": "Eebe waa kan billaabay abuurka Khalqiga, hadana soo celin wayna u fududahay arrintaasu, Eebaana sifo sare ku muta Samooyinka iyo Dhulka, waana adkaade falsan."}, {"id": 495918, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3437, "text": "Eebe wuxuu idiin yeeley tusaale naftiina ah, waxaad hanataan miyey idinlawadaagaan wax laydinku arzuqay, ood si isku mid ah u maamulaysaan, ugana cabsanaysaan sidaad isaga cabsataan, saasuuna u caddayn aayaadka ciddii wax kasi."}, {"id": 495919, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3438, "text": "haseyeeshee waxay raaceen kuwii dulmilowga ahaa Hawadooda iyagoon wax ogayn, yaa hanuunin kara ruux Eebe dhumid u qadaray, wax u gargaarana lahayn."}, {"id": 495920, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3439, "text": "ee Wajigaaga u jeedi Diinta adoo toosan (Diintu) waa abuuridda Eebe ee Dadka uu ku abuuray, wax badali kara abuurka Eebana ma jiro, saasina waa Diinta toosan laakiin dadka babidiis ma oga."}, {"id": 495921, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3440, "text": "idinkoo Eebe u noqon, kana yaaba, Salaadana ooga, Galadana ha ka mid noqonina."}, {"id": 495922, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3441, "text": "ee Diintooda ku kala tagay noqdayna kooxo, Xisbi walba uu ku farxi waxa agtiisa ah."}, {"id": 495923, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3442, "text": "hadduu Dadka taabto dhib waxay baryaan Eebahood iyagoo u noqon, markuuse dhadhansiiyo naxariis qayb iyaga ka mid ah waxay wax lawadaaijyaan Eebahood."}, {"id": 495924, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3443, "text": "si ay uga gaaloobaan waxaan siinney, iska raaxaysta, waadna ogaandoontaan Cidhibta raaxadaasi."}, {"id": 495925, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3444, "text": "mise waxaan ku soo dejiney xujo, oo ku hadli waxay Eebe la wadaajin."}, {"id": 495926, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3445, "text": "haddaan dhadhansiino dadka naxariis way ku farxi \u00abkibraan\u00bb hadduu dhibku dhacana waxay hor marsadeen darteed way quustaan."}, {"id": 495927, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3446, "text": "miyeyna arkayn in Eebe u waasiciyo rizqiga cidduudoono, kuna cidhiidhyo (Cidduu doono) arrintaasna Calaamooyinbaa ugu sugan kuwa rumeyn (Xaqa)."}, {"id": 495928, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3447, "text": "ee Qaraabada sii xaqeeda iyo Miskiinka iyo Musaafurka, saasaa u khayroon kuwa dooni Wajiga Eebe, (u dhawaanshahiisa) kuwaasina waa kuwa liibaanay."}, {"id": 495929, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3448, "text": "waxaad bixisaan oo Ribo ah si aad uga badsataan xoolaha dadka ma badnaado Eebe agtiisa, wixiise aad bixisaan oo Zako (xalaal ah) idinkoo ku dooni Wajiga Eebe kuwaas waa loo laablaabi."}, {"id": 495930, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3449, "text": "Eebe waa kan idin abuuray Iladana idin arzaaqay hadana idin Dili, hadana idin soo noolayn, majiraa shurakadiina mid fali sidaas wax ka mid ah, Eebaa ka nasahana kana sareeya waxay la wadaajin."}, {"id": 495931, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3450, "text": "xumaanbaa ka daahirtay Barri iyo Badba, waxay kasbatay Gaemo Dad dardiis, si Eebe u dhadhansiiyo waxay camalfaleen qaarkiis, inay noqdaan."}, {"id": 495932, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3451, "text": "waxaad dhahdaa ku socda Dhulka fiiriyana siday noqotay cidhibtii kuwii hore, wuxuu ahaa badankoodu Mushrikiin."}, {"id": 495933, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3452, "text": "ee Wajigaaga Diinta toosani (Islaamka) u jeedi, maalin aan la eelin karin ka hor, Eebe xaggiisana ka iman, Maalintaas Dadku way kala tagi."}, {"id": 495934, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3453, "text": "ruuxii Gaalooba isagay (dhibi) Gaalimadiisu, ruuxii camal wanaagsan fala Naftiisa yuu u goglay."}, {"id": 495935, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3454, "text": "si Eebe u abaalmariyo kuwa rumeeyey xaqa oo camal wanaagsan falay fadligiisa, Eebana ma jeela Gaalo."}, {"id": 495936, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3455, "text": "Calaamaadka Eebe ku tusin waxaa ka mid ah inuu dabaylaha diro iyagoo ku bishaarayn (Roob) iyo in Eebe idin dhadhansiiyo naxariistiisa iyo in doontu ku socoto amarkiisa iyo inaad dalabtaan fadligiisa, markaas aad Eebe ku mahdisaan."}, {"id": 495937, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3456, "text": "waxaan dhab ahaan ugu diray hortaa Rasuulo qoomkoodii, waxayna ulayimaadeen xujooyin waana ka aarsanay kuwii dambi Falay, waxaana korkanaga ah u gargaarka Mu'miniinta."}, {"id": 495938, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3457, "text": "Eebe waa kan diray Dabaylaha oo markaas kicisa Daruuro kuna fidiya Samada siduu doono kana yeela Goosimo aad aragtid Roobka oo ka soobixi dhexdeeda, markaasuu Eebe Roobka siiyaa Ciduu doono oo adoomadiisa ka mid ahna markaasay bishaaraystaan."}, {"id": 495939, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3458, "text": "waxayna ahaayeen soo dajinta Roobka ka hor kuwo Quusan."}, {"id": 495940, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3459, "text": "ee day naxariista Eebe raadkeeda sida Eebe, ugu nooleeyo dhulka intuu dhintay ka dib Eebaha wuxuu nooleeyaa wixii dhintay wax walbana waa arkaa."}, {"id": 495941, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3460, "text": "haddaan ku diro Dabaylo (daran) Beertooda ay arkaan iyadoo doorsoon, waxay markaas ka dib noqdaan kuwo Gaalooba."}, {"id": 495942, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3461, "text": "adigu wax ma maqashiin kartid kuwo dhintay, Mid dhagala' oo jeedsadayna wax ma maqashiin kartid."}, {"id": 495943, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3462, "text": "ruux Indha la'na baadi kama hanuunin kartid, ma maqashiinkartid (xaqa) ruuxaan rumeynin Aayaadkanaga oo muslimiin ahayn."}, {"id": 495944, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3463, "text": "Eebe waa kan idin ka abuuray Tabaryare, hadana idinka yeelay Tabaryare ka dib xoog, hadana xoog dabadi idinka yeelay Tabaryari iyo Gabow, wuxuu doono yuu abuuraa, waana wax walba oge, wax walba karc (oo uu doono)."}, {"id": 495945, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3464, "text": "maalinta Qiyaamadu kacdo waxay ku dhaartaan dambiilayaashu inayan nagaanin Saacad mooyee, saasaana (xaqa) looga leexiyey."}, {"id": 495946, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3465, "text": "kuwii waxay dheheen cilmiga iyo Iimaanka la siiyey waad ku nagaateen Qadarka Eebe tan iyo maalinta soobixinta, kanna waa maalintii soobixinta, laakiin waxaad ahaydeen kuwo aan ogayn."}, {"id": 495947, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3466, "text": "Maanta (Qiyaamada) ma anfacdo kuwii dulmiga falay cudur daar iyo toobad keen."}, {"id": 495948, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3467, "text": "dhab ahaanbaan ugu caddaynay Dadka Quraankan dhexdiisa tusaale kasta, haddaadna ula timaado aayad waxay dhihi kuwii Gaaloobay waxaan baadhil (xumaan) ahayn kuma sugnidin."}, {"id": 495949, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3468, "text": "saasaa Eebe u daboolaa Quluubta kuwaan wax ogayn (wax anfaca)."}, {"id": 495950, "identifier": "so.abduh", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3469, "text": "ee samir yabooha Eebe waa xaqe, yeyna ku fududayn kuwaan yaqiinsanayn (Xaqa)."}]