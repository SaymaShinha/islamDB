[{"id": 497636, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5164, "text": "Waxaa Ilaahay nasaha (weyneeya) waxa Cirka iyo Dhulka ku sugan, Eebana waa adkaade falsan."}, {"id": 497637, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5165, "text": "dadka xaqa rumeeyayow maxaad u sheegaysaan wax aydaan falaynin."}, {"id": 497638, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5166, "text": "Waxaa Cadho weyn Eebe agtiisa ku ah inaad sheegtaan wax aydaan falaynin."}, {"id": 497639, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5167, "text": "Eebe wuxuu jecel yahay kuwa ku dagaallama Jidkiisa (Jihaadka) iyagoo af ah oo aad mooddo dhismo laysku dhejiyey (midnimadooda)."}, {"id": 497640, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5168, "text": "Xusuusta markii (Nabi) Muuse ku yidhi dadkiisii maxaad ii dhibaysaan idinkoo og inaan Rasuul Eebe idiin ahay, markay xaqii ka iisheen yaa Eebana sii iilay qalbigooda, Ilaahayna ma hanuuniyo kuwa faasiqiinta ah."}, {"id": 497641, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5169, "text": "xusuusta marka (nabi) Ciise ibnu Maryama ku yidhi Banii Israa'iil waxaan ahay Rasuul Eebe idiin soo diray oo rumayn Kitaabkii Tawraad ee horreeyey, kuna bishaarayn Rasuul gadaashayda imaan oo Magaciisuna yahay \u00abAxmed\u00bb, markuu xuja cad ula yimidna waxay dheheen kani waa sixir cad."}, {"id": 497642, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5170, "text": "yaa ka dulmi badan ruux ku been abuurtay Ilaahay isagoo loogu yeedhi Islaamka, Eebe ma hanuuniyo dadka daalimiinta ah."}, {"id": 497643, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5171, "text": "Waxay doonayaan (Gaaladu) inay Nuurka (xaqa) Eebe ku bakhtiiyaan Afkooda Eebena wuu taam yeeli nuurkiisa haba naceen Gaaladuye."}, {"id": 497644, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5172, "text": "Eebe waa kan diray Rasuulkiisa isagoo hanuun wada iyo Diin xaq ah si uu (Eebe) Diimaha oo dhan uga sare mariyo haba naceen Mushrikiintuye (Gaaladu)."}, {"id": 497559, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5173, "text": "Kuwa xaqa rumeeyow ma idin tusiyaa ganacsi idinka koriya Cadaab daran."}, {"id": 497645, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5174, "text": "Waxaad rumaynaysaa Ilaahay iyo Rasuulkiisa waxaadna ku dagaalamaysaan Jidkiisa xaqa ah Xoolihiina iyo Naftiina idinkoo u huri, saasaana idiin khayr roon haddaad tihiin kuwo wax og."}, {"id": 497646, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5175, "text": "Markaas Eebe dambigiinuu dhaafi wuxuuna idin gelin Jannooyin ay dhex socdaan Webiyaal iyo Guryo wanaagsan oo ku dhexyaalla Jannada Cadnin (nagaanshaha), taasina waa liibaan weyn."}, {"id": 497647, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5176, "text": "Arrin kale ood jeceshihiinna (wuu idiin siin) waa gargaarka Eebe iyo fatxi (guul) dhaw, kuwa xaqa rumeeyeyna u bishaaree."}, {"id": 497648, "identifier": "so.abduh", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5177, "text": "dadka xaqa rumeeyow noqda gargaarayaasha Eebe sidii kuwii Nabi Ciise ibnu Maryama Saaxiibadiis ahaa (Xawaariyintii) ee uu ku yidhi yaa Eebe iila gargaara, markaas ay dheheen annagaa ah gargaarayaashii Eebe, ka dib ay xaqa rumaysay koox Banii Israa'iil ka mid ah, kooxna ay ka gaalawday; Eebena ayiday (xoojiyey) kuwii xaqa rumeeyey (kana adkaadeen Cadawgoodii) oo iyagu ka muuqdeen (ka taliyeen) dhulkii."}]