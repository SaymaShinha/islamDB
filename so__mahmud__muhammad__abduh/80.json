[{"id": 498224, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5759, "text": "Wajiguu u uruuriyay oo jeedsaday (Nabigu)."}, {"id": 498225, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5760, "text": "Inuu u yimid kii Indhaha la'aa."}, {"id": 498226, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5761, "text": "Miyaad ogtahay inay u dhawdahay inuu hanuuno oo Daahir noqdo."}, {"id": 498227, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5762, "text": "Ama uu xusuusto oy xusuustu anfacdo."}, {"id": 498228, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5763, "text": "Ruuxiise is deeqtoonaysiiya (islana waynaada)."}, {"id": 498229, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5764, "text": "Ma; Adaa u bambixi."}, {"id": 498230, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5765, "text": "Maxaa kaa saaran hadduusan hanuunin."}, {"id": 498231, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5766, "text": "Ruuxiise kuu yimaadda isagoo dagdagi."}, {"id": 498232, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5767, "text": "Oo Eeba ka yaabi."}, {"id": 498233, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5768, "text": "Miyaad ka shuqloomi."}, {"id": 498234, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5769, "text": "Saas ma; aha ee Aayadahani waa waano."}, {"id": 498235, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5770, "text": "Ruuxii doonana wuu ku waana qaadan."}, {"id": 498236, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5771, "text": "Waxay ku sugantahay (waanadu) Kutub Sharaf leh."}, {"id": 498237, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5772, "text": "Oo la kor yeelay lana Daahiriyay."}, {"id": 498238, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5773, "text": "Oy Gacanta ku hayaan Malaa'ig (wax qorta)."}, {"id": 498239, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5774, "text": "Sharafna leh oo baarri ah."}, {"id": 498240, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5775, "text": "Gaalo halla lacnado Gaalnima badanaa."}, {"id": 498241, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5776, "text": "Muxuu Eebe ka abuuray kuwaas."}, {"id": 498242, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5777, "text": "Dhibicbuu ka abuuray wuuna ekeeyay."}, {"id": 498243, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5778, "text": "Jidkana wuu u fudaydiyay."}, {"id": 498244, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5779, "text": "Markaasuu dilaa Qabrina ku Asturaa."}, {"id": 498245, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5780, "text": "Markuu doonana wuu soo bixin."}, {"id": 498246, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5781, "text": "Saas ma; aha (ee Dadku) wixii la faray ma yeelin."}, {"id": 498247, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5782, "text": "Dadku ha fiiriyo Cuntadiisa."}, {"id": 498248, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5783, "text": "Inaan Roob ku soo shubno."}, {"id": 498249, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5784, "text": "Dhulkana markaas aan jeexno."}, {"id": 498250, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5785, "text": "Kana soo bixinno Midho."}, {"id": 498251, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5786, "text": "Iyo cinab iyo Daaqa (Xoolaha)"}, {"id": 498252, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5787, "text": "Iyo Zeytuun iyo Timir."}, {"id": 492664, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5788, "text": "Iyo Beerow waawayn."}, {"id": 492666, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5789, "text": "Iyo Faakiho (khudaar) iyo Daaq."}, {"id": 492665, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5790, "text": "Si aad ugu intifaacdaan idinka iyo xoolihiinnu."}, {"id": 492667, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5791, "text": "Markay Qayladii darrayd timaaddo (Suurka)."}, {"id": 492669, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5792, "text": "Waa maalinta Ruuxu ka carari Walaalkiis."}, {"id": 492668, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5793, "text": "Iyo Hooyadiis Iyo Aabihiis."}, {"id": 492671, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5794, "text": "Iyo Haweeneydiisii iyo Carruurtiisii."}, {"id": 492672, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5795, "text": "Ruux walbana Maalintaas waxaa shuqlin xaalkiisa."}, {"id": 492673, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5796, "text": "Wajiyaal Maalintaas way Nuuri (Farxi)."}, {"id": 492670, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5797, "text": "Wayna Qosli oy bishaaraysan."}, {"id": 492675, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5798, "text": "Wajiyaalna Maalintaas waxaa ka muuqan Dulli."}, {"id": 492676, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5799, "text": "Waxaana dabooli Madow (iyo naxdin)."}, {"id": 492677, "identifier": "so.abduh", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5800, "text": "Kuwaasina waa Gaalada Faasiqiinta ah (aadna u xun)."}]