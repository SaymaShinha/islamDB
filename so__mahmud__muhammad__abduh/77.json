[{"id": 498090, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5623, "text": "Waxaan ku dhaartay Dabaysha wanaaga lala Diro."}, {"id": 498091, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5624, "text": "iyo Dabaysha sida daran u dhabata."}, {"id": 498092, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5625, "text": "Iyo Dabaysha roobka kicisa."}, {"id": 498093, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5626, "text": "Iyo malaa'igta Xaqa iyo Baadilka kala Bixisa."}, {"id": 498094, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5627, "text": "iyo malaa'igta waxyiga soo Dajisa."}, {"id": 498095, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5628, "text": "Si Dadka loo waaniyo looguna digo."}, {"id": 498096, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5629, "text": "Ee waxa dadow Eebe ku Yaboohay wuu dhici."}, {"id": 498097, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5630, "text": "Markase xiddiguhu Nuurku ka tago."}, {"id": 498098, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5631, "text": "Oo samadu Dildillaacdo."}, {"id": 498099, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5632, "text": "Oo Buuraha la Rujiyo."}, {"id": 498100, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5633, "text": "Oo Rasuulladii la kulmiyo (iyo Dadkoodii)."}, {"id": 498101, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5634, "text": "Maalion wayn oo loo Qabtay Darteed."}, {"id": 498102, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5635, "text": "In dadka la kala Bixiyo Dartiis."}, {"id": 498103, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5636, "text": "Maalinta kala Bixinta ma Taqaannaa."}, {"id": 498104, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5637, "text": "Halaag wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniyay."}, {"id": 498105, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5638, "text": "Miyaannaan Halaagin Ummadihii hore."}, {"id": 498106, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5639, "text": "Haddana Aannaan Raccinhaynin kuwii Dambe."}, {"id": 498107, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5640, "text": "Saasaana ku Falaynaa Dambiilayaasha."}, {"id": 498108, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5641, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniya."}, {"id": 498109, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5642, "text": "Miyaannaan Idinka Abuurin biyo tabar yar."}, {"id": 498110, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5643, "text": "Markaasna yeellay meel ilaashan."}, {"id": 498111, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5644, "text": "Tan iyo Muddo la yaqaanno."}, {"id": 498112, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5645, "text": "Annagaa wax Qaddarray, wax Qaddarana u Fiican."}, {"id": 498113, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5646, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniya."}, {"id": 498114, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5647, "text": "Miyaanaan ka yeelin Dhulka kafaalaqaade."}, {"id": 498115, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5648, "text": "Nolol iyo Geeriba."}, {"id": 498116, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5649, "text": "Waxaana yeellay Dhulka Dhexdiisa buuro sugan oo waawayn, waxaana idinka Waraabinnay biyo macaan."}, {"id": 498117, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5650, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniyay."}, {"id": 498118, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5651, "text": "Aada gaalooy cadaabkii xaggiisa aad Beenin Jirteen."}, {"id": 498119, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5652, "text": "Aada hoos (Qiiba naarta) oo saddex madax ah."}, {"id": 498120, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5653, "text": "Oon hoosinayn, ololla (Kulaylna) wax ka tarayn."}, {"id": 498121, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5654, "text": "Waxayna Naartu tuuri Dhimbilo daran ood Mooddo daar."}, {"id": 498122, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5655, "text": "Ama aad Mooddo geel madaw ah."}, {"id": 498123, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5656, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniyay."}, {"id": 498124, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5657, "text": "Taasina waa maalin aynan hadlaynin Dadku."}, {"id": 498125, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5658, "text": "Lamana idmo si Ay u cudurdaartaan."}, {"id": 498126, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5659, "text": "Halaagna wuxuu u sugnaaday maalintaas kuwa xaqa Beeniyay."}, {"id": 498127, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5660, "text": "Maalintan waa maalintii kala Bixinta, Waana idin kulminnay idinka iyo kuwii Horreeyayba."}, {"id": 498128, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5661, "text": "Ee Haddaad wax Dhagrikartaan i Dhagra."}, {"id": 498129, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5662, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniyay."}, {"id": 498130, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5663, "text": "Kuwa Eebe ka Dhawrsada waxay ku sugnaan hoos iyo ilo;"}, {"id": 498131, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5664, "text": "(Waxayna Cuni) khudaartay Doonaan."}, {"id": 498132, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5665, "text": "Waxaana Lagu dhihi Cuna oo cabba Idinkoo shifaysan Camalkiinnii Dartiis."}, {"id": 498133, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5666, "text": "Sasaana ku abaalMarinaynaa kuwa wanaagga fala."}, {"id": 498134, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5667, "text": "Halaagna wuxuu u Sugnaaday kuwa xaqa Beeniya."}, {"id": 498135, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5668, "text": "(Gaalada waxaa lagu Dhihi) Cuna oo Raaxaysta wax yar, waxaad tihiin Dambiilayaale;"}, {"id": 498136, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5669, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniyay."}, {"id": 498137, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5670, "text": "Markii la yidhaahdo Gaalada tukada ma tukadaan."}, {"id": 498138, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5671, "text": "Halaagna wuxuu u sugnaaday Maalintaas kuwa xaqa Beeniya."}, {"id": 498139, "identifier": "so.abduh", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5672, "text": "Hadalkeebay Quraanka Dabadiis Rumaynhayaan."}]