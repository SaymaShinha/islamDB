[{"id": 496090, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3607, "text": "mahad Eebaa iska leh ay awooddiisa kujiraan waxa samooyinka iyo dhulka ku sugan, mahadna isagaa muta aakharo iyo aduunba, waana falsame xeel dheer (waxwalba oge)."}, {"id": 496091, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3608, "text": "wuxuuna ogyahay waxa dhulka gali iyo waxa ka soobixi iyo waxa ka soodagi samada iyo waxa u kori, waana naxariiste dambidhaafa ah."}, {"id": 496092, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3609, "text": "kuwii gaaloobay waxay dhaheen nooma imaaneyso saacadda (Qiyaame), waxaad dhahdaa saas ma aha ee Eebahay baan ku dhartey way idiin imaan, Eebahay waa wax maqan oge, wax darro la egna kagama qarsoona samooyinka iyo dhulka dhexdooda iyo wax waxaas ka yar iyo wax la weyn midna, wax walbana kitaab cad bey ku suganyihiin."}, {"id": 496093, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3610, "text": "Si Eebe u abaal mariyo kuwa rumeeyey (xaqa) oo camal fiican Falay kuwaasna waxay mudan dambi dhaaf iyo risqi wanaagsan."}, {"id": 496094, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3611, "text": "Kuwa u socda iney aayaadkanaga (kaleexdaan) iyagoo u maleyn iney daalin (Eebe) kuwaasi waxay mudan cadaab daran oo xun."}, {"id": 496095, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3612, "text": "Waxey arki kuwii cilmiga la siiyeyna (mu'miniinta) in waxa laga soo dajiyey xagga Eebe xaq yahey kuna hanuunin jidka Eebaha adkaada ee la mahdiyo."}, {"id": 496096, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3613, "text": "Waxey dhaheen kuwii gaaloobey maydin tusinaa nin idiin ka warrami in marka laydinkala geeyo (jidkiina) kala gayn dhamaanteed in abuur cusub leydin abuuri."}, {"id": 496097, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3614, "text": "Ma Eebuu ku been abuuran mase wuu waalanyahay, saas ma aha ee kuwaan rumeynin aakhiro waxay ku sugnaan cadaab iyo baadi fog."}, {"id": 496098, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3615, "text": "Miyeyna arkaynin waxa hortooda ah iyo waxa gadaashooda ah oo samo iyo dhul ah hadaan doono waxaanu la goyn dhulka, mise waxaan ku soo rideynaa goosimo samada kamid ah arintaasna aayadbaa ugu sugan adoonkastoo towbadkeen badan."}, {"id": 496099, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3616, "text": "Dhab ahaanbaan u siinay Nabi Daawuud fadli (dheeraad) xaganaga ah, waxaana nidhi buurahoow ka celceliya tasbiixda, shimbirahoowna (la celceliya tasbiixda,) waxaana u jilcincy birta."}, {"id": 496100, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3617, "text": "Waxaana ku nidhi ka samce dirieyada (dharka dagaalka) sugna tixiddeeda wanaagna fala, waxaad faleeysaana waan arkaa."}, {"id": 496101, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3618, "text": "Nabi suleymaanna waxaan u Sakhirray dabeysha, kallahaadeedu waabil, carraabaheeduna waa bil, waxaan u dureeriney il Nuxaad ah (maar) Jinigana waxaa ka mid ah kuwo ku hor shaqeeya idanka Eebe, ruuxii ka mid ah oo ka iilata amarkanaga waxaan dhadhansiineeynaa cadaabka saciiro."}, {"id": 496102, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3619, "text": "Waxay u sameyn suleymaan wuxuu doono oo mixraabyo (Fooqaq) iyo suurooyin, iyo xeedhooyin dar oo kale ah, iyo dharyo sugan, waxaana ku nidhi fala ehelka Daawuudow mahadnaq, waxaana yar adoomadayda shugriga badan."}, {"id": 496103, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3620, "text": "Markaan geerida ku xukunay Nabi suleymaan wax tusiyey geeridiisa ma jiro oon ahayn aboorka cunay ushiisa, markuu dhacay yaa waxaa u cadaaday jinigii hadey waxa maqan ogyihiin ineyna ku nagaadeen cadaab wax duleeya (shaqadiisa darteed)."}, {"id": 496104, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3621, "text": "Guryihii reer sabaad waxaa ku sugnayd calaamo, Beeraana ka xigay xaga midig iyo xagga bidixba, waxaana Eebe ku yidhi cuna risqiga Eebihiin kuna mahadiya, waa balad wanaagsan iyo Eebe dambi dhaafbadane."}, {"id": 496105, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3622, "text": "Wayse jeedsadeen, waxaana ku diray daad daran, waxaana ku badaley labadoodii beerood beero kale oo geedo khadhaadh iyo qoryo iyo sidri yar ah."}, {"id": 496106, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3623, "text": "Sidaasaana ku abaalmarinay gaalnimadoodii darteed, abaalmarin xunna waxaan abaal marinaa kuwa gaalnimada badan."}, {"id": 496107, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3624, "text": "Waxaana yeeley dhexdooda iyo magaalooyinkii barakeysnaa (Yaman iyo Qudus) magaalooyin muuqdo waxaana qadaray socodkooda, waxaana ku nidhi ku socda dhexdeeda habeen iyo maalinba idinkoo aamin ah."}, {"id": 496108, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3625, "text": "Waxayse dhaheen Eebow noo kala fogee socodka, wayna dulmiyeen naftooda, waxaana ka yeelay wax lagu sheekeysto. waana kala goyney, sidaasna waxaa ugu sugan calaamo ruux kastoo Samirbadanoo Mahadin badan."}, {"id": 496109, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3626, "text": "Dhab ahaan waxaa ku rumoobey gaalada korkooda Ibliis Malihiisii, wayna raaceen wax yar oo ka mid ah oo mu'min ah mooyee."}, {"id": 496110, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3627, "text": "Mu'miniinta awood kuma laha ibliis wax kalana uma aanaan yeelin inay kala muuqato ruuxa rumeeyey Aakhiro iyo kan ka shakisan xaggeeda. Eebahaana wax walba waa ilaaliye."}, {"id": 496111, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 372, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3628, "text": "Waxaad dhahdaa u yeedha kuwaad sheegateen Eebe ka sokow (iney ilaahyihiin) kuma hantaan darro wax la eb samooyinka iyo dhulka midna, wax wadaag ahna \u00abshirkadna\u00bb kumalch dhexdooda Eebe wax u kaalmeeyana kuma laha khaliqiga."}, {"id": 496112, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3629, "text": "Kumana anfacdo shafeeco Eebe agtiisa, ruuxuusan idmin. Markase laga faydo argagaxa quluubtooda waxay dhihi muxuuyidhi Eebihiin, markaasay dhihi kuwa shafeeci wuxuu yidhi xaq waana Eebaha sarreeya. ee weyn."}, {"id": 496113, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3630, "text": "Waxaad dhahdaa yaa idinka arsaaqa samooyinka iyo dhulka, waxaad dhahdaa Eebe, anaga iyo idinka mid unbaa hanuunsan ama baadicad ku sugan."}, {"id": 496114, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3631, "text": "Waxaad dhahdaa laydin warsan mahayo waxaan dambaabney anagana nalama warsado waxaad camalfaleysaan."}, {"id": 496115, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3632, "text": "Waxaad dhahdaa waxaa ina kulmin Eebaheen, markaasuu ina kala xukumin si xaqa ah. waana caadil wax walba og."}, {"id": 496116, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3633, "text": "waxaad dhahdaa i tusiya kuwaas Eebe haleeshiiseen oo shurako ah (wax la wadaago) saas ma aha ee Eebe waa adkaade falsan."}, {"id": 496117, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3634, "text": "Kuu maanaan dirin waxaan dadkoo dhan ahayn, adoo u bishaaraynaya Mu'miniinta, una digi gaalada, laakiin dadka badidiisu ma oga."}, {"id": 496118, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3635, "text": "Waxayna dhihi waa goorma yaboohaasi (soobixinta) hadaad run sheegaysaan."}, {"id": 496119, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3636, "text": "Waxuud dhahdaa waxaa idiin sugnaaday Waqti aydaan saacad ka dib dhacaynin kana hor dhacaynin."}, {"id": 496120, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3637, "text": "Waxay dheheenna kuwii galoobay rumeynmayno quraankan iyo kuwii ka horreeyey midna, hadaad arki labayd daalimiinta oo taagan Eebe agtiisa (qiyaamada) oo qaarkood qaarka kale hadal u celin ooy ku dhihi kuwii raaciyadda ahaa kuwii madaxda ahaa ee isla wayna, hadaydaan jirin waxaan ahaan laheyn Mu'miniin. (waxaad arki laheyd arin weyn oo lala yaabo)."}, {"id": 496121, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3638, "text": "Kuwii iskibriyey (Madaxdii) waxay ku dhaheen kuwii tabarta yaraa (Raaciyaddii) ma anagaa idinka celinay hanuunka markuu idiinyimid saas ma ah ee idinkaa danbiilayaal ahaa."}, {"id": 496122, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3639, "text": "Kuwii tabarta yaraa (raaciyaddii) waxay ku dhaheen kuwii iskibriyay (Madaxdii) saas ma aha ee waxaa hanuunka naga celiyay dhagartii Habeen iyo Maalinba markaad nafarayseen inaan Eebe ka gaalowno una yeelo shariigyo, Waxay qarsadeen qoomarno markay arkeen cadaabka, Waxaana yeelay kuwii gaaloobay luquntooda Biro Naar ah. Miyaa laga abaalmarinin gaaladu waxay camal faleen mooyee."}, {"id": 496123, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3640, "text": "Magaalo kasta oo aan dige (Nabi) u dirro waxay dhahaan kuwoodii loo nicmeeyay anagu waxa laydiinku soo diray waan ka gaalownay."}, {"id": 496124, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3641, "text": "Waxayna dhaheen anagaa xoolo iyo Caruurba idinka badan, nalamana cadaabayo."}, {"id": 496125, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3642, "text": "Waxaad dhahdaa Eebehay wuxuu u fidiyaa rizqiga ruxuu doono, wuuna ku cidhidhhiyaa ruxuu doono, laakiin dadka badankiisu ma oga."}, {"id": 496127, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3643, "text": "Ma aha xoolahiinnu iyo caruurtiinnu kuwii noo kiin dhaweeya, hase yeeshee ruuxii rumeeya (xaqa) oo camal fiican cala, kuwaas waxaa u sugnaaday abaalmarin labalaab ah camalkooda dartiis, waxayna ku aamin noqdaan qawlado janno."}, {"id": 496128, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3644, "text": "Kuwa u socda ka (reebidda) aayaadkanaga iyagoo isleh Eebe daaliya, kuwaas waxaa lagu kulmin cadaab."}, {"id": 496129, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3645, "text": "Waxaad dhahdaa Eebehay wuxuu risqiga u fidiyaa cidduu doono oo addoomadiisa ka mid ah, kuu doonana wuu ku cidhiidhyaa, waxaad bixisaan oo kastana Eebaa idiin bedeli, Eebena isagaa u khayroon wax wax arsaaqa."}, {"id": 496130, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3646, "text": "Xusuuso maalinta uu Eebe kulmin dhamaan gaalada, markaas uu ku odhan malaa'igtiisa kuwani ma idinkay idin caabudi jireen."}, {"id": 496131, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3647, "text": "Markaas ay dhihi adaa ceeb ka fog Eebow, adaa gargaarahanaga ah iyaga ka sokay, waxayse caabudi jireen Jinni badidoodna isagay rumaysnaayeen."}, {"id": 496132, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3648, "text": "Maantase \u00abQiyaamada\u00bb uma hanato qaarkiin qaarkakale naefi iyo dhib midna. waxaana ku dhahaynaa kuwii dulmi falay dhadhamiya cadaabka naartaad beeninayseen."}, {"id": 496133, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3649, "text": "Marka lagu akhriyo korkooda aayaadkanaga waxay dhahayaan kani ma aha mid dooni inuu idinka iilo waxay Aabbayowgiin caabudijireen mooyee, waxayna dheheen kan (quraanku) ma aha waxaan beenabuurasho ahayn, waxayna dhaheen kuwii Gaaloobay Xaqu markuu u yimid kani waxaan sixir muuqda ahayn ma aha."}, {"id": 496134, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3650, "text": "Ma aanaan siinin Kutubo ay akhriyaan, umana aanaan dirin xagooda dige kaa horreeyay (Carabta)."}, {"id": 496135, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3651, "text": "Waxay beeniyeen (xaqa) kuwii ka horreeeyayna, mana gaadhin (kuwii Nabiga beeniyay) Toban meelood meel waxaan siinay kuwaas, waxayna beeniyeen Rasuuladaydii, Sayse noqotay ciqaabtaydii (aan ciqaabay kuwaas xaqa beeniyay)."}, {"id": 496136, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3652, "text": "Waxaad dhahdaa Nabiyow mid kaliya yaan idinku waanin inaad u jahaysataan Eebe laba labo iyo mid mid markaasna aad fikirtaan inaan saaxiibkiin \u00abNabiga Muxamed ah\u00bb uusan waallayn, uu digihiinna cadaab daran hortiisna uu yahay,"}, {"id": 496137, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3653, "text": "Waxaad dhahdaa Nabiyow idiinma warsanin ujuuro idinkaase iskalch (haii laydin warsado), Ujuuradayda Eebe unbaa isiin, wax walbana Eebe wuu daalacan."}, {"id": 496138, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3654, "text": "Waxaad dhahdaa Eebehay xaquu soo dajiyaan (Xumaanta ku baaabi'iyaa) waana wax maqan oge badane."}, {"id": 496045, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3655, "text": "Waxaad dhahdaa xaqii waa yimid, baadhilna wa tirtirmay,"}, {"id": 496139, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3656, "text": "Waxaad dhahdaa haddaandhumo waxaad uun u dhumay naftayda, hadaan hanuunana waa wixii Eebe ii waxyooday, sababtiisa Eebe waa maqli dhawe."}, {"id": 496140, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3657, "text": "Hadaad arki lahayd Nabiyow markay argagaxaan gaaladu oo meeley u cararaan jirin oo meel dhaw laga qabto, (waxaad arki lahayd arrin wayn)."}, {"id": 496141, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3658, "text": "Waxay dhihi markaas waxaan rumaynay (xaqa) xagay uga ahaan inay meel fog wax ku qaataan (rumayn)."}, {"id": 496142, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3659, "text": "Dhab ahaanna waa iyagii uga gaaloobay xaqa, horray ay ka tuurayeen waxayan ogayn meel fog."}, {"id": 496143, "identifier": "so.abduh", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3660, "text": "Waana la kala ooday iyaga iyo waxay doonayeen sidii lagu falay kuwii la midka ahaa ee ka horeeyay, waxayna ahaayeen kuwo shaki daran ku sugan."}]