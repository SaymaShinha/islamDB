[{"id": 496956, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4474, "text": "waa soo hormaray macnaheedu."}, {"id": 496957, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4475, "text": "soo dajinta Kitaabka (Quraanka) ah wuxuu ka ahaaday Eebaha adkaada iyo falkan san."}, {"id": 496958, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4476, "text": "Samooyinka iyo Dhulka dhexdiisana aayadbaa ugu sugan Mu'miniinta."}, {"id": 496959, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4477, "text": "abuurkiinna iyo waxa Eebe ku fidin \u00ababuuri\u00bb Dhulka oo wax socda ah aayaadbaa ugu sugan ciddii wax yaqiinsan."}, {"id": 496960, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4478, "text": "iskhilaafka Habeenka iyo Maalinta iyo waxa Eebe ka soodajiyey Samada oo Rizqi ah oo ku nooleeyo Dhulka intuu dhintay ka dib iyo gagadoonka Dabaylaha waxaa ugu sugan aayaad ciddii wax kasi."}, {"id": 496961, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4479, "text": "taasi waa aayaadka Eebe aan ku akhriyeyno korkaaga si xaq ah, ee hadalkay Eebe ka dib iyo aayaadkiisa rumayn."}, {"id": 496962, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4480, "text": "halaagna wuxuu u sugnaaday beenaale dambi badan dhammaantiis."}, {"id": 496963, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4481, "text": "oo maqli aayaadka Eebe oo la akhrin haddana daa'ima gaalnimo, isagoo iskibrin sidii isagoon maqlin, ugu bishaaree cadaab daran."}, {"id": 496964, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4482, "text": "markuu ogaado aayaadkanaga wax ka mid ah wuxuu ka yeeshaa jees-jees, kuwaasina waxaa u sugnaaday cadaab wax dulleeya."}, {"id": 496965, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4483, "text": "waxaa koobi \u00abkana dambeysa\u00bb Naarta Jahannamo, waxna uma taro waxay kasbadeen, iyo waxay ka yeesheen Eebe ka sokow owliyo, waxayna mudan Cadaab wayn."}, {"id": 496966, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4484, "text": "Quraankani waa hanuun, kuwii ka gaaloobay aayaadka Eebehoodna waxaa u sugnaaday Cadaab xumaan daran."}, {"id": 496967, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4485, "text": "Eebe waa kan idiinfududeeyey Badda say ugu socoto Doonnidu \u00abiyo Markabku\u00bb dhexdiisa amarka Eebe, iyo inaad gaadhaan Fadligiisa iyo inaad ku mahadisaan."}, {"id": 496968, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4486, "text": "wuxuu idiin fududeeyey waxa ku sugan samooyinka iyo Dhulka Dhammaan xagiisay ka ahaadeen, arrintaasna waxa ugu sugan aayaad ciddii fikiri."}, {"id": 496969, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4487, "text": "waxaad u dhahdaa kuwa rumeeyey u dhaafa kuwaan ka yaabayn ciqaabta Eebe si uu uga abaal mariyo qoom waxay kasbadeen."}, {"id": 496970, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4488, "text": "ruuxii camal fiicanfala Naftiisuu u falay, ruuxii Xumaan sameeyana waxay uun dhibhi Naftiisa markaas xagga Eebe uunbaa laydiin celin."}, {"id": 496971, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4489, "text": "dhab ahaanbaan u siinay ree Binii Israa'iil Kitaabka iyo Xukun iyo Nabinimo, waxaana ku arzuqnay wax wanaagsan, waxaana ka fadillay Caalamkii (markaas)."}, {"id": 496972, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4490, "text": "waxaana siinay xujooyin ka mid ah amar, ismana khilaafin, inta cilmi u yimid ka dib mooyee, waxayna u diideen Xasad iyo Dulmi, dhexdooda ah, Eebahaana wuu kala xukumi dhexdooda Malinta Qiyaame waxay isku khilaafsanyihiin."}, {"id": 496973, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4491, "text": "waxaana ku yeellay waddo amarka Diinta ah, Ee raac, hana raacin hawada kuwaan wax ogayn."}, {"id": 496974, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4492, "text": "iyagu kaagama taraan Eebe xagiisa waxba, Daalimiintana qaarkood wuxuu u yahay sokeeye qaarka kale, Eebana waa u wali kuwa dhawrsada."}, {"id": 496975, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4493, "text": "kaasi waa (Quraanku) waa aragti (Nuurka) Dadka iyo hanuun iyo Naxariis ciddii waxyaqiinsan."}, {"id": 496976, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4494, "text": "mise waxay umaleeyeen kuwii kashaday xumaanta inaan kayeeleyno kuwa xaqa rumeeyey ee camal fiican falay oo kale, nolol iyo geeriba, waxaa xun waxay xukumi."}, {"id": 496977, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4495, "text": "Eebaa u abuuray samooyinka iyo Dhulka, xaq iyo in laga abaalmariyo Naf walba waxay kasbatay, lamana dulmiyo."}, {"id": 496978, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4496, "text": "ka waran ruux ka yeeshay ilaahiisa hawadiisa, oo Eebe dhumiyey isagoo cilmi leh, oo daboolay maqalkiisa iyo qalbigiisa, yelayna aragiisa dabool, yaa hanuunin Eebe kadib, miyaydaan wax xusuusanayn."}, {"id": 496979, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4497, "text": "waxay dheheen Gaaladii wax kale ma jiro ee waa uun nolashanada dhaw, waana dhimaneynaa qaarna noolaan, waxaan waqtiga ahayna nama dilo, wax ogaansho ahna uma leh arrintaas waxaan male ahayna kuma socdaan."}, {"id": 496980, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4498, "text": "marka lagu akhriyo korkooda aayaadkanaga oo cad wax xuja ah ma helaan oon ahayn noo keena aabbayowganno haddaad run sheegaysaan."}, {"id": 496981, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4499, "text": "waxaad dhahdaa Eebaa idin nooleeya idina dili, idinna kulmin Maalinta Qiyaame shakina ma leh, laakiin Dadka badankiis ma oga."}, {"id": 496982, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4500, "text": "Eebaa iska leh xukunka samooyinka iyo Dhulka, Maalintay kacdo Saacadda Qiyaame maalintaasay Khasaari xumaanlowgu."}, {"id": 496983, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4501, "text": "waxaadna arki umad kastoo Lawyahay ku fadhida Qiyaamada, Umad kastana waxaa loogu yeedhi Kitaabkii \u00abcamalkeeda\u00bb Maalintaas waxaa laydiinka abaalmarin waxaad camalfaleyseen."}, {"id": 496984, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4502, "text": "kani waa Kitaabkanagii wuxuuna ku hadli xaq korkiina, annaguna waan qoraynay waxaad camal falaysaan."}, {"id": 496985, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4503, "text": "kuwa xaqa rumeeyey oo camal fiican falay wuxuu galin Eebahood Naxariistiisa, taasina waa liibaan wayn."}, {"id": 496986, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4504, "text": "kuwa Gaaloobayse waxaa lagu dhihi miyeyna ahayn aayaadkeyga kuwa korkiina lagu akhriyo ood iskibriseen noqoteen qoom dambi falayaal ah."}, {"id": 496987, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4505, "text": "marka la dhaho yabooha Eebe waa xaq, Saacadda (Qiyaamana) shaki ma leh, waxaad dhahdaan ma naqaan Saacadda (qiyaame) waannu uun malayn mase yaqiinsanin."}, {"id": 496988, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4506, "text": "waxaa u muuqan Gaalada xumaantay camalfaleen (Ciqaabteedi) waxaana koobi waxay ku jees jeesi jireen (ciqaabtiisii)."}, {"id": 496989, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4507, "text": "waxaana loo dhihi Maanta (Qiyaamada) waanu idinka tagi (idinko cadaaban) sidaad u halmaanteen la kulanka Maalintiinan, Hooygiinuna waa Naar wax idiin gargaarana ma jiro."}, {"id": 496990, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4508, "text": "Arrintaasna waxaa ugu waxan inaad ka yeelateen aayaadka Eebe Jees jees, idinna dhagartay nolosha dhaw (ee adduun) Maanta Qiyaamada lagama bixiyo Naarta, cudurdaarna lama warsado."}, {"id": 496991, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4509, "text": "mahad Eebaa iskaleh Eebaha Samooyinka iyo Dhulka, Eebaha caalamkana ah."}, {"id": 496992, "identifier": "so.abduh", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4510, "text": "waxaana u sugnaatay waynida Samooyinka iyo Dhulka dhexdiisa, waana adkaade falsan."}]