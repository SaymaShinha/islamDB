[{"id": 497265, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Wuxuu ku dhaartay (Eebe) Xiddiggu markuu hoobto."}, {"id": 497266, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Ee saaxiibkiin (Nabigu) ma dhumin, xaqana kama leexan."}, {"id": 497267, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4787, "text": "Kumana hadlo hawadiisa."}, {"id": 496861, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4788, "text": "quraankuna waxaan waxyi Eebe ahayn ma aha."}, {"id": 497268, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4789, "text": "Waxaana wax baray (oo xagga Eebe ka soo gaadhsiiyay) ku xoog wayn, (Malaku Jibriil)."}, {"id": 497269, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4790, "text": "Ee caafimaad iyo quwaba leh, abuuriddiisuna egtahay."}, {"id": 497270, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4791, "text": "Wuxuu joogay jihadii sare (Jibriil)."}, {"id": 497271, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Kaddibna u soo dhawaaday kuna soo hoobtay (Nabiga)."}, {"id": 497272, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4793, "text": "Intuu uga jirsado Qaanso (xadhiggeed) ama ka dhaw."}, {"id": 497273, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Wuxuuna Eebe waxyooday Addoonkiisa (Nabiga) wuxuu u waxyooday."}, {"id": 497274, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Qalbiguna Ma beeniyo wuxuu arkay (Nabigu)."}, {"id": 497275, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Ee ma idinkaa ka murmi \u00abkana doodi\u00bb wuxuu arkay."}, {"id": 497276, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Dhabnimuu Nabigu u arkay Jibrill mar kale."}, {"id": 497277, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4798, "text": "Sidratul-Muntaha agteeda."}, {"id": 497278, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4799, "text": "Agteedana waxaa ah Jannatul-ma'waa."}, {"id": 497279, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4800, "text": "(Wuxuu arkay) waqtiguu daboolayay Sidrada wax daboola."}, {"id": 497280, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4801, "text": "Arragguna kama iilan wuxuu arkay (Nabigu) mana xadgudbin."}, {"id": 497281, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Dhabnimo yuuna u arkay (Nabigu) Aayado Eebe oo waawayn."}, {"id": 497282, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Ka warrama (Sanamyada) Laata iyo Cuza."}, {"id": 497283, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4804, "text": "Iya Manaatadii saddexaad ee kale."}, {"id": 497284, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Idinku ma waxaad leedihiin Lab Eebana Dhaddigga."}, {"id": 497285, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Taasi waa qayb dulan ah."}, {"id": 497287, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Sanammaduna magacyo aad magacowdeen idinka iyo Aabayawgiin mooyeen wax kale ma aha, wax xuja ahna Eebe uma soo dajinin, waxaan mala ahaynna ma raacayaan iyo waxa naftoodu doonto, xagga Eebana waxaa uga yimid hanuun."}, {"id": 497288, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Mise dadka waxaa u sugnaaday waxay yididiishaan."}, {"id": 497289, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Aakhiro iyo Adduunba Eebaa iska leh."}, {"id": 497290, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4810, "text": "Badanaa Malag Samada ku sugan oon shafeecadiisu waxba anfacaynin in Eebe idmo mooyee, Cidduu la doono oo ka raalli noqdo."}, {"id": 497291, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Kuwa aan rumaynin Aakhiro waxay Malaa'igta ku magacaabi magac dhaddig."}, {"id": 497292, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Wax cilmi ahna uma laha, ee waxay raaci mala uun, malana xaqa wax kama taro."}, {"id": 497293, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Ee iskaga jeedso (nabiyow) Ruux ka tagay xukayga (Quraanka) oon doonin waxaan nolosha Adduunka ahayn."}, {"id": 497294, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Kaasina waa garaadkooda aqooneed intuu gaadhay, Eebana waa ogyahay cidda ka dhuntay jidkiisa, waana ogyahay cidda hanuunsan."}, {"id": 497295, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Samaawaadka iyo Dhulka waxa ku sugan Eebaa iska leh, si uu uga abaalmariyo kuwa xumaanta falay camalkoodii, ugana abaalmariyo kuwii wanaagga falay wanaag."}, {"id": 497296, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Waana kuwa ka fogaada dambiga waawayn iyo xumaanta, wax yaryar mooyeen, Eebana waa waasac dambidhaafkiisu, wuuna idin ogyahay markuu idinka abuuray Dhulka iyo markaad ku asturantihiin uurka hooyadiin, ee Naftiinna ha ammaanina Eebaa og cidda dhawrsashada badane."}, {"id": 497297, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Ka warrama midka xaqa ka jeedsaday."}, {"id": 497298, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4818, "text": "Oo wax ar bixiyay inta kalana reebtay."}, {"id": 497299, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Ma agtiisaa cilmiga maqan yaallaa oo wuu arkaa."}, {"id": 497300, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Miyaan looga warramin (dadka) wixii ku yiillay Kitaabkii (Nabi Muuse)."}, {"id": 497145, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4821, "text": "Iyo kii (Nabi Ibraahimkii) ee oofiyay (wixii lafaray)."}, {"id": 492650, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4822, "text": "Inaan nafna dambi naf kale qaadaynin."}, {"id": 492653, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4823, "text": "Ruuxna wuxuusan camalfalin heleeynin."}, {"id": 492651, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4824, "text": "Camalkiisana la arki doono."}, {"id": 492646, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4825, "text": "Ka dibna looga abaalmarin doono si buuxda."}, {"id": 492654, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4826, "text": "Eebaa xaggiisana loo ahaan."}, {"id": 492661, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4827, "text": "Eebe waakan wax ka qosliya kana oohiya."}, {"id": 492656, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4828, "text": "Isagaana wax dila waxna nooleeya."}, {"id": 497301, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4829, "text": "Isagaana abuuray labada nooc ee ah lab iyo dhaddigba."}, {"id": 497302, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4830, "text": "Kana ahaysiiyay dhibic soo baxday."}, {"id": 497303, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4831, "text": "Isagaana ahaysiin soo celinta kale."}, {"id": 497304, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4832, "text": "Eebe isagaa wax Hodmiya isagaana wax faqiiriya."}, {"id": 497305, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4833, "text": "Waana Eebaha xiddigga Shicra."}, {"id": 497306, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4834, "text": "Eebe isagaa halaagay Caadkii hore."}, {"id": 497307, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4835, "text": "Iyo Thamuudba waxna kama reebin."}, {"id": 497308, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4836, "text": "(Nabi Nuux) Qoomkiisiina horuu (uhalaagay), waxayna ahaayeen kuwa dulmi badan oo xadgudub badan."}, {"id": 497309, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4837, "text": "Qoomkii (Nabi Luudhna) wuu riday (Eebe)."}, {"id": 497310, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4838, "text": "Wuxuuna ka daboolay wuxuu ku daboolay (oo caddibaad ah)."}, {"id": 497311, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4839, "text": "Teebaadse nicmooyinka Eebe shakinaysaan (kana doodaysaan) dadow."}, {"id": 497312, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Kan (Nabiga iyo Quraankuba) waa dige ka mid ah digayaashii hore."}, {"id": 497313, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Wayna dhawaatay Saacaddii Qiyaame."}, {"id": 497314, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4842, "text": "Wax Eebe ka soo hadhay oo ogna ma jiro (markay dhici)."}, {"id": 497315, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Ma quraankanaad la yaabantihiin."}, {"id": 497316, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4844, "text": "Ood qoslaysaan (idinkoo beenin) oydaanna ooyeynin (cabsi darteed)."}, {"id": 497317, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4845, "text": "Idinkoo waliba hilmaansan (islana wayn)."}, {"id": 497318, "identifier": "so.abduh", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4846, "text": "Ee dadow Eebe u sujuuda caabudana."}]