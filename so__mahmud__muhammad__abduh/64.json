[{"id": 497670, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5200, "text": "Waxaa Eebe nasaha (weyneeya) waxa Samaawaatka (Cirka) iyo Dhulka ku sugan awood iyo mahadbana Eebaa iska leh, wax walbana isagaa kara."}, {"id": 497671, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5201, "text": "Eebe waa kan idin anbuuray qaarkiinna gaaluu noqday, qaarkiinna Mu'miniin, Eebana waxaad Camal falaysan waa arkaa."}, {"id": 497672, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5202, "text": "Samaawaatka (Cirka) iyo Dhulkaba wuxuu Eebe u abuuray si xaq ah, wuuna idin sawiray Eebe, dadoow suuraddiinana wuu wanaagjiyey, isagaana loo noqon (Ahkhiro)."}, {"id": 497673, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5203, "text": "Wuxuu ogyahay waxa samaawaatka (Cirka) iyo Dhulka ku sugan, wuxuuna ogyahay waxaad qarinaysaan iyo waxaad muujinaysaan, Eebana waa ogyahay waxa laabta (Qalbiga) ku jira."}, {"id": 497674, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5204, "text": "Miyuuna idin soo gaarin warkii kuwii gaaloobay ee horreeyey, waxay dhadhamiyeen xumaantii camalkoodii, waxaana u sugnaaday caddibaad daran."}, {"id": 497675, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5205, "text": "Arrintaas waxaa ugu wacan in Rasuullo ula yimaadeen xujooyin ayna dheheen (Gaaladii) ma dadbaa na hanuunin ayna gaaloobeen oo jeedsadeen, iskana deeqtoonaysiiyeen (weynaysiiyeen) Eebe, Eebana waa midka hodanka ah lana mahadiyo."}, {"id": 497676, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5206, "text": "Waxay sheegeen kuwii gaaloobay inaan la soo bixinaynin, waxaad dhahdaa saas ma aha, Eebaa wuxuu ku dhaartay in la idin soo bixin laydinkana warrami waxaad camal fasheen, taasina waa u fududahay Eebe."}, {"id": 497677, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5207, "text": "Ee rumeeya Eebe iyo Rasuulkiisa iyo nuurka Eebe soo dejiyey (Quraanka) Eebana waxaad camal falaysaan waa ogyahay."}, {"id": 497678, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5208, "text": "Maalinta qiyaamada ee Ilaahay idin soo kulmin waa maalin kado, ruuxiise Ilaah rumeeya camal wanaagsanna fala Eebe xumaantiisa (dambigiisa) wuu asturi, wuxuuna galin Janooyin ay dhex dureeraan Wabiyaal way kuna waaraan waligood, taasina waa liibaan weyn."}, {"id": 497679, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5209, "text": "kuwa gaaloobayse beeniyeyna Aayaadka Eebe waa kuwa Ahlu Naar ah waxayna ku waari dhexdeeda, meel loo ahaadana iyadaa u xun."}, {"id": 497680, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5210, "text": "Wixii Musiiba ah (dhib ah) oo dhaca waa idinka Eebe, ruuxiise Eebe rumeeya waxaa hanuuni (oo xasili) qalbigiisa, Eebana wax walba waa ogyahay."}, {"id": 497681, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5211, "text": "Eebe adeeca Rasuulkiisana adeeca, haddaadse jeedsataan (xaqa diidaan) waxaa Rasuulka saaran uun inuu si cad wax u gaargiiyo."}, {"id": 497682, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5212, "text": "Eebe mooyee Ilaah kale ma jiro, isaga uunna ha tala saarteen Mu'miniintu."}, {"id": 497683, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5213, "text": "Kuwa xaqa rumeeyow Haweenkiinna iyo Caruurtiina waxaa ka mid ah kuwo Col idiin ah ee ka digtoonaada, haddaadse iska cafidaan saamaxdaanna Eebe waa dambi dhaafe naxariiste ah."}, {"id": 497684, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5214, "text": "Xoolihiinnu iyo Caruurtiinnu waa uun fidno (ibtilo) Eebe agtiisaase ajir weyn (ku sugan yahay)."}, {"id": 497685, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5215, "text": "Eebe ka dhawrsada intaad kartaan, maqla oo adeecana (weyneeya), waxna baxsada, ruuxii laga dhawro bakhaylnimada naftiisa waa liibaanay."}, {"id": 497686, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5216, "text": "haddaad Eebe amaah fiican amaahsataan (wax sadaqaysataan) Eebe waa idiin laab laabi, dambigiinana wuu dhaafi, Eebana waa kan la mahadiyo ee ee dusha san."}, {"id": 497687, "identifier": "so.abduh", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5217, "text": "waxa maqan iyo waxa joogaba Eebe wa ogyahay waana adkaade Falsan."}]