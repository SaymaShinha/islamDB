[{"id": 497945, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5476, "text": "(Nabiga) Isduuduubayow."}, {"id": 497946, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5477, "text": "Istaag habeenka (Tuko) wax yar Mooyee."}, {"id": 497947, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5478, "text": "Nuskiisa tuko ama ka nusqaami wax yar."}, {"id": 497948, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5479, "text": "Ama ka badi, Quraanku u akhri si fiican."}, {"id": 497949, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5480, "text": "Annagaa Kugu Dakin Hadal Culus (wayne)."}, {"id": 497950, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5481, "text": "Habeenka Saacadihiisa (oo la tukado) yaa ku daran khushuucda Aadna u toosan."}, {"id": 497951, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5482, "text": "Maalintii waxaa kuu sugnaaday shaqa Dheer."}, {"id": 497952, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5483, "text": "Magaca Eebana xus cibaadadiisana u go'."}, {"id": 497953, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5484, "text": "Qorrax ka soobax iyo u Dhaeba Eebaa iska leh, isaga Mooyee ilaah kalena ma jiro ee ka yeelo wakiil (la cuskado)."}, {"id": 497954, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5485, "text": "kuna Samir waxay sheegi gaaladu, si Quruxsanna uga hijroo (iskaga tag)."}, {"id": 497955, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5486, "text": "Isu kaanna daa aniga iyo kuwa Beeniyay xaqa ee Nicmaysan wax yarna Sug."}, {"id": 497956, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5487, "text": "Agtannada waxaa ah caddibaad iyo Naarta Jaxiimo."}, {"id": 497925, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5488, "text": "Iyo cunno lagu Margado iyo Cadaab daran."}, {"id": 497957, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5489, "text": "(waxay ku dhiei Arrintaasu) Maalinta Dhulka iyo Buuruhu Gilgilan, Buuruhuna ay noqon Tamuux socda."}, {"id": 497958, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5490, "text": "Annagaa idiin dirray Rasuul markhaati ah, sidaan ugu dirray fircoonba Rasuul."}, {"id": 497959, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5491, "text": "Fircoonna wuu Caasiyay Rasuulkii (Nabi Muuse). Firconna waxaan ku Qabannay qabasha Daran."}, {"id": 497960, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5492, "text": "Ee seebaad Gaalloy uga dhawrsanaysaan Caddibaadda Maalinta qiyaame ee carruurta Cirroole ka Yeeli Hadaad Gaalawdaan."}, {"id": 497961, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5493, "text": "Samaduna Maalintaas Darteed ay la Dildillaaci Balaanqaadka Eebana waa wax Dhici."}, {"id": 497962, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5494, "text": "Aayadahani Waa waano Ee Ruuxii Doona Eeba xaggiisa jid ha uga yeesho."}, {"id": 497963, "identifier": "so.abduh", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 509, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5495, "text": "Eebahaa wuxuu ogyahay Nabiyow inaad tukatid wax ka yar habeenka saddex Meelood laba iyo Badhkiis iyo saddex meelood meel, iyo in koox ka mida kuwa kula jira ay tukato, Eebana waa u qadaranyahay Habeenka iyo Maalintuba, wuxuuna Ogyahay inaydaan Koobi karin, wuuna Idinka toobad aqbalay ee Akhriya wixii idiin Fududaada oo quraan ah, wuxuu Eebe ogyahay inay idin ka mid Noqon kuwa Buka iyo kuwo safri iyagoo dooni Khayrka Eebe iyo kuwo u dagaallami Jidka Eebe ee akhriya wixii idiin Fududaada oo Quraan ah, Salaaddana ooga Zakadana Bixiya Eebana wax wanaagsan daynsada Waxaad naftiinna u hormarsataan oo khayr ahna waxaad ka ogaandoontaan kana Heleysaan Eeba Agtiisa Isagoo khayr ah iyo Ajri wayn, ee Eebe Dambi dhaaf waydiista waa Dambi Dhaafe Naxariiste."}]