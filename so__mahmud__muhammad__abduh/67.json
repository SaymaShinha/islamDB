[{"id": 497713, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5242, "text": "Waxaa barako badnaaday (oo weynaaday) Eebaha Xukunku awooddiisa ku jiro ee wax walbana kara."}, {"id": 497714, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5243, "text": "Eebaha abuuray geeri iyo nololba, si uu dadow idiin imtixaamo kiinna Camal fiican, Eebana waa adkaade dambidhaafe ah."}, {"id": 497715, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5244, "text": "Eebe waa midka abuuray Toddobo Samo oo isdul saaran kuna arki maysid abuurka Eebaha Raxmaanka ah kala tagsani, bal daymada celi inaad aragto wax dillaac ah."}, {"id": 497716, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5245, "text": "Haddana celi daymada waxaa kugu soo noqon aragga oo dhiban daallanna."}, {"id": 497717, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5246, "text": "Eebe wuxuu ku quxiyey Samada dhow Nalal (Xiddigaha) wuxuuna ka dhigay Xiddigahaas kuwo lagu gano Shayaadiinta, Wuxuuna Eebe u diyaariyay kuwaas Naarta Saciiro."}, {"id": 497718, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5247, "text": "Kuwa ka gaaloobay Eebahood waxaa u sugnaaday Caddibaadda Jhannamo, meel loo ahaadana iyadaa u xun."}, {"id": 497719, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5248, "text": "Marka Gaalada lagu tuuro Naarta waxay ka maqli Cod xun oo daran iyadoo karaysa."}, {"id": 497720, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5249, "text": "waxay u dhawdahay inay Cadha daraadeed la kala dillaacdo, marka Koox (Gaala ah) lagu ridaba waxaa warsada Malaa'igta Naarta ka shaqeysa iyagoo ku dhihi miyuusan idiin imaanin wax idiin diga."}, {"id": 497699, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5250, "text": "Waxayna ugu jawaabi waa noo yimid wax noo diga waanse beeninay, annagoo leh ma soo dejinin Eebe waxba, idinkuna waxaad ku sugan tihiin baadi weyn."}, {"id": 497722, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5251, "text": "Gaaladii waxay dhaheen haddaan wax maqlaynno ama aan wax kasayno Naarta Saciiro ehelkeeda ma aanaan noqoneen."}, {"id": 497723, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5252, "text": "Waxay kirteen (Gaaladu) dambigoodii waxaana u sugnaatay fogaansho Naarta Saciiro ehelkeeda."}, {"id": 497724, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5253, "text": "Kuwa Eebe ka yaaba iyagoon la arkaynin (Eebe mooyee) waxaa u sugnaaday dambi dhaaf iyo ajri weyn."}, {"id": 497725, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5254, "text": "Ama hadalka hoos dhiga dadow ama kor u qaada Eebe waa ogyahay waxa laabta ku jira."}, {"id": 497726, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5255, "text": "Ma waxaan wax ogeyn Eebaha wax walba abuuray ee wax walba daalacan."}, {"id": 497727, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5256, "text": "Eebe waa kan Dhulka ka dhigay mid laylan ee ku socda Waddooyinkiisa kana Cuna Quudkiisa Eebaana Dadku loo soo kulmin (Qiyaamada)."}, {"id": 497728, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5257, "text": "Miyaad ka aamin noqoteen Eebaha Sare inuu Dhulka idin la gooyo, ayna idinla dhaqdhaqaaqdo."}, {"id": 497729, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5258, "text": "Miyaadse ka aamin noqoteen Eebaha Sare inuu Dhagaxyo Naar ah idinku soo daadiyo, waad ogaan doontaan digiinta Eebe Cidhibteeda."}, {"id": 497730, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5259, "text": "Waxaa Eebe beeniyey Gaaladii (reer Makaad) ka horreeyey, sidayse ahayd Ciqaabti Eebe."}, {"id": 497731, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5260, "text": "Miyeyna dadku arkaynin Shimbirka korkooda ah marna baalka fidin marna laabi wax haya oon Eebaha Raxmaan ahayna uusan jirin, Eebana wax walba wuu arkaa."}, {"id": 497732, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5261, "text": "Waa kuma Gargaaraha Eebaha Raxmaan ah ka soo hadhay ee u gaargaari Gaalada, Gaaladu waxay ku sugan yihiin kadsoomid."}, {"id": 497733, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5262, "text": "Waa kuma kan wax quudiya haddii Eebe haysto Risqigiisa, Gaaladu waxay ku Madax adaygeen isla weyni iyo Xaq diidnimo."}, {"id": 497734, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5263, "text": "Midkee wanaagsan Ruux Wajiga k socda iyo mid si toosan oo siman Wadda Toosan ugu socda."}, {"id": 497735, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5264, "text": "Dheh Nabiyow Eebe waa midka Dadkow idin ahaysiiyey, Maqal iyo Arag iyo Caqlina idiin yeelay, wax yar baadse Eebe ku shukridaan."}, {"id": 497736, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5265, "text": "Dheh Eebe waa midka dadkow Dhulka idinku beeray isagaana (Qiyaamada) loo soo kulmin doona Dadka."}, {"id": 497737, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5266, "text": "Waxay dhihi Gaaladu waa goorma Waqtiguu dhici waxa naloogu gooddin haddaad run sheegaysaan."}, {"id": 497738, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5267, "text": "Dheh Eebaa og goortaas, waxaanse ahay Anigu u dige Cad (muuqda)."}, {"id": 497739, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5268, "text": "Markaysa Gaaladu Naarta oo u dhow arkaan waxaa xumaan oo doorsoomi Wajiga Gaalada, waxaana lagu dhihi waaka Cadibaaddii aad ddonayseen (dedejisanayseen)."}, {"id": 497740, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5269, "text": "Dheh balse ka warrama haddii Eebe aniga (Nabiga) iyo inta ila jirta uu halligo ama u naxariisto yaa Gaalo ka korin Caddibaad daran."}, {"id": 497741, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5270, "text": "Dheh Eebe waa Raxmaan waana rumaynay, waana Tala saaranay, waadse ogaan doontaan (Gaalooy) Cidda Baadi Cad ku sugan."}, {"id": 497742, "identifier": "so.abduh", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5271, "text": "Dheh balse Warrama hadday Biyuhu Dhulka ku gudhaan yaa idin siin Biya durduri (ma jiro Eebe waxaan ahayn)."}]