[{"id": 498337, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5885, "text": "Marka Samadu jeexjeexanto (qiyaamada)."}, {"id": 498338, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5886, "text": "Oy Eebe Addeecdo waana xaq addeeciddiisunee."}, {"id": 498339, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5887, "text": "Marka dhulka la fidiyo (qiyaamada)."}, {"id": 498340, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5888, "text": "Oo uu soo tuuro waxa ku hoos jira, kana madhnaado."}, {"id": 498341, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5889, "text": "Oos Eeba Addeeco waana xaq adeeciddaasina. (wuxuu la kulmi Ruuxwalba Camalkiisa)."}, {"id": 498342, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5890, "text": "Dadow waxaad u dhibboon ood u camalfali xagga Eebe, waadna la kulmi."}, {"id": 498343, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5891, "text": "Ruusiise Kitaabkiisa (fiican) Midigta laga siiyo (waa mu'minkee)."}, {"id": 498344, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5892, "text": "Waxaa la xisaabin xisaab fudud."}, {"id": 498345, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5893, "text": "Wuxuuna ku noqon Ciddiisii (Jannada joogtay) isagoo faraxsan."}, {"id": 498346, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5894, "text": "Ruuxiise Kitaabkiisa laga siiyo gadaashiisa (waa Dadka xune)."}, {"id": 498347, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5895, "text": "Wuxuu u yeedhan Halaag."}, {"id": 498348, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5896, "text": "Wuxuuna gali Naarta Saciira."}, {"id": 498349, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5897, "text": "Maxaa yeelay Markuu dadkiisii dhexjoogay yuu kabirsanaa (Adduunkii)."}, {"id": 498350, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5898, "text": "Wuxuuna malaynhayay inuusan Eebe u soo noqonaynin (qiyaame)."}, {"id": 498351, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5899, "text": "Saas ma ahee Eebe waa arkaa."}, {"id": 498352, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5900, "text": "Waxaan ku dhaartay \u00abbuu yidhi Eebe\u00bb Midabka Qorrax dhaca."}, {"id": 498353, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5901, "text": "Iyo Habeenka iyo wuxuu qariyay."}, {"id": 498354, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5902, "text": "Iyo Dayaxa markuu Nuurku buux Samo."}, {"id": 498355, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5903, "text": "Ee Dadkow waxaad koraysaan xaalado kala duwan/"}, {"id": 498356, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5904, "text": "Dadka maxaa u sugnaaday oy la rumaynwaayeen (xaqa)."}, {"id": 498357, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": true, "sajda_id": 14, "sajda_obligatory": false, "sajda_recommended": true, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5905, "text": "Marka Quraanka lagu akhriyana ayna u sujuudaynin."}, {"id": 498358, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5906, "text": "Saas ma ahee kuwa Gaaloobay waxay beenin (xaqa)."}, {"id": 498359, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5907, "text": "Eebana waa ogyahay waxay qarsan."}, {"id": 498360, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5908, "text": "Ugu bishaaree caddibaad daran."}, {"id": 498361, "identifier": "so.abduh", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5909, "text": "Hase yeeshee kuwa rumeeyay xaqa camalfiicanna falay waxay mudan Ajri aan go'aynin."}]