[{"id": 495951, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3470, "text": "waxay ku tusin mucjisada Quraanka (sida soo hormartay)."}, {"id": 495952, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3471, "text": "tan waa aayaadka Kitaabka (Quraanka) xigmada leh."}, {"id": 495953, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3472, "text": "waana hanuunka iyo naxariista samalfalayaasha."}, {"id": 495954, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3473, "text": "kuwa salaadda ooga, Zakadana bixiya, iyagoo aakhiro yaqiinsan."}, {"id": 495955, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3474, "text": "kuwaasi waxay ku suganyihiin hanuun Eebe, kuwaasina waa kuwa liibaanay."}, {"id": 495956, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3475, "text": "Dadka waxaa ka mid ah kuwo gadan hadal wax shuqliya si uu uga dhumiyo (Dadka) Jidka Eebe iyagoon wax ogayn, Jidka Eebene ku Jees jeesa, kuwaasi waxay mudan Cadaab Dulleeya."}, {"id": 495957, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3476, "text": "marka lagu akhriyo aayadkanaga wuu jeedsadaa isagoo iskibrin, sidii isagoon maqal, oo dhagihiisu culusyihiin, ugu bishaaree Cadaab daran."}, {"id": 495958, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3477, "text": "kuwa rumeeyey (xaqa) camal wanaagsanna falay waxay mudan Janatu Naciima."}, {"id": 495959, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3478, "text": "iyagoo ku waari dhexdeeda waana yabooh Eebe oo xaq ah, Eebana waa adkaade falsan."}, {"id": 495960, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3479, "text": "isaga abuuray Samooyinka Tiir aad aragtaan la'aantiis, dhulkana wuxuu ku tuuray Buuro (suga) inayan dhaqdhaqaaqin, wuxuuna ku fidiyey dhexdiisa daabad kasta, Samadana waxaan ka soo dejinay Biyo, waxaana ku soo bixiney nooc kasta oo quruxsan."}, {"id": 495961, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3480, "text": "kaasi waa abuurkii Eebe, ee i tusiya waxay abuureen kuwa ka soo hadhay Eebe, saas ma aha, ee dulmi falayaashu waxay ku suganyihiin baadi Cad."}, {"id": 495962, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3481, "text": "dhab dhaan ayaan luqmaan u siinay Xigmad iyo inuu Eebe ku mahdiyo, ruuxii mahdiya wuxuu uun u mahdiyey naftiisa, ruuxii Gaaloobana Eebe waa ka kaaftoonyahay waana amaananyahay."}, {"id": 495963, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3482, "text": "(xusuuso) markuu ku yidhi Luqmaan Wiilkiisa isagoo waanin Wiilkayow Eebe hawshariigyeelin, shirkigu waa dulmi weyne."}, {"id": 495964, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3483, "text": "waxaan u dardaaranay dadka labadiisii Waalid, way sidday Hooyadis iyadoo tabaryar tabar yari kalana ku sugan, Gudhidiisuna waa Labo sano ee ku mahadi ani iyo Labadaadii Waalid, xagaygaana loo ahaan."}, {"id": 495965, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3484, "text": "hadday kugula dadaalaan (Labada waalid) inaad ii shariig yeesho (ila wadaajiso) waxaadan aqoon u lahayn ha ku adeecin kulana noolow adduunka si fiican, raacna Jidka kuwa ii noqda, xagayga yaa laydiin soo celin waxaana idiinka warami waxaad camalfali jirteen."}, {"id": 495966, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3485, "text": "Wiilkayow hadday xumaantu tahay Xabbad Khardal ah oo kale (wax yar) oo sallax dhexdii ku sugnaato ama samooyinka dhexdooda ama Dhulka dhexdiisa wuu keeni Eebe, Eebana waa oge xeeldheer."}, {"id": 495967, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3486, "text": "Wiilkayow oog Salaadda, oo far waanaagga oo reeb waxa lanaco kuna Samir waxa ku gaadha, arrintaasu waxay ka midtahay umuuraha wanaagsane."}, {"id": 495968, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3487, "text": "hana ka jeedin Wajiga dadka hana ugu socon dhulka kibir, Eebana ma jeela mid kastoo isla weyn oo faanbadane."}, {"id": 495969, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3488, "text": "socodkaagana ku dhexdhexayso, hoosna u dhig Codkaaga, Cod waxaa ugu xun Codka Dameerahee."}, {"id": 495970, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3489, "text": "miyeydaan arkayn in Eebe idiin sakhiray waxa Samooyinka iyo Dhulka ku sugan oo idin mid qarsoonba, Dadkana waxaa ka mid ah kuwo Eebe ku murmi Cilmi iyo hanuun iyo Kitaab nuuriya la'aantiis."}, {"id": 495971, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3490, "text": "marka loodhaho raaca waxa Eebe soo dajiyeyna waxay dhahaan waxaannu raaci waxaan ka helay Aabayaalkanno, haduu Shaydaan ugu yeedho Cadaabka Saciirana (miyey raaci)."}, {"id": 495972, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3491, "text": "ruuxii u jeediya Wajigiisa xagga Eebe isagoo samo fali wuxuu qabsaday guntin (Ballan adag), xagga Eebaana Cihdhibta arrimuhu u noqon."}, {"id": 495973, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3492, "text": "Ciddiise Gaalowda yuuna ku walbahaar galin Gaalimadiisu, xagganagay u soo noqon waxaana uga warramaynaa waxay camalfali jireen, illeen Eebaa og waxa laabta ku sugane."}, {"id": 495974, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3493, "text": "waanu u raaxayn wax yar, markaasaannu u dhibaatayn (u iili) Cadaab adag."}, {"id": 495975, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3494, "text": "haddaad waydiisid yaa abuuray samooyinka iyo dhulka waay odhan ilaahay, waxaad dhahdaa mahad Eebaa iska leh, hadankoodse ma oga."}, {"id": 495976, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3495, "text": "Eebaa iska leh waxa samooyinka iyo Dhulka ku sugan, Eebana waa hodan amaanan."}, {"id": 495977, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3496, "text": "hadday waxa dhulka ku sugan oo geeda ah Qalimo noqdaan Badduna khadayso oo gadaasheeda Todobo badood yihiin madhamaateen Kalimooyinka Eebe, Eebana waa adkaade falsan."}, {"id": 495978, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3497, "text": "abuurkiinna iyo soobixintiinu ma aha sida Nafkaliya mooyee (Fudaydka), Eebana waa maqle arke ah."}, {"id": 495979, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3498, "text": "miyeydaan ogayn in Eebe Habeenka dhex galiyo Maalinta, Maalintana dhexgaliyo Habeenka sakhirayna Qoraxda iyo Dayaxa mid, kastana uu u socdo muddo magacaaban, Eebana waxaad Camalfalaysaan waa ogyahay."}, {"id": 495980, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3499, "text": "Taasna wa in Eebe xaq yahay waxay caabudi ee ka soohadhayna baadil yahay iyo in Eebe yahay Sareeye wayn."}, {"id": 495981, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3500, "text": "miyeydaan ogayn in Doontu dhexsocoto Badda Nicmada Eebe darteed si uu idiin tusiyo aayaadkiisa, arrintaasna aayaadbaa ugu sugan Samirbadane shugri badan oo dhan."}, {"id": 495982, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3501, "text": "marka hirku daboolo dadka oo Hoos la moodo waxay baryaan Eebe iyagoo u kali yeeli Diinta (Cibaadada) markuu u koriyo xagga Barrigana waxaa ka mid noqon Dadka mid dhexdexeysan, mana diido aayadkanaga mid khaa'in ah oo gaalimo badan mooyee."}, {"id": 495983, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3502, "text": "Dadow ka dhawrsada Eebihiin oo ka Cabsada Maalin uusan ka abaalmarinayn Waalid Ilmihiisa, Ilmuhuna Waalidka waxba, yabooha Eebana waa Xaq, ee yeyna idinku dhagrin nolosha adduunyo, yuuna idinku dhagrin Eebe dhagar badane (shaydaan)."}, {"id": 495984, "identifier": "so.abduh", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 3503, "text": "Eebe agtiisuu yahay ogaanshaha Saacadda (Qiyaame) iyo Roob di'iddu, wuxuuna ogyahay waxa makaanka (Ilma galeenka) ku sugan, nafna ma oga waxay mudan Barri, Nafna ma oga Dhulkay ku dhiman, Eebaa og xeelna dheer."}]