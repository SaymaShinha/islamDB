[{"id": 497096, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4613, "text": "Kuwa xaqarumeeyow ha ku dagdagina Eebe iyo Rasuulkiisa hortiisa (waxayan fasixin), Eebana ka dhawrsada ileen wax walba wuu maqli wuuna ogyahaye."}, {"id": 497097, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4614, "text": "Kuwa xaqa rumeeyow ha ka koryeelina codkiinna codka Nabiga, hana kula jahrina (qaylina) hadal sida qaarkiin qaar ugula jahro (qaylyo) si aan camalkiinnu u burin idinkoon agayn."}, {"id": 497098, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4615, "text": "kuwa codkooda hoos u dhiga Rasuulka agtiisa kuwaasi waa kuwo Eebe imtixaanay quluubtooda dhawrsasho darteed, waxayna mudanyihiin dambi dhaaf iyo ajri wayn."}, {"id": 497099, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4616, "text": "kuwa kaaga dhawaaqi qolalka gadaashooda badankoodu wax ma kasayaan."}, {"id": 497100, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4617, "text": "Hadday samraan intaad uga soo bixi yaa u khayr badnaan lahayda, Eebana waa dambi dhaafe naxariista,"}, {"id": 497101, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4618, "text": "Kuwa xaqa rumeeyow hadduu idiinla yimaaddo faasiq war, hubsada, si aydaan dad u dhibin ogaansho la'aan, markaasna aad shallaydaan waxaad fasheen."}, {"id": 497102, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4619, "text": "Ogaadana in Rasuulkii Eebe idin dhexjoogo, hadduu idinku addeeco waxyaala hadanna aad dhibboonaysaan, wuxuuse Eebe idinla jeclaaday limaan, una qurxiyay quluubtiinna, wuxuuna idinla nacay gaalnimo, faasiqnimo iyo caasinnimo, kuwaasina waa kuwa hanuunsan."}, {"id": 497103, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4620, "text": "Waa fadliga (siinta) Eebe iyo naxriistiisa, Eebana wax walba waa ogyahay, waana falsame."}, {"id": 497104, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4621, "text": "Hadday Laba qolo oo mu'miniinta ah dagaallamaan heshiisiiya dhexdooda, hadday midu ku xadgudubto tan kale la dirira tan xadgudubtay intay amarka Eebe uga noqoto (xaqa aqoonsato), Hadday u noqotana si caddaalad ah u wanaajiya dhexdooda una garsoora, Eebe wuxuu jecelahay kuwa garsooree."}, {"id": 497105, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4622, "text": "Mu'miniintu waa uun walaala, ee wanaajiya walaalihiin dhexdooda, Eebana ka dhawrsada, waxaad u dhawdihiin inuu idiin naxariistee."}, {"id": 497106, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4623, "text": "Kuwa xaqa rumeeyow yuuna yasin Ragna Rag (kale) waxay u dhawdahay inuu ka khayr badanyahaye, Haweenna yayna haween yasin waxay u dhawyihiin inay ka khayr badanyihiine, hana ceebeynina Naftiinna, hana isugu dhawaaqina naanays xun, waxaa xun magaca faasiqinimada iimaan kadib, Ciddaan tawbad keeninna waa daalimiin."}, {"id": 497107, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4624, "text": "Kuwa xaqa rumeeyow ka dhawrsada wax badan oo mala ah, maxaa yeelay malaha qaarkiis waa dambi, hana is jaasuusina, qaarkiinna qaar yuusan xamanin, miyuu jecelyahay midkiinna inuu cuno Hilibka walaalkiis oo mayd ah, waad necebtihiin arrintaa, Eebana ka dhawrsada waa tooba aqbale naxariistee."}, {"id": 497108, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4625, "text": "Dadow waxaan idinka abuuray lab iyo dhaddig, waxaana idinka yeellay shucuub iyo qabiilooyin si aad isu aqoonsataan, Ruuxase ugu sharaf badan Eebe agtiisa waa ka idiinku dhawrsasho badan, Eebana wax walba waa ogyahay,"}, {"id": 497109, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4626, "text": "Waxay dheheen ree baadiyihii carbeed waan rumaynay (xaqa), waxaad ku dhahdaa maydaan rumaynin, laakiin dhaha waan hoggansannay, Iimaankuna wali Quluubtiinna ma galin, haddaadse adeecdaan Eebe iyo Rasuulkiisa kama nusqaamiyo camalkiinna waxba, Eebana waa dambi dhaafe naxariista."}, {"id": 497110, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4627, "text": "Kuwa mu'miniinta ah waa kuwa uun rumeeyay Eebe iyo Rasuulkiisa oon shakiyin, kuna Jahada Xoolahooda iyo naftooda jidka Eebe dartiis, kuwaasi waa kuwa runlowga ah."}, {"id": 497111, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4628, "text": "Waxaad dhahdaa ma Eebbaad usheegaysaan diintiinna isagoo og waxa Samaawaadka iyo Dhulkaba ku sugan, Eebana wax walba waa ogyahay."}, {"id": 497112, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4629, "text": "Waxau kugu mannaysan inay Islaameen, waxaad dhahdaa haygu mannaysanina Islaamkiinna Eebaa manna idinku leh inuu idinku hanuuniyay Iimaanka haddaad run sheegeysaan."}, {"id": 497113, "identifier": "so.abduh", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4630, "text": "Eebe waa ogyahay waxa Samaawaadka ku maqan iyo dhulkaba, Eebana waa arkaa waxaad camal falaysaan."}]