[{"id": 498572, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6131, "text": "Ma aha Kuwii Gaaloobay ee ahaa ehlu kitaabka (Yahuud iyo Nasaara) iyo Gaaladii kuwo ka taga (xumaanta) intuu uga yimaaddo wax cad (xuja ah)."}, {"id": 498573, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6132, "text": "Oo ah Rasuul Eebc oo akhriyi Kutub daahir ah."}, {"id": 498574, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6133, "text": "Oo xukun fiicani ku yaal."}, {"id": 498575, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6134, "text": "Mona kala tagin Gaalada ehlu Kitaabka ah intuu uga yimaaddo xaq cad (xasad dartiis)."}, {"id": 498576, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6135, "text": "Dadkana waxaan Eebe caabudiddiisa ahayn lama farin iyagoo u kali yeeli Eebe Diinte, toosanna, Salaaddana oogi Zakadana bixin, Saasina waadiinta toosan."}, {"id": 498577, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6136, "text": "Kuwa Gaaloobay oo ehlu Kitaabka ah iyo Gaaladii waxay gali Naarta Jahannama ayna Ini waari dhexdeeda, kuwaasina waa kuwa ugli sharbadan khalqiga."}, {"id": 498578, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6137, "text": "Kuwa rumeeyayse xaqa Camalfiicanna falay kuwaasi waa kuwa ugu khayr badan khalqiga."}, {"id": 498579, "identifier": "so.abduh", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 6138, "text": "Abaalkooda ay Eebe agtiisa kii leeyihiin waa Jannooyin Nagaadi, dhexdeedana ay dureeri Wabiyaal ay kuna waari dhexdeeda waligood, Eebaa ka raalli noqday iyana way ka raali noqdeen, arrintaasna waxay u sugnaatay Ruusii Eeba ka yaaba."}]