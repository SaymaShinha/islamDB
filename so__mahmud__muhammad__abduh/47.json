[{"id": 497029, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4546, "text": "kuwii Gaaloobay oo jidka Eebana ka leexiyay Dadka Eebe waa buriyay camalkooda."}, {"id": 497030, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4547, "text": "kuwa rumeeyay xaqase oo camal fiican falay oo rumeeyay Quraanka lagu soo dajiyay (Nabiga) Muxammad ah ee xaqa ah Eebe wuu astuuri xumaantooda, xaalkoodana wuu wanaajin."}, {"id": 497031, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4548, "text": "saasna waxaa ugu wacan in kuwa Gaaloobayna ay raaceen Baadhil, kuwa rumeeyayna ay raaceen Baadhil, kuwa rumeeyayna ay raaceen xaqa Eebe, saasuuna Eebe u yeelaa tusaale."}, {"id": 497032, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4549, "text": "Haddaad kula kulantaan Gaalada Dagaal Luqunta ka garaaca, markaad sugtaanna ku adkeeya xidhidda, markaas ama iska siidaaya ama haysfurteen, inta Dagaalku ka xasili, xaalku waa saas, hadduu Eebe doonana wuu ka aarsankaraa, hase yeeshee wuxuu saas u falay inuu qaarkiin qaarka kale ku imtixaano, kuwa Jidka Eebe lagu dilana Camalkooda Eebe ma halleeyo."}, {"id": 497033, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4550, "text": "Eebana wuu toosin oo hanuunin kuwaas, wuuna wanaajinn xaalkooda."}, {"id": 497034, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4551, "text": "Wuxuuna galin Janno (uu baray) u caddeeyay."}, {"id": 497035, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4552, "text": "Kuwa xaqa rumeeyow haddaad u gargaartaan Eebe (Diintiisa) isna wuu idiin gargaari, Gommadihinnana wuu sugi (Geesinnimo)."}, {"id": 497036, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4553, "text": "Kuwa Gaaloobayse Halaag iyo (hoos u dhac) yaa u sugnaaday, Camalkoodiina Eebe wuu halleeyay."}, {"id": 497037, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4554, "text": "Saas waxaa ugu wacan inay neceen waxa Eebe soo dajiyay oo xaqa ah, markaasuu hoobiyay Camalkoodii."}, {"id": 497038, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4555, "text": "Miyayna ku soconin dhulka oo markaas ay arkaan siday noqotay Cidhibtii kuwii Gaaloobay ee Ree Maka ka horreeyay, Eebaa halaagay, Gaaladana waxaa u sugnaaday saasoo kale halaag."}, {"id": 497039, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4556, "text": "Saas waxaa ugu wacan in Eebe yahay gargaaraha Mu'miniinta, Gaaladuna ayna gargaare lahayn."}, {"id": 497040, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4557, "text": "Eebe wuxuu galin Mu'miniinta Camalka fiicanna falay Jannooyin ay dureeri dhexdeeda Wabiyaal, kuwa Gaaloobayna way raaxaysan (Addnunka) una cuni sida xooluhu wax u cunaan, Naarna waa Gurigay ku hoyan."}, {"id": 497041, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4558, "text": "Imisay jirtay Magaalo ka xoog badan Magaaladaadan ku bixisay (Maka) aanna hallignay, gargaarana aan helin."}, {"id": 497042, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4559, "text": "Ruux xaqa cad ee Eebe ku sugan ma la midbaa Ruux loo qurxiyay Camalkiisa xun oo haddana waxa naftiisu jeceshahay (ee xun) raaca."}, {"id": 497043, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4560, "text": "Tilmaanta Jannada loogu yaboohay mu'miniinta waa in Wabiyaal biya ah oon doorsoomaynin dhexdeeda yihiin, iyo Wabiyaal Caano ah ooo iyana dhadhankoodu doorsoomaynin, iyo Wabiyaal Khamro (xalaal ah) oo u macaan dadka cabbi, iyo Wabiyaal Malab ah oo la sifeeyay, waxayna Jannada ku leeyihiin nooc kasta ooMidho ah, iyo dambidhaafka Eebe. Ma la midbaa Ruux Narta ku waari lagana waraabin Biyo kulayl daran oo Xiidmahooda gooya/"}, {"id": 497044, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4561, "text": "Waxaa ka mid ah Gaalada kuwo ku dhagaysan (Nabiyow), Markay agtaada ka baxaanna ku dhihi kuwa Cilmiga leh (Asxaabta) Muxuu lahaa dhawaan, kuwaasi waa kuwa Eebe daabacay Quluubtooda, waxay naftoodu jeclaydna (oo xunna) raacay."}, {"id": 497045, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4562, "text": "Kuwa hanuunay (Eebe) wuxuu u siyaadiyaa hanuun, wuxuuna waafajiyaa (Eebe) kayaabiddiisa."}, {"id": 497046, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4563, "text": "Gaaladu ma waxay sugi in Qiyaamadu kado ugu timaaddo, waxaase yimidba Calaamooyinkeedii, ee sideebay xusuus ugu sugnaan hadday Qiyaamadu timaaddo."}, {"id": 497047, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4564, "text": "Ogow (kuna sugnow Nabiyow) inaan Eebe mooyeen Ilaah kale jirin, una dambi dhaaf waydii naftaada iyo mu'miniinta Rag iyo Haweenba, Eebana waa ogyahay dhaqdhqaaqiinna iyo nagaadigiinnaba."}, {"id": 497048, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4565, "text": "Waxay kuwa xaqa rumeeyay dhahaan Maa la soo dejiyo Suurad, markiise la soodajiyo Suurad sugan Dagaalkana (Jahaadkana) lagu sheego waxaad arkaysaa kuwa quluubta ka buka oo ku soo fiirin (Nabiyow) sida Ruux sakaraad hayo oo kale, halaag baynamutaan kuwaasi."}, {"id": 497049, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4566, "text": "Daacadnimo iyo hadal wanaagsan yaa u khayr roonaan lahayd, markase xaalku dhaboobo (Jahaad la galo) haday Eebe u runsheegaan yaa u khayr badan."}, {"id": 497050, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4567, "text": "Amaad mudantihiin haddaad xaqa (Jahaadka) ka jeedsataan inaad fasaadisaan dhulka, Qaraabadiinnana goysaan."}, {"id": 497051, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4568, "text": "Kuwaasi waa kuwa Eebe lacnaday, dhagaha iyo Indhahana tiray."}, {"id": 497052, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4569, "text": "Miyayna Quraanka u fiirsanin (sidhab ah) mise Quluubtoodaa daboolan."}, {"id": 497053, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4570, "text": "Kuwa riddoobay (gaalnimo ku noqday) inta xaqu iyo hanuunku u caddaaday ka dib shaydaanbaa u qurxiyay, yididiilo (been ahna) uguna yaboohay."}, {"id": 497054, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4571, "text": "Saas waxaa ugu wacan iney kuwa riddoobay ku dheheen kuwa xaqa nacay waxaan idinku (waafaqaynaa) adeecaynaa amarka qaarkiis (colnimada Nabiga), Eebana waa ogyahay waxay qarsan."}, {"id": 497055, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4572, "text": "Ee say noqon markay nafta ka qaadayso Malaa'igtu iyadoo ka garaaci Wajiga iyo gadaashaba."}, {"id": 497056, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4573, "text": "Saas waxaa ugu wacan inay raaceen kuwaasi wax Eebe ka cadhoodo, neceenna raalli ahaanshihiisa, markaasuu buriyay camalkoodii."}, {"id": 497057, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4574, "text": "Kuwii Qalbiga ka bukay ma waxay u maleeyeen inaan Eebe soo muujinayn xumaanta (iyo xasadka) Qalbiga kaga jira."}, {"id": 497058, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4575, "text": "Haddaan doonno waan ku tusin karnaa kuwaas, waxaadna ku aqoonsan Calaamad, waxaad kaloo ku oqoonsan hadalka leexsan, Eebana waa ohyahay camalkooda."}, {"id": 497059, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4576, "text": "waannu idin imtixaanaynaa intaan ka muujinno kuwiinna Jahaadi iyo kuwa samra, waana imtixaanaynaa Xaalkiinna."}, {"id": 497060, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4577, "text": "Kuwa gaaloobay ee Jidka Eebe ka leexiya dadka Rasuulkana khilaafa (lana collooba) inta xaqu u caddaaday ka dib waxba Eebe kama dhimayaan Camalkoodana wuu burrin."}, {"id": 497061, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4578, "text": "Kuwa xaqa rumeeyow Eebe adeeca, Rasuukiisana adeeca, Camalkiinnana hayska burrinina."}, {"id": 497062, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4579, "text": "Kuwa gaaloobay oo Jidka Eebe ka leexiyay dadka kadibna dhinta iyagoo gaala ah Eebe uma Dambi dhaafo."}, {"id": 497063, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4580, "text": "Ha tabar darraanina mu'miniintaay ood markaa heshiis u yeedhaan, idinkaaba ka sarreeyee Eebana idinla jiraaye, Camalkiinnana ma nusqaaminaayee."}, {"id": 497064, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4581, "text": "Nolosha Adduunyo waa uun ciyaar iyo dheeldheel, haddaadse xaqa rumeysaan ood dhawrsataan wuxuu idin siin (Eebe) abaalkiinna, xoolihiinnana idin waydiisan maayo."}, {"id": 497065, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4582, "text": "Hadduu idin waydiiyo (Eebe) xoolihiinna (dhammaan) idinkuna celceliyo waad bakhayloobaysaan, waxaana soo bixi colnimadiinna."}, {"id": 497066, "identifier": "so.abduh", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4583, "text": "Kuwaas xaqa rumeeyow waxaa laydiinku yeedhi inaad jidka Eebe wax ku bixisaan, waxaase idinka mid ah kuwo ku bakhayli, Ruuxiise bakhayla wuxuu uun kala bakhaylay Naftiisa Eebana waa hodon, idinkuna waxaad tihiin fuqaro, haddaad xaqa ka jeedsataanse wuxuu Eebe idinku baddali kuwa kale oon idin lamid noqonayn."}]