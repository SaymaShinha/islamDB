[{"id": 497574, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5105, "text": "Eebe wuxuu maqlay Haweenaydii hadalkeeda Nabiga kula doodaysay xaalkii ninkeeda, oo Eebe u sheeganaysay \u00abdhibaatadeeda\u00bb, Eebana wuu maqlayey doodooda, maxaayeelay Ilaahay waa ka wax walba maqle arka."}, {"id": 497575, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5106, "text": "Kuwa haweenkooda kala mid dhiga hooyadood oo kale haweenkoodu ma aha hooyadood, hooyo waa midda wax dhashay, kuwaasina waxay ku hadlayaan hadal xun iyo been Ilaahayna hadal xun iyo been Ilaahayna waa Saamax badane Dhaafid badane."}, {"id": 497576, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5107, "text": "Kuwa haweenkooda kala mid dhigi hooyadood haddana ku noqon \u00abhaweenkii\u00bb waxaa ku waajib ah qoor xoraynteed intayan istaaban, Arrintaasi waa waano, Ilaahayna waxa dadku camal fali waa ogyahay."}, {"id": 497577, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5108, "text": "Ruuxaan helin qoor uu xoreeyo wuxuu soomi Laba Bilood oo israacsan intayan istaaban, ciddiise aan karin soonka wuxuu quudin Lixdan miskiin, arrintu waa saas si aad Eebe u rumeysaan iyo Rasuulkiisa, taasina waa xuduuddii Ilaahay Gaalase waxaa u sugnaaday Cadaab kulul."}, {"id": 497578, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5109, "text": "Kuwa Eebe iyo Rasuulkiisa hilaafaya waxay mudan yihiin halaag sidii loo halaagay kuwii ka horreeyey, Eebana wuxuu soo dajiyey Aayado cad cad Gaalana waxaa u sugnaaday cadaab wax ihaaneeya."}, {"id": 497579, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5110, "text": "Maalinbuuna ihaanayn Ilaahay uu soo bixin dhammaantood, ugana warrami waxay camal faleen, Eebana waa koobay iyana waa hilmaameen Ilaahayna wax kasta wuu daalacan (arkaa)."}, {"id": 497580, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5111, "text": "Ma ogtahay in Ilaahay ogyahay Cirka iyo Dhulka waxa ku jira, Seddexdii faqiba Ilaahay baa afreeya, Shantana Ilaahay baa Lixeeya; waxa ka yar iyo waxa ka badanba Ilaaahaybaa la jooga meeshay ahaadaanba, wuxuuna uga qarrami waxay camal faleen Maalinta Qiyaamo Ilaahayna wax walba waa ogyahay."}, {"id": 497581, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5112, "text": "Miyaadan ogayn kuwa laga reebay faqa xun oo haddana ku noqon waxa laga reebay, kuna faqi dambi iyo cadawnimo iyo caasinnimada Rasuulka, markay Nabiga u yimaadaanna ku salaamaya waxaan Eebe ku salaamin oo hoos u dhihi maa Eebe nagu cadaabo waxaan ku hadlayno waxaa ku filan Jahaannamo oo ay galayaan waana Guri xun."}, {"id": 497582, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5113, "text": "Kuwa rumeeyayow markaad faqaysaan ha ku faqina dambi, colnimo iyo caasinnimada Rasuulka ee ku faqa wanaag dhawrsani, Eebana ka yaaba isagaa la isu soo kulmine."}, {"id": 497583, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5114, "text": "Faqa xun Shaydaan buu kayimaaddaa suu u walbahaar geliyo kuwa xaqa rumeeyay, waxbase kama dhibo idan Ilaahay Mooyee, Ilaahay uun ha tala saarteen kuwa xaqa rumeeyey."}, {"id": 497584, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5115, "text": "Kuwa xaqa rumeeyayow haddii la idin dhaho isu waasiciya fadhiga; isuwaasiciya Ilaahay wanaagga ha idiin waasiciyee, marka la idin yiraahdo Jihaadka iyo Khayrka u degdegana; u degdega Ilaahay wuu koryeelayaa kuwa xaqa rumeeya kuwa cilmiga lehna daragooyin yuu koryeelayaa Eebana waxa la camal fali waa ogyahay."}, {"id": 497585, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5116, "text": "Kuwa xaqa rumeeyayow markaad Nabiga qaabilaysaan hormariya Sadaqo, saasaa idiin khayr badan idiina nadiifsan, haddaydaan helaynin Eebe waa dambi dhaaf badane Naxariis badane."}, {"id": 497586, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5117, "text": "Ma waxaad ka cabsateen inaad hormarsataan Sadaqooyin markaad Nabiga la kulmaysaan; haddaydaan saas falin Eebe waa idin ka toobada aqbalaye ee Salaadda ooga Sakadana bixiya Ilaahaay iyo Rasuulkiisana adeeca, Eebana waa ogyahay waxaad camal falaysaan."}, {"id": 497587, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5118, "text": "Ma ogtahay kuwii ka sokeeye dhigtay kuwa Eebe u cadhooday kuwaasi idin ka mid ma aha iyagana ka mid ma aha (waa munaafiqiinta) waxayna ku dhaaran been iyagoo og."}, {"id": 497588, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5119, "text": "Eebe wuxuu u darbay kuwaas cadaab daran maxaa yeelay wax xun bay falayeen."}, {"id": 497589, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5120, "text": "Dhaartoodii bay gaashaan ka dhigteen dadkana xaqiibay ka leexiyeen waxayna mudan yihiin cadaab wax ihaaneeya."}, {"id": 497591, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5121, "text": "Xoolahooda iyo Caruurtoodu wax ugama taraan Eebe xaggiisa, waana kuwa ehelu Naar ah wayna ku waari."}, {"id": 497592, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5122, "text": "Maalinta Eebe soo bixiyo dhamaantood way u dhaaran Eebe siday idinka idiinku dhaartaan iyagoo u malayn inay wax (xuja ah) haystaan waase kuwo beenaalayaal ah."}, {"id": 497593, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5123, "text": "Waxaa ka qaalib noqday Shaydaan oo hilaamsiiyey xusidda Eebe, kuwaasi waa Xisbiga shaydaanka, Xisbiga shaydaankuna waa kuwa khasaaray."}, {"id": 497594, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5124, "text": "Kuwa Ilaahay iyo Rasuulkiisa khilaafaya waxay ka mid noqon kuwa dullooba."}, {"id": 497595, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5125, "text": "Eebe wuxuu xugmiyey inuu adkaado isaga iyo Rasuulkiisa, maxaa yeelay waa xoog badane adkaade ah."}, {"id": 497596, "identifier": "so.abduh", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 5126, "text": "Ma helaysid dad Ilaahay rumeeyey iyo Maalintii dambeeysay oo haddana jecel ruux Ilaahay iyo Rasuulkiisa khilaafaya haba ahaadeen Aabayowgood, Caruurtooda, Walaahood ama Qaraabadooda, kuwaas (aan jeelaanin) waa kuwo qalbigooda iimaanka lagu dhigay Eebena ku xoojiyey gargaarkiisa, wuxuuna gelin Jannooyin Wabiyaal dhex socdaan ay kuna waari, kuwaas Ilaahay baa ka raalli noqday iyana way ka raallii noqdeen waana Xisbiga Ilaahay; Xisbiga Ilaahayna waa kuwa Liibaanay."}]