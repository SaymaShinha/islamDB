[{"id": 497371, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4902, "text": "(Eebaha) Raxmaan ah."}, {"id": 497372, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4903, "text": "Wuxuu baray (Nabiga iyo dadka kalaba) Quraanka."}, {"id": 497373, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4904, "text": "Dadkana wuu abuuray."}, {"id": 497374, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4905, "text": "Wuxuuna baray dadka (Hadalka)."}, {"id": 497375, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4906, "text": "Qorraxda iyo dayaxuna waxay ku socdaan xisaab."}, {"id": 497376, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4907, "text": "Xiddigga iyo Geedkuba way Sujuudaan (Eebe Dartiis)."}, {"id": 497377, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4908, "text": "Samadana Eebaa koryeelay, Garsoorna wuu dajiyay."}, {"id": 497378, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4909, "text": "Si aydaan uga gudbin Caddaaladda."}, {"id": 497379, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4910, "text": "Miisaanka ku hagaajiya Caddaalad, hana nuqaamininna."}, {"id": 497380, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4911, "text": "Dhulkana Eebe wuxuu u dajiyay khalqiga."}, {"id": 497381, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4912, "text": "Dhexdiisa waxaa ah Faakihad (khudaar) iyo Timir Fiido leh."}, {"id": 497382, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4913, "text": "Xubuubna (midho) ball leh iyo udgoona wuu abuuray."}, {"id": 497383, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4914, "text": "Nicmooyinka Eebe teebaad beeninaysaan (Insi iyo Jinniyow)."}, {"id": 497384, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4915, "text": "Dadka Eebaa ka abuuray dhoobo Jilay oo kale ah."}, {"id": 497385, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4916, "text": "Jaankana wuxuu ka abuuray ololka Naarta."}, {"id": 497386, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4917, "text": "Nicmooyinka Eebe teebaad beeninaysaan."}, {"id": 497387, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4918, "text": "Ilaaheen waa Eebaha Labada Qorrax ka soo bax iyo Labada Qorrax u dhac."}, {"id": 497388, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4919, "text": "Nicmooyinka Eebe teebaad beeninaysaan."}, {"id": 497389, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4920, "text": "Eebe waan kan Labada Badood dariseeyey iyagoo kulmi."}, {"id": 497390, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4921, "text": "Dhexdoodana yeelay soohdin (kala reebta) iskumana xadgudbaan."}, {"id": 497391, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4922, "text": "Nicmooyinka Eebe teebaad beeninaysaan."}, {"id": 497392, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4923, "text": "Waxaa lagala baxaa Baddaas Lu'Lu'iyo Murjaan (Jawhar)."}, {"id": 497393, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4924, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497394, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4925, "text": "Eebe waxaa u sugnaaday Doomaha socda Badda oo Buuro la mid ah."}, {"id": 497395, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4926, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497396, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4927, "text": "Dhammaan waxa (Dhulka) korkiisa ah wuu tagi."}, {"id": 497397, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4928, "text": "Waxaase hadhi Eebaha wayn ee sharafta leh."}, {"id": 497398, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4929, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497399, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4930, "text": "Waxaa Eebe wax waydiista Samada iyo Dhulka waxa ku sugan, goorwalbana wuxuu maamuli arrin."}, {"id": 497400, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4931, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497401, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4932, "text": "Waana idin xisaabin si dhab ah Insi iyo Jinniyow."}, {"id": 497402, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4933, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497403, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4934, "text": "Jinni iyo Insiyow haddaad kartaan inaad ka baxdaan Samada Hajooyinkooda iyo Dhulka ka baxa, kamana bixikartaan xujo la'aan."}, {"id": 497404, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4935, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497405, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4936, "text": "Eebe (hadduu doono) wuxuu idinku diri Olol Naar ah, iyo Maar (la dhalaaliyay) umana gargaarsanaysaan."}, {"id": 497406, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4937, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497407, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4938, "text": "Markay Samadu dillaacdo oy noqoto ubax guduudan oo subag la mid ah (dhalaasho) markaas dadkaa la abaalmarin)."}, {"id": 497408, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4939, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497409, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4940, "text": "Maalintaas Ruux la warsan dambigiisa Insi iyo Jinniba ma jiro."}, {"id": 497410, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4941, "text": "Nicmooyinka Eebe teebaad beeninaysaan."}, {"id": 497411, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4942, "text": "Dambiilayaasha waxaa lagu aqoonsan astaamahooda, waxaana la qaban Foodda iyo gommadaha (Naartaana lagu tuuri)."}, {"id": 497412, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4943, "text": "Nimcooyinka Eebe teebaad beeninaysaan,"}, {"id": 497413, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4944, "text": "Tani waa Jahannamadii ay beeniyeen Dambiilayaashu (yaa lagu dhihi)."}, {"id": 497414, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4945, "text": "Waxay ku meereysan Jahannamo iyo biyo kulayl daran dhexdooda."}, {"id": 497415, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "so", "name": "Abduh", "transelator_en_name": "Mahmud Muhammad Abduh", "format": "text", "type": "translation", "ayah_number": 4946, "text": "Nimcooyinka Eebe teebaad beeninaysaan."}, {"id": 497416, "identifier": "so.abduh", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation"