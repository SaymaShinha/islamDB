[{"id": 142514, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5496, "text": "Der du dich zugedeckt hast,"}, {"id": 142515, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5497, "text": "Steh auf und warne,"}, {"id": 142516, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5498, "text": "Und preise die Gr\u00f6\u00dfe deines Herrn,"}, {"id": 142517, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5499, "text": "Und reinige deine Kleider,"}, {"id": 142518, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5500, "text": "Und entferne dich von der Unreinheit (des G\u00f6tzendienstes),"}, {"id": 142519, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5501, "text": "Und poch nicht auf dein Verdienst, um mehr zu erhalten,"}, {"id": 142520, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5502, "text": "Und sei geduldig, bis dein Herr sein Urteil f\u00e4llt."}, {"id": 142521, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5503, "text": "Wenn dann in das Horn gesto\u00dfen wird,"}, {"id": 142522, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5504, "text": "Dann ist es an jenem Tag ein schwerer Tag,"}, {"id": 142523, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5505, "text": "F\u00fcr die Ungl\u00e4ubigen nicht leicht."}, {"id": 142524, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5506, "text": "La\u00df Mich in Ruhe mit dem, den Ich allein erschaffen habe,"}, {"id": 142525, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5507, "text": "Und dem Ich ausgedehntes Verm\u00f6gen verliehen habe,"}, {"id": 142526, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5508, "text": "Und auch S\u00f6hne, die als Zeugen da sind,"}, {"id": 142527, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5509, "text": "Und dem Ich alles sch\u00f6n geebnet habe,"}, {"id": 142528, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5510, "text": "Der aber erhofft, da\u00df Ich (ihm) noch mehr gebe."}, {"id": 142529, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5511, "text": "Nein, er zeigte sich widerspenstig gegen meine Zeichen."}, {"id": 142530, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5512, "text": "Ich werde M\u00fchsal auf ihm schwer lasten lassen."}, {"id": 142531, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5513, "text": "Er dachte nach und wog ab."}, {"id": 142532, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5514, "text": "Dem Tod geweiht sei er, wie seine Abw\u00e4gung war!"}, {"id": 142533, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5515, "text": "Noch einmal: Dem Tod geweiht sei er, wie seine Abw\u00e4gung war!"}, {"id": 142534, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5516, "text": "Dann schaute er,"}, {"id": 142535, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5517, "text": "Dann runzelte er die Stirn und zeigte Widerwillen,"}, {"id": 142536, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5518, "text": "Dann kehrte er den R\u00fccken und verhielt sich hochm\u00fctig,"}, {"id": 142537, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5519, "text": "Und er sagte: \u00abDas ist doch nur eine Zauberei, die (sonst) \u00fcberliefert wird."}, {"id": 142538, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5520, "text": "Das sind nur die Worte von Menschen.\u00bb"}, {"id": 142539, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5521, "text": "Ich werde ihn im H\u00f6llenfeuer brennen lassen."}, {"id": 142540, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5522, "text": "Woher sollst du wissen, was das H\u00f6llenfeuer ist?"}, {"id": 142541, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5523, "text": "Es l\u00e4\u00dft keinen Rest bestehen, und es l\u00e4\u00dft nichts \u00fcbrigbleiben."}, {"id": 142542, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5524, "text": "Es versengt die Haut."}, {"id": 142543, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5525, "text": "Es hat \u00fcber sich neunzehn (W\u00e4chter)."}, {"id": 142544, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5526, "text": "Als W\u00e4chter des Feuers haben Wir nur Engel eingesetzt. Und ihre Zahl haben Wir zu einer Versuchung gemacht f\u00fcr die, die ungl\u00e4ubig sind, damit diejenigen Gewi\u00dfheit erlangen, denen das Buch zugekommen ist, und damit diejenigen, die glauben, an Glauben zunehmen, und damit diejenigen, denen das Buch zugekommen ist, und auch die Gl\u00e4ubigen keinen Zweifel hegen, und damit diejenigen, in deren Herzen Krankheit ist, und (auch) die Ungl\u00e4ubigen sagen: \u00abWas will denn Gott mit einem solchen Gleichnis?\u00bb Auf diese Weise f\u00fchrt Gott irre, wen Er will, und leitet recht, wen Er will. Und \u00fcber die Heerscharen deines Herrn wei\u00df nur Er Bescheid. Und es ist nur eine Ermahnung f\u00fcr die Menschen."}, {"id": 142545, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5527, "text": "Nein, beim Mond,"}, {"id": 142546, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5528, "text": "Bei der Nacht, wenn sie den R\u00fccken kehrt,"}, {"id": 142547, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5529, "text": "Beim Morgen, wenn er anbricht,"}, {"id": 142548, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5530, "text": "Siehe, sie ist eine der gr\u00f6\u00dften (Gestalten des Verderbens),"}, {"id": 142549, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5531, "text": "Zur Warnung f\u00fcr die Menschen,"}, {"id": 142550, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5532, "text": "F\u00fcr jeden von euch, der vorankommen oder zur\u00fcckbleiben will."}, {"id": 142551, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5533, "text": "Ein jeder haftet f\u00fcr das, was er erworben hat,"}, {"id": 142552, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5534, "text": "Ausgenommen die von der Rechten;"}, {"id": 142553, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5535, "text": "Sie befinden sich in G\u00e4rten, und sie fragen einander"}, {"id": 142554, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5536, "text": "Nach den \u00dcbelt\u00e4tern:"}, {"id": 142555, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5537, "text": "\u00abWas hat euch ins H\u00f6llenfeuer getrieben?\u00bb"}, {"id": 142556, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5538, "text": "Sie sagen: \u00abWir geh\u00f6rten nicht zu denen, die beteten,"}, {"id": 142557, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5539, "text": "Und wir pflegten nicht den Bed\u00fcrftigen zu speisen,"}, {"id": 142558, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5540, "text": "Und wir gingen auf ausschweifende Reden ein mit denen, die solche hielten,"}, {"id": 142559, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5541, "text": "Und wir erkl\u00e4rten den Tag des Gerichtes f\u00fcr L\u00fcge,"}, {"id": 142560, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5542, "text": "Bis das sichere Ende uns ereilte.\u00bb"}, {"id": 142561, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5543, "text": "Nun n\u00fctzt ihnen die F\u00fcrsprache der F\u00fcrsprechenden nicht."}, {"id": 142562, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5544, "text": "Was ist mit ihnen, da\u00df sie sich von der Ermahnung abwenden,"}, {"id": 142563, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5545, "text": "Als w\u00e4ren sie aufgeschreckte Wildesel,"}, {"id": 142564, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5546, "text": "Die vor einem jagenden L\u00f6wen fliehen?"}, {"id": 142565, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5547, "text": "Aber nein, jeder von ihnen will, da\u00df ihm aufgeschlagene Bl\u00e4tter zukommen."}, {"id": 142566, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5548, "text": "Nein, sie f\u00fcrchten sich nicht vor dem Jenseits."}, {"id": 142567, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5549, "text": "Nein, er ist eine Erinnerung."}, {"id": 142568, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5550, "text": "Wer will, gedenkt seiner."}, {"id": 142569, "identifier": "de.khoury", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5551, "text": "Sie werden aber (seiner) nicht gedenken, es sei denn, Gott will es. Ihm geb\u00fchrt, da\u00df man Ihn f\u00fcrchtet, und Ihm geb\u00fchrt zu vergeben."}]