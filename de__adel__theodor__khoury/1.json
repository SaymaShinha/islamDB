[{"id": 137234, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 1, "text": "Im Namen Gottes, des Erbarmers, des Barmherzigen."}, {"id": 137235, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 2, "text": "Lob sei Gott, dem Herrn der Welten,"}, {"id": 137236, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 3, "text": "Dem Erbarmer, dem Barmherzigen,"}, {"id": 137232, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4, "text": "Der Verf\u00fcgungsgewalt besitzt \u00fcber den Tag des Gerichtes!"}, {"id": 143404, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5, "text": "Dir dienen wir, und Dich bitten wir um Hilfe."}, {"id": 143405, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6, "text": "F\u00fchre uns den geraden Weg,"}, {"id": 143406, "identifier": "de.khoury", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 7, "text": "Den Weg derer, die Du begnadet hast, die nicht dem Zorn verfallen und nicht irregehen."}]