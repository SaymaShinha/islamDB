[{"id": 142982, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5968, "text": "Ist die Geschichte von der alles bedeckenden Katastrophe zu dir. gelangt?"}, {"id": 142983, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5969, "text": "An jenem Tag wird es gesenkte Gesichter geben,"}, {"id": 142984, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5970, "text": "Die sich abarbeiten und M\u00fchsal erleiden,"}, {"id": 142985, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5971, "text": "In einem gl\u00fchenden Feuer. brennen"}, {"id": 142986, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5972, "text": "Und aus einer siedenden Quelle zu trinken bekommen."}, {"id": 142987, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5973, "text": "Ihre Speise besteht nur aus trockenen Dornen,"}, {"id": 142988, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5974, "text": "Die weder fett machen noch gegen den Hunger helfen."}, {"id": 142989, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5975, "text": "An jenem Tag wird es (auch) fr\u00f6hliche Gesichter geben,"}, {"id": 142990, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5976, "text": "Die mit ihrem M\u00fchen zufrieden sind"}, {"id": 142991, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5977, "text": "Und in einem hochgelegenen Garten sind,"}, {"id": 142992, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5978, "text": "In dem sie keine unbedachten Reden h\u00f6ren."}, {"id": 142993, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5979, "text": "Es gibt darin eine flie\u00dfende Quelle,"}, {"id": 142994, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5980, "text": "Und es gibt darin erh\u00f6hte Betten,"}, {"id": 142995, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5981, "text": "Hingestellte Humpen,"}, {"id": 142996, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5982, "text": "In Reihen gelegte Kissen"}, {"id": 142997, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5983, "text": "Und ausgebreitete Teppiche."}, {"id": 142998, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5984, "text": "Schauen sie denn nicht zu den Kamelen, wie sie erschaffen wurden,"}, {"id": 142999, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5985, "text": "Und zum Himmel, wie er emporgehoben wurde,"}, {"id": 143000, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5986, "text": "Und zu den Bergen, wie sie aufgerichtet wurden,"}, {"id": 143001, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5987, "text": "Und zu der Erde, wie sie als Fl\u00e4che gemacht wurde?"}, {"id": 143002, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5988, "text": "So ermahne. Du bist ja ein Mahner."}, {"id": 143003, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5989, "text": "Du hast sie nicht fest in der Hand."}, {"id": 143004, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5990, "text": "Wer sich aber abkehrt und ungl\u00e4ubig bleibt,"}, {"id": 143005, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5991, "text": "Den peinigt Gott mit der gr\u00f6\u00dften Pein."}, {"id": 143006, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5992, "text": "Zu Uns erfolgt ihre R\u00fcckkehr,"}, {"id": 143007, "identifier": "de.khoury", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5993, "text": "Und dann obliegt es Uns, mit ihnen abzurechnen."}]