[{"id": 143166, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6169, "text": "Der Wettstreit um noch mehr lenkt euch ab,"}, {"id": 143167, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6170, "text": "Da\u00df ihr (sogar) die Gr\u00e4ber besucht."}, {"id": 143168, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6171, "text": "Nein, ihr werdet es noch zu wissen bekommen."}, {"id": 143169, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6172, "text": "Noch einmal: Nein, ihr werdet es noch zu wissen bekommen."}, {"id": 143170, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6173, "text": "Nein, wenn ihr es nur mit Gewi\u00dfheit w\u00fc\u00dftet!"}, {"id": 143171, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6174, "text": "Ihr werdet bestimmt die H\u00f6lle sehen."}, {"id": 143172, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6175, "text": "Noch einmal: Ihr werdet sie mit v\u00f6lliger Gewi\u00dfheit sehen."}, {"id": 143173, "identifier": "de.khoury", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 6176, "text": "Dann werdet ihr an jenem Tag euer angenehmes Leben zu verantworten haben."}]