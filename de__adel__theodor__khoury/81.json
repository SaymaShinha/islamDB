[{"id": 142819, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5801, "text": "Wenn die Sonne (von einer H\u00fclle) umwunden wird,"}, {"id": 137216, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5802, "text": "Und wenn die Sterne herabst\u00fcrzen,"}, {"id": 137215, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5803, "text": "Und wenn die Berge versetzt werden,"}, {"id": 142820, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5804, "text": "Und wenn die im zehnten Monat stehenden Kamelstuten vernachl\u00e4ssigt werden,"}, {"id": 142821, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5805, "text": "Und wenn die wilden Tiere versammelt werden,"}, {"id": 142822, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5806, "text": "Und wenn die Meere angef\u00fcllt werden,"}, {"id": 142823, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5807, "text": "Und wenn die Seelen zu ihren Partnern gesellt werden,"}, {"id": 142824, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5808, "text": "Und wenn das M\u00e4dchen, das verscharrt wurde, gefragt wird,"}, {"id": 142825, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5809, "text": "Wegen welcher S\u00fcnde es denn get\u00f6tet wurde,"}, {"id": 142826, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5810, "text": "Und wenn die Bl\u00e4tter ausgebreitet werden,"}, {"id": 142827, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5811, "text": "Und wenn der Himmel (wie ein Fell) abgezogen wird,"}, {"id": 142828, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5812, "text": "Und wenn die H\u00f6lle angefacht wird,"}, {"id": 142829, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5813, "text": "Und wenn das Paradies herangebracht wird,"}, {"id": 142830, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5814, "text": "Dann wird jeder erfahren, was er vorgebracht hat."}, {"id": 142831, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5815, "text": "Nein, Ich schw\u00f6re bei den r\u00fcckl\u00e4ufigen Planeten,"}, {"id": 142832, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5816, "text": "Die dahineilen und sich in ihrem Bau verbergen,"}, {"id": 142833, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5817, "text": "Und bei der Nacht, wenn sie an einem Ende steht,"}, {"id": 142834, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5818, "text": "Und beim Morgen, wenn er anbricht:"}, {"id": 142835, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5819, "text": "Das ist die Rede eines edlen Gesandten,"}, {"id": 142836, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5820, "text": "Der Kraft besitzt und beim Herrn des Thrones hochgestellt ist,"}, {"id": 142837, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5821, "text": "Dem man gehorcht und der treu ist."}, {"id": 142838, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5822, "text": "Euer Gef\u00e4hrte ist kein Besessener."}, {"id": 142839, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5823, "text": "Er hat ihn gewi\u00df am deutlichen Horizont gesehen,"}, {"id": 142840, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5824, "text": "Und er h\u00e4lt nicht aus Geiz das (ihm offenbarte) Verborgene zur\u00fcck."}, {"id": 142841, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5825, "text": "Das ist nicht die Rede eines gesteinigten Satans."}, {"id": 142842, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5826, "text": "Wo geht ihr denn hin?"}, {"id": 142843, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5827, "text": "Das ist nur eine Ermahnung f\u00fcr die Weltenbewohner,"}, {"id": 142844, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5828, "text": "F\u00fcr die von euch, die sich recht verhalten wollen."}, {"id": 142845, "identifier": "de.khoury", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5829, "text": "Und ihr wollt nicht, es sei denn, Gott will es, (Er), der Herr der Welten."}]