[{"id": 142731, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5713, "text": "Bei denen, die mit Gewalt entrei\u00dfen,"}, {"id": 142732, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5714, "text": "Und denen, die mit Leichtigkeit herausziehen,"}, {"id": 142733, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5715, "text": "Und denen, die schnell dahinschwimmen,"}, {"id": 142734, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5716, "text": "Und allen vorauseilen,"}, {"id": 142735, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5717, "text": "Und die Angelegenheiten regeln!"}, {"id": 142736, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5718, "text": "Am Tag, da das Beben einsetzt"}, {"id": 142737, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5719, "text": "Und das n\u00e4chste darauf folgt,"}, {"id": 142738, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5720, "text": "An jenem Tag werden die Herzen in unruhiger Angst dastehen,"}, {"id": 142739, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5721, "text": "Mit gesenkten Blicken."}, {"id": 142740, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5722, "text": "Sie sagen: \u00abSollen wir wirklich in den fr\u00fcheren Zustand zur\u00fcckgebracht werden?"}, {"id": 142741, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5723, "text": "Auch wenn wir zu morschen Knochen geworden sind?\u00bb"}, {"id": 142742, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5724, "text": "Sie sagen: \u00abDas w\u00e4re dann eine verlustreiche R\u00fcckkehr.\u00bb"}, {"id": 142743, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5725, "text": "Es wird ein einziger Schelteschrei sein,"}, {"id": 142744, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5726, "text": "Und schon befinden sie sich auf der Erdoberfl\u00e4che."}, {"id": 142745, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5727, "text": "Ist die Geschichte von Mose zu dir gelangt?"}, {"id": 142746, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5728, "text": "Als sein Herr ihm im heiligen Tal Tuwaa zurief:"}, {"id": 142747, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5729, "text": "\u00abGeh zu Pharao. Er zeigt ein \u00dcberma\u00df an Frevel."}, {"id": 142748, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5730, "text": "Und sag: M\u00f6chtest du dich nicht l\u00e4utern,"}, {"id": 142749, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5731, "text": "Und da\u00df ich dich zu deinem Herrn rechtleite, so da\u00df du gottesf\u00fcrchtig wirst?\u00bb"}, {"id": 142750, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5732, "text": "Er lie\u00df ihn das gr\u00f6\u00dfte Zeichen sehen."}, {"id": 142751, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5733, "text": "Er aber erkl\u00e4rte (es) f\u00fcr L\u00fcge und war ungehorsam."}, {"id": 142752, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5734, "text": "Dann kehrte er den R\u00fccken und eilte davon."}, {"id": 142753, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5735, "text": "Er versammelte (sein Volk) und rief (ihnen) zu."}, {"id": 142754, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5736, "text": "Er sagte: \u00abIch bin euer Herr, der H\u00f6chste.\u00bb"}, {"id": 142755, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5737, "text": "Da ergriff ihn Gott mit der Strafe des Jenseits und des Diesseits."}, {"id": 142756, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5738, "text": "Darin ist eine Lehre f\u00fcr die Gottesf\u00fcrchtigen."}, {"id": 142757, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5739, "text": "Ist es etwa schwerer, euch zu erschaffen, als den Himmel, den Er aufgebaut hat?"}, {"id": 142758, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5740, "text": "Er hob sein Dach empor und gestaltete ihn."}, {"id": 142759, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5741, "text": "Und Er lie\u00df seine Nacht dunkel werden und seinen Morgen hervorkommen."}, {"id": 142760, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5742, "text": "Und Er breitete danach die Erde aus."}, {"id": 142761, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5743, "text": "Er lie\u00df ihr Wasser und ihre Weidefl\u00e4chen aus ihr hervorkommen."}, {"id": 142762, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5744, "text": "Und Er gr\u00fcndete die Berge fest."}, {"id": 142763, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5745, "text": "Dies als Nutznie\u00dfung f\u00fcr euch und euer Vieh."}, {"id": 142764, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5746, "text": "Wenn dann die gro\u00dfe, alles \u00fcberw\u00e4ltigende Katastrophe kommt,"}, {"id": 142765, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5747, "text": "Am Tag, da der Mensch sich an das erinnert, was er erstrebt hat,"}, {"id": 142766, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5748, "text": "Und die H\u00f6lle denen sichtbar gemacht wird, die sehen k\u00f6nnen,"}, {"id": 142767, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5749, "text": "Dann wird dem, der ein \u00dcberma\u00df an Frevel gezeigt"}, {"id": 142768, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5750, "text": "Und das diesseitige Leben vorgezogen hat,"}, {"id": 142769, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5751, "text": "Die H\u00f6lle zur Heimst\u00e4tte."}, {"id": 142770, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5752, "text": "Und dem, der den Stand seines Herrn gef\u00fcrchtet und seiner Seele ihre (b\u00f6sen) Neigungen verwehrt hat,"}, {"id": 142771, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5753, "text": "Wird das Paradies zur Heimst\u00e4tte."}, {"id": 142772, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5754, "text": "Sie fragen dich nach der Stunde, wann sie feststehen wird."}, {"id": 142773, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5755, "text": "Was hast du denn davon, sie \u00f6fter zu erw\u00e4hnen?"}, {"id": 142774, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5756, "text": "Ihre Schlu\u00dfetappe f\u00fchrt zu deinem Herrn."}, {"id": 142775, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Du hast nur die zu warnen, die sie f\u00fcrchten."}, {"id": 142776, "identifier": "de.khoury", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 5758, "text": "Am Tag, da sie sie sehen werden, ist es so, als h\u00e4tten sie nur einen Abend verweilt oder den Morgen darauf."}]