[{"id": 141771, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4736, "text": "Beim Berg;"}, {"id": 141772, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4737, "text": "Bei einem Buch, das zeilenweise niedergeschrieben ist"}, {"id": 141560, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4738, "text": "Auf ausgebreitetem Pergament;"}, {"id": 137198, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4739, "text": "Bei dem (zur Pilgerzeit) besuchten Haus"}, {"id": 137202, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4740, "text": "Und bei der hochgezogenen Decke,"}, {"id": 137205, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4741, "text": "Und beim angef\u00fcllten Meer!"}, {"id": 137203, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4742, "text": "Die Pein deines Herrn wird eintreffen."}, {"id": 137204, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4743, "text": "Es gibt niemanden, der sie abwehren k\u00f6nnte."}, {"id": 137206, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4744, "text": "Am Tag, da der Himmel in heftiges Schwanken ger\u00e4t"}, {"id": 137207, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4745, "text": "Und die Berge sich fortbewegen,"}, {"id": 141773, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4746, "text": "Wehe an jenem Tag denen, die (die Botschaft) f\u00fcr L\u00fcge erkl\u00e4ren,"}, {"id": 141774, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4747, "text": "Die in ausschweifenden Reden ihr Spiel treiben!"}, {"id": 141775, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4748, "text": "Am Tag, da sie unerbittlich ins Feuer der H\u00f6lle gesto\u00dfen werden:"}, {"id": 141776, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4749, "text": "\u00abDas ist das Feuer, das ihr immer wieder f\u00fcr L\u00fcge erkl\u00e4rt habt."}, {"id": 141777, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4750, "text": "Ist dies nun Zauberei, oder k\u00f6nnt ihr nicht sehen?"}, {"id": 141778, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4751, "text": "Ihr sollt darin brennen. Es ist f\u00fcr euch gleich, ob ihr es geduldig ertragt oder nicht. Euch wird das vergolten, was ihr zu tun pflegtet.\u00bb"}, {"id": 141779, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4752, "text": "Die Gottesf\u00fcrchtigen befinden sich in G\u00e4rten und Wonne."}, {"id": 141780, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4753, "text": "Sie empfinden Wohlbehagen an dem, was ihnen ihr Herr zukommen l\u00e4\u00dft. Und ihr Herr bewahrt sie vor der Pein der H\u00f6lle."}, {"id": 141781, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4754, "text": "\u00abE\u00dft und trinkt zu eurem Wohl f\u00fcr das, was ihr zu tun pflegtet.\u00bb"}, {"id": 141782, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4755, "text": "Sie lehnen sich auf gereihten Betten. Und Wir verm\u00e4hlen sie mit gro\u00df\u00e4ugigen Huri."}, {"id": 141783, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4756, "text": "Und zu denen, die glauben und denen ihre Nachkommenschaft im Glauben folgt, lassen Wir ihre Nachkommenschaft sto\u00dfen. Und Wir verringern ihnen nichts von ihren Werken. Ein jeder haftet f\u00fcr das, was er erworben hat."}, {"id": 141784, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4757, "text": "Und Wir versorgen sie mit Fr\u00fcchten und Fleisch von dem, was sie begehren."}, {"id": 141785, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4758, "text": "Darin greifen sie untereinander nach einem Becher, der nicht zu unbedachter Rede verleitet und in dem nichts S\u00fcndhaftes steckt."}, {"id": 141786, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4759, "text": "Und unter ihnen machen die Runde J\u00fcnglinge, die zu ihnen geh\u00f6ren, als w\u00e4ren sie wohlverwahrte Perlen."}, {"id": 141787, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4760, "text": "Und sie treten aneinander heran und fragen sich gegenseitig."}, {"id": 141788, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4761, "text": "Sie sagen: \u00abFr\u00fcher waren wir inmitten unserer Angeh\u00f6rigen erschrocken."}, {"id": 141789, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4762, "text": "Da erwies Gott uns eine Wohltat und bewahrte uns vor der Pein des gl\u00fchenden Windes."}, {"id": 141790, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4763, "text": "Fr\u00fcher pflegten wir Ihn anzurufen. Er ist der, der g\u00fctig und barmherzig ist.\u00bb"}, {"id": 141791, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4764, "text": "Ermahne nun; du bist dank der Gnade deines Herrn weder ein Wahrsager noch ein Besessener."}, {"id": 141792, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Oder sagen sie etwa: \u00abEin Dichter. Wir warten ab, was f\u00fcr ein Unheil das Schicksal ihm bringen wird\u00bb?"}, {"id": 141793, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4766, "text": "Sprich: Wartet ab, ich geh\u00f6re mit euch zu denen, die abwarten."}, {"id": 141794, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Oder befiehlt ihnen etwa ihr Verstand dies, oder sind sie vielmehr Leute, die ein \u00dcberma\u00df an Frevel zeigen?"}, {"id": 141795, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Oder sagen sie: \u00abEr hat ihn sich selbst in den Mund gelegt\u00bb? Nein, vielmehr glauben sie nicht."}, {"id": 141796, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4769, "text": "Sie sollen doch eine Botschaft gleicher Art beibringen, so sie die Wahrheit sagen."}, {"id": 141797, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4770, "text": "Oder sind sie aus etwas anderem erschaffen worden, oder sind sie gar selbst die Sch\u00f6pfer?"}, {"id": 141798, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Oder haben sie etwa die Himmel und die Erde erschaffen? Nein, vielmehr hegen sie keine Gewi\u00dfheit."}, {"id": 141799, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Oder besitzen sie etwa die Vorratskammern deines Herrn, oder sind sie es, die alles fest in der Hand haben?"}, {"id": 141800, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Oder haben sie eine Leiter, auf der sie (emporsteigen und) lauschen k\u00f6nnen? Dann soll doch ihr Lauscher eine offenkundige Erm\u00e4chtigung beibringen."}, {"id": 141801, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Oder sind f\u00fcr Ihn die T\u00f6chter und f\u00fcr euch die S\u00f6hne bestimmt?"}, {"id": 141802, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Oder verlangst du etwa einen Lohn von ihnen, so da\u00df sie mit Schulden belastet w\u00e4ren?"}, {"id": 141803, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Oder liegt ihnen das Unsichtbare vor, so da\u00df sie (es) aufschreiben?"}, {"id": 141804, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Oder wollen sie arglistig handeln? Diejenigen, die ungl\u00e4ubig sind, sind es, die der List erliegen."}, {"id": 141805, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Oder haben sie einen anderen Gott als Gott? Preis sei Gott! (Erhaben ist Er) \u00fcber das, was sie (Ihm) beigesellen."}, {"id": 141806, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4779, "text": "Und s\u00e4hen sie ein St\u00fcck vom Himmel herabfallen, sie w\u00fcrden sagen: \u00abEine aufgeschichtete Wolke.\u00bb"}, {"id": 141807, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4780, "text": "La\u00df sie nur, bis sie ihrem Tag begegnen, an dem sie vom Donnerschlag getroffen werden,"}, {"id": 141808, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4781, "text": "Dem Tag, da ihre List ihnen nichts n\u00fctzt und sie keine Unterst\u00fctzung erfahren."}, {"id": 141809, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4782, "text": "F\u00fcr diejenigen, die Unrecht tun, ist eine Pein au\u00dfer dieser bestimmt, aber die meisten von ihnen wissen nicht Bescheid."}, {"id": 141810, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4783, "text": "Sei geduldig, bis dein Herr sein Urteil f\u00e4llt. Du stehst vor unseren Augen. Und sing das Lob deines Herrn, wenn du dich hinstellst."}, {"id": 141811, "identifier": "de.khoury", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4784, "text": "Und preise Ihn in der Nacht und beim Schwinden der Sterne."}]