[{"id": 141711, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Bei denen, die heftig aufwirbeln,"}, {"id": 141712, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4677, "text": "Bei denen, die eine Last tragen,"}, {"id": 141713, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4678, "text": "Bei denen, die leicht dahinziehen,"}, {"id": 141714, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4679, "text": "Bei denen, die verschiedene Befehle ausf\u00fchren,"}, {"id": 141715, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Siehe, was euch angedroht ist, ist wahr,"}, {"id": 141716, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4681, "text": "Und das Gericht wird eintreffen."}, {"id": 141717, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4682, "text": "Beim Himmel voller Adern,"}, {"id": 141718, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Ihr haltet unterschiedliche Reden."}, {"id": 141719, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Der wird davon abwendig gemacht, wer sich abwenden l\u00e4\u00dft."}, {"id": 141720, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Dem Tod geweiht seien die, die nur Sch\u00e4tzungen anstellen,"}, {"id": 141721, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4686, "text": "Die sich in abgr\u00fcndiger Achtlosigkeit befinden."}, {"id": 141722, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Sie fragen: \u00abWann wird denn der Tag des Gerichtes sein?\u00bb"}, {"id": 141723, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4688, "text": "- \u00abAm Tag, da sie im Feuer gepr\u00fcft werden."}, {"id": 141724, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4689, "text": "Kostet nun eure Pr\u00fcfung. Das ist es, was ihr zu beschleunigen w\u00fcnschtet.\u00bb"}, {"id": 141725, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Die Gottesf\u00fcrchtigen aber werden in G\u00e4rten und an Quellen sein,"}, {"id": 141726, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4691, "text": "Sie nehmen, was ihr Herr ihnen zukommen l\u00e4\u00dft. Sie waren ja vordem rechtschaffen."}, {"id": 141727, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4692, "text": "Nur ein wenig schliefen sie in der Nacht,"}, {"id": 141728, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4693, "text": "Und in der Morgend\u00e4mmerung baten sie um Vergebung,"}, {"id": 141729, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Und sie r\u00e4umten dem Bettler und dem Unbemittelten ein Recht auf ihr Verm\u00f6gen ein."}, {"id": 141730, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4695, "text": "Und auf der Erde gibt es Zeichen f\u00fcr die, die Gewi\u00dfheit hegen,"}, {"id": 141731, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4696, "text": "Und auch in euch selbst. Wollt ihr denn nicht sehen?"}, {"id": 141732, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4697, "text": "Und im Himmel ist euer Lebensunterhalt und das, was euch versprochen wird."}, {"id": 141733, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4698, "text": "Beim Herrn des Himmels und der Erde, das ist so wahr, wie ihr reden k\u00f6nnt."}, {"id": 141734, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Ist denn die Geschichte von den geehrten G\u00e4sten Abrahams zu dir gelangt?"}, {"id": 141735, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Als sie bei ihm eintraten und sagten: \u00abFriede!\u00bb Er sagte: \u00abFriede! Lauter unbekannte Leute.\u00bb"}, {"id": 141736, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4701, "text": "Er schlich sich zu seinen Angeh\u00f6rigen. Er brachte ein (gebratenes) Mastkalb herbei"}, {"id": 141737, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4702, "text": "Und setzte es ihnen vor. Er sagte: \u00abWollt ihr nicht essen?\u00bb"}, {"id": 141738, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4703, "text": "Er empfand Angst vor ihnen. Sie sagten: \u00abHab keine Angst.\u00bb Und sie verk\u00fcndeten ihm einen klugen Knaben."}, {"id": 141739, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Da kam seine Frau schreiend herbei. Sie schlug sich ins Gesicht und sagte: \u00abEine unfruchtbare alte Frau!\u00bb"}, {"id": 141740, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Sie sagten: \u00abSo hat dein Herr gesprochen. Er ist der, der weise ist und Bescheid wei\u00df.\u00bb"}, {"id": 141741, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4706, "text": "Er sagte: \u00abWas ist euer Anliegen, ihr Boten?\u00bb"}, {"id": 141742, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Sie sagten: \u00abWir sind gesandt zu Leuten, die \u00dcbelt\u00e4ter sind,"}, {"id": 141743, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4708, "text": "Um \u00fcber sie Steine aus Ton niederkommen zu lassen,"}, {"id": 141744, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4709, "text": "Die bei deinem Herrn gekennzeichnet sind f\u00fcr die Ma\u00dflosen.\u00bb"}, {"id": 141745, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Und Wir brachten die, die in ihr gl\u00e4ubig waren, hinaus."}, {"id": 141746, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4711, "text": "Aber Wir fanden in ihr nur ein (einziges) Haus von Gottergebenen."}, {"id": 141747, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Und Wir hinterlie\u00dfen in ihr ein Zeichen f\u00fcr diejenigen, die die schmerzhafte Pein f\u00fcrchten."}, {"id": 141748, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4713, "text": "(Ein Zeichen ist) auch in Mose, als Wir ihn mit einer offenkundigen Erm\u00e4chtigung zu Pharao sandten."}, {"id": 141749, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Der kehrte sich ab samt seiner Streitmacht und sagte: \u00abEin Zauberer oder ein Besessener.\u00bb"}, {"id": 141750, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Da ergriffen Wir ihn und seine Truppen und warfen sie ins Meer. Er hatte sich Tadel zugezogen."}, {"id": 141751, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4716, "text": "Und auch in den Aad, als Wir \u00fcber sie den lebensfeindlichen Wind sandten,"}, {"id": 141752, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4717, "text": "Der nichts von allem, wor\u00fcber er kam, \u00fcbriglie\u00df, ohne es werden zu lassen wie etwas, was auseinanderf\u00e4llt."}, {"id": 141753, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4718, "text": "Und auch in den Thamud, als zu ihnen gesprochen wurde: \u00abGenie\u00dft f\u00fcr eine Weile.\u00bb"}, {"id": 141754, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Sie rebellierten gegen den Befehl ihres Herrn. Da ergriff sie der Donnerschlag, w\u00e4hrend sie hinschauten,"}, {"id": 141755, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4720, "text": "Und sie konnten sich nicht mehr aufrichten und erfuhren keine Unterst\u00fctzung mehr."}, {"id": 141756, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4721, "text": "(Verderben lie\u00dfen Wir) fr\u00fcher auch das Volk Noachs. Das waren frevlerische Leute."}, {"id": 141757, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4722, "text": "Und den Himmel haben Wir mit Kraft aufgebaut. Und Wir verf\u00fcgen \u00fcber breite M\u00f6glichkeiten."}, {"id": 141758, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Und die Erde haben Wir ausgebreitet. Wie trefflich haben Wir sie geebnet!"}, {"id": 141759, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Und von allem haben Wir ein Paar erschaffen, auf da\u00df ihr es bedenket."}, {"id": 141760, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4725, "text": "Fl\u00fcchtet nun zu Gott. Ich bin euch von Ihm ein deutlicher Warner."}, {"id": 141761, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Und setzt nicht Gott einen anderen Gott zur Seite. Ich bin euch von Ihm ein deutlicher Warner."}, {"id": 141762, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4727, "text": "So ist auch zu denen, die vor ihnen lebten, kein Gesandter gekommen, ohne da\u00df sie gesagt h\u00e4tten: \u00abEin Zauberer oder ein Besessener.\u00bb"}, {"id": 141763, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Haben sie es denn einander (als Verm\u00e4chtnis) aufgetragen? Nein, sie sind Leute, die ein \u00dcberma\u00df an Frevel zeigen."}, {"id": 141764, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4729, "text": "So kehre dich von ihnen ab. Du ziehst dir damit keinen Tadel zu."}, {"id": 141765, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Und ermahne, denn die Ermahnung n\u00fctzt den Gl\u00e4ubigen."}, {"id": 141766, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Und Ich habe die Djinn und die Menschen nur dazu erschaffen, da\u00df sie Mir dienen."}, {"id": 141767, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4732, "text": "Ich will von ihnen keinen Unterhalt, und Ich will nicht, da\u00df sie Mir zu essen geben."}, {"id": 141768, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Gott ist es, der Unterhalt beschert und Kraft und Festigkeit besitzt."}, {"id": 141769, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4734, "text": "F\u00fcr diejenigen, die Unrecht tun, ist ein Los bestimmt wie das Los ihrer Gef\u00e4hrten. So sollen sie Mich nicht um Beschleunigung bitten."}, {"id": 141770, "identifier": "de.khoury", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Khoury", "transelator_en_name": "Adel Theodor Khoury", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Wehe denen, die ungl\u00e4ubig sind, vor ihrem Tag, der ihnen angedroht ist!"}]