[{"id": 30564, "identifier": "quran-kids", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Kids", "transelator_en_name": "Kids", "format": "text", "type": "quran", "ayah_number": 5759, "text": "\u0639\u064e\u0628\u064e\u0633\u064e|He frowned|95|322|17539$\u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670\u0653|and turned away,|137|364|9215$"}, {"id": 24949, "identifier": "quran-kids", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Kids", "transelator_en_name": "Kids", "format": "text", "type": "quran", "ayah_number": 5760, "text": "\u0623\u064e\u0646|Because|75|174|655$\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f|came to him|101|298|9065$\u0671\u0644\u0652\u0623\u064e\u0639\u0652\u0645\u064e\u0649\u0670|the blind man.|114|213|5615$"}, {"id": 30658, "identifier": "quran-kids", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Kids", "transelator_en_name": "Kids", "format": "text", "type": "quran", "ayah_number": 5761, "text": "\u0648\u064e\u0645\u064e\u0627|But what|80|116|46$\u064a\u064f\u062f\u0652\u0631\u0650\