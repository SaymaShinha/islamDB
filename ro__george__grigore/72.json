[{"id": 435548, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5448, "text": "Spune: \u201cMi s-a dezv\u0103luit c\u0103 un p\u00e2lc de ginni st\u0103tea \u015fi asculta; apoi au spus: \u2018Da, am auzit un Coran minunat"}, {"id": 435549, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5449, "text": "care c\u0103l\u0103uze\u015fte pe calea cea bun\u0103. Noi credem \u00een el \u015fi pe nimeni nu vom al\u0103tura Domnului nostru,"}, {"id": 435550, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5450, "text": "c\u0103ci El \u2014 \u00eenal\u0163\u0103-se a Domnului nostru m\u0103re\u0163ie \u2014 nu \u015fi-a luat nici so\u0163ie \u015fi nici fiu,"}, {"id": 435551, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5451, "text": "chiar dac\u0103 un smintit dintre noi a spus nes\u0103buin\u0163e despre Dumnezeu!"}, {"id": 435552, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5452, "text": "Noi ne \u00eenchipuiam c\u0103 nici oamenii \u015fi nici ginnii nu vor spune minciuni despre Dumnezeu,"}, {"id": 435553, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5453, "text": "\u00eens\u0103 sunt unii b\u0103rba\u0163i dintre oameni care cer izb\u0103vire unor b\u0103rba\u0163i dintre ginni, \u00eens\u0103 ace\u015ftia le spore\u015fte doar prigonirea."}, {"id": 435554, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5454, "text": "Ei \u00ee\u015fi \u00eenchipuie, a\u015fa cum v\u0103 \u00eenchipui\u0163i \u015fi voi, c\u0103 Dumnezeu nu va \u00eenvia pe nimeni."}, {"id": 435555, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5455, "text": "Noi am atins cerul \u015fi l-am aflat plin de aprigi p\u0103zitori \u015fi limbi de foc."}, {"id": 435556, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5456, "text": "Ne-am a\u015fezat \u00een jil\u0163uri ca s\u0103 ascult\u0103m, \u00eens\u0103 cine ascult\u0103 afl\u0103 o limb\u0103 de foc p\u00e2ndindu-l."}, {"id": 435557, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5457, "text": "Nu \u015ftim dac\u0103 se vrea vreun r\u0103u celor de pe p\u0103m\u00e2nt, ori dac\u0103 Domnul lor \u00eei vrea pe Calea cea bun\u0103"}, {"id": 435558, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5458, "text": "Unii dintre noi sunt drep\u0163i, al\u0163ii nu sunt \u015fi c\u0103i felurite urm\u0103m."}, {"id": 435559, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5459, "text": "Noi \u015ftim c\u0103 n-am putea sl\u0103bi puterea lui Dumnezeu pe p\u0103m\u00e2nt \u015fi nici nu i-am putea sc\u0103pa Lui fugind."}, {"id": 435560, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5460, "text": "Noi am crezut \u00een Calea cea Dreapt\u0103 \u00eendat\u0103 ce am auzit de ea. Cel care crede \u00een Domnul s\u0103u nu se teme nici de neajunsuri \u015fi nici de prigonire."}, {"id": 435561, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5461, "text": "\u015ei printre noi sunt supu\u015fi, \u015fi printre noi sunt r\u0103zvr\u0103ti\u0163i. Cei care s-au supus au ales Calea cea bun\u0103,"}, {"id": 435562, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5462, "text": "iar cei r\u0103zvr\u0103ti\u0163i vor fi Gheenei vreascuri."}, {"id": 435563, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5463, "text": "Dac\u0103 merg drept, le vom da s\u0103 bea ap\u0103 din bel\u015fug"}, {"id": 435564, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5464, "text": "pentru a-i pune la \u00eencercare. Dumnezeu va m\u00e2na la o os\u00e2nd\u0103 din ce \u00een ce mai mare pe cei care se \u00eempotrivesc amintirii Domnului lor."}, {"id": 435565, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5465, "text": "Locurile de \u00eenchin\u0103ciune sunt ale lui Dumnezeu! Nu chema dimpreun\u0103 cu Dumnezeu pe nimeni!"}, {"id": 435566, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5466, "text": "C\u00e2nd robul lui Dumnezeu s-a ridicat pentru a-i chema, era c\u00e2t pe-aici s\u0103 se \u00eengr\u0103m\u0103deasc\u0103 pe el.\u2019\u201d"}, {"id": 435567, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5467, "text": "Spune: \u201cEu \u00eel chem pe Domnul meu \u015fi nu \u00cei al\u0103tur Lui pe nimeni.\u201d"}, {"id": 435568, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5468, "text": "Spune: \u201cEu nu v\u0103 pot nici p\u0103gubi \u015fi nici c\u0103l\u0103uzi.\u201d"}, {"id": 435569, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5469, "text": "Spune: \u201cNimeni nu m\u0103 ap\u0103r\u0103 de Dumnezeu, iar eu nu aflu ad\u0103post dec\u00e2t la El,"}, {"id": 435570, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5470, "text": "propov\u0103duind din dezv\u0103luirile lui Dumnezeu \u015fi din soliile Sale.\u201d Cei care nu dau ascultare lui Dumnezeu \u015fi trimisului S\u0103u, \u00een focul Gheenei vor ve\u015fnici."}, {"id": 435571, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5471, "text": "C\u00e2nd vor vedea \u00eemplinirea a ceea ce li s-a f\u0103g\u0103duit \u00ee\u015fi vor da seama cine este cel mai slab \u00eentru ajutor \u015fi cel mai mic \u00eentru num\u0103r."}, {"id": 435572, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5472, "text": "Spune: \u201cNu \u015ftiu dac\u0103 ceea ce vi s-a f\u0103g\u0103duit este aproape ori Domnul meu v\u0103 va mai da un r\u0103gaz.\u201d"}, {"id": 435573, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5473, "text": "El cunoa\u015fte Taina \u00eentru totul, \u00eens\u0103 nu descoper\u0103 nim\u0103nui Taina Sa,"}, {"id": 435574, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5474, "text": "numai celui de care-i mul\u0163umit ca trimis. El va m\u00e2na \u00een fa\u0163a lor \u015fi \u00een spatele lor o p\u00e2nd\u0103"}, {"id": 435575, "identifier": "ro.grigore", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5475, "text": "ca s\u0103 afle dac\u0103 ei propov\u0103duiesc soliile Domnului lor. El cuprinde ceea ce ei au \u015fi \u0163ine socoteala fiec\u0103rui lucru."}]