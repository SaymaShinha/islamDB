[{"id": 433616, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3504, "text": "Alif. Lam. Mim."}, {"id": 433617, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3505, "text": "Pogor\u00e2rea C\u0103r\u0163ii, f\u0103r\u0103 nici o \u00eendoial\u0103, este de la Domnul lumilor."}, {"id": 433618, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3506, "text": "Vor spune, oare: \u201cEl a n\u0103scocit-o!\u201d Ba nu! Aceast\u0103 Carte este Adev\u0103rul de la Domnul t\u0103u ca tu s\u0103 previi un popor la care nici un prevenitor nu a mai venit \u00eenaintea ta. Poate se vor l\u0103sa c\u0103l\u0103uzi\u0163i!"}, {"id": 433619, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3507, "text": "Dumnezeu este Cel ce a creat \u00een \u015fase zile cerurile, p\u0103m\u00e2ntul \u015fi ceea ce se afl\u0103 \u00eentre ele, apoi s-a a\u015fezat pe Tron. Nu ave\u0163i afar\u0103 de El obl\u0103duitor \u015fi nici mijlocitor. Nu chibzui\u0163i, oare?"}, {"id": 433620, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3508, "text": "El c\u00e2rmuie\u015fte totul de la cer la p\u0103m\u00e2nt, \u015fi apoi totul se va ridica la El \u00eentr-o Zi mare c\u00e2t o mie de ani, dup\u0103 cum num\u0103ra\u0163i voi."}, {"id": 433621, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3509, "text": "El cunoa\u015fte t\u0103inuitul \u015fi m\u0103rturisitul. El este Puternicul, Milostivul"}, {"id": 433622, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3510, "text": "care a f\u0103cut frumos ceea ce a creat \u015fi care a \u00eenceput crearea omului din lut,"}, {"id": 433623, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3511, "text": "apoi i-a f\u0103cut pe urma\u015fii lui s\u0103 ias\u0103 dintr-o ap\u0103 lep\u0103dat\u0103."}, {"id": 433624, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3512, "text": "El l-a des\u0103v\u00e2r\u015fit \u015fi a suflat \u00een el din duhul S\u0103u. El v-a creat vou\u0103 auzul, v\u0103zul \u015fi inimile. Voi, pu\u0163in, \u00eei sunte\u0163i \u00eens\u0103 mul\u0163umitori!"}, {"id": 433625, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3513, "text": "Ei spun: \u201cDup\u0103 ce ne vom f\u0103r\u00e2mi\u0163a \u00een \u0163\u0103r\u00e2n\u0103, vom avea iar\u0103\u015fi o nou\u0103 f\u0103ptur\u0103?\u201d, c\u0103ci ei nu cred \u00een \u00cent\u00e2lnirea cu Domnul lor."}, {"id": 433626, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3514, "text": "Spune: \u201c\u00cengerul mor\u0163ii c\u0103ruia i-a\u0163i fost \u00eencredin\u0163a\u0163i v\u0103 va culege, apoi la Domnul vostru ve\u0163i fi \u00eentor\u015fi.\u201d"}, {"id": 433627, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3515, "text": "O, dac\u0103 i-ai vedea atunci pe cei nelegiui\u0163i cu capetele plecate \u00eenaintea Domnului lor! \u201cDomnul nostru! Am v\u0103zut \u015fi am auzit. \u00centoarce-ne pe p\u0103m\u00e2nt, c\u0103ci vom face numai bine \u015fi vom crede cu t\u0103rie.\u201d"}, {"id": 433628, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3516, "text": "Dac\u0103 am fi vrut, am fi d\u0103ruit fiec\u0103rui suflet c\u0103l\u0103uzirea. Spusa de la Mine se \u00eempline\u015fte: \u201cVoi umple Gheena cu ginni \u015fi cu oameni laolalt\u0103.\u201d"}, {"id": 433629, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3517, "text": "Gusta\u0163i c\u0103 a\u0163i uitat \u00eent\u00e2lnirea cu aceast\u0103 zi a voastr\u0103. \u015ei Noi v-am uitat! Gusta\u0163i os\u00e2nda ve\u015fniciei pentru ceea ce a\u0163i f\u0103ptuit."}, {"id": 433630, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": true, "sajda_id": 10, "sajda_obligatory": true, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3518, "text": "Numai aceia cred \u00een semnele Noastre, care se prosterneaz\u0103 c\u00e2nd le sunt amintite \u015fi care-L pream\u0103resc \u00een laude pe Domnul lor \u015fi nu se arat\u0103 \u00eeng\u00e2mfa\u0163i."}, {"id": 433631, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3519, "text": "Ei se smulg din a\u015fternuturile lor ca s\u0103-L cheme pe Domnul lor cu team\u0103 \u015fi r\u00e2vn\u0103 \u015fi dau milostenie din ceea ce i-am \u00eenzestrat."}, {"id": 433632, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3520, "text": "Nici un suflet nu \u015ftie ce bucurii \u00eei sunt sortite ca r\u0103splat\u0103 a faptelor sale!"}, {"id": 433633, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3521, "text": "Credinciosul este deopotriv\u0103 cu cel stricat?! Nu, ei nu sunt deopotriv\u0103!"}, {"id": 433634, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3522, "text": "Cei care cred \u015fi s\u0103v\u00e2r\u015fesc fapte bune vor avea Gr\u0103dinile Limanului ca loc de \u015federe pentru faptele lor,"}, {"id": 433635, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3523, "text": "iar cei strica\u0163i vor avea Focul ca liman. De fiece dat\u0103 c\u00e2nd vor \u00eencerca s\u0103 ias\u0103, vor fi da\u0163i \u00eenapoi spun\u00e2ndu-li-se: \u201cGusta\u0163i os\u00e2nda Focului pe care \u00eel socotea\u0163i minciun\u0103.\u201d"}, {"id": 433636, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3524, "text": "Noi \u00eei vom face s\u0103 guste os\u00e2nda cea bl\u00e2nd\u0103 \u00eenainte de os\u00e2nda cea mare. Poate se vor \u00eentoarce!"}, {"id": 433637, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3525, "text": "Cine este mai nedrept dec\u00e2t cel care se \u00eentoarce de la semnele Domnului s\u0103u dup\u0103 ce i-au fost amintite? Noi ne r\u0103zbun\u0103m pe nelegiui\u0163i."}, {"id": 433638, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3526, "text": "Noi am d\u0103ruit Cartea lui Moise \u2014 nu te \u00eendoi c\u0103 n-a primit-o \u2014 \u015fi am r\u00e2nduit-o c\u0103l\u0103uzire fiilor lui Israel."}, {"id": 433639, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3527, "text": "Noi am f\u0103cut c\u0103petenii dintre ei care s\u0103-i c\u0103l\u0103uzeasc\u0103 din porunca Noastr\u0103, c\u00e2nd erau r\u0103bd\u0103tori \u015fi credeau cu t\u0103rie \u00een semnele Noastre."}, {"id": 433640, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3528, "text": "Domnul t\u0103u va judeca \u00eentre ei \u00een Ziua \u00cenvierii \u015fi le va deslu\u015fi de ce se \u00eenvr\u0103jbeau."}, {"id": 433641, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3529, "text": "Oare nu le este \u00eenv\u0103\u0163\u0103tur\u0103 v\u0103z\u00e2nd c\u00e2te leaturi am nimicit \u00eenaintea lor \u015fi prin ale c\u0103ror a\u015fez\u0103minte ei merg acum? \u00centru aceasta sunt semne! Ei nu aud, oare?!"}, {"id": 433642, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3530, "text": "Ei nu au v\u0103zut cum m\u00e2n\u0103m apa c\u0103tre \u0163inutul sterp \u015fi facem s\u0103 creasc\u0103 din el gr\u00e2ne din care m\u0103n\u00e2nc\u0103 ei \u015fi dobitoacele lor? Ei v\u0103d, oare?"}, {"id": 433643, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3531, "text": "Ei spun: \u201cC\u00e2nd va veni biruin\u0163a aceasta, dac\u0103 spune\u0163i adev\u0103rul?\u201d"}, {"id": 433644, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3532, "text": "Spune: \u201cZiua cuceririi nu sluje\u015fte la nimic celor care nu au crezut, c\u0103ci ei nu au nimic de a\u015fteptat!\u201d"}, {"id": 433645, "identifier": "ro.grigore", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 3533, "text": "\u00cendep\u0103rteaz\u0103-te de ei \u015fi a\u015fteapt\u0103! C\u0103ci \u015fi ei a\u015fteapt\u0103..."}]