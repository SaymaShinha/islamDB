[{"id": 436208, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6107, "text": "Cite\u015fte \u00een numele Domnului t\u0103u ce a creat!"}, {"id": 436209, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6108, "text": "Cel ce a creat omul dintr-un cheag de s\u00e2nge."}, {"id": 436210, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6109, "text": "Cite\u015fte, c\u0103ci Domnul t\u0103u este Mult-d\u0103ruitorul,"}, {"id": 436211, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6110, "text": "El l-a \u00eenv\u0103\u0163at pe om cu condeiul,"}, {"id": 436212, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6111, "text": "l-a \u00eenv\u0103\u0163at ceea ce nu \u015ftia."}, {"id": 436213, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Omul, dimpotriv\u0103, se \u00eeng\u00e2mfeaz\u0103"}, {"id": 436214, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6113, "text": "\u00eendat\u0103 ce se vede bogat."}, {"id": 436215, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6114, "text": "Tot la Domnul t\u0103u este \u00eens\u0103 \u00eentoarcerea!"}, {"id": 436216, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6115, "text": "L-ai v\u0103zut pe cel care \u00eel \u00eempiedica"}, {"id": 436217, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6116, "text": "pe un rob (al lui Dumnezeu) s\u0103 se roage?"}, {"id": 436218, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6117, "text": "L-ai socotit c-ar fi pe Calea cea dreapt\u0103,"}, {"id": 436219, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6118, "text": "c\u00e2nd poruncea pietate?"}, {"id": 436220, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6119, "text": "L-ai v\u0103zut apoi hulind \u015fi \u00eentorc\u00e2nd spatele?"}, {"id": 436184, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6120, "text": "Nu \u015ftie el oare c\u0103 Dumnezeu este Atoatev\u0103z\u0103tor?"}, {"id": 436221, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6121, "text": "Ba nu! Dac\u0103 nu \u00eenceteaz\u0103, \u00eel vom scutura de c\u00e2rlion\u0163ul din frunte,"}, {"id": 436222, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6122, "text": "c\u00e2rlion\u0163 mincinos \u015fi p\u0103c\u0103tos."}, {"id": 436223, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6123, "text": "S\u0103-\u015fi cheme ob\u015ftea lui!"}, {"id": 436224, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6124, "text": "Noi vom chema \u00eengerii-p\u0103zitorii!"}, {"id": 436225, "identifier": "ro.grigore", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 6125, "text": "Nu! Nu-i da ascultare! Ci prosterneaz\u0103-te \u015fi apropie-te (de Dumnezeu)!"}]