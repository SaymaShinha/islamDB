[{"id": 434887, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Pe stelele ce asfin\u0163esc!"}, {"id": 434888, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Prietenul vostru nu se r\u0103t\u0103ce\u015fte \u015fi nici nu cade \u00een gre\u015feal\u0103:"}, {"id": 434889, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4787, "text": "el nu vorbe\u015fte din patim\u0103."}, {"id": 434890, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4788, "text": "Aceasta este o Dezv\u0103luire ce i-a fost dezv\u0103luit\u0103."}, {"id": 434891, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4789, "text": "I-a f\u0103cut-o cunoscut\u0103 cel tare la putere,"}, {"id": 434892, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4790, "text": "cel cu iscusin\u0163\u0103, ce s-a a\u015fezat."}, {"id": 434893, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4791, "text": "pe orizontul cel mai \u00eenalt,"}, {"id": 434894, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4792, "text": "apoi s-a apropiat st\u00e2nd aninat."}, {"id": 434895, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4793, "text": "El era la o dep\u0103rtare de dou\u0103 arunc\u0103turi de suli\u0163\u0103"}, {"id": 434896, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4794, "text": "\u015fi atunci i-a dezv\u0103luit robului S\u0103u ceea ce i-a dezv\u0103luit."}, {"id": 434897, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Inima nu a socotit am\u0103gire ceea ce el a v\u0103zut."}, {"id": 434898, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4796, "text": "V\u0103 \u00eendoi\u0163i de ceea ce el a v\u0103zut?"}, {"id": 434899, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4797, "text": "El l-a v\u0103zut \u00eenc\u0103 o dat\u0103,"}, {"id": 434900, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4798, "text": "l\u00e2ng\u0103 Lotusul de la cap\u0103t"}, {"id": 434901, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4799, "text": "unde se afl\u0103 Gr\u0103dina L\u0103ca\u015fului,"}, {"id": 434902, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4800, "text": "atunci c\u00e2nd Lotusul era \u00eenv\u0103luit de ceea ce era \u00eenv\u0103luit."}, {"id": 434903, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4801, "text": "Privirea sa nu s-a ab\u0103tut \u015fi nici nu a fost stingherit\u0103."}, {"id": 434904, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4802, "text": "El a v\u0103zut semnele cele mari ale Domnului s\u0103u."}, {"id": 434905, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Ce socoti\u0163i despre Al-Lat \u015fi Al-Uzza,"}, {"id": 434906, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4804, "text": "\u015fi cealalt\u0103, Manat, a treia?"}, {"id": 434907, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Ve\u0163i avea voi b\u0103ie\u0163i \u015fi El fete?"}, {"id": 434908, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Ce \u00eemp\u0103r\u0163ire samavolnic\u0103!"}, {"id": 434909, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Nu sunt dec\u00e2t ni\u015fte nume pe care voi \u015fi ta\u0163ii vo\u015ftri li le-a\u0163i dat, f\u0103r\u0103 ca Dumnezeu s\u0103 v\u0103 fi pogor\u00e2t vou\u0103 vreo \u00eemputernicire pentru aceasta. P\u0103rin\u0163ii vo\u015ftri nu-\u015fi urmeaz\u0103 dec\u00e2t \u00eenchipuirea \u015fi ceea ce poftesc sufletele lor, cu toate c\u0103 le-a venit de la Domnul lor c\u0103l\u0103uzire."}, {"id": 434910, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Va avea omul ceea ce-\u015fi dore\u015fte?"}, {"id": 434911, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Ale lui Dumnezeu sunt Via\u0163a de Apoi \u015fi Via\u0163a Dint\u00e2i."}, {"id": 434912, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4810, "text": "C\u00e2\u0163i \u00eengeri sunt \u00een cer? Mijlocirea lor nu va sluji la nimic dec\u00e2t dup\u0103 ce Dumnezeu \u00eeng\u0103duie aceasta pentru cine voie\u015fte El \u015fi pentru cel de care El este mul\u0163umit."}, {"id": 434913, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Cei care nu cred \u00een Via\u0163a de Apoi dau \u00eengerilor nume de femei."}, {"id": 434915, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Ei nu au nici o \u015ftiin\u0163\u0103, ci urmeaz\u0103 o \u00eenchipuire. \u00cenchipuirea nu sluje\u015fte la nimic \u00een fa\u0163a Adev\u0103rului."}, {"id": 434916, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4813, "text": "\u00cendep\u0103rteaz\u0103-te de cel care \u00eentoarce spatele amintirii \u015fi care nu dore\u015fte dec\u00e2t Via\u0163a de Acum."}, {"id": 434917, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Aceasta este toat\u0103 \u00eentinderea \u015ftiin\u0163ei lor! Domnul t\u0103u \u00eel \u015ftie prea bine pe cel care se r\u0103t\u0103ce\u015fte de la calea Sa, precum \u00eel \u015ftie prea bine \u015fi pe cel drept c\u0103l\u0103uzit."}, {"id": 434918, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Ale lui Dumnezeu sunt cele din ceruri \u015fi de pe p\u0103m\u00e2nt. El \u00eei va r\u0103spl\u0103ti pe cei care fac r\u0103ul dup\u0103 ceea ce fac, precum \u00eei va r\u0103spl\u0103ti \u015fi pe cei care fac binele cu \u015fi mai bine."}, {"id": 434919, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Domnul t\u0103u este darnic la iertare cu cei care se feresc de p\u0103catele grele \u015fi de desfr\u00e2u, precum \u015fi cu cei care s\u0103v\u00e2r\u015fesc gre\u015feli u\u015foare. El v\u0103 \u015ftie prea bine de pe c\u00e2nd v-a \u00een\u0103l\u0163at din p\u0103m\u00e2nt \u015fi de pe c\u00e2nd era\u0163i fe\u0163i \u00een p\u00e2ntecele mamelor voastre. Nu v\u0103 da\u0163i voi singuri m\u00e2ntuirea! El \u00eei \u015ftia prea bine pe cei care se tem!"}, {"id": 434920, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4817, "text": "L-ai v\u0103zut pe cel care a \u00eentors spatele,"}, {"id": 434921, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4818, "text": "Are el \u015ftiin\u0163a Tainei ca s-o poat\u0103 vedea?"}, {"id": 434922, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Nu i s-a dat de \u015ftire oare de ceea ce era \u00een sulurile lui Moise"}, {"id": 434923, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4820, "text": "\u015fi \u00een cele ale lui Abraham cel at\u00e2t de credincios?"}, {"id": 434924, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4821, "text": "Nim\u0103nui nu i se va pune \u00een c\u00e2rc\u0103 povara altuia."}, {"id": 434925, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4822, "text": "Omul nu va avea dec\u00e2t ceea ce trude\u015fte."}, {"id": 434926, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4823, "text": "Truda sa va fi recunoscut\u0103"}, {"id": 434927, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4824, "text": "\u015fi va fi din plin r\u0103spl\u0103tit."}, {"id": 434928, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4825, "text": "La Domnul t\u0103u este cap\u0103tul!"}, {"id": 434929, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4826, "text": "El este Cel ce d\u0103ruie\u015fte r\u00e2sul \u015fi Cel ce d\u0103ruie\u015fte pl\u00e2nsul."}, {"id": 434930, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4827, "text": "El este Cel ce d\u0103ruie\u015fte moartea \u015fi Cel ce d\u0103ruie\u015fte via\u0163a."}, {"id": 434931, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4828, "text": "El a creat perechea, b\u0103rbat \u015fi femeie,"}, {"id": 434932, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4829, "text": "dintr-o pic\u0103tur\u0103 de s\u0103m\u00e2n\u0163\u0103 dup\u0103 ce a fost v\u0103rsat\u0103."}, {"id": 434933, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4830, "text": "A Lui este Facerea cea de apoi."}, {"id": 434934, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4831, "text": "El \u00eembog\u0103\u0163e\u015fte \u015fi \u00eendestuleaz\u0103."}, {"id": 434935, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4832, "text": "El este Domnul lui Sirius."}, {"id": 434936, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4833, "text": "El i-a nimicit pe adi\u0163ii de odinioar\u0103,"}, {"id": 434937, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4834, "text": "precum \u015fi pe tamudi\u0163i \u2014 pe nimeni n-a l\u0103sat dintre ei \u2014"}, {"id": 434938, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4835, "text": "\u015fi mai \u00eenainte pe poporul lui Noe \u2014 oameni nedrep\u0163i \u015fi tic\u0103lo\u015fi."}, {"id": 434939, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4836, "text": "El a d\u0103r\u00e2mat cetatea cea de-a-ndoaselea,"}, {"id": 434940, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4837, "text": "acoperind-o cu ceea ce trebuia s-o acopere."}, {"id": 434941, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4838, "text": "Care sunt binefacerile Domnului t\u0103u pe care tu le t\u0103g\u0103dui?"}, {"id": 434942, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4839, "text": "Aceasta este o mustrare dintre mustr\u0103rile dint\u00e2i."}, {"id": 434943, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Cea care trebuie s\u0103 se apropie, se va apropia!"}, {"id": 434944, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Nimeni, afar\u0103 de Dumnezeu, nu o poate dezv\u0103lui."}, {"id": 434945, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4842, "text": "Sunte\u0163i mira\u0163i de aceast\u0103 spus\u0103?"}, {"id": 434946, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4843, "text": "R\u00e2de\u0163i \u00een loc s\u0103 pl\u00e2nge\u0163i?"}, {"id": 434947, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4844, "text": "Voi sunte\u0163i nep\u0103s\u0103tori!"}, {"id": 434948, "identifier": "ro.grigore", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0,