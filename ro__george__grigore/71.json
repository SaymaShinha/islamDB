[{"id": 435521, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5420, "text": "Noi l-am trimis pe Noe poporului s\u0103u: \u201cPrevine-\u0163i poporul \u00eenainte s\u0103-i vin\u0103 o dureroas\u0103 os\u00e2nd\u0103!\u201d"}, {"id": 435522, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5421, "text": "El spuse: \u201cO, popor al meu! Eu vou\u0103 v\u0103 sunt predicator limpede la vorb\u0103."}, {"id": 435523, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5422, "text": "\u00cenchina\u0163i-v\u0103 lui Dumnezeu! Teme\u0163i-v\u0103 de El! Da\u0163i-i ascultare!"}, {"id": 435524, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5423, "text": "El v\u0103 iart\u0103 vou\u0103 din p\u0103catele voastre \u015fi v\u0103 d\u0103 vou\u0103 un r\u0103gaz p\u00e2n\u0103 la un anumit soroc. C\u00e2nd va veni \u00eens\u0103 sorocul lui Dumnezeu, nimic nu-l va \u00eent\u00e2rzia. O, dac\u0103 a\u0163i \u015fti!\u201d"}, {"id": 435525, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5424, "text": "El spuse: \u201cDomnul meu! Eu mi-am chemat poporul noapte \u015fi zi,"}, {"id": 435526, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5425, "text": "\u00eens\u0103 chemarea mea mai mult i-a pus pe fug\u0103."}, {"id": 435527, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5426, "text": "De fiece dat\u0103 c\u00e2nd i-am chemat ca Tu s\u0103 le ier\u0163i lor, ei \u015fi-au b\u0103gat degetele \u00een urechi, s-au \u00eenf\u0103\u015furat \u00een ve\u015fmintele lor \u015fi s-au \u00eenver\u015funat ar\u0103t\u00e2ndu-se plini de m\u00e2ndrie."}, {"id": 430350, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5427, "text": "I-am chemat r\u0103spicat."}, {"id": 435528, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5428, "text": "Le-am spus pe fa\u0163\u0103 \u015fi \u00een tain\u0103."}, {"id": 435529, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5429, "text": "Le-am spus: \u201cCere\u0163i-i iertare Domnului vostru, c\u0103ci El este Iert\u0103tor."}, {"id": 435530, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5430, "text": "El v\u0103 va trimite vou\u0103, din cer, mult\u0103 ploaie."}, {"id": 435531, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5431, "text": "El v\u0103 va spori averile \u015fi num\u0103rul fiilor \u015fi v\u0103 va da gr\u0103dini \u015fi r\u00e2uri."}, {"id": 435532, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5432, "text": "De ce nu a\u015ftepta\u0163i de la Dumnezeu ceva vrednic de El."}, {"id": 435533, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5433, "text": "El este Cel ce v-a creat r\u00e2nd pe r\u00e2nd."}, {"id": 435534, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5434, "text": "N-a\u0163i v\u0103zut cum Dumnezeu a creat cele \u015fapte ceruri a\u015fezate unul asupra celuilalt?"}, {"id": 435535, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5435, "text": "\u015ei a f\u0103cut luna \u00een ele precum o lumin\u0103 \u015fi soarele precum un opai\u0163."}, {"id": 435536, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5436, "text": "Dumnezeu v-a f\u0103cut s\u0103 r\u0103s\u0103ri\u0163i din p\u0103m\u00e2nt precum ierburile,"}, {"id": 435537, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5437, "text": "apoi v\u0103 va \u00eenapoia \u00een el ca s\u0103 v\u0103 scoat\u0103 deodat\u0103."}, {"id": 435538, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5438, "text": "Dumnezeu v-a f\u0103cut vou\u0103 p\u0103m\u00e2ntul covor"}, {"id": 435539, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5439, "text": "ca voi s\u0103-l str\u0103bate\u0163i pe drumuri largi.\u201d"}, {"id": 435540, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5440, "text": "Noe spuse: \u201cDomnul meu! Ei nu m-au ascultat \u015fi l-au urmat pe cel ale c\u0103rui bog\u0103\u0163ii \u015fi fii nu fac dec\u00e2t s\u0103-i apropie pierzania.\u201d"}, {"id": 435541, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5441, "text": "Ei au viclenit \u015fi au spus:"}, {"id": 435542, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5442, "text": "\u201cNu-i p\u0103r\u0103si\u0163i niciodat\u0103 pe dumnezeii vo\u015ftri! Nu-i p\u0103r\u0103si\u0163i pe Wadd, \u015fi nici pe Suwa\u2019, \u015fi nici pe Yaghut, \u015fi nici pe Ya\u2019uq, \u015fi nici pe Nasr!\u201d"}, {"id": 435543, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5443, "text": "Ace\u015ftia au r\u0103t\u0103cit mul\u0163i oameni. Tu nu faci dec\u00e2t s\u0103 m\u0103re\u015fti r\u0103t\u0103cirea celor nedrep\u0163i."}, {"id": 435544, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5444, "text": "Ei au fost \u00eeneca\u0163i \u015fi arunca\u0163i \u00een Foc pentru p\u0103catele lor \u015fi nu aflar\u0103 nici un ajutor afar\u0103 de Dumnezeu."}, {"id": 435545, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5445, "text": "Noe spuse: \u201cDomnul meu! Nu l\u0103sa pe p\u0103m\u00e2nt nici un locuitor dintre t\u0103g\u0103duitori."}, {"id": 435546, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5446, "text": "Dac\u0103 \u00eei la\u015fi, ei \u00eei vor r\u0103t\u0103ci pe robii T\u0103i \u015fi nu vor z\u0103misli dec\u00e2t desfr\u00e2na\u0163i mult-t\u0103g\u0103duitori."}, {"id": 435547, "identifier": "ro.grigore", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5447, "text": "Domnul meu! Iart\u0103-mi mie \u015fi p\u0103rin\u0163ilor mei, precum \u015fi celui ce intr\u0103, credincios, \u00een casa mea, credincio\u015filor \u015fi credincioaselor. Spore\u015fte doar pr\u0103p\u0103dul celor nedrep\u0163i!\u201d"}]