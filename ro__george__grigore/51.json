[{"id": 434785, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Pe \u00eempr\u0103\u015ftietoarele ce \u00eempr\u0103\u015ftie!"}, {"id": 433072, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4677, "text": "Pe c\u0103r\u0103toarele de povar\u0103!"}, {"id": 433485, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4678, "text": "Pe alerg\u0103toarele cu iu\u0163eal\u0103!"}, {"id": 434786, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4679, "text": "Pe \u00eemp\u0103r\u0163itoarele de porunc\u0103!"}, {"id": 434787, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Ceea ce vi s-a f\u0103g\u0103duit este adev\u0103rat."}, {"id": 434788, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4681, "text": "Judecata de Apoi este f\u0103r\u0103 t\u0103gad\u0103."}, {"id": 434789, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4682, "text": "Pe cerul cu ale sale c\u0103r\u0103ri!"}, {"id": 434790, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Voi ave\u0163i asupra lui p\u0103reri felurite,"}, {"id": 434791, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4684, "text": "\u015fi \u00eel \u00eentoarce pe dos cel care este \u00eentors."}, {"id": 434792, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4685, "text": "S\u0103 fie uci\u015fi b\u0103nuitorii,"}, {"id": 434793, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4686, "text": "ei, cei care sunt \u00eentr-o pr\u0103pastie \u015fi nu-\u015fi dau seama!"}, {"id": 434794, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Ei \u00eentreab\u0103: \u201cC\u00e2nd va veni Ziua Judec\u0103\u0163ii?\u201d"}, {"id": 434795, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4688, "text": "\u00cen Ziua c\u00e2nd vor fi pu\u015fi la \u00eencercare \u00een Foc:"}, {"id": 434796, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4689, "text": "\u201cGusta\u0163i \u00eencercarea voastr\u0103! Aceasta este ceea ce voi cerea\u0163i degrab\u0103!\u201d"}, {"id": 434797, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Cei tem\u0103tori vor tr\u0103i \u00een Gr\u0103dini cu izvoare,"}, {"id": 434798, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4691, "text": "lu\u00e2nd ceea ce Domnul lor le va d\u0103rui. Ei au fost \u00eenainte dintre f\u0103ptuitorii de bine"}, {"id": 434799, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4692, "text": "ce dormeau noaptea pu\u0163in,"}, {"id": 434800, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4693, "text": "iar dis-de-diminea\u0163\u0103 cereau iertare."}, {"id": 434801, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Din averile lor aveau parte \u015fi cer\u015fetorul \u015fi lipsitul!"}, {"id": 434802, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4695, "text": "Pe p\u0103m\u00e2nt sunt semne pentru cei care cred cu t\u0103rie."}, {"id": 434803, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4696, "text": "Precum \u015fi \u00een voi \u00een\u015fiv\u0103. Nu le vede\u0163i, oare?"}, {"id": 434804, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4697, "text": "\u00cen cer este \u00eenzestrarea voastr\u0103 \u015fi ceea ce vi s-a f\u0103g\u0103duit."}, {"id": 434805, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4698, "text": "Pe Domnul cerului \u015fi al p\u0103m\u00e2ntului! Acesta este Adev\u0103rul precum faptul c\u0103 vorbi\u0163i."}, {"id": 434806, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4699, "text": "A venit la tine spusa despre oaspe\u0163ii prea-cinsti\u0163i ai lui Abraham?"}, {"id": 434807, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Ei intrar\u0103 la el spun\u00e2ndu-i: \u201cPace!\u201d Abraham spuse: \u201cPace! O, oameni necunoscu\u0163i!\u201d"}, {"id": 434808, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4701, "text": "El s-a dus apoi pe nesim\u0163ite la ai s\u0103i \u015fi a adus un vi\u0163el gras."}, {"id": 434809, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4702, "text": "L-a apropiat de ei, spun\u00e2nd: \u201cNu m\u00e2nca\u0163i?\u201d"}, {"id": 434810, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4703, "text": "\u015ei atunci \u00eel prinse frica de ei. Ace\u015ftia spuser\u0103: \u201cS\u0103 nu-\u0163i fie fric\u0103! \u015ei \u00eei vestir\u0103 un fecior \u00eensemnat."}, {"id": 434811, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Femeia sa \u00eenaint\u0103 v\u0103it\u00e2ndu-se \u015fi lovindu-se peste fa\u0163\u0103. Ea spuse: \u201cO b\u0103tr\u00e2n\u0103 stearp\u0103?...\u201d"}, {"id": 434812, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Ei spuser\u0103: \u201cDomnul t\u0103u a spus astfel. El este \u00cen\u0163eleptul, \u015etiutorul."}, {"id": 434813, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4706, "text": "Abraham spuse: \u201cO, voi trimi\u015filor! Care este treaba voastr\u0103?\u201d"}, {"id": 434814, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Ei spuser\u0103: \u201cNoi am fost trimi\u015fi la un popor de nelegiui\u0163i"}, {"id": 434815, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4708, "text": "ca s\u0103 trimitem asupra lui bolovani de lut"}, {"id": 434816, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4709, "text": "\u00eensemna\u0163i de Domnul t\u0103u pentru cei desfr\u00e2na\u0163i.\u201d"}, {"id": 434817, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Noi i-am scos a\u015fadar din cetate pe cei credincio\u015fi \u015fi"}, {"id": 434818, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4711, "text": "nu am aflat dec\u00e2t o cas\u0103 de supu\u015fi."}, {"id": 434819, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Am pus \u00een ea un semn pentru cei care se tem de Os\u00e2nda cea dureroas\u0103,"}, {"id": 434820, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4713, "text": "precum \u00een Moise c\u00e2nd l-am trimis cu o \u00eemputernicire deslu\u015fit\u0103 la Faraon"}, {"id": 434821, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4714, "text": "care s-a \u00eentors c\u0103tre suita sa spun\u00e2nd: \u201cEste fie vr\u0103jitor, fie \u00eendr\u0103cit!\u201d"}, {"id": 434822, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Noi i-am luat pe ei \u015fi o\u015ftile lor \u015fi i-am aruncat \u00een mare, \u00een vreme ce el se dojenea pe sine."}, {"id": 434823, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4716, "text": "A\u015fa a fost \u015fi cu adi\u0163ii c\u00e2nd am trimis asupra lor v\u00e2ntul pustiilor"}, {"id": 434824, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4717, "text": "care nu a l\u0103sat nimic peste care a trecut f\u0103r\u0103 s\u0103 nu-l fac\u0103 f\u0103r\u00e2me."}, {"id": 434825, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4718, "text": "A\u015fa a fost \u015fi cu tamudi\u0163ii c\u00e2nd li s-a spus: \u201cBucura\u0163i-v\u0103 o vreme!\u201d"}, {"id": 434826, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Ei n-au luat \u00eens\u0103 \u00een seam\u0103 porunca Domnului lor, iar tr\u0103snetul i-a luat pe c\u00e2nd priveau."}, {"id": 434827, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4720, "text": "Atunci nu s-au putut \u0163ine pe picioare \u015fi nici ajuta."}, {"id": 434828, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4721, "text": "A\u015fa a fost \u015fi cu poporul lui Noe: Era un popor de desfr\u00e2na\u0163i."}, {"id": 434829, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4722, "text": "\u015ei cerul? Noi l-am zidit cu tr\u0103inicie \u015fi i-am dat apoi l\u0103rgime!"}, {"id": 434830, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4723, "text": "\u015ei p\u0103m\u00e2ntul? Noi l-am a\u015fternut \u015fi ce buni \u00eentinz\u0103tori am fost!"}, {"id": 434831, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Din fiece lucru am creat o pereche. Poate v\u0103 ve\u0163i aminti!"}, {"id": 434832, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4725, "text": "Zori\u0163i c\u0103tre Dumnezeu! Eu de la El v\u0103 previn deslu\u015fit!"}, {"id": 434833, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Nu pune\u0163i l\u00e2ng\u0103 Dumnezeu alt dumnezeu! Eu de la El v\u0103 previn deslu\u015fit!"}, {"id": 434834, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4727, "text": "A\u015fa a fost \u00eentotdeauna. Nici un trimis n-a venit la cei dinaintea lor f\u0103r\u0103 ca ei s\u0103 nu-i fi spus: \u201cEste fie vr\u0103jitor, fie \u00eendr\u0103cit!\u201d"}, {"id": 434836, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Ei s-au sf\u0103tuit unii pe al\u0163ii asupra lui? Acesta este un popor de tic\u0103lo\u015fi!"}, {"id": 434837, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4729, "text": "\u00centoarce-le spatele \u015fi nu vei fi dojenit!"}, {"id": 434838, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Adu-le aminte, c\u0103ci amintirea le este credincio\u015filor de folos."}, {"id": 434839, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Eu nu i-am creat nici pe ginnii \u015fi nici pe oamenii dec\u00e2t ca s\u0103 Mi se \u00eenchine."}, {"id": 434840, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4732, "text": "Eu nu vreau nici un dar de la ei. Eu nu vreau s\u0103 M\u0103 osp\u0103teze."}, {"id": 434841, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Dumnezeu este \u00cemp\u0103r\u0163itorul de bunuri. El este St\u0103p\u00e2nul de nezdruncinat al puterii."}, {"id": 434842, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Cei care s-au tic\u0103lo\u015fit \u00een p\u0103cate, precum p\u0103catele semenilor lor, s\u0103 nu o cear\u0103 degrab\u0103 (Judecata)!"}, {"id": 434843, "identifier": "ro.grigore", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Vai t\u0103g\u0103duitorilor pentru Ziua ce li s-a f\u0103g\u0103duit!"}]