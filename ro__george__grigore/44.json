[{"id": 434524, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4415, "text": "Ha. Mim."}, {"id": 434525, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4416, "text": "Pe Cartea cea deslu\u015fit\u0103!"}, {"id": 434526, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4417, "text": "Noi am pogor\u00e2t-o \u00eentr-o noapte binecuv\u00e2ntat\u0103. Noi suntem Cei care predic\u0103m!"}, {"id": 434527, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4418, "text": "\u00cen noaptea aceea, va fi l\u0103murit\u0103 fiece Porunc\u0103 \u00een\u0163eleapt\u0103!"}, {"id": 434528, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4419, "text": "Cu Porunc\u0103 de la Noi, o vom trimite"}, {"id": 434529, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4420, "text": "ca milostivenie de la Domnul t\u0103u. El este Auzitorul, \u015etiutorul."}, {"id": 434530, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4421, "text": "El este Domnul cerurilor \u015fi al p\u0103m\u00e2ntului \u015fi a ceea ce se afl\u0103 \u00eentre ele. Dac\u0103 crede\u0163i cu t\u0103rie!"}, {"id": 434531, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4422, "text": "Nu este dumnezeu afar\u0103 de El. El d\u0103ruie\u015fte via\u0163a. El d\u0103ruie\u015fte moartea. El este Domnul vostru \u015fi Domnul str\u0103mo\u015filor vo\u015ftri dint\u00e2i."}, {"id": 434532, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4423, "text": "Ei stau \u00eens\u0103 la \u00eendoial\u0103, juc\u00e2ndu-se."}, {"id": 434534, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4424, "text": "P\u00e2nde\u015fte Ziua c\u00e2nd cerul va aduce un fum gros"}, {"id": 434535, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4425, "text": "care va \u00eenv\u0103lui oamenii: aceasta este o os\u00e2nd\u0103 dureroas\u0103"}, {"id": 434536, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4426, "text": "\u201cDomnul nostru! \u00cendep\u0103rteaz\u0103 de la noi os\u00e2nda, c\u0103ci noi suntem credincio\u015fi!\u201d"}, {"id": 434537, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4427, "text": "Cum de au amintirea? Un profet cu vorba limpede a venit la ei."}, {"id": 434538, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4428, "text": "Ei i-au \u00eentors \u00eens\u0103 spatele, spun\u00e2nd: \u201cEste un \u00eensemnat, un \u00eendr\u0103cit!\u201d"}, {"id": 434539, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4429, "text": "Vom \u00eendep\u0103rta os\u00e2nda pu\u0163in, \u00eens\u0103 voi v\u0103 ve\u0163i \u00eentoarce (la ce a\u0163i fost)."}, {"id": 434540, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4430, "text": "\u00cen Ziua c\u00e2nd \u00eei vom zdrobi de tot, atunci ne vom r\u0103zbuna."}, {"id": 434541, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4431, "text": "\u00cenaintea lor, Noi am pus la \u00eencercare poporul lui Faraon \u015fi a venit la ei un trimis cinstit."}, {"id": 434542, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4432, "text": "\u201cAduce\u0163i la mine pe robii lui Dumnezeu! Eu v\u0103 sunt vou\u0103 trimis vrednic de \u00eencredere!"}, {"id": 434543, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4433, "text": "Nu v\u0103 ridica\u0163i \u00eempotriva lui Dumnezeu! Eu vin la voi cu o \u00eemputernicire deslu\u015fit\u0103."}, {"id": 434544, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4434, "text": "Eu caut izb\u0103vire la Domnul meu \u015fi Domnul vostru ca voi s\u0103 nu m\u0103 ucide\u0163i cu pietre."}, {"id": 434545, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4435, "text": "L\u0103sa\u0163i-m\u0103 singur, dac\u0103 nu ave\u0163i \u00eencredere \u00een mine.\u201d"}, {"id": 434546, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4436, "text": "El \u00eel chem\u0103 pe Domnul s\u0103u: \u201cAce\u015fti oameni sunt nelegiui\u0163i.\u201d"}, {"id": 434547, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4437, "text": "\u201cPleac\u0103 cu robii Mei noaptea! Ve\u0163i fi urm\u0103ri\u0163i..."}, {"id": 434548, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4438, "text": "Las\u0103 marea s\u0103 se despice! Ei vor fi o o\u015ftire \u00eenecat\u0103\u201d"}, {"id": 434549, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4439, "text": "C\u00e2te gr\u0103dini \u015fi izvoare au p\u0103r\u0103sit,"}, {"id": 434550, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4440, "text": "\u015fi sem\u0103n\u0103turi \u015fi loc \u00eembel\u015fugat,"}, {"id": 434551, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4441, "text": "\u015fi pl\u0103ceri \u00een care se desf\u0103tau!"}, {"id": 434552, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4442, "text": "Aceasta, c\u0103ci Noi am l\u0103sat aceste bunuri mo\u015ftenire unui alt popor."}, {"id": 434553, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4443, "text": "N-au pl\u00e2ns dup\u0103 ei nici cerul, nici p\u0103m\u00e2ntul \u015fi nici nu au fost a\u015ftepta\u0163i."}, {"id": 434554, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4444, "text": "Noi i-am m\u00e2ntuit pe fiii lui Israel de os\u00e2nda umilitoare,"}, {"id": 433651, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4445, "text": "de Faraon care era trufa\u015f peste m\u0103sur\u0103."}, {"id": 434175, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4446, "text": "Noi cu \u015ftiin\u0163\u0103 i-am ales dintre lumi."}, {"id": 434555, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4447, "text": "Le-am adus semne cu o dovad\u0103 v\u0103dit\u0103."}, {"id": 434556, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4448, "text": "Ace\u015fti oameni spuneau:"}, {"id": 434557, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4449, "text": "\u201cNu este dec\u00e2t moartea noastr\u0103 dint\u00e2i, iar dup\u0103 aceea nu vom mai fi scula\u0163i."}, {"id": 434558, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4450, "text": "Aduce\u0163i-i pe ta\u0163ii no\u015ftri, dac\u0103 spune\u0163i adev\u0103rul!\u201d"}, {"id": 434559, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4451, "text": "Oare sunt ei mai buni dec\u00e2t poporul lui Tubba\u2019 ori dec\u00e2t cei dinaintea lor pe care i-am nimicit fiindc\u0103 erau nelegiui\u0163i?"}, {"id": 434560, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4452, "text": "Noi nu am creat \u00een joac\u0103 nici cerurile, nici p\u0103m\u00e2ntul \u015fi nici ceea ce se afl\u0103 \u00eentre ele."}, {"id": 434561, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4453, "text": "Noi le-am creat \u00eentru Adev\u0103r, \u00eens\u0103 cei mai mul\u0163i nu \u015ftiu!"}, {"id": 434562, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4454, "text": "Ziua Cumpenei va fi sorocul adun\u0103rii tuturor,"}, {"id": 434563, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4455, "text": "ziua c\u00e2nd nimeni nu va putea obl\u0103dui pe nimeni \u015fi c\u00e2nd oamenii nu vor mai fi ajuta\u0163i,"}, {"id": 434564, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4456, "text": "afar\u0103 de cei pe care Dumnezeu \u00eei va milui. El este Puternicul, Milostivul."}, {"id": 434565, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4457, "text": "Copacul Zaqqum"}, {"id": 433041, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4458, "text": "este hrana p\u0103c\u0103tosului"}, {"id": 434566, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4459, "text": "ce \u00eei va fierbe \u00een p\u00e2ntece precum argintul topit,"}, {"id": 434567, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4460, "text": "precum apa clocotind\u0103."}, {"id": 434568, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4461, "text": "\u201cLua\u0163i-l \u015fi t\u00e2r\u00e2\u0163i-l \u00een fundul Iadului!"}, {"id": 434569, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4462, "text": "Turna\u0163i-i pe cap, ca os\u00e2nd\u0103, ap\u0103 clocotind\u0103...\u201d"}, {"id": 434570, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4463, "text": "\u201cGust\u0103! Tu e\u015fti puternicul, m\u0103rinimosul!?\u201d"}, {"id": 434571, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4464, "text": "Aceasta este cea de care v\u0103 \u00eendoia\u0163i!"}, {"id": 434572, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4465, "text": "Cei tem\u0103tori vor sta \u00eentr-un loc tihnit,"}, {"id": 434573, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4466, "text": "\u00een gr\u0103dini, la izvoare."}, {"id": 434574, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4467, "text": "Vor fi \u00eenve\u015fm\u00e2nta\u0163i \u00een m\u0103tase \u015fi brocart \u015fi a\u015feza\u0163i fa\u0163\u0103 \u00een fa\u0163\u0103."}, {"id": 434575, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4468, "text": "Noi le vom d\u0103rui drept so\u0163ii hurii cu ochii mari."}, {"id": 434576, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4469, "text": "Acolo, vor cere \u00een tihn\u0103 fructe de tot soiul."}, {"id": 434577, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4470, "text": "Ei nu vor mai gusta moartea, dup\u0103 moartea lor dint\u00e2i, c\u0103ci Dumnezeu i-a ferit de os\u00e2nda Iadului."}, {"id": 434578, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4471, "text": "Har Domnului t\u0103u! Aceasta este fericirea cea mare!"}, {"id": 434579, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4472, "text": "Noi l-am f\u0103cut lesne de \u00een\u0163eles \u00een limba ta. Poate \u00ee\u015fi vor aminti!"}, {"id": 434580, "identifier": "ro.grigore", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4473, "text": "P\u00e2nde\u015fte-i, c\u0103ci \u015fi ei te p\u00e2ndesc pe tine!"}]