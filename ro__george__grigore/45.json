[{"id": 434581, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4474, "text": "Ha. Mim."}, {"id": 434582, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4475, "text": "Pogor\u00e2rea C\u0103r\u0163ii este de la Dumnezeu, Puternicul, \u00cen\u0163eleptul."}, {"id": 434583, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4476, "text": "\u00cen ceruri, precum \u015fi pe p\u0103m\u00e2nt, sunt semne pentru credincio\u015fi."}, {"id": 434584, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4477, "text": "\u00cen f\u0103ptura voastr\u0103 \u015fi a dobitoacelor pe care le-a r\u0103sp\u00e2ndit sunt semne pentru un popor care crede cu t\u0103rie."}, {"id": 434585, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4478, "text": "\u00cen deosebirea nop\u0163ii de zi, \u00een apa d\u0103t\u0103toare de hran\u0103 pe care Dumnezeu o trimite din cer d\u00e2nd prin ea via\u0163\u0103 p\u0103m\u00e2ntului dup\u0103 moarte, \u00een dezl\u0103n\u0163uirea v\u00e2nturilor, sunt semne pentru un popor ce are minte."}, {"id": 434587, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4479, "text": "Acestea sunt versetele lui Dumnezeu pe care \u0163i le recit\u0103m \u00eentru Adev\u0103r. \u00cen ce spus\u0103, dup\u0103 Dumnezeu \u015fi versetele Sale, vor mai crede?"}, {"id": 434588, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4480, "text": "Vai clevetitorului p\u0103c\u0103tos!"}, {"id": 434589, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4481, "text": "El ascult\u0103 versetele lui Dumnezeu recit\u00e2ndu-i-se, \u00eens\u0103 st\u0103 \u00eeng\u00e2mfat de parc\u0103 nici nu le-ar fi auzit. Veste\u015fte-i o dureroas\u0103 os\u00e2nd\u0103!"}, {"id": 434590, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4482, "text": "C\u00e2nd cunoa\u015fte din semnele Noastre, le ia \u00een zeflemea. Ace\u015ftia sunt cei care vor avea o os\u00e2nd\u0103 umilitoare."}, {"id": 434591, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4483, "text": "Gheena va fi \u00een spatele lor. Ceea ce au agonisit nu le va sluji la nimic \u015fi nici obl\u0103duitorii pe care \u015fi i-au luat \u00een afara lui Dumnezeu \u015fi vor avea parte de o mare os\u00e2nd\u0103."}, {"id": 434592, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4484, "text": "Aceasta este o c\u0103l\u0103uzire. Cei care t\u0103g\u0103duiesc semnele Domnului lor vor suferi, ca os\u00e2nd\u0103, o urgie dureroas\u0103."}, {"id": 434593, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4485, "text": "Dumnezeu este Cel ce v-a supus vou\u0103 marea ca pe ea s\u0103 pluteasc\u0103 cor\u0103biile din porunca Sa \u015fi pentru ca voi s\u0103 c\u0103uta\u0163i harul S\u0103u. Poate ve\u0163i mul\u0163umi!"}, {"id": 434594, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4486, "text": "El v-a supus vou\u0103 cele din ceruri, precum \u015fi de pe p\u0103m\u00e2nt. Totul este de la El. \u00centru acestea sunt semne pentru un popor care g\u00e2nde\u015fte!"}, {"id": 434595, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4487, "text": "Spune celor care cred s\u0103 ierte celor care nu n\u0103d\u0103jduiesc la zilele lui Dumnezeu, pentru c\u0103 El va r\u0103spl\u0103ti fiece popor dup\u0103 ceea ce a agonisit."}, {"id": 434596, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4488, "text": "Cine face un bine, sie\u015fi \u015fi-l face. Cine face un r\u0103u, sie\u015fi \u015fi-l face. Voi la Domnul vostru ve\u0163i fi \u00eentor\u015fi."}, {"id": 434597, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4489, "text": "Noi le-am d\u0103ruit fiilor lui Israel Cartea, \u00een\u0163elepciunea \u015fi proorocirea, i-am \u00eenzestrat cu bunuri \u015fi i-am ales \u00eenaintea lumilor."}, {"id": 434598, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4490, "text": "Le-am dat dovezi v\u0103dite ale Poruncii, iar ei nu s-au \u00eenvr\u0103jbit dec\u00e2t dup\u0103 ce le-a venit \u015ftiin\u0163a, ur\u00e2ndu-se unii pe al\u0163ii. Da, Domnul t\u0103u \u00eei va judeca \u00een Ziua \u00cenvierii pentru ceea ce se \u00eenvr\u0103jbeau."}, {"id": 434599, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4491, "text": "\u00cen ceea ce prive\u015fte Porunca, Noi te-am pus pe un drum sigur. Urmeaz\u0103-l! Nu urma poftele celor care nu \u015ftiu!"}, {"id": 434600, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4492, "text": "Ei nu-\u0163i vor sluji la nimic \u00eenaintea lui Dumnezeu. Cei nedrep\u0163i sunt obl\u0103duitori unii altora. Dumnezeu este Obl\u0103duitorul celor tem\u0103tori."}, {"id": 434601, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4493, "text": "Aceasta este oamenilor un \u00cendemn deslu\u015fit, o c\u0103l\u0103uzire \u015fi o milostivenie pentru un popor care crede cu t\u0103rie."}, {"id": 434602, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4494, "text": "Oare cei care s\u0103v\u00e2r\u015fesc fapte rele socot c\u0103 Noi o s\u0103-i a\u015fez\u0103m ca pe cei care s\u0103v\u00e2r\u015fesc fapte bune fiindc\u0103 to\u0163i ar fi deopotriv\u0103 \u00eentru via\u0163\u0103 \u015fi moarte? R\u0103u mai judec\u0103!"}, {"id": 434603, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4495, "text": "Dumnezeu a creat cerurile \u015fi p\u0103m\u00e2ntul \u00eentru Adev\u0103r ca fiece suflet s\u0103 fie r\u0103spl\u0103tit dup\u0103 ceea ce a agonisit. Ei nu vor fi nedrept\u0103\u0163i\u0163i."}, {"id": 434604, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4496, "text": "L-ai v\u0103zut pe cel care \u00ee\u015fi ia pofta drept dumnezeu al s\u0103u? Dumnezeu cu \u015ftiin\u0163\u0103 l-a r\u0103t\u0103cit. I-a pecetluit auzul \u015fi inima, \u015fi un v\u0103l i-a pus peste v\u0103z. Cine \u00een afar\u0103 de Dumnezeu \u00eel va c\u0103l\u0103uzi? Oare nu chibzui\u0163i?"}, {"id": 434605, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4497, "text": "Ei spun: \u201cNu avem dec\u00e2t Via\u0163a de Acum. Murim \u015fi tr\u0103im. Doar timpul ne nimice\u015fte. Ei nu au nici o \u015ftiin\u0163\u0103 despre acestea, ci numai ceea ce \u00ee\u015fi \u00eenchipuie."}, {"id": 434606, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4498, "text": "C\u00e2nd versetele Noastre le sunt recitate ca dovezi v\u0103dite, nu au alt r\u0103spuns dec\u00e2t \u00een a spune: \u201cAduce\u0163i-i pe ta\u0163ii no\u015ftri, dac\u0103 spune\u0163i adev\u0103rul!\u201d"}, {"id": 434607, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4499, "text": "Spune: \u201cDumnezeu v\u0103 d\u0103ruie\u015fte via\u0163a, apoi v\u0103 d\u0103ruie\u015fte moartea. El v\u0103 va str\u00e2nge \u00een Ziua \u00cenvierii.\u201d Nu este nici o \u00eendoial\u0103 \u00een aceasta. Cei mai mul\u0163i oameni nu \u015ftiu \u00eens\u0103."}, {"id": 434608, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4500, "text": "A lui Dumnezeu este \u00eemp\u0103r\u0103\u0163ia cerurilor \u015fi a p\u0103m\u00e2ntului. \u00cen Ziua c\u00e2nd va veni Ceasul, \u00een ziua aceea, cei care \u00eel credeau zadarnic vor fi pierdu\u0163i."}, {"id": 434609, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4501, "text": "Vei vedea fiece adunare \u00eengenunchiat\u0103. Fiece adunare va fi chemat\u0103 \u00eenaintea C\u0103r\u0163ii sale: \u201cAst\u0103zi ve\u0163i fi r\u0103spl\u0103ti\u0163i pentru ceea ce a\u0163i f\u0103ptuit.\u201d"}, {"id": 434610, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4502, "text": "\u201cAceasta este Cartea Noastr\u0103! Ea roste\u015fte asupra voastr\u0103 \u00eentru Adev\u0103r. Noi am scris ceea ce a\u0163i f\u0103ptuit.\u201d"}, {"id": 434611, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4503, "text": "Celor care cred \u015fi s\u0103v\u00e2r\u015fesc fapte bune, Domnul lor va pogor\u00ee asupra lor milostivenia Sa. Aceasta este fericirea v\u0103dit\u0103!"}, {"id": 434612, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4504, "text": "Celor care au t\u0103g\u0103duit: \u201cNu v-au fost recitate versetele Mele? V-a\u0163i \u00eeng\u00e2mfat, c\u0103ci sunte\u0163i un popor nelegiuit.\u201d"}, {"id": 434613, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4505, "text": "C\u00e2nd vi se spunea: \u201cF\u0103g\u0103duiala lui Dumnezeu este Adev\u0103r, nu este nici o \u00eendoial\u0103 asupra Ceasului\u201d, voi spunea\u0163i: \u201cNu \u015ftim ce va fi Ceasul acela, \u00eens\u0103 noi g\u00e2ndim c\u0103 nu este dec\u00e2t o \u00eenchipuire, a\u015fa c\u0103 nu suntem convin\u015fi.\u201d"}, {"id": 434614, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4506, "text": "Faptele rele pe care le-au s\u0103v\u00e2r\u015fit le vor apare, iar cea de care \u00ee\u015fi b\u0103teau joc \u00eei va \u00eenv\u0103lui."}, {"id": 434615, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4507, "text": "Li se va spune: \u201cV\u0103 uit\u0103m ast\u0103zi, precum \u015fi voi a\u0163i uitat \u00eent\u00e2lnirea cu aceast\u0103 zi a voastr\u0103. Limanul vostru este Focul. Nimeni nu v\u0103 este de ajutor."}, {"id": 434616, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4508, "text": "Aceasta, c\u0103ci a\u0163i luat \u00een zeflemea semnele lui Dumnezeu, iar Via\u0163a de Acum v-a am\u0103git. Din Ziua aceea, nu vor mai fi sco\u015fi din Gheena \u015fi nici nu vor fi chema\u0163i la c\u0103in\u0163\u0103."}, {"id": 434617, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4509, "text": "M\u0103rire lui Dumnezeu, Domnul cerurilor, Domnul p\u0103m\u00e2ntului, Domnul tuturor lumilor!"}, {"id": 434618, "identifier": "ro.grigore", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 4510, "text": "A Lui este m\u0103re\u0163ia, at\u00e2t \u00een ceruri, precum \u015fi pe p\u0103m\u00e2nt. El este Puternicul, \u00cen\u0163eleptul!"}]