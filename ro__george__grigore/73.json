[{"id": 435576, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5476, "text": "O, tu cel \u00eenf\u0103\u015furat \u00een mantie!"}, {"id": 435577, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5477, "text": "Scoal\u0103-te noaptea, \u00eens\u0103 nu pu\u0163in,"}, {"id": 435578, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5478, "text": "jum\u0103tate din ea ori mai scade pu\u0163in"}, {"id": 435579, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5479, "text": "ori mai adaug\u0103 \u015fi psalmodiaz\u0103 Coranul!"}, {"id": 435580, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5480, "text": "Noi \u00ee\u0163i vom da un cuv\u00e2nt cump\u0103nitor:"}, {"id": 435581, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5481, "text": "\u00eenceputul nop\u0163ii este mai puternic la ad\u00e2ncit \u015fi mai drept la rostit,"}, {"id": 435582, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5482, "text": "c\u0103ci peste zi ai trud\u0103 lung\u0103."}, {"id": 435583, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5483, "text": "Aminte\u015fte Numele Domnului t\u0103u \u015fi Lui d\u0103ruie\u015fte-te \u00eentru totul."}, {"id": 435584, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5484, "text": "El este Domnul R\u0103s\u0103ritului \u015fi al Asfin\u0163itului. Nu este dumnezeu afar\u0103 de El. \u0162ie ia-\u0163i-L obl\u0103duitor!"}, {"id": 435585, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5485, "text": "Rabd\u0103 c\u00e2nd \u00eei auzi ceea ce spun \u015fi cuviincios \u00eendep\u0103rteaz\u0103-te de ei."}, {"id": 435586, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5486, "text": "Las\u0103-M\u0103 pe Mine cu cei care hulesc, cu cei care tr\u0103iesc \u00een \u00eendestulare, \u015fi mai d\u0103-le un scurt r\u0103gaz!"}, {"id": 435587, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5487, "text": "Noi avem fiare \u015fi iad,"}, {"id": 435588, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5488, "text": "\u015fi m\u00e2ncare \u00eenec\u0103toare \u015fi os\u00e2nd\u0103 dureroas\u0103"}, {"id": 435589, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5489, "text": "pentru ziua c\u00e2nd p\u0103m\u00e2ntul \u015fi mun\u0163ii se vor zgudui, c\u00e2nd mun\u0163ii vor fi asemena unei dune r\u0103v\u0103\u015fite."}, {"id": 435590, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5490, "text": "Noi v-am trimis vou\u0103 un profet care este martor a ceea ce sunte\u0163i, a\u015fa cum am trimis un profet \u015fi la Faraon."}, {"id": 435591, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5491, "text": "Faraon nu i-a dat ascultare profetului \u015fi Noi, atunci, l-am luat cu o grea lovitur\u0103."}, {"id": 435592, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5492, "text": "Cum v\u0103 ve\u0163i ap\u0103ra, dac\u0103 a\u0163i fost t\u0103g\u0103duitori, de ziua care p\u00e2n\u0103 \u015fi pe prunci \u00eei va \u00eenc\u0103run\u0163i?"}, {"id": 435593, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5493, "text": "Cerul se va despica atunci, iar f\u0103g\u0103duiala se va \u00eemplini."}, {"id": 435594, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5494, "text": "Aceasta este o amintire! Cel care vrea poate apuca c\u0103tre Domnul s\u0103u pe o cale!"}, {"id": 435595, "identifier": "ro.grigore", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 509, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ro", "name": "Grigore", "transelator_en_name": "George Grigore", "format": "text", "type": "translation", "ayah_number": 5495, "text": "Domnul t\u0103u \u015ftie c\u0103 tu \u015fi mul\u0163i dimpreun\u0103 cu tine v\u0103 scula\u0163i pentru rug\u0103ciune, aproape dou\u0103 treimi ori o jum\u0103tate ori o treime din noapte. Dumnezeu a dat m\u0103sura nop\u0163ii \u015fi a zilei. El \u015ftie c\u0103 voi nu \u0163ine\u0163i socoteal\u0103 de aceasta, \u00eens\u0103 v\u0103 prime\u015fte c\u0103in\u0163a. Citi\u0163i tot ce pute\u0163i din Coran! El \u015ftie c\u0103 unii dintre voi sunt bolnavi, c\u0103 al\u0163ii str\u0103bat p\u0103m\u00e2ntul \u00een c\u0103utarea harului lui Dumnezeu, iar al\u0163ii lupt\u0103 pe calea lui Dumnezeu. Citi\u0163i tot ce pute\u0163i din Coran! Face\u0163i-v\u0103 rug\u0103ciunea, da\u0163i milostenie \u015fi da\u0163i-I lui Dumnezeu un frumos \u00eemprumut. Binele pe care vi-l face\u0163i voi \u00een\u015fiv\u0103 \u00eenainte, \u00eel ve\u0163i afla la Dumnezeu drept cea mai bun\u0103 \u015fi mai mare r\u0103splat\u0103. Cere\u0163i iertare lui Dumnezeu! Dumnezeu este Iert\u0103tor, Milostiv!"}]