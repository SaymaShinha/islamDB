[{"id": 354678, "identifier": "hi.hindi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 5552, "text": "\u092e\u0948\u0902 \u0930\u094b\u091c\u0947 \u0958\u092f\u093e\u092e\u0924 \u0915\u0940 \u0958\u0938\u092e \u0916\u093e\u0924\u093e \u0939\u0942\u0901"}, {"id": 354679, "identifier": "hi.hindi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 5553, "text": "(\u0914\u0930 \u092c\u0941\u0930\u093e\u0908 \u0938\u0947) \u092e\u0932\u093e\u092e\u0924 \u0915\u0930\u0928\u0947 \u0935\u093e\u0932\u0947 \u091c\u0940 \u0915\u0940 \u0958\u0938\u092e \u0916\u093e\u0924\u093e \u0939\u0942\u0901 (\u0915\u093f \u0924\u0941\u092e \u0938\u092c \u0926\u094b\u092c\u093e\u0930\u093e) \u095b\u0930\u0942\u0930 \u095b\u093f\u0928\u094d\u0926\u093e \u0915\u093f\u090f \u091c\u093e\u0913\u0917\u0947"}, {"id": 354680, "identifier": "hi.hindi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631