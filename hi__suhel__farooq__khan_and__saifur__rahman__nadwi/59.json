ommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 5139, "text": "\u092b\u093f\u0930 \u0909\u0928\u0915\u094b \u0915\u0939\u0940\u0902 \u0938\u0947 \u0915\u0941\u092e\u0915 \u092d\u0940 \u0928 \u092e\u093f\u0932\u0947\u0917\u0940 (\u092e\u094b\u092e\u093f\u0928\u094b\u0902) \u0924\u0941\u092e\u094d\u0939\u093e\u0930\u0940 \u0939\u0948\u092c\u0924 \u0909\u0928\u0915\u0947 \u0926\u093f\u0932\u094b\u0902 \u092e\u0947\u0902 \u0959\u0941\u0926\u093e \u0938\u0947 \u092d\u0940 \u092c\u095d\u0915\u0930 \u0939\u0948, \u092f\u0947 \u0907\u0938 \u0935\u091c\u0939 \u0938\u0947 \u0915\u093f \u092f\u0947 \u0932\u094b\u0917 \u0938\u092e\u091d \u0928\u0939\u0940\u0902 \u0930\u0916\u0924\u0947"}, {"id": 354282, "identifier": "hi.hindi", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 5140, "text": "\u092f\u0947 \u0938\u092c \u0915\u0947 \u0938\u092c \u092e\u093f\u0932\u0915\u0930 \u092d\u0940 \u0924\u0941\u092e\u0938\u0947 \u0928\u0939\u0940\u0902 \u0932\u095c \u0938\u0915\u0924\u0947, \u092e\u0917\u0930 \u0939\u0930 \u0924\u0930\u092b \u0938\u0947 \u092e\u0939\u092b\u0942\u095b \u092c\u0938\u094d\u0924\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 \u092f\u093e (\u0936\u0939\u0930 \u092a\u0928\u093e\u0939 \u0915\u0940) \u0926\u0940\u0935\u093e\u0930\u094b\u0902 \u091