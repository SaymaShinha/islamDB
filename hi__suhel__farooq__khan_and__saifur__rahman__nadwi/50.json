[{"id": 353786, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4631, "text": "\u0958\u093e\u095e \u0958\u0941\u0930\u093e\u0928 \u092e\u091c\u0940\u0926 \u0915\u0940 \u0958\u0938\u092e (\u092e\u094b\u0939\u092e\u094d\u092e\u0926 \u092a\u0948\u095a\u092e\u094d\u092c\u0930 \u0939\u0948\u0902)"}, {"id": 353662, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4632, "text": "\u0932\u0947\u0915\u093f\u0928 \u0907\u0928 (\u0915\u093e\u092b\u093f\u0930\u094b\u0902) \u0915\u094b \u0924\u093e\u091c\u094d\u091c\u0941\u092c \u0939\u0948 \u0915\u093f \u0909\u0928 \u0939\u0940 \u092e\u0947\u0902 \u090f\u0915 (\u0905\u095b\u093e\u092c \u0938\u0947) \u0921\u0930\u093e\u0928\u0947 \u0935\u093e\u0932\u093e (\u092a\u0948\u095a\u092e\u094d\u092c\u0930) \u0909\u0928\u0915\u0947 \u092a\u093e\u0938 \u0906 \u0917\u092f\u093e \u0924\u094b \u0915\u0941\u092b\u094d\u095e\u093e\u0930 \u0915\u0939\u0928\u0947 \u0932\u0917\u0947 \u092f\u0947 \u0924\u094b \u090f\u0915 \u0905\u091c\u0940\u092c \u092c\u093e\u0924 \u0939\u0948"}, {"id": 353636, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4633, "text": "\u092d\u0932\u093e \u091c\u092c \u0939\u092e \u092e\u0930 \u091c\u093e\u090f\u0901\u0917\u0947 \u0914\u0930 (\u0938\u095c \u0917\u0932 \u0915\u0930) \u092e\u093f\u091f\u091f\u0940 \u0939\u094b \u091c\u093e\u090f\u0901\u0917\u0947 \u0924\u094b \u092b\u093f\u0930 \u092f\u0947 \u0926\u094b\u092c\u093e\u0930 \u095b\u093f\u0928\u094d\u0926\u093e \u0939\u094b\u0928\u093e (\u0905\u0915\u094d\u0932 \u0938\u0947) \u092c\u0908\u0926 (\u092c\u093e\u0924 \u0939\u0948)"}, {"id": 353797, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4634, "text": "\u0909\u0928\u0915\u0947 \u091c\u093f\u0938\u094d\u092e\u094b\u0902 \u0938\u0947 \u095b\u092e\u0940\u0928 \u091c\u093f\u0938 \u091a\u0940\u095b \u0915\u094b (\u0916\u093e \u0916\u093e \u0915\u0930) \u0915\u092e \u0915\u0930\u0924\u0940 \u0939\u0948 \u0935\u0939 \u0939\u092e\u0915\u094b \u092e\u093e\u0932\u0942\u092e \u0939\u0948 \u0914\u0930 \u0939\u092e\u093e\u0930\u0947 \u092a\u093e\u0938 \u0924\u094b \u0924\u0939\u0930\u0940\u0930\u0940 \u092f\u093e\u0926\u0926\u093e\u0936\u094d\u0924 \u0915\u093f\u0924\u093e\u092c \u0932\u094c\u0939\u0947 \u092e\u0939\u092b\u0942\u095b \u092e\u094c\u091c\u0942\u0926 \u0939\u0948"}, {"id": 353798, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4635, "text": "\u092e\u0917\u0930 \u091c\u092c \u0909\u0928\u0915\u0947 \u092a\u093e\u0938 \u0926\u0940\u0928 (\u0939\u0958) \u0906 \u092a\u0939\u0941\u0901\u091a\u093e \u0924\u094b \u0909\u0928\u094d\u0939\u094b\u0928\u0947 \u0909\u0938\u0947 \u091d\u0941\u0920\u0932\u093e\u092f\u093e \u0924\u094b \u0935\u0939 \u0932\u094b\u0917 \u090f\u0915 \u0910\u0938\u0940 \u092c\u093e\u0924 \u092e\u0947\u0902 \u0909\u0932\u091d\u0947 \u0939\u0941\u090f \u0939\u0948\u0902 \u091c\u093f\u0938\u0947 \u0958\u0930\u093e\u0930 \u0928\u0939\u0940\u0902"}, {"id": 353799, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4636, "text": "\u0924\u094b \u0915\u094d\u092f\u093e \u0907\u0928 \u0932\u094b\u0917\u094b\u0902 \u0928\u0947 \u0905\u092a\u0928\u0947 \u090a\u092a\u0930 \u0906\u0938\u092e\u093e\u0928 \u0915\u0940 \u0928\u095b\u0930 \u0928\u0939\u0940\u0902 \u0915\u0940 \u0915\u093f \u0939\u092e\u0928\u0947 \u0909\u0938\u0915\u094b \u0915\u094d\u092f\u094b\u0902 \u0915\u0930 \u092c\u0928\u093e\u092f\u093e \u0914\u0930 \u0909\u0938\u0915\u094b \u0915\u0948\u0938\u0940 \u095b\u0940\u0928\u0924 \u0926\u0940 \u0914\u0930 \u0909\u0928\u0938\u0947 \u0915\u0939\u0940\u0902 \u0936\u093f\u0917\u093e\u092b\u094d\u0924 \u0924\u0915 \u0928\u0939\u0940\u0902"}, {"id": 353800, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4637, "text": "\u0914\u0930 \u095b\u092e\u0940\u0928 \u0915\u094b \u0939\u092e\u0928\u0947 \u092b\u0948\u0932\u093e\u092f\u093e \u0914\u0930 \u0909\u0938 \u092a\u0930 \u092c\u094b\u091d\u0932 \u092a\u0939\u093e\u095c \u0930\u0916 \u0926\u093f\u092f\u0947 \u0914\u0930 \u0907\u0938\u092e\u0947\u0902 \u0939\u0930 \u0924\u0930\u0939 \u0915\u0940 \u0959\u0941\u0936\u0928\u0941\u092e\u093e \u091a\u0940\u095b\u0947\u0902 \u0909\u0917\u093e\u0908 \u0924\u093e\u0915\u093f \u0924\u092e\u093e\u092e \u0930\u0942\u091c\u0942 \u0932\u093e\u0928\u0947 \u0935\u093e\u0932\u0947"}, {"id": 353801, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4638, "text": "(\u092c\u0928\u094d\u0926\u0947) \u0939\u093f\u0926\u093e\u092f\u0924 \u0914\u0930 \u0907\u092c\u0930\u0924 \u0939\u093e\u0938\u093f\u0932 \u0915\u0930\u0947\u0902"}, {"id": 353802, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4639, "text": "\u0914\u0930 \u0939\u092e\u0928\u0947 \u0906\u0938\u092e\u093e\u0928 \u0938\u0947 \u092c\u0930\u0915\u0924 \u0935\u093e\u0932\u093e \u092a\u093e\u0928\u0940 \u092c\u0930\u0938\u093e\u092f\u093e \u0924\u094b \u0909\u0938\u0938\u0947 \u092c\u093e\u095a (\u0915\u0947 \u0926\u0930\u0916\u094d\u0924) \u0909\u0917\u093e\u090f \u0914\u0930 \u0916\u0947\u0924\u0940 \u0915\u093e \u0905\u0928\u093e\u091c \u0914\u0930 \u0932\u092e\u094d\u092c\u0940 \u0932\u092e\u094d\u092c\u0940 \u0916\u091c\u0942\u0930\u0947\u0902"}, {"id": 353803, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4640, "text": "\u091c\u093f\u0938\u0915\u093e \u092c\u094c\u0930 \u092c\u093e\u0939\u092e \u0917\u0941\u0925\u093e \u0939\u0941\u0906 \u0939\u0948"}, {"id": 353804, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4641, "text": "(\u092f\u0947 \u0938\u092c \u0915\u0941\u091b) \u092c\u0928\u094d\u0926\u094b\u0902 \u0915\u0940 \u0930\u094b\u095b\u0940 \u0926\u0947\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f (\u092a\u0948\u0926\u093e \u0915\u093f\u092f\u093e) \u0914\u0930 \u092a\u093e\u0928\u0940 \u0939\u0940 \u0938\u0947 \u0939\u092e\u0928\u0947 \u092e\u0941\u0930\u094d\u0926\u093e \u0936\u0939\u0930 (\u0909\u095e\u0924\u093e\u0926\u093e \u095b\u092e\u0940\u0928) \u0915\u094b \u095b\u093f\u0928\u094d\u0926\u093e \u0915\u093f\u092f\u093e"}, {"id": 353805, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4642, "text": "\u0907\u0938\u0940 \u0924\u0930\u0939 (\u0958\u092f\u093e\u092e\u0924 \u092e\u0947\u0902 \u092e\u0941\u0930\u094d\u0926\u094b\u0902 \u0915\u094b) \u0928\u093f\u0915\u0932\u0928\u093e \u0939\u094b\u0917\u093e \u0909\u0928\u0938\u0947 \u092a\u0939\u0932\u0947 \u0928\u0942\u0939 \u0915\u0940 \u0958\u094c\u092e \u0914\u0930 \u0959\u0928\u094d\u0926\u0958 \u0935\u093e\u0932\u094b\u0902 \u0914\u0930 (\u0958\u094c\u092e) \u0938\u092e\u0942\u0926 \u0928\u0947 \u0905\u092a\u0928\u0947 \u0905\u092a\u0928\u0947 \u092a\u0948\u095a\u092e\u094d\u092c\u0930\u094b\u0902 \u0915\u094b \u091d\u0941\u0920\u0932\u093e\u092f\u093e"}, {"id": 353806, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4643, "text": "\u0914\u0930 (\u0958\u094c\u092e) \u0906\u0926 \u0914\u0930 \u092b\u093f\u0930\u0914\u0928 \u0914\u0930 \u0932\u0942\u0924 \u0915\u0940 \u0958\u094c\u092e"}, {"id": 353807, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4644, "text": "\u0914\u0930 \u092c\u0928 \u0915\u0947 \u0930\u0939\u0928\u0947 \u0935\u093e\u0932\u094b\u0902 (\u0958\u094c\u092e \u0936\u0941\u0910\u092c) \u0914\u0930 \u0924\u0941\u092c\u094d\u092c\u093e \u0915\u0940 \u0958\u094c\u092e \u0914\u0930 (\u0909\u0928) \u0938\u092c\u0928\u0947 \u0905\u092a\u0928\u0947 (\u0905\u092a\u0928\u0947) \u092a\u0948\u095a\u092e\u094d\u092c\u0930\u094b\u0902 \u0915\u094b \u091d\u0941\u0920\u0932\u093e\u092f\u093e \u0924\u094b \u0939\u092e\u093e\u0930\u093e (\u0905\u095b\u093e\u092c \u0915\u093e) \u0935\u093e\u092f\u0926\u093e \u092a\u0942\u0930\u093e \u0939\u094b \u0915\u0930 \u0930\u0939\u093e"}, {"id": 353808, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4645, "text": "\u0924\u094b \u0915\u094d\u092f\u093e \u0939\u092e \u092a\u0939\u0932\u0940 \u092c\u093e\u0930 \u092a\u0948\u0926\u093e \u0915\u0930\u0915\u0947 \u0925\u0915 \u0917\u092f\u0947 \u0939\u0948\u0902 (\u0939\u0930\u0917\u093f\u095b \u0928\u0939\u0940\u0902) \u092e\u0917\u0930 \u092f\u0947 \u0932\u094b\u0917 \u0905\u095b \u0938\u0930\u0947 \u0928\u094c (\u0926\u094b\u092c\u093e\u0930\u093e) \u092a\u0948\u0926\u093e \u0915\u0930\u0928\u0947 \u0915\u0940 \u0928\u093f\u0938\u094d\u092c\u0924 \u0936\u0915\u093c \u092e\u0947\u0902 \u092a\u095c\u0947 \u0939\u0948\u0902"}, {"id": 353809, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4646, "text": "\u0914\u0930 \u092c\u0947\u0936\u0915 \u0939\u092e \u0939\u0940 \u0928\u0947 \u0907\u0928\u094d\u0938\u093e\u0928 \u0915\u094b \u092a\u0948\u0926\u093e \u0915\u093f\u092f\u093e \u0914\u0930 \u091c\u094b \u0916\u094d\u092f\u093e\u0932\u093e\u0924 \u0909\u0938\u0915\u0947 \u0926\u093f\u0932 \u092e\u0947\u0902 \u0917\u0941\u095b\u0930\u0924\u0947 \u0939\u0948\u0902 \u0939\u092e \u0909\u0928\u0915\u094b \u091c\u093e\u0928\u0924\u0947 \u0939\u0948\u0902 \u0914\u0930 \u0939\u092e \u0924\u094b \u0909\u0938\u0915\u0940 \u0936\u0939\u0930\u0917 \u0938\u0947 \u092d\u0940 \u091c\u094d\u092f\u093e\u0926\u093e \u0958\u0930\u0940\u092c \u0939\u0948\u0902"}, {"id": 353810, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4647, "text": "\u091c\u092c (\u0935\u0939 \u0915\u094b\u0908 \u0915\u093e\u092e \u0915\u0930\u0924\u093e \u0939\u0948\u0902 \u0924\u094b) \u0926\u094b \u0932\u093f\u0916\u0928\u0947 \u0935\u093e\u0932\u0947 (\u0915\u0947\u0930\u093e\u092e\u0928 \u0958\u093e\u0924\u0947\u092c\u0940\u0928) \u091c\u094b \u0909\u0938\u0915\u0947 \u0926\u093e\u0939\u093f\u0928\u0947 \u092c\u093e\u090f\u0902 \u092c\u0948\u0920\u0947 \u0939\u0948\u0902 \u0932\u093f\u0916 \u0932\u0947\u0924\u0947 \u0939\u0948\u0902"}, {"id": 353730, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0928\u0926\u0935\u0940", "transelator_en_name": "Suhel Farooq Khan and Saifur Rahman Nadwi", "format": "text", "type": "translation", "ayah_number": 4648, "text": "\u0915\u094b\u0908 \u092c\u093e\u0924 \u0909\u0938\u0915\u0940 \u095b\u092c\u093e\u0928 \u092a\u0930 \u0928\u0939\u0940\u0902 \u0906\u0924\u0940 \u092e\u0917\u0930 \u090f\u0915 \u0928\u093f\u0917\u0947\u0939\u092c\u093e\u0928 \u0909\u0938\u0915\u0947 \u092a\u093e\u0938 \u0924\u0948\u092f\u093e\u0930 \u0930\u0939\u0924\u093e \u0939\u0948"}, {"id": 353811, "identifier": "hi.hindi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \