[{"id": 417090, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5713, "text": "Ved dem som trekker ut og spenner!"}, {"id": 417091, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5714, "text": "Ved dem som g\u00e5r ivrig frem!"}, {"id": 417092, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5715, "text": "Ved dem som siger frem,"}, {"id": 417093, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5716, "text": "og s\u00e5 tar ledelsen"}, {"id": 417094, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5717, "text": "og tar seg av saken!"}, {"id": 417095, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5718, "text": "Den dag n\u00e5r rystelsen ryster,"}, {"id": 417096, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5719, "text": "og f\u00f8lges av rystelse,"}, {"id": 417097, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5720, "text": "p\u00e5 denne dag vil hjertene banke"}, {"id": 417098, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5721, "text": "og blikkene sl\u00e5s ned."}, {"id": 417099, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5722, "text": "De sier: \u00abSkal vi bringes tilbake til det vi var?"}, {"id": 417100, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5723, "text": "N\u00e5r vi er morkne ben?\u00bb"}, {"id": 417101, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5724, "text": "Og de tilf\u00f8yer: \u00abDet vil v\u00e6re en retur med tap!\u00bb"}, {"id": 417102, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5725, "text": "Men det lyder et eneste skrall,"}, {"id": 417103, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5726, "text": "og s\u00e5 er de lys v\u00e5kne."}, {"id": 417104, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5727, "text": "Historien om Moses har vel n\u00e5dd deg?"}, {"id": 417105, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5728, "text": "Da Herren ropte til ham i den hellige dal Tuwa:"}, {"id": 417106, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5729, "text": "\u00abG\u00e5 til Farao! Han er oppsetsig!"}, {"id": 417107, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5730, "text": "Og si: \u2019\u00d8nsker du ikke \u00e5 rense deg,"}, {"id": 417108, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5731, "text": "og at jeg leder deg hen til Herren, s\u00e5 du kan vise gudsfrykt?\u2019\u00bb"}, {"id": 417109, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5732, "text": "S\u00e5 viste Han ham det st\u00f8rste j\u00e6rtegnet,"}, {"id": 417110, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5733, "text": "men han forkastet og var ulydig."}, {"id": 417111, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5734, "text": "S\u00e5 vendte han ryggen og fjernet seg raskt,"}, {"id": 417112, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5735, "text": "sammenkalte folk"}, {"id": 417113, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5736, "text": "og sa: \u00abJeg er deres h\u00f8yeste herre.\u00bb"}, {"id": 417114, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5737, "text": "S\u00e5 tok Gud ham fatt med den hinsidiges og denne verdens straff."}, {"id": 417115, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5738, "text": "I dette er visselig en l\u00e6repenge for den som har gudsfykt!"}, {"id": 417116, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5739, "text": "Er dere mer krevende \u00e5 skape, eller himmelen Han har bygget?"}, {"id": 417117, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5740, "text": "Han hevet opp dens tak og gav den form,"}, {"id": 417118, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5741, "text": "lot dens natt bli m\u00f8rk og lot morgenlyset komme frem."}, {"id": 417119, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5742, "text": "S\u00e5 bredte Han jorden ut"}, {"id": 417120, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5743, "text": "og brakte frem av den vann og gr\u00f8nne enger,"}, {"id": 417121, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5744, "text": "og Han gav fjellene fast feste,"}, {"id": 417122, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5745, "text": "til glede for dere og deres hjorder."}, {"id": 417123, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5746, "text": "N\u00e5r s\u00e5 den store katastrofe inntreffer,"}, {"id": 417124, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5747, "text": "den dag n\u00e5r mennesket kommer i hu hva han jaget etter,"}, {"id": 417125, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5748, "text": "og helvete bringes frem for alle som ser,"}, {"id": 417126, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5749, "text": "da skal den som var oppsetsig"}, {"id": 417127, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5750, "text": "og foretrakk jordelivet,"}, {"id": 417128, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5751, "text": "f\u00e5 helvete til herberge."}, {"id": 417129, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5752, "text": "Men den som fryktet Herrens maktstilling og nektet seg sine lyster,"}, {"id": 417130, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5753, "text": "\u2013 se, paradisets have er herberget."}, {"id": 417131, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5754, "text": "De sp\u00f8r deg om timen, om n\u00e5r den kommer."}, {"id": 417132, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5755, "text": "Hva kan vel du si om dette?"}, {"id": 417133, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5756, "text": "Dens endelige tid beror hos Herren."}, {"id": 417134, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Du er bare en advarer for dem som frykter den."}, {"id": 417135, "identifier": "no.berg", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5758, "text": "Den dag de f\u00e5r se den, blir det som om de hadde ventet bare en kveld eller en morgenstund."}]