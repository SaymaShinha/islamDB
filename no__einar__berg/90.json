[{"id": 417396, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Nei, sannelig, jeg sverger ved denne byen,"}, {"id": 417397, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6025, "text": "og du bor jo i denne byen,"}, {"id": 417398, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6026, "text": "og ved en far og hans avkom!"}, {"id": 417399, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Vi har visselig skapt mennesket til m\u00f8ye."}, {"id": 417400, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6028, "text": "Tror han ikke at noen har makt over ham?"}, {"id": 417401, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6029, "text": "Han sier: \u00abJeg har satt over styr rikdom i massevis.\u00bb"}, {"id": 417402, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6030, "text": "Tror han at ingen har sett ham?"}, {"id": 417403, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Har Vi ikke gitt ham to \u00f8yne,"}, {"id": 417404, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6032, "text": "tunge, og to lepper."}, {"id": 417405, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6033, "text": "Har Vi ikke vist ham de to veier?"}, {"id": 417406, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Men han gav seg ikke i kast med den trange og bratte."}, {"id": 417407, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6035, "text": "Vet du da hva den trange vei er?"}, {"id": 417408, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6036, "text": "Det er \u00e5 skaffe en slave frihet,"}, {"id": 417409, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6037, "text": "og p\u00e5 hungerens dag \u00e5 gi mat"}, {"id": 417410, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6038, "text": "til en foreldrel\u00f8s slektning,"}, {"id": 417411, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6039, "text": "eller til en fattig stakkar,"}, {"id": 417412, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6040, "text": "og s\u00e5 v\u00e6re av dem som tror, og som anbefaler hverandre standhaftighet, og r\u00e5der hverandre til medf\u00f8lelse."}, {"id": 417413, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6041, "text": "Dette er h\u00f8yre h\u00e5nds folk!"}, {"id": 417414, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6042, "text": "Men de som fornekter V\u00e5rt ord, de er venstre h\u00e5nds folk."}, {"id": 417415, "identifier": "no.berg", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Over dem hvelver seg Ild."}]