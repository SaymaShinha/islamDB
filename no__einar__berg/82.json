[{"id": 417205, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5830, "text": "N\u00e5r himmelen revner,"}, {"id": 417206, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5831, "text": "n\u00e5r stjernene str\u00f8s utover,"}, {"id": 417207, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5832, "text": "n\u00e5r havene flyter over,"}, {"id": 417208, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5833, "text": "n\u00e5r gravene blir endevendt,"}, {"id": 417209, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5834, "text": "da skal enhver f\u00e5 vite hva han har utrettet og hva han har fors\u00f8mt."}, {"id": 417210, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5835, "text": "Menneske! Hva har forledet deg vedr\u00f8rende din h\u00f8ysinnede Herre?"}, {"id": 417211, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5836, "text": "Han som har skapt deg, formet deg og gitt deg rette proporsjoner"}, {"id": 417212, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5837, "text": "i den skikkelse Han \u00f8nsket \u00e5 gi deg?"}, {"id": 417213, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5838, "text": "Men sannelig, dere fornekter dommen!"}, {"id": 417214, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5839, "text": "Men over dere er voktere,"}, {"id": 417215, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5840, "text": "\u00e6rverdige skrivere"}, {"id": 417216, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5841, "text": "som vel vet hva dere gj\u00f8r!"}, {"id": 417217, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5842, "text": "De fromme skal v\u00e6re i salighet,"}, {"id": 417218, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5843, "text": "mens de syndefulle skal v\u00e6re i helvete."}, {"id": 417219, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5844, "text": "Der skal de brenne p\u00e5 dommens dag,"}, {"id": 417220, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5845, "text": "og derfra kan de ikke komme bort."}, {"id": 417221, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5846, "text": "Hvordan kan du vite hva dommens dag er?"}, {"id": 417222, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5847, "text": "Hvordan kan du virkelig vite hva dommens dag er?"}, {"id": 417223, "identifier": "no.berg", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5848, "text": "Den dag da ingen makter \u00e5 utrette noe for en annen. Saken ligger denne dag i Guds h\u00e5nd!"}]