[{"id": 411579, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5994, "text": "Ved morgengryet!"}, {"id": 411581, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5995, "text": "Ved ti netter!"}, {"id": 411582, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5996, "text": "Ved det like og det ulike!"}, {"id": 417369, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5997, "text": "Ved natten n\u00e5r den svinner hen!"}, {"id": 417370, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5998, "text": "Er det ikke i dette en ed for den som har innsikt?"}, {"id": 417371, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 5999, "text": "Du har vel sett hva Herren gjorde med folket Ad?"}, {"id": 417372, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6000, "text": "Med det s\u00f8ylerike Iram,"}, {"id": 417373, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6001, "text": "hvis like ikke er skapt i verden?"}, {"id": 417374, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6002, "text": "Og med folket Thamod, som hugget seg inn i fjellsiden i dalen?"}, {"id": 417375, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6003, "text": "Og Farao, han med pyramidene?"}, {"id": 417376, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6004, "text": "De som vandret i overmot p\u00e5 jorden,"}, {"id": 417377, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6005, "text": "og spredte ufred og fordervelse?"}, {"id": 417378, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6006, "text": "Men Herren lot tuktens svepe ramme dem!"}, {"id": 417379, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6007, "text": "Herren er p\u00e5 vakt!"}, {"id": 417380, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6008, "text": "N\u00e5r Herren setter mennesket p\u00e5 pr\u00f8ve ved \u00e6re og velstand, sier det: \u00abHerren har hedret meg!\u00bb"}, {"id": 417381, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6009, "text": "Men hvis Han setter det p\u00e5 pr\u00f8ve ved \u00e5 utm\u00e5le knapt hans levem\u00e5te, s\u00e5 sier det: \u00abHerren ringeakter meg!\u00bb"}, {"id": 417382, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6010, "text": "Nei, sannelig, men dere hedrer ikke den faderl\u00f8se"}, {"id": 417383, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6011, "text": "og ansporer ikke til \u00e5 gi den fattige mat,"}, {"id": 417384, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6012, "text": "dere sluker gr\u00e5dig hans lille arv."}, {"id": 417385, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6013, "text": "Dere elsker rikdom over all m\u00e5te."}, {"id": 417386, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6014, "text": "Nei, sannelig! N\u00e5r jorden jevnes til st\u00f8v,"}, {"id": 417387, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6015, "text": "n\u00e5r Herren kommer, og englene, rekke etter rekke,"}, {"id": 417388, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6016, "text": "n\u00e5r helvete bringes frem, p\u00e5 denne dag vil mennesket saktens erindre. Men hva tjener det da til med ettertanke?"}, {"id": 417389, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6017, "text": "Og det vil si: \u00abOm jeg bare hadde sendt i forveien noe godt for mitt liv!\u00bb"}, {"id": 417390, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6018, "text": "Ingen kan straffe som Han gj\u00f8r p\u00e5 denne dag."}, {"id": 417391, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6019, "text": "Ingen lenker slik som Han."}, {"id": 417392, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6020, "text": "Du sjel, som har ro og harmoni,"}, {"id": 417393, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6021, "text": "vend tilbake til din Herre tilfreds og godtatt:"}, {"id": 417394, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6022, "text": "G\u00e5 inn blant Mine tjenere!"}, {"id": 417395, "identifier": "no.berg", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "no", "name": "Einar Berg", "transelator_en_name": "Einar Berg", "format": "text", "type": "translation", "ayah_number": 6023, "text": "G\u00e5 inn i Mitt paradis!"}]