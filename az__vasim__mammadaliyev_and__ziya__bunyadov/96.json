[{"id": 93267, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6107, "text": "(Ya Pey\u011f\u0259mb\u0259r! Qur\u2019ani-K\u0259rimi b\u00fct\u00fcn m\u0259xluqat\u0131) yoxdan yaradan R\u0259bbinin ad\u0131 il\u0259 (bismillah dey\u0259r\u0259k) oxu!"}, {"id": 93268, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6108, "text": "O, insan\u0131 laxtalanm\u0131\u015f qandan yaratd\u0131."}, {"id": 93269, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6109, "text": "(Ya Pey\u011f\u0259mb\u0259r!) Oxu! S\u0259nin R\u0259bbin \u0259n b\u00f6y\u00fck k\u0259r\u0259m sahibidir!"}, {"id": 93270, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6110, "text": "O R\u0259bbin ki, q\u0259l\u0259ml\u0259 (yazma\u011f\u0131) \u00f6yr\u0259tdi."}, {"id": 93271, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6111, "text": "(O R\u0259bbin ki) insana bilm\u0259dikl\u0259rini \u00f6yr\u0259tdi."}, {"id": 93272, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Xeyr, insan az\u011f\u0131nl\u0131q ed\u0259r,"}, {"id": 93273, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6113, "text": "\u00d6z\u00fcn\u00fcn d\u00f6vl\u0259tli oldu\u011funu g\u00f6rd\u00fcy\u00fc \u00fc\u00e7\u00fcn!"}, {"id": 93274, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6114, "text": "Ax\u0131 s\u0259nin ax\u0131r d\u00f6n\u00fc\u015f\u00fcn R\u0259bbin\u0259dir!"}, {"id": 93275, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6115, "text": "G\u00f6rd\u00fcnm\u00fc o kims\u0259ni (\u018fbu C\u0259hli) ki, mane olur"}, {"id": 93276, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6116, "text": "Bir b\u0259nd\u0259y\u0259 (s\u0259n\u0259) namaz q\u0131ld\u0131\u011f\u0131 vaxt?"}, {"id": 93277, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6117, "text": "Bir de g\u00f6r\u0259k, \u0259g\u0259r o do\u011fru yoldad\u0131rsa,"}, {"id": 93278, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6118, "text": "Yaxud (xalqa Allahdan) qorxma\u011f\u0131 \u0259mr edirs\u0259, (s\u0259n bel\u0259 bir \u0259m\u0259l sahibinin namaz q\u0131lmas\u0131na n\u0259 \u00fc\u00e7\u00fcn mane olur, ona ibad\u0259t etm\u0259yi niy\u0259 qada\u011fan edirs\u0259n? Bundan da r\u0259zil i\u015f olarm\u0131)?!"}, {"id": 93279, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6119, "text": "(Ya Pey\u011f\u0259mb\u0259r!) Bir de g\u00f6r\u0259k, (\u0259g\u0259r s\u0259nin namaz q\u0131lma\u011f\u0131na mane olan kims\u0259 Qur\u2019an\u0131) yalan say\u0131r v\u0259 (imandan) \u00fcz d\u00f6nd\u0259rirs\u0259,"}, {"id": 93280, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6120, "text": "M\u0259g\u0259r bilmir ki, Allah (onun b\u00fct\u00fcn \u0259m\u0259ll\u0259rini) g\u00f6r\u00fcr?!"}, {"id": 93281, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6121, "text": "Yox, yox! (\u018fbu C\u0259hl bu yaramaz \u0259m\u0259ll\u0259rin\u0259 son qoysun). \u018fg\u0259r son qoymasa, and olsun ki, Biz onu k\u0259kilind\u0259n yap\u0131\u015f\u0131b (C\u0259h\u0259nn\u0259m\u0259) s\u00fcr\u00fckl\u0259y\u0259c\u0259yik -"}, {"id": 93282, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6122, "text": "\u00d6z\u00fc d\u0259 yalan\u00e7\u0131, g\u00fcnahkar k\u0259kilind\u0259n!"}, {"id": 93283, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6123, "text": "Qoy o \u00f6z\u00fcn\u00fcn b\u00fct\u00fcn t\u0259r\u0259fdarlar\u0131n\u0131 (k\u00f6m\u0259y\u0259) \u00e7a\u011f\u0131rs\u0131n!"}, {"id": 93284, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6124, "text": "Biz d\u0259 z\u0259banil\u0259ri (C\u0259h\u0259nn\u0259md\u0259ki \u0259zab m\u0259l\u0259kl\u0259rini) \u00e7a\u011f\u0131raca\u011f\u0131q!"}, {"id": 93285, "identifier": "az.mammadaliyev", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6125, "text": "Yox, yox! S\u0259n (ey Pey\u011f\u0259mb\u0259r!) ona uyma! S\u0259n ancaq (R\u0259bbin\u0259) s\u0259cd\u0259 et v\u0259 (Ona) yax\u0131nla\u015f!"}]