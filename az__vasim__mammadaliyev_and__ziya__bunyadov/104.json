[{"id": 93337, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6180, "text": "(Dalda) qeyb\u0259t edib (\u00fczd\u0259) t\u0259\u2019n\u0259 vuran h\u0259r k\u0259sin vay hal\u0131na!"}, {"id": 93338, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6181, "text": "O kims\u0259 ki, mal y\u0131\u011f\u0131b onu d\u00f6n\u0259-d\u00f6n\u0259 sayar (\u00e7oxaldar)."}, {"id": 93339, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6182, "text": "V\u0259 el\u0259 z\u0259nn ed\u0259r ki, mal-d\u00f6vl\u0259ti onu \u0259b\u0259di ya\u015fadacaqd\u0131r."}, {"id": 93340, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Xeyr! (He\u00e7 d\u0259 onun g\u00fcman etdiyi kimi deyildir). O m\u00fctl\u0259q (C\u0259h\u0259nn\u0259min alt m\u0259rt\u0259b\u0259l\u0259rind\u0259n biri olan) H\u00fct\u0259m\u0259y\u0259 at\u0131lacaqd\u0131r!"}, {"id": 93341, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6184, "text": "S\u0259n n\u0259 bilirs\u0259n (haradan bilirs\u0259n) ki, H\u00fct\u0259m\u0259 n\u0259dir?!"}, {"id": 93342, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6185, "text": "O, Allah\u0131n yanar odudur."}, {"id": 93343, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6186, "text": "El\u0259 bir od ki, \u00fcr\u0259kl\u0259ri yand\u0131r\u0131b-yaxar."}, {"id": 93344, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6187, "text": "O, bel\u0259l\u0259rinin (qeyb\u0259t\u00e7il\u0259rin v\u0259 t\u0259\u2019n\u0259 vuranlar\u0131n) \u00fcz\u00fcn\u0259 qapan\u0131b kilidl\u0259n\u0259c\u0259kdir."}, {"id": 93345, "identifier": "az.mammadaliyev", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "M\u0259mm\u0259d\u0259liyev & B\u00fcnyadov", "transelator_en_name": "Vasim Mammadaliyev and Ziya Bunyadov", "format": "text", "type": "translation", "ayah_number": 6188, "text": "Onlar (C\u0259h\u0259nn\u0259md\u0259) h\u00fcnd\u00fcr s\u00fctunlara ba\u011flanm\u0131\u015f olacaqlar!"}]