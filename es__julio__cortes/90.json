[{"id": 255256, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6024, "text": "\u00a1No! \u00a1Juro por esta ciudad,"}, {"id": 255257, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6025, "text": "en la que est\u00e1s avecindado!"}, {"id": 255258, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6026, "text": "\u00a1Por todo padre y lo que ha engendrado!"}, {"id": 255259, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Hemos creado al hombre en aflicci\u00f3n."}, {"id": 255260, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6028, "text": "\u00bfCree que nadie podr\u00e1 contra \u00e9l?"}, {"id": 255261, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6029, "text": "Dice: \u00abHe consumido una hacienda considerable\u00bb."}, {"id": 255262, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6030, "text": "\u00bfCree que nadie le ha visto?"}, {"id": 255263, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6031, "text": "\u00bfNo le hemos dado dos ojos,"}, {"id": 255264, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6032, "text": "una lengua y dos labios?"}, {"id": 255265, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6033, "text": "\u00bfNo le hemos mostrado las dos v\u00edas?"}, {"id": 255266, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Pues nunca se ha puesto a subir la Cuesta."}, {"id": 255267, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6035, "text": "Y \u00bfc\u00f3mo sabr\u00e1s qu\u00e9 es la Cuesta?"}, {"id": 255268, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6036, "text": "Es manumitir a un esclavo,"}, {"id": 255269, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6037, "text": "Alimentar en tiempo de hambre"}, {"id": 255270, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6038, "text": "A un pariente pr\u00f3ximo hu\u00e9rfano,"}, {"id": 255271, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6039, "text": "A un pobre en la miseria."}, {"id": 255272, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6040, "text": "Es, tambi\u00e9n, formar parte de los que creen, de los que se recomiendan mutuamente la paciencia y la misericordia."}, {"id": 255273, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6041, "text": "\u00c9sos son los de la Derecha."}, {"id": 255274, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6042, "text": "En cambio, los que no creen en Nuestros signos, \u00e9sos son los de la izquierda."}, {"id": 255275, "identifier": "es.cortes", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Se cerrar\u00e1 un fuego sobre ellos."}]