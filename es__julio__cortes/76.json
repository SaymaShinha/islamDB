[{"id": 254825, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5592, "text": "\u00bfHa pasado el hombre por un per\u00edodo de tiempo en que no era nada digno de menci\u00f3n?"}, {"id": 254826, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5593, "text": "Hemos creado al hombre de una gota, de ingredientes, para ponerle a prueba. Le hemos dado el o\u00eddo, la vista."}, {"id": 254827, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5594, "text": "Agradecido o desagradecido, le hemos dirigido por el Camino."}, {"id": 254828, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5595, "text": "Para los infieles hemos preparado cadenas, argollas y fuego de gehena."}, {"id": 254829, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5596, "text": "Los justos beber\u00e1n de copas de una mezcla alcanforada."}, {"id": 254830, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5597, "text": "de una fuente de la que beber\u00e1n los siervos de Al\u00e1 y que har\u00e1n manar en abundancia."}, {"id": 254831, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5598, "text": "Fueron fieles a sus promesas y temieron un d\u00eda cuyo mal ser\u00e1 de alcance universal."}, {"id": 254832, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5599, "text": "Por mucho amor que tuvieran al alimento, se lo daban al pobre, al hu\u00e9rfano y al cautivo:"}, {"id": 254833, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5600, "text": "\u00abOs damos de comer s\u00f3lo por agradar a Al\u00e1. No queremos de vosotros retribuci\u00f3n ni gratitud."}, {"id": 254834, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5601, "text": "Tememos, de nuestro Se\u00f1or, un d\u00eda terrible, calamitoso\u00bb."}, {"id": 254835, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5602, "text": "Al\u00e1 les preservar\u00e1 del mal de ese d\u00eda y les llenar\u00e1 de esplendor y alegr\u00eda."}, {"id": 254836, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5603, "text": "Les retribuira, por haber tenido paciencia, con un Jard\u00edn y con vestiduras de seda."}, {"id": 254837, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5604, "text": "Reclinados all\u00ed en sof\u00e1s, estar\u00e1n resguardados all\u00ed del calor y del fr\u00edo excesivo."}, {"id": 254838, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5605, "text": "Cerca de ellos, les cubrir\u00e1n sus sombras; sus frutos podr\u00e1n ser cogidos muy f\u00e1cilmente."}, {"id": 254839, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5606, "text": "Se har\u00e1n circular entre ellos vasijasa de plata y copas de cristal,"}, {"id": 254840, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5607, "text": "de un cristal de plata, de medidas determinadas."}, {"id": 254841, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5608, "text": "All\u00ed se les servir\u00e1 una copa que contendr\u00e1 una mezcla de jengibre,"}, {"id": 254842, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5609, "text": "tomada de una fuente de all\u00ed, que se llama Salsabil."}, {"id": 254843, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5610, "text": "Y circular\u00e1n entre ellos criados j\u00f3venes de eterna juventud. Vi\u00e9ndoles, se les creer\u00eda perlas desparramadas."}, {"id": 254844, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5611, "text": "Cuando se mira all\u00e1, no se ve sino delicia y suntuosidad."}, {"id": 254845, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5612, "text": "Vestir\u00e1n de verde sat\u00e9n y de brocado y llevar\u00e1n brazaletes de plata. Su Se\u00f1or les servir\u00e1 una bebida pura."}, {"id": 254846, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5613, "text": "\u00abEsto se os ha dado como retribuci\u00f3n. Vuestro esfuerzo ha sido agradecido\u00bb."}, {"id": 254847, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5614, "text": "Somos Nosotros Quienes te llemos revelado el Cor\u00e1n de hecho."}, {"id": 254848, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5615, "text": "Espera, pues, paciente, la decisi\u00f3n f de tu Se\u00f1or y no obedezcas a quien de ellos sea pecador o desagradecido."}, {"id": 254849, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5616, "text": "\u00a1Y menciona el nombre de tu Se\u00f1or ma\u00f1ana y tarde!"}, {"id": 254850, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5617, "text": "\u00a1Por la noche, prost\u00e9rnate ante \u00c9l! \u00a1Glorif\u00edcale largamente por la noche!"}, {"id": 254851, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5618, "text": "\u00c9stos aman la vida fugaz y descuidan un d\u00eda grave."}, {"id": 254852, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5619, "text": "Nosotros los hemos creado y hemos fortalecido su constituci\u00f3n. Si quisi\u00e9ramos, podr\u00edamos sustituirlos por otros semejantes."}, {"id": 254853, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5620, "text": "Esto es un Recuerdo. El que quiera \u00a1que emprenda camino hacia su Se\u00f1or!"}, {"id": 254854, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5621, "text": "Pero vosotros no lo querr\u00e9is, a menos que Al\u00e1 quiera. Al\u00e1 es omnisciente, sabio."}, {"id": 254855, "identifier": "es.cortes", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5622, "text": "lntroduce en Su misericordia a quien \u00c9l quiere. Pero a los imp\u00edos les ha preparado un castigo doloroso."}]