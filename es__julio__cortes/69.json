[{"id": 254557, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5324, "text": "La Inevitable."}, {"id": 254558, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5325, "text": "\u00bfQu\u00e9 es la Inevitable?"}, {"id": 254559, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5326, "text": "Y \u00bfc\u00f3mo sabr\u00e1s qu\u00e9 es la Inevitable?"}, {"id": 254560, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5327, "text": "Los tamudeos y los aditas desmintieron la Calamidad."}, {"id": 254561, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5328, "text": "Los tamudeos fueron aniquilados por el Rayo."}, {"id": 254562, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5329, "text": "Los aditas fueron aniquilados por un viento glacial, impetuoso,"}, {"id": 254563, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5330, "text": "que desencaden\u00f3 contra ellos para devastarlo todo durante siete noches y ocho d\u00edas. Se ve\u00eda a la gente yacer en ellas como troncos de palmeras derribadas."}, {"id": 254564, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5331, "text": "\u00bfPuedes t\u00fa ver alg\u00fan rastro de ellos?"}, {"id": 254565, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5332, "text": "Pecaron Fara\u00f3n, los que fueron antes de \u00e9l y las vueltas de arriba abajo."}, {"id": 254566, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5333, "text": "Desobedecieron al Enviado de su Se\u00f1or y \u00c9ste les sorprendi\u00f3 de un modo irresistible."}, {"id": 254567, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5334, "text": "Cuando las aguas lo inundaron todo, os llevamos en la embarcaci\u00f3n,"}, {"id": 254568, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5335, "text": "para hacer de ella un recuerdo para vosotros, para que el o\u00eddo atento lo retuviera."}, {"id": 254569, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5336, "text": "Cuando se toque la trompeta una sola vez,"}, {"id": 254570, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5337, "text": "y la tierra y las monta\u00f1as sean alzadas y pulverizadas de un solo golpe,"}, {"id": 254571, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5338, "text": "ese d\u00eda suceder\u00e1 el Acontecimiento."}, {"id": 254572, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5339, "text": "El cielo se hender\u00e1, pues ese d\u00eda estar\u00e1 quebradizo."}, {"id": 254573, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5340, "text": "Los \u00e1ngeles estar\u00e1n en sus confines y ese d\u00eda ocho de ellos llevar\u00e1n, encima, el Trono de tu Se\u00f1or."}, {"id": 254574, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5341, "text": "Ese d\u00eda se os expondr\u00e1: nada vuestro quedar\u00e1 oculto."}, {"id": 254575, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5342, "text": "Aqu\u00e9l que reciba su Escritura en la diestra, dir\u00e1: \u00ab\u00a1Tomad! \u00a1Leed mi Escritura!"}, {"id": 254576, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5343, "text": "\u00a1Ya contaba con ser juzgado!\u00bb"}, {"id": 254577, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5344, "text": "Gozar\u00e1 de una vida agradable"}, {"id": 254578, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5345, "text": "en un Jard\u00edn elevado,"}, {"id": 254579, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5346, "text": "cuyos frutos estar\u00e1n al alcance de la mano."}, {"id": 254580, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5347, "text": "\u00ab\u00a1Comed y bebed en paz el fruto de lo que hicisteis en d\u00edas pasados!\u00bb"}, {"id": 254581, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5348, "text": "Aqu\u00e9l que reciba su Escritura en la siniestra, dir\u00e1: \u00ab\u00a1Ojal\u00e1 no se me hubiera entregado la Escritura"}, {"id": 254582, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5349, "text": "y no hubiera conocido el resultado de mi juicio!"}, {"id": 254583, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5350, "text": "\u00a1 Ojal\u00e1 hubiera sido definitiva!"}, {"id": 254584, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5351, "text": "De nada me ha servido mi hacienda."}, {"id": 254585, "identifier": "es.cortes", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5352, "text": "Mi poder me ha abandonado\u00bb"}, {"id": 254586, "identifier": "es.cortes", "surah_number": 69, "surah_a