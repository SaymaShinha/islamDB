[{"id": 255290, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6059, "text": "\u00a1Por la noche cuando extiende su velo!"}, {"id": 255291, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6060, "text": "\u00a1Por el d\u00eda cuando resplandece!"}, {"id": 255292, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6061, "text": "\u00a1Por Quien ha creado al var\u00f3n y a la hembra!"}, {"id": 255293, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Vuestro esfuerzo, en verdad, da resultados diversos."}, {"id": 255294, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6063, "text": "A quien da, teme a Dios,"}, {"id": 255295, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6064, "text": "y cree en lo m\u00e1s bello,"}, {"id": 255296, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6065, "text": "le facilitaremos el acceso a la mayor felicidad."}, {"id": 255297, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6066, "text": "En cambio, a quien es avaro, cree bastarse a s\u00ed mismo"}, {"id": 255298, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6067, "text": "y desmiente lo m\u00e1s bello,"}, {"id": 255299, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6068, "text": "le facilitaremos el acceso a la mayor adversidad,"}, {"id": 255300, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6069, "text": "y de nada le servir\u00e1 su hacienda cuando sea precipitado."}, {"id": 255301, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6070, "text": "S\u00ed, es de incumbencia Nuestra la Direcci\u00f3n."}, {"id": 255302, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6071, "text": "S\u00ed, es cosa Nuestra la otra vida y \u00e9sta."}, {"id": 255303, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6072, "text": "Os he prevenido contra un fuego llameante,"}, {"id": 255304, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6073, "text": "en el cual s\u00f3lo arder\u00e1 el infame,"}, {"id": 255305, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6074, "text": "que desmiente y se desv\u00eda,"}, {"id": 255306, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6075, "text": "el cual evitar\u00e1 quien de veras teme a Dios,"}, {"id": 255307, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6076, "text": "que da su hacienda para purificarse,"}, {"id": 255308, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6077, "text": "que, cuando hace un favor, no lo hace con \u00e1nimo de ser retribuido,"}, {"id": 255309, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6078, "text": "sino tan s\u00f3lo por deseo de agradar a su alt\u00edsimo Se\u00f1or."}, {"id": 255310, "identifier": "es.cortes", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 6079, "text": "\u00a1S\u00ed, \u00e9se quedar\u00e1 satisfecho!"}]