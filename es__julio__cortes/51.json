[{"id": 253933, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4676, "text": "\u00a1Por los que aventan!"}, {"id": 253934, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4677, "text": "\u00a1Por las que llevan una carga!"}, {"id": 253935, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4678, "text": "\u00a1Por las que se deslizan ligeras!"}, {"id": 253936, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4679, "text": "\u00a1Por los que distribuyen una orden!"}, {"id": 253937, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4680, "text": "\u00a1Ciertamente, aquello con que se os amenaza es verdad!"}, {"id": 253938, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4681, "text": "Y el Juicio, s\u00ed, tendr\u00e1 lugar."}, {"id": 253939, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4682, "text": "\u00a1Por el cielo surcado de \u00f3rbitas!"}, {"id": 253940, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Est\u00e1is en desacuerdo."}, {"id": 253941, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Algunos son desviados de \u00e9l."}, {"id": 253942, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4685, "text": "\u00a1Malditos sean los que siempre est\u00e1n conjeturando,"}, {"id": 253943, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4686, "text": "que est\u00e1n en un abismo, despreocupados,"}, {"id": 253944, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4687, "text": "que preguntan: \u00ab\u00bfCu\u00e1ndo llegar\u00e1 el d\u00eda del Juicio?\u00bb!"}, {"id": 253945, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4688, "text": "El d\u00eda que se les pruebe al fuego:"}, {"id": 253946, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4689, "text": "\u00ab\u00a1Gustad vuestra prueba! Esto es lo que estabais impacientes por conocer\u00bb."}, {"id": 253947, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Los que temen a Al\u00e1 estar\u00e1n entre jardines y fuentes,"}, {"id": 253948, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4691, "text": "tomando lo que su Se\u00f1or les d\u00e9. Hicieron el bien en el pasado;"}, {"id": 253949, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4692, "text": "de noche dorm\u00edan poco;"}, {"id": 253950, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4693, "text": "al rayar el alba, ped\u00edan perd\u00f3n,"}, {"id": 253951, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4694, "text": "y parte de sus bienes correspond\u00eda de derecho al mendigo y al indigente."}, {"id": 253952, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4695, "text": "En la tierra hay signos para los convencidos,"}, {"id": 253953, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4696, "text": "y en vosotros mismos tambi\u00e9n. \u00bfEs que no veis?"}, {"id": 253954, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4697, "text": "Y en el cielo ten\u00e9is vuestro sustento y lo que se os ha prometido."}, {"id": 253955, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4698, "text": "\u00a1Por el Se\u00f1or del cielo y de la tierra, que es tanta verdad como que habl\u00e1is!"}, {"id": 253956, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4699, "text": "\u00bfTe has enterado de la historia de los hu\u00e9spedes honrados de Abraham?"}, {"id": 253957, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Cuando entraron en su casa. Dijeron: \u00ab\u00a1Paz!\u00bb. Dijo: \u00ab\u00a1Paz! Sois gente desconocida\u00bb."}, {"id": 253958, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4701, "text": "Se fue discretamente a los suyos y trajo un ternero cebado,"}, {"id": 253959, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4702, "text": "que les ofreci\u00f3. Dijo: \u00ab\u00bfEs que no com\u00e9is?\u00bb"}, {"id": 253960, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4703, "text": "Y sinti\u00f3 temor de ellos. Dijeron: \u00ab\u00a1No temas!\u00bb Y le dieron la buena nueva de un muchacho lleno de ciencia."}, {"id": 253961, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Su mujer, entonces, se puso a gritar. Golpe\u00f3se el rostro y dijo: \u00abPero \u00a1si soy una vieja est\u00e9ril!\u00bb"}, {"id": 253962, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Dijeron: \u00abAs\u00ed ha dicho tu Se\u00f1or. Es \u00c9l el Sabio, el Omnisciente\u00bb."}, {"id": 253963, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4706, "text": "Dijo: \u00ab\u00bfQu\u00e9 es lo que os trae, \u00a1enviados!?\u00bb"}, {"id": 253964, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Dijeron: \u00abSe nos ha enviado a un pueblo pecador"}, {"id": 253965, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4708, "text": "para enviar contra ellos piedras de barro cocido,"}, {"id": 253966, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4709, "text": "marcadas junto a tu Se\u00f1or para los inmoderados\u00bb."}, {"id": 253967, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Y sacamos a los creyentes que en ella hab\u00eda,"}, {"id": 253968, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4711, "text": "pero s\u00f3lo encontramos en ella una casa de gente sometida a Al\u00e1."}, {"id": 253969, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Y dejamos en ella un signo para los que temen el castigo doloroso."}, {"id": 253970, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4713, "text": "Y en Mois\u00e9s. Cuando le enviamos a Fara\u00f3n con una autoridad manifiesta."}, {"id": 253971, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Pero, seguro de su poder, se volvi\u00f3 y dijo: \u00ab\u00a1Es un mago o un poseso!\u00bb"}, {"id": 253972, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Entonces les sorprendimos, a \u00e9l y a sus tropas, y los arrojamos al mar. Hab\u00eda incurrido en censura."}, {"id": 253973, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4716, "text": "Y en los aditas. Cuando enviamos contra ellos el viento desvastador,"}, {"id": 253974, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4717, "text": "que pulverizaba todo cuanto encontraba a su paso."}, {"id": 253975, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4718, "text": "Y en los tamudeos. Cuando se les dijo: \u00ab\u00a1Gozad a\u00fan por alg\u00fan tiempo!\u00bb"}, {"id": 253976, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Pero infringieron la orden de su Se\u00f1or y les sorprendi\u00f3 el Rayo, vi\u00e9ndolo venir."}, {"id": 253977, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4720, "text": "No pudieron tenerse en pie, ni defenderse."}, {"id": 253978, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4721, "text": "Y al pueblo de No\u00e9. Fue un pueblo perverso."}, {"id": 253979, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4722, "text": "Y el cielo, lo construimos con fuerza. Y, ciertamente, asignamos un vasto espacio."}, {"id": 253980, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Y la tierra, la extendimos. \u00a1Qu\u00e9 bien que la preparamos!"}, {"id": 253981, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Todo lo creamos por parejas. Quiz\u00e1s, as\u00ed, os dej\u00e9is amonestar."}, {"id": 253982, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4725, "text": "\u00ab\u00a1Refugiaos, pues, en Al\u00e1! Soy para vosotros, de Su parte, un monitor que habla claro."}, {"id": 253983, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4726, "text": "\u00a1No pong\u00e1is a otro dios junto con Al\u00e1! Soy para vosotros, de Su parte, un monitor que habla claro\u00bb."}, {"id": 253984, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4727, "text": "Asimismo, no vino a los que fueron antes ning\u00fan enviado que no dijeran: \u00ab\u00a1Es un mago o un poseso!\u00bb"}, {"id": 253985, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4728, "text": "\u00bfEs que se han legado eso unos a otros? \u00a1No! \u00a1Son gente rebelde!"}, {"id": 253986, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4729, "text": "\u00a1Ap\u00e1rtate de ellos y, as\u00ed, no incurrir\u00e1s en censura!"}, {"id": 253987, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4730, "text": "\u00a1Y amonesta, que la amonestaci\u00f3n aprovecha a los creyentes!"}, {"id": 253988, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4731, "text": "No he creado a los genios y a los hombres sino para que Me sirvan."}, {"id": 253989, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4732, "text": "No quiero de ellos ning\u00fan sustento, no quiero que Me alimenten."}, {"id": 253990, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Al\u00e1 es el Proveedor de todo, el Fuerte, el Firme."}, {"id": 253991, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Los imp\u00edos correr\u00e1n la misma suerte que corrieron sus semejantes. \u00a1Que no Me den, pues, prisa!"}, {"id": 253992, "identifier": "es.cortes", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 4735, "text": "\u00a1Ay de los que no creen, por el d\u00eda con que se les amenaza!"}]