[{"id": 255061, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5830, "text": "Cuando el cielo se hienda,"}, {"id": 255062, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5831, "text": "cuando las estrellas se dispersen,"}, {"id": 255063, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5832, "text": "cuando los mares sean desbordados,"}, {"id": 255064, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5833, "text": "cuando las sepulturas sean vueltas al rev\u00e9s,"}, {"id": 255065, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5834, "text": "sabr\u00e1 cada cual lo que hizo y lo que dej\u00f3 de hacer."}, {"id": 255066, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5835, "text": "\u00a1Hombre! \u00bfQu\u00e9 es lo que te ha enga\u00f1ado acerca de tu noble Se\u00f1or,"}, {"id": 255067, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5836, "text": "Que te ha creado, dado forma y disposici\u00f3n armoniosas,"}, {"id": 255068, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5837, "text": "Que te ha formado del modo que ha querido?"}, {"id": 255069, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5838, "text": "\u00a1Pero no! Desment\u00eds el Juicio,"}, {"id": 255070, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5839, "text": "pero hay quienes os guardan:"}, {"id": 255071, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5840, "text": "nobles, escribas,"}, {"id": 255072, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5841, "text": "que saben lo que hac\u00e9is."}, {"id": 255073, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5842, "text": "S\u00ed, los justos estar\u00e1n en delicia,"}, {"id": 255074, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5843, "text": "mientras que los pecadores estar\u00e1n en fuego de gehena."}, {"id": 255075, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5844, "text": "En \u00e9l arder\u00e1n el d\u00eda del Juicio"}, {"id": 255076, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5845, "text": "y no se ausentar\u00e1n de \u00e9l."}, {"id": 255077, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5846, "text": "Y \u00bfc\u00f3mo sabr\u00e1s qu\u00e9 es el d\u00eda del Juicio?"}, {"id": 255078, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5847, "text": "S\u00ed, \u00bfc\u00f3mo sabr\u00e1s qu\u00e9 es el d\u00eda del Juicio?"}, {"id": 255079, "identifier": "es.cortes", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Cortes", "transelator_en_name": "Julio Cortes", "format": "text", "type": "translation", "ayah_number": 5848, "text": "El d\u00eda que nadie pueda hacer nada en favor de nadie. Y ser\u00e1 Al\u00e1 Quien, ese d\u00eda, decida."}]