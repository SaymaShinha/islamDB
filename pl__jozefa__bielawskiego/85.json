[{"id": 423535, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5910, "text": "Na niebo ozdobione konstelacjami!"}, {"id": 423536, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5911, "text": "Na Dzie\u0144 przyobiecany!"}, {"id": 423537, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5912, "text": "Na \u015bwiadka i na to, czemu on daje \u015bwiadectwo!"}, {"id": 423538, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5913, "text": "Zabici zostali Ludzie Rowu"}, {"id": 423539, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5914, "text": "- a ogie\u0144 ci\u0105gle podsycany"}, {"id": 423540, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5915, "text": "Gdy oni siedzieli wok\u00f3\u0142 niego"}, {"id": 423541, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5916, "text": "- \u015bwiadkowie tego, co czyniono wiernym."}, {"id": 423542, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5917, "text": "Oni zem\u015bcili si\u0119 na nich tylko dlatego, \u017ce uwierzyli w Boga, Pot\u0119\u017cnego, Godnego Chwa\u0142y,"}, {"id": 423544, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Do kt\u00f3rego nale\u017cy kr\u00f3lestwo niebios i ziemi. I B\u00f3g jest \u015bwiadkiem ka\u017cdej rzeczy!"}, {"id": 423545, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Zaprawd\u0119, dla tych, kt\u00f3rzy poddawali pr\u00f3bie wierz\u0105cych i wierz\u0105ce, a potem si\u0119 nie nawr\u00f3cili - dla nich b\u0119dzie kara Gehenny; i dla nich b\u0119dzie kara ognia pal\u0105cego."}, {"id": 423546, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5920, "text": "Zaprawd\u0119, dla tych, kt\u00f3rzy uwierzyli i kt\u00f3rzy pe\u0142nili dobre dzie\u0142a - dla nich b\u0119d\u0105 Ogrody, gdzie w dole p\u0142yn\u0105 strumyki. To jest wielkie osi\u0105gni\u0119cie!"}, {"id": 423547, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Zaprawd\u0119, surowo\u015b\u0107 twego Pana jest straszna!"}, {"id": 423548, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Oto On stwarza po raz pierwszy i powtarza stworzenie!"}, {"id": 423549, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5923, "text": "On - Przebaczaj\u0105cy, Mi\u0142o\u015bciwy!"}, {"id": 423550, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5924, "text": "On - W\u0142adca Tronu Godnego Chwa\u0142y!"}, {"id": 423551, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5925, "text": "On czyni to, co chce!"}, {"id": 423552, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Czy dosz\u0142o do ciebie opowiadanie o wojskach,"}, {"id": 423553, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5927, "text": "O Faraonie i o ludzie Samud?"}, {"id": 423554, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Lecz ci, kt\u00f3rzy nie wierz\u0105, zaprzeczaj\u0105 prawdzie,"}, {"id": 423555, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5929, "text": "A B\u00f3g otacza ich niewidzialny."}, {"id": 423556, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5930, "text": "To za\u015b jest Koran godny chwa\u0142y"}, {"id": 423557, "identifier": "pl.bielawskiego", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5931, "text": "- na tablicy strze\u017conej!"}]