[{"id": 422448, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4847, "text": "Przybli\u017cy\u0142a si\u0119 Godzina i rozerwa\u0142 si\u0119 ksi\u0119\u017cyc."}, {"id": 422479, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Lecz ilekro\u0107 ujrz\u0105 oni znak, odwracaj\u0105 si\u0119 i m\u00f3wi\u0105: \"To ustawiczne czary!\""}, {"id": 422480, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4849, "text": "Zadali oni k\u0142am i poszli za swoimi nami\u0119tno\u015bciami. Lecz ka\u017cda rzecz jest ustanowiona."}, {"id": 422481, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4850, "text": "Dotar\u0142y ju\u017c do nich, z pewno\u015bci\u0105, takie opowie\u015bci, w kt\u00f3rych jest odstraszenie,"}, {"id": 422482, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4851, "text": "M\u0105dro\u015b\u0107 dalekosi\u0119\u017cna; jednak na nic si\u0119 zdaj\u0105 ostrze\u017cenia."}, {"id": 422483, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Przeto odwr\u00f3\u0107 si\u0119 od nich w Dniu, kiedy wezwie wzywaj\u0105cy do rzeczy strasznej."}, {"id": 422484, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4853, "text": "Oni wyjd\u0105 z grob\u00f3w z opuszczonymi spojrzeniami. B\u0119d\u0105 podobni do szara\u0144czy rozsypanej,"}, {"id": 422485, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4854, "text": "\u015bpiesz\u0105cy z wyci\u0105gni\u0119tymi szyjami ku wzywaj\u0105cemu. Niewierni b\u0119d\u0105 m\u00f3wili: \"To jest trudny dzie\u0144!\""}, {"id": 422486, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4855, "text": "Niegdy\u015b lud Noego zada\u0142 temu k\u0142am i za k\u0142amc\u0119 uznali oni Naszego s\u0142ug\u0119. Powiedzieli: \"Op\u0119tany!'~ - i on zosta\u0142 odepchni\u0119ty."}, {"id": 422487, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4856, "text": "I zawo\u0142a\u0142 on do swego Pana: \"Oto jestem zwyci\u0119\u017cony! Dopom\u00f3\u017c mi wi\u0119c!\""}, {"id": 422488, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4857, "text": "Otworzyli\u015bmy wi\u0119c bramy nieba z wod\u0105 lej\u0105c\u0105 si\u0119 jak potok."}, {"id": 422489, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4858, "text": "I uczynili\u015bmy, i\u017c ziemia trysn\u0119\u0142a \u017ar\u00f3d\u0142ami, tak i\u017c spotka\u0142y si\u0119 wody wed\u0142ug powzi\u0119tego rozkazu."}, {"id": 422490, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4859, "text": "I ponie\u015bli\u015bmy go na tym, co zosta\u0142o zrobione z desek i w\u0142\u00f3kien palmy."}, {"id": 422491, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4860, "text": "I p\u0142yn\u0119\u0142a ona przed Naszymi oczami, jako zap\u0142ata dla tego, kt\u00f3ry zosta\u0142 odrzucony."}, {"id": 422492, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4861, "text": "I pozostawili\u015bmy j\u0105 jako znak. Lecz czy znajdzie si\u0119 kto\u015b, kto b\u0119dzie pami\u0119ta\u0142?"}, {"id": 422493, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4862, "text": "Jaka\u017c by\u0142a Moja kara i Moje ostrze\u017cenia!"}, {"id": 422494, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4863, "text": "I uczynili\u015bmy \u0142atwym Koran dla przypomnienia, lecz czy znajdzie si\u0119 kto\u015b, kto b\u0119dzie pami\u0119ta\u0142?"}, {"id": 422495, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4864, "text": "Za k\u0142amstwo uzna\u0142 to lud Ad. I jaka by\u0142a wtedy Moja kara i Moje ostrze\u017cenia?!"}, {"id": 422496, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4865, "text": "Oto wys\u0142ali\u015bmy przeciw nim rycz\u0105cy wicher w tym dniu nieszcz\u0119\u015bcia nieustannego."}, {"id": 422497, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4866, "text": "Wyrywa\u0142 on ludzi, jak gdyby byli pniami palm wyrwanych."}, {"id": 422498, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4867, "text": "Jaka\u017c by\u0142a Moja kara i Moje ostrze\u017cenia!"}, {"id": 422499, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4868, "text": "I uczynili\u015bmy \u0142atwym Koran dla przypomnienia; lecz czy si\u0119 znajdzie kto\u015b, kto b\u0119dzie pami\u0119ta\u0142?"}, {"id": 422500, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4869, "text": "Za k\u0142amstwo uzna\u0142 lud Samud ostrze\u017cenia."}, {"id": 422501, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4870, "text": "Oni powiedzieli: \"Czy mamy post\u0119powa\u0107 za \u015bmiertelnikiem, jednym z nas samych? W\u00f3wczas popadliby\u015bmy w zab\u0142\u0105dzenie i szale\u0144stwo!"}, {"id": 422502, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4871, "text": "Czy napomnienie zosta\u0142o dane jemu tylko spo\u015br\u00f3d nas? Przeciwnie! On jest bezczelnym k\u0142amc\u0105!\""}, {"id": 422503, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4872, "text": "Oni dowiedz\u0105 si\u0119 jutro, kto jest bezczelnym k\u0142amc\u0105!"}, {"id": 422504, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4873, "text": "Oto My posy\u0142amy im wielb\u0142\u0105dzic\u0119 jako do\u015bwiadczenie dla nich. Obserwuj ich pilnie i b\u0105d\u017a cierpliwy!"}, {"id": 422505, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4874, "text": "I obwie\u015b\u0107 im, \u017ce woda ma by\u0107 podzielona mi\u0119dzy nich: a wszyscy pi\u0107 b\u0119d\u0105 po kolei."}, {"id": 422506, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4875, "text": "Wtedy wezwali swego towarzysza, a on zakrz\u0105tn\u0105\u0142 si\u0119 i przeci\u0105\u0142 jej \u015bci\u0119gna."}, {"id": 422507, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4876, "text": "Jaka\u017c by\u0142a Moja kara i Moje ostrze\u017cenia!"}, {"id": 422508, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4877, "text": "Oto wys\u0142ali\u015bmy przeciw nim jeden krzyk, a oni stali si\u0119 jak zesch\u0142e witki buduj\u0105cego ogrodzenie."}, {"id": 422509, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4878, "text": "My uczynili\u015bmy \u0142atwym Koran dla przypomnienia. Lecz czy znajdzie si\u0119 kto\u015b, kto b\u0119dzie pami\u0119ta\u0142?"}, {"id": 422510, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Za k\u0142amstwo uzna\u0142 lud Lota ostrze\u017cenia."}, {"id": 422511, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4880, "text": "Oto wys\u0142ali\u015bmy przeciw nim huragan nios\u0105cy kamienie; jedynie rodzin\u0119 Lota uratowali\u015bmy o \u015bwicie,"}, {"id": 422512, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4881, "text": "Jako dobrodziejstwo od Nas. W ten spos\u00f3b wynagradzamy tych, kt\u00f3rzy s\u0105 wdzi\u0119czni."}, {"id": 422513, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4882, "text": "On ostrzega\u0142 ich przed Nasz\u0105 srogo\u015bci\u0105, lecz oni pow\u0105tpiewali w te ostrze\u017cenia."}, {"id": 422514, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4883, "text": "Starali si\u0119 nawet odsun\u0105\u0107 od niego jego go\u015bci, lecz My o\u015blepili\u015bmy ich. Zakosztujcie wi\u0119c Mojej kary i Moich ostrze\u017ce\u0144!"}, {"id": 422515, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4884, "text": "I wcze\u015bnie rankiem spad\u0142a na nich kara ustanowiona."}, {"id": 422516, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4885, "text": "Zakosztujcie wi\u0119c Mojej kary i Moich ostrze\u017ce\u0144!"}, {"id": 422517, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4886, "text": "My uczynili\u015bmy \u0142atwym Koran dla przypomnienia. Lecz czy znajdzie si\u0119 kto\u015b, kto b\u0119dzie pami\u0119ta\u0142?"}, {"id": 422518, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4887, "text": "I przysz\u0142y te\u017c ostrze\u017cenia do rodu Faraona."}, {"id": 422519, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4888, "text": "Oni za k\u0142amstwo uznali wszystkie Nasze znaki. Przeto pochwycili\u015bmy ich, tak jak to czyni pot\u0119\u017cny, wszechmocny."}, {"id": 422520, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4889, "text": "Czy wasi niewierni s\u0105 lepsi ni\u017c tamci? Albo czy znajdziecie jakie\u015b usprawiedliwienie w pismach?"}, {"id": 422521, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4890, "text": "A mo\u017ce oni powiedz\u0105: \"My wszyscy zebrani b\u0119dziemy zwyci\u0119zcami!\""}, {"id": 422522, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4891, "text": "Niebawem to zbiorowisko b\u0119dzie rozbite i oni odwr\u00f3c\u0105 si\u0119 plecami."}, {"id": 422523, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4892, "text": "Tak, ta Godzina jest ich wyznaczonym czasem, a b\u0119dzie ona straszna i gorzka."}, {"id": 422524, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4893, "text": "Zaprawd\u0119, grzesznicy znajduj\u0105 si\u0119 w zab\u0142\u0105dzeniu i ob\u0142\u0105kaniu."}, {"id": 422525, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4894, "text": "W tym Dniu, kiedy b\u0119d\u0105 wleczeni twarzami po ziemi do ognia: \"Zakosztujcie dotkni\u0119cia ognia piekielnego!\""}, {"id": 422526, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4895, "text": "Zaprawd\u0119, stworzyli\u015bmy ka\u017cd\u0105 rzecz wed\u0142ug pewnej miary."}, {"id": 422527, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4896, "text": "Nasz rozkaz jest tylko jeden, jak mgnienie oka."}, {"id": 422528, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4897, "text": "Zniszczyli\u015bmy podobnych wam, lecz czy znajdzie si\u0119 kto\u015b, kto b\u0119dzie pami\u0119ta\u0142?"}, {"id": 422529, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4898, "text": "Ka\u017cda rzecz, jak\u0105 oni uczynili, znajduje si\u0119 w pismach."}, {"id": 422530, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4899, "text": "Wszystko - ma\u0142e i wielkie - jest zapisane."}, {"id": 422531, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4900, "text": "Zaprawd\u0119, bogobojni b\u0119d\u0105 przebywa\u0107 w\u015br\u00f3d ogrod\u00f3w i strumyk\u00f3w,"}, {"id": 422532, "identifier": "pl.bielawskiego", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 4901, "text": "W siedzibie szczerej prawdy, w obecno\u015bci Kr\u00f3la Wszechpot\u0119\u017cnego!"}]