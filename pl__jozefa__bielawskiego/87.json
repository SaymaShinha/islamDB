[{"id": 423575, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5949, "text": "Wys\u0142awiaj imi\u0119 twego Pana, Najwy\u017cszego,"}, {"id": 423576, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5950, "text": "Kt\u00f3ry stworzy\u0142 i ukszta\u0142towa\u0142 harmonijnie,"}, {"id": 423577, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5951, "text": "Kt\u00f3ry wyznaczy\u0142 los i poprowadzi\u0142;"}, {"id": 423578, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5952, "text": "To On ka\u017ce zieleni\u0107 si\u0119 pastwisku,"}, {"id": 423579, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5953, "text": "A potem czyni je ciemniej\u0105c\u0105 pasz\u0105."}, {"id": 423580, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5954, "text": "My nauczymy ci\u0119 recytowa\u0107 i ty zapomnisz"}, {"id": 423581, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5955, "text": "Tylko to, co B\u00f3g zechce. Przecie\u017c B\u00f3g zna to, co jawne i co skryte!"}, {"id": 423582, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5956, "text": "I My uczynimy \u0142atw\u0105 dla ciebie drog\u0119 ku temu, co najpomy\u015blniejsze."}, {"id": 423583, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5957, "text": "Napominaj wi\u0119c, je\u015bli napomnienie jest po\u017cyteczne!"}, {"id": 423584, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Opami\u0119ta si\u0119 ten, kto si\u0119 obawia;"}, {"id": 423585, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5959, "text": "A odsunie si\u0119 od napomnienia najn\u0119dzniejszy,"}, {"id": 423586, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5960, "text": "Kt\u00f3ry si\u0119 b\u0119dzie pali\u0142 w ogniu najwi\u0119kszym,"}, {"id": 423587, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5961, "text": "Gdzie ani nie umrze, ani te\u017c nie b\u0119dzie \u017cy\u0142."}, {"id": 423588, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5962, "text": "Za\u015b szcz\u0119\u015bliwy b\u0119dzie ten, kto si\u0119 oczy\u015bci"}, {"id": 423589, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5963, "text": "I b\u0119dzie wspomina\u0142 imi\u0119 swego Pana, i modli\u0142 si\u0119."}, {"id": 423590, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5964, "text": "Lecz wy wolicie \u017cycie tego \u015bwiata,"}, {"id": 423591, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5965, "text": "Tymczasem \u017cycie ostateczne jest lepsze i bardziej trwa\u0142e."}, {"id": 423592, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5966, "text": "Zaprawd\u0119, to jest w dawnych ksi\u0119gach,"}, {"id": 423593, "identifier": "pl.bielawskiego", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5967, "text": "Ksi\u0119gach Abrahama i Moj\u017cesza!"}]