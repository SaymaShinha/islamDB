[{"id": 423558, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5932, "text": "Na niebo i na gwiazd\u0119 nocn\u0105!"}, {"id": 423559, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5933, "text": "A co ciebie pouczy, co to jest gwiazda nocna?"}, {"id": 423560, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5934, "text": "To jest gwiazda przenikaj\u0105ca."}, {"id": 423561, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5935, "text": "Ka\u017cda dusza ma nad sob\u0105 str\u00f3\u017ca!"}, {"id": 423562, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5936, "text": "Niech wi\u0119c rozwa\u017cy cz\u0142owiek, z czego on zosta\u0142 stworzony!"}, {"id": 423563, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5937, "text": "Zosta\u0142 stworzony z cieczy wytryskaj\u0105cej,"}, {"id": 423564, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5938, "text": "Kt\u00f3ra wychodzi spomi\u0119dzy l\u0119d\u017awi i \u017ceber."}, {"id": 423565, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5939, "text": "Zaprawd\u0119, On jest w\u0142adny sprowadzi\u0107 go"}, {"id": 423566, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5940, "text": "W Dniu, kiedy b\u0119d\u0105 badane skryte my\u015bli."}, {"id": 423567, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5941, "text": "Nie b\u0119dzie on mia\u0142 w\u00f3wczas ani si\u0142y, ani pomocnika!"}, {"id": 423568, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5942, "text": "Na niebo, kt\u00f3re w\u0142ada powrotem!"}, {"id": 423569, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5943, "text": "Na ziemi\u0119 posiadaj\u0105c\u0105 rozpadliny!"}, {"id": 423570, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5944, "text": "Zaprawd\u0119, to jest s\u0142owo rozstrzygaj\u0105ce,"}, {"id": 423571, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5945, "text": "Nie \u017cadna zabawa!"}, {"id": 423572, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Zaprawd\u0119, oni zamy\u015blaj\u0105 podst\u0119p!"}, {"id": 423573, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5947, "text": "I Ja zamy\u015blam podst\u0119p!"}, {"id": 423574, "identifier": "pl.bielawskiego", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 5948, "text": "Daj wi\u0119c nieco wytchnienia niewiernym; daj im jeszcze chwil\u0119 zw\u0142oki!"}]