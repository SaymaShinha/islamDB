[{"id": 423812, "identifier": "pl.bielawskiego", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6189, "text": "Czy nie widzia\u0142e\u015b, jak post\u0105pi\u0142 tw\u00f3j Pan z towarzyszami s\u0142onia?"}, {"id": 423813, "identifier": "pl.bielawskiego", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6190, "text": "Czy nie zniweczy\u0142 ich podst\u0119pu?"}, {"id": 423814, "identifier": "pl.bielawskiego", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6191, "text": "Czy nie rzuci\u0142 przeciwko nim ptak\u00f3w lec\u0105cych stadami,"}, {"id": 423815, "identifier": "pl.bielawskiego", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6192, "text": "Kt\u00f3re obrzuci\u0142y ich kamieniami z palonej gliny?"}, {"id": 423816, "identifier": "pl.bielawskiego", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6193, "text": "I uczyni\u0142 ich jak objedzone pole zbo\u017ca."}]