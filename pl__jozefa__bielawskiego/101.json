[{"id": 421162, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6158, "text": "Wydarzenie przera\u017caj\u0105ce!"}, {"id": 423782, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6159, "text": "Co to jest wydarzenie przera\u017caj\u0105ce?"}, {"id": 423783, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6160, "text": "I co ciebie pouczy, co to jest wydarzenie przera\u017caj\u0105ce?"}, {"id": 423784, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6161, "text": "Tego Dnia ludzie b\u0119d\u0105 podobni do rozproszonych motyli,"}, {"id": 423785, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6162, "text": "A g\u00f3ry b\u0119d\u0105 podobne do r\u00f3\u017cnobarwnej gr\u0119plowanej we\u0142ny."}, {"id": 423786, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6163, "text": "Wtedy ten, kt\u00f3rego szalki b\u0119d\u0105 ci\u0119\u017ckie,"}, {"id": 423787, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6164, "text": "Zazna \u017cycia przyjemnego;"}, {"id": 423788, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6165, "text": "A ten; kt\u00f3rego szalki b\u0119d\u0105 lekkie,"}, {"id": 423789, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6166, "text": "Pogr\u0105\u017cy si\u0119 w g\u0142\u0119bi Czelu\u015bci."}, {"id": 423790, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6167, "text": "A co ciebie pouczy, co to jest Czelu\u015b\u0107?"}, {"id": 423791, "identifier": "pl.bielawskiego", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pl", "name": "Bielawskiego", "transelator_en_name": "J\u00f3zefa Bielawskiego", "format": "text", "type": "translation", "ayah_number": 6168, "text": "To jest Ogie\u0144 Pal\u0105cy!"}]