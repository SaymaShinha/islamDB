[{"id": 86597, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5673, "text": "Onlar n\u0259 bar\u0259d\u0259 bir-birind\u0259n soru\u015furlar?"}, {"id": 86598, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5674, "text": "B\u00f6y\u00fck bir x\u0259b\u0259r bar\u0259sind\u0259 \u2013"}, {"id": 86599, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5675, "text": "el\u0259 bir x\u0259b\u0259r ki, onlar onun haqq\u0131nda ziddiyy\u0259td\u0259dirl\u0259r."}, {"id": 86600, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5676, "text": "Xeyr, onlar Qiyam\u0259tin h\u0259qiq\u0259t oldu\u011funu h\u00f6km\u0259n bil\u0259c\u0259kl\u0259r!"}, {"id": 86601, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5677, "text": "Bir daha xeyr! Onlar h\u00f6km\u0259n bil\u0259c\u0259kl\u0259r!"}, {"id": 86602, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5678, "text": "M\u0259g\u0259r Biz yeri yataq etm\u0259dikmi?"}, {"id": 86603, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5679, "text": "Da\u011flar\u0131 da payalar?!"}, {"id": 86604, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5680, "text": "Biz sizi c\u00fct yaratd\u0131q."}, {"id": 86605, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5681, "text": "Yuxunuzu da sizin \u00fc\u00e7\u00fcn rahatl\u0131q etdik."}, {"id": 86606, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5682, "text": "Gec\u0259ni \u00f6rt\u00fck etdik,"}, {"id": 86607, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5683, "text": "g\u00fcnd\u00fcz\u00fc is\u0259 ruzi qazanmaq vaxt\u0131."}, {"id": 86608, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5684, "text": "\u00dcst\u00fcn\u00fczd\u0259 yeddi m\u00f6hk\u0259m g\u00f6y qurduq"}, {"id": 86609, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5685, "text": "v\u0259 \u00e7ox parlaq bir \u00e7\u0131raq yaratd\u0131q."}, {"id": 86610, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5686, "text": "Biz buludlardan \u015f\u0131r\u0131lt\u0131yla ya\u011f\u0131\u015f ya\u011fd\u0131rd\u0131q ki,"}, {"id": 86611, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5687, "text": "onunla tax\u0131l v\u0259 dig\u0259r bitkil\u0259r yeti\u015fdir\u0259k,"}, {"id": 86612, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5688, "text": "habel\u0259 a\u011faclar\u0131n\u0131n qol-budaqlar\u0131 bir-birin\u0259 sarma\u015fan ba\u011flar salaq."}, {"id": 86613, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5689, "text": "H\u0259qiq\u0259t\u0259n, ay\u0131rd etm\u0259 g\u00fcn\u00fc b\u0259lli bir vaxta t\u0259yin edilmi\u015fdir!"}, {"id": 86614, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5690, "text": "Sur \u00fcf\u00fcr\u00fcl\u0259c\u0259yi g\u00fcn siz d\u0259st\u0259-d\u0259st\u0259 g\u0259l\u0259c\u0259ksiniz."}, {"id": 86615, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5691, "text": "O g\u00fcn g\u00f6y\u00fcn qap\u0131lar\u0131 a\u00e7\u0131lacaqd\u0131r."}, {"id": 86616, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5692, "text": "Da\u011flar da h\u0259r\u0259k\u0259t\u0259 g\u0259tirilib il\u011f\u0131ma \u00e7evril\u0259c\u0259k."}, {"id": 86617, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5693, "text": "H\u0259qiq\u0259t\u0259n, C\u0259h\u0259nn\u0259m kafirl\u0259r \u00fc\u00e7\u00fcn pusquda dayan\u0131b g\u00f6zl\u0259yir."}, {"id": 86618, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5694, "text": "O, h\u0259ddi a\u015fanlar\u0131n qay\u0131daca\u011f\u0131 yerdir."}, {"id": 86619, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5695, "text": "Onlar orada \u0259srl\u0259r boyu qalacaqlar."}, {"id": 86620, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5696, "text": "Onlar orada n\u0259 bir s\u0259rinlik, n\u0259 d\u0259 s\u0259rinl\u0259dirici i\u00e7ki dadacaqlar."}, {"id": 86621, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5697, "text": "\u0130\u00e7dikl\u0259ri yaln\u0131z qaynar su v\u0259 irin olacaq \u2013"}, {"id": 86622, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5698, "text": "T\u00f6r\u0259tdikl\u0259ri \u0259m\u0259ll\u0259rin\u0259 uy\u011fun bir c\u0259za kimi."}, {"id": 86623, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5699, "text": "Do\u011frusu, onlar haqq-hesab \u00e7\u0259kil\u0259c\u0259kl\u0259rin\u0259 \u00fcmid etmirdil\u0259r."}, {"id": 86624, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5700, "text": "Ay\u0259l\u0259rimizi d\u0259 q\u0259tiyy\u0259tl\u0259 t\u0259kzib edirdil\u0259r."}, {"id": 86625, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5701, "text": "Biz h\u0259r bir \u015feyi hesablay\u0131b yazm\u0131\u015f\u0131q."}, {"id": 86626, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5702, "text": "El\u0259 is\u0259 dad\u0131n \u0259zab\u0131! Biz siz\u0259 \u0259zabdan ba\u015fqa bir \u015fey art\u0131rmayaca\u011f\u0131q!"}, {"id": 86627, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5703, "text": "H\u0259qiq\u0259t\u0259n, m\u00fctt\u0259qil\u0259r \u00fc\u00e7\u00fcn nicat yeri vard\u0131r."}, {"id": 86628, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5704, "text": "H\u0259m\u00e7inin ba\u011flar v\u0259 \u00fcz\u00fcml\u00fckl\u0259r,"}, {"id": 86629, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5705, "text": "dol\u011fun d\u00f6\u015fl\u00fc h\u0259mya\u015f\u0131d q\u0131zlar"}, {"id": 86630, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5706, "text": "v\u0259 l\u0259b\u0259l\u0259b doldurulmu\u015f q\u0259d\u0259hl\u0259r d\u0259 vard\u0131r!"}, {"id": 86631, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5707, "text": "Onlar orada n\u0259 m\u0259nas\u0131z s\u00f6zl\u0259r, n\u0259 d\u0259 iftira e\u015fid\u0259c\u0259kl\u0259r."}, {"id": 86632, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5708, "text": "Bunlar s\u0259nin R\u0259bbind\u0259n olan m\u00fckafatlar, art\u0131qlamas\u0131 il\u0259 veril\u0259n t\u00f6hf\u0259l\u0259rdir."}, {"id": 86633, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5709, "text": "G\u00f6yl\u0259rin, yerin v\u0259 onlar\u0131n aras\u0131nda olanlar\u0131n M\u0259rh\u0259m\u0259tli R\u0259bbind\u0259n m\u00fckafatd\u0131r. Onlar Allah\u0131n h\u00fczurunda bir s\u00f6z dem\u0259y\u0259 qadir olmayacaqlar."}, {"id": 86634, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5710, "text": "Ruh (C\u0259brail) v\u0259 m\u0259l\u0259kl\u0259r s\u0259f-s\u0259f duraca\u011f\u0131 g\u00fcn, M\u0259rh\u0259m\u0259tli Allah\u0131n izin verdiyi kims\u0259l\u0259rd\u0259n v\u0259 do\u011fru s\u00f6z dey\u0259nl\u0259rd\u0259n ba\u015fqa he\u00e7 k\u0259s dan\u0131\u015fmayacaq."}, {"id": 86635, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5711, "text": "O g\u00fcn, ger\u00e7\u0259k bir g\u00fcnd\u00fcr. Kim ist\u0259s\u0259 \u00f6z R\u0259bbi yan\u0131nda s\u0131\u011f\u0131nacaq tapar!"}, {"id": 86636, "identifier": "az.musayev", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5712, "text": "H\u0259qiq\u0259t\u0259n, Biz sizi yax\u0131n \u0259zab bar\u0259d\u0259 x\u0259b\u0259rdar etdik. O g\u00fcn insan \u00f6z \u0259ll\u0259ri il\u0259 n\u0259l\u0259r etdiyini g\u00f6r\u0259c\u0259k, kafir is\u0259 dey\u0259c\u0259kdir: \u201cKa\u015f m\u0259n torpaq olayd\u0131m!\u201d"}]