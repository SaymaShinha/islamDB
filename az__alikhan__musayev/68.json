[{"id": 85473, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5277, "text": "hans\u0131n\u0131z\u0131n d\u0259li oldu\u011funu."}, {"id": 86073, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5292, "text": "Onlar s\u0259h\u0259r qalxd\u0131qda bir-birl\u0259rini \u00e7a\u011f\u0131rd\u0131lar:"}, {"id": 86094, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5293, "text": "\u201c\u018fg\u0259r meyv\u0259 d\u0259r\u0259c\u0259ksinizs\u0259, ba\u011f\u0131n\u0131za erk\u0259n gedin!\u201d"}, {"id": 86124, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5275, "text": "H\u0259qiq\u0259t\u0259n d\u0259, s\u0259n b\u00f6y\u00fck \u0259xlaq sahibis\u0259n!"}, {"id": 86174, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5294, "text": "Onlar ba\u011fa yolland\u0131lar, yol boyu da bir-birl\u0259rin\u0259 bel\u0259 p\u0131\u00e7\u0131lday\u0131rd\u0131lar:"}, {"id": 86186, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5276, "text": "S\u0259n d\u0259 g\u00f6r\u0259c\u0259ks\u0259n, onlar da \u2013"}, {"id": 86210, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5272, "text": "Nun. And olsun q\u0259l\u0259m\u0259 v\u0259 q\u0259l\u0259m\u0259 al\u0131nanlara!"}, {"id": 86211, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5273, "text": "M\u0259hz R\u0259bbinin m\u0259rh\u0259m\u0259tin\u0259 g\u00f6r\u0259 s\u0259n d\u0259li deyils\u0259n."}, {"id": 86212, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5274, "text": "\u015e\u00fcbh\u0259siz ki, s\u0259nin \u00fc\u00e7\u00fcn t\u00fck\u0259nm\u0259z bir m\u00fckafat haz\u0131rlanm\u0131\u015fd\u0131r."}, {"id": 86213, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5278, "text": "\u015e\u00fcbh\u0259siz ki, R\u0259bbin Onun yolundan azanlar\u0131 v\u0259 haqq yolda olanlar\u0131 daha yax\u015f\u0131 tan\u0131y\u0131r."}, {"id": 86214, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5279, "text": "El\u0259 is\u0259 haqq\u0131 yalan sayanlara g\u00fcz\u0259\u015ft\u0259 getm\u0259."}, {"id": 86215, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5280, "text": "M\u00fc\u015frikl\u0259r ist\u0259yirl\u0259r ki, s\u0259n onlara g\u00fcz\u0259\u015ft ed\u0259s\u0259n, onlar da s\u0259n\u0259 g\u00fcz\u0259\u015ft etsinl\u0259r."}, {"id": 86216, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5281, "text": "\u0130ta\u0259t etm\u0259 h\u0259r tez-tez and i\u00e7\u0259n\u0259, al\u00e7a\u011fa,"}, {"id": 86217, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5282, "text": "qeyb\u0259t ed\u0259n\u0259, dedi-qodu yayana,"}, {"id": 86218, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5283, "text": "xeyr\u0259 mane olana, h\u0259ddi a\u015fana, g\u00fcnahkara,"}, {"id": 86219, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5284, "text": "da\u015f \u00fcr\u0259kliy\u0259, bunlardan sonra da ba\u015fqas\u0131n\u0131n ad\u0131n\u0131 m\u0259nims\u0259y\u0259n f\u0131r\u0131ldaq\u00e7\u0131ya \u2013"}, {"id": 86220, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5285, "text": "o adam var-d\u00f6vl\u0259t v\u0259 o\u011ful-u\u015faq sahibi olsa bel\u0259."}, {"id": 86221, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5286, "text": "Ay\u0259l\u0259rimiz ona oxundu\u011fu zaman: \u201cBu, ke\u00e7mi\u015fd\u0259kil\u0259rin \u0259satirl\u0259rind\u0259ndir!\u201d\u2013 dey\u0259r."}, {"id": 86222, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5287, "text": "Biz onun burnuna dam\u011fa vuraca\u011f\u0131q."}, {"id": 86223, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5288, "text": "H\u0259qiq\u0259t\u0259n, Biz ba\u011f sahibl\u0259rini s\u0131na\u011fa \u00e7\u0259kdiyimiz kimi onlar\u0131 da s\u0131na\u011fa \u00e7\u0259kdik. O zaman ba\u011f sahibl\u0259ri s\u0259h\u0259r a\u00e7\u0131landa ba\u011fda olan meyv\u0259l\u0259ri m\u00fctl\u0259q d\u0259r\u0259c\u0259kl\u0259rin\u0259 and i\u00e7mi\u015fdil\u0259r"}, {"id": 86224, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5289, "text": "v\u0259 he\u00e7 bir istisna da etm\u0259mi\u015fdil\u0259r (\u201cin\u015fallah\u201d dem\u0259mi\u015fdil\u0259r)."}, {"id": 86225, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5290, "text": "Onlar yuxuda ik\u0259n R\u0259bbinin \u0259zab\u0131 o ba\u011f\u0131 b\u00fcr\u00fcd\u00fc."}, {"id": 86226, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5291, "text": "Ba\u011f yan\u0131b z\u00fclm\u0259t gec\u0259 kimi qapqara oldu."}, {"id": 86227, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5295, "text": "\u201cBu g\u00fcn oraya \u2013 yan\u0131n\u0131za he\u00e7 bir kas\u0131b girm\u0259sin!\u201d"}, {"id": 86228, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5296, "text": "Onlar bu m\u0259qs\u0259dl\u0259 kas\u0131blar\u0131 ba\u011fa buraxmama\u011fa qadir olacaqlar\u0131n\u0131 z\u0259nn edib erk\u0259n getdil\u0259r."}, {"id": 86229, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5297, "text": "Ba\u011f\u0131 g\u00f6r\u0259nd\u0259 is\u0259 bel\u0259 dedil\u0259r: \u201cDey\u0259s\u0259n, biz d\u00fcz g\u0259lm\u0259mi\u015fik!"}, {"id": 86230, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5298, "text": "Xeyr! Biz bu ba\u011f\u0131n xeyrind\u0259n m\u0259hrum olmu\u015fuq!\u201d"}, {"id": 86231, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5299, "text": "Onlar\u0131n \u0259n insafl\u0131s\u0131 dedi: \u201cM\u0259g\u0259r m\u0259n siz\u0259 Allah\u0131n \u015f\u0259nin\u0259 t\u0259rifl\u0259r s\u00f6yl\u0259yin dem\u0259mi\u015fdimmi?\u201d"}, {"id": 86232, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5300, "text": "Onlar dedil\u0259r: \u201cR\u0259bbimiz pakd\u0131r, m\u00fcq\u0259dd\u0259sdir! H\u0259qiq\u0259t\u0259n d\u0259, biz \u00f6z\u00fcm\u00fcz\u0259 z\u00fclm etmi\u015fik!\u201d"}, {"id": 86233, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5301, "text": "Onlar d\u00f6n\u00fcb bir-birl\u0259rini q\u0131nama\u011fa ba\u015flad\u0131lar"}, {"id": 86234, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5302, "text": "v\u0259 dedil\u0259r: \u201cVay hal\u0131m\u0131za! Biz h\u0259ddi a\u015fanlar\u0131q!"}, {"id": 86235, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5303, "text": "Ola bilsin ki, R\u0259bbimiz biz\u0259 bu ba\u011f\u0131n \u0259v\u0259zin\u0259 ondan daha yax\u015f\u0131s\u0131n\u0131 versin. \u018flb\u0259tt\u0259, biz R\u0259bbimiz\u0259 y\u00f6n\u0259lirik\u201d."}, {"id": 86236, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5304, "text": "D\u00fcnyadak\u0131 \u0259zab bel\u0259dir. Axir\u0259t \u0259zab\u0131 is\u0259 daha b\u00f6y\u00fckd\u00fcr! Ka\u015f bil\u0259ydil\u0259r!"}, {"id": 86237, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5305, "text": "\u015e\u00fcbh\u0259siz ki, m\u00fctt\u0259qil\u0259r \u00fc\u00e7\u00fcn R\u0259bbin d\u0259rgah\u0131nda N\u0259im ba\u011flar\u0131 haz\u0131rlanm\u0131\u015fd\u0131r."}, {"id": 86238, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5306, "text": "Biz he\u00e7 m\u00fcs\u0259lmanlar\u0131 g\u00fcnahkarlara tay tutar\u0131qm\u0131?"}, {"id": 86239, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5307, "text": "Siz\u0259 n\u0259 olub, nec\u0259 h\u00f6km verirsiniz?"}, {"id": 86240, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5308, "text": "Yoxsa sizin el\u0259 bir kitab\u0131n\u0131z vard\u0131r ki, \u00fcsyankarla ita\u0259tkar\u0131n eyni oldu\u011funu ondan oxuyursunuz?"}, {"id": 86241, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5309, "text": "Ya da onda b\u0259y\u0259ndiyiniz h\u0259r bir \u015feyin sizin olaca\u011f\u0131 yaz\u0131lm\u0131\u015fd\u0131r?"}, {"id": 86242, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5310, "text": "Yaxud Biziml\u0259 Qiyam\u0259t g\u00fcn\u00fcn\u0259d\u0259k \u0259hd ba\u011flam\u0131s\u0131n\u0131z ki, ist\u0259diyiniz h\u0259r \u015fey sizin olacaqd\u0131r?"}, {"id": 86243, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5311, "text": "Onlardan soru\u015f ki, kim buna zamin duracaqd\u0131r?"}, {"id": 86244, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5312, "text": "Yoxsa onlar\u0131n \u015f\u0259rikl\u0259ri vard\u0131r? \u018fg\u0259r do\u011fru dan\u0131\u015f\u0131rlarsa, onda qoy \u015f\u0259rikl\u0259rini g\u0259tirsinl\u0259r."}, {"id": 86245, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5313, "text": "Aya\u011f\u0131n a\u00e7\u0131laca\u011f\u0131 v\u0259 onlar s\u0259cd\u0259y\u0259 \u00e7a\u011f\u0131r\u0131lacaqlar\u0131, lakin buna qadir ola bilm\u0259y\u0259c\u0259kl\u0259ri g\u00fcn \u2013"}, {"id": 86246, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5314, "text": "onlar\u0131n bax\u0131\u015flar\u0131 yaz\u0131q g\u00f6rk\u0259m alacaq, \u00f6zl\u0259rini d\u0259 zill\u0259t b\u00fcr\u00fcy\u0259c\u0259kdir. H\u0259r\u00e7\u0259nd ki, onlar d\u00fcnyada ik\u0259n sa\u011f-salamat halda s\u0259cd\u0259 q\u0131lma\u011fa d\u0259v\u0259t olunurdular."}, {"id": 86247, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5315, "text": "Bu S\u00f6z\u00fc yalan sayanlar\u0131 M\u0259nim \u00f6hd\u0259m\u0259 burax. Biz onlar\u0131 tovlay\u0131b bilm\u0259dikl\u0259ri bir y\u00f6nd\u0259n t\u0259dric\u0259n \u0259zaba s\u00fcr\u00fckl\u0259yirik."}, {"id": 86248, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5316, "text": "H\u0259l\u0259lik M\u0259n onlara m\u00f6hl\u0259t verir\u0259m. \u015e\u00fcbh\u0259siz ki, M\u0259nim kafirl\u0259r \u00fc\u00e7\u00fcn qurdu\u011fum hiyl\u0259m \u00e7ox g\u00fccl\u00fcd\u00fcr."}, {"id": 86249, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5317, "text": "Yoxsa s\u0259n onlardan bir muzd ist\u0259yirs\u0259n v\u0259 onlar da a\u011f\u0131r bir borca d\u00fc\u015f\u00fcbl\u0259r?"}, {"id": 86250, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5318, "text": "Yoxsa qeyb onlar\u0131n \u0259lind\u0259dir v\u0259 onlar da ona bax\u0131b ist\u0259dikl\u0259rini yaz\u0131rlar?"}, {"id": 86251, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5319, "text": "S\u0259n \u00f6z R\u0259bbinin h\u00f6km\u00fcn\u0259 s\u0259bir et v\u0259 bal\u0131q sahibi Yunus kimi olma! O zaman o, q\u0259m-q\u00fcss\u0259 i\u00e7ind\u0259 R\u0259bbin\u0259 dua etmi\u015fdi."}, {"id": 86252, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5320, "text": "\u018fg\u0259r R\u0259bbinin m\u0259rh\u0259m\u0259ti ona yeti\u015fm\u0259s\u0259ydi, o, q\u0131nanm\u0131\u015f halda he\u00e7 bir bitki bitm\u0259y\u0259n bo\u015f yer\u0259 at\u0131lacaqd\u0131."}, {"id": 86253, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5321, "text": "Lakin R\u0259bbi onu se\u00e7ib \u0259m\u0259lisalehl\u0259rd\u0259n etdi."}, {"id": 86254, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5322, "text": "H\u0259qiq\u0259t\u0259n, kafirl\u0259r Zikri (Quran\u0131) e\u015fitdikl\u0259ri zaman \u00f6z bax\u0131\u015flar\u0131 il\u0259 az qala s\u0259ni g\u00f6z\u0259 g\u0259tir\u0259l\u0259r. Onlar \u00e7ar\u0259sizlikd\u0259n: \u201cO, d\u0259lidir!\u201d\u2013 deyirl\u0259r."}, {"id": 86255, "identifier": "az.musayev", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5323, "text": "Halbuki bu Quran al\u0259ml\u0259r \u00fc\u00e7\u00fcn \u00f6y\u00fcd-n\u0259sih\u0259td\u0259n ba\u015fqa bir \u015fey deyildir."}]