[{"id": 86761, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5849, "text": "\u00c7\u0259kid\u0259 v\u0259 \u00f6l\u00e7\u00fcd\u0259 aldadanlar\u0131n vay hal\u0131na!"}, {"id": 86762, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5850, "text": "O k\u0259sl\u0259rin ki, camaatdan bir \u015fey ald\u0131qda onu tam almaq ist\u0259y\u0259rl\u0259r,"}, {"id": 86763, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5851, "text": "onlar \u00fc\u00e7\u00fcn bir \u015fey \u00f6l\u00e7d\u00fckd\u0259 v\u0259 ya \u00e7\u0259kdikd\u0259 onu ziyana salarlar."}, {"id": 86764, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Onlar dirildil\u0259c\u0259kl\u0259rini fikirl\u0259\u015fmirl\u0259rmi?"}, {"id": 86765, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5853, "text": "H\u0259m d\u0259 \u0259z\u0259m\u0259tli bir g\u00fcnd\u0259."}, {"id": 86766, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5854, "text": "O g\u00fcnd\u0259 ki, insanlar al\u0259ml\u0259rin R\u0259bbinin qar\u015f\u0131s\u0131nda dayanacaqlar."}, {"id": 86767, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5855, "text": "Xeyr! H\u0259qiq\u0259t\u0259n, g\u00fcnahkarlar\u0131n kitab\u0131 Siccind\u0259dir."}, {"id": 86768, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5856, "text": "S\u0259n haradan bil\u0259s\u0259n ki, Siccin n\u0259dir?!"}, {"id": 86769, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5857, "text": "O, yaz\u0131l\u0131 bir kitabd\u0131r!"}, {"id": 86770, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5858, "text": "O g\u00fcn haqq\u0131 yalan hesab ed\u0259nl\u0259rin vay hal\u0131na!"}, {"id": 86771, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5859, "text": "O k\u0259sl\u0259rin ki, Haqq-hesab g\u00fcn\u00fcn\u00fc yalan hesab edirl\u0259r."}, {"id": 86772, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5860, "text": "Amma bunu, h\u0259ddini a\u015fan g\u00fcnahkardan ba\u015fqa, he\u00e7 k\u0259s yalan hesab etm\u0259z."}, {"id": 86773, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5861, "text": "Ay\u0259l\u0259rimiz ona oxundu\u011fu zaman: \u201cBu, ke\u00e7mi\u015fd\u0259kil\u0259rin uydurdu\u011fu \u0259fsan\u0259l\u0259rdir!\u201d\u2013 deyir."}, {"id": 86774, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5862, "text": "Xeyr! Onlar\u0131n q\u0259lbini qazand\u0131qlar\u0131 g\u00fcnahlar b\u00fcr\u00fcm\u00fc\u015fd\u00fcr."}, {"id": 86775, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5863, "text": "Xeyr, h\u0259min g\u00fcn onlarla R\u0259bbi aras\u0131na p\u0259rd\u0259 \u00e7\u0259kil\u0259c\u0259k!"}, {"id": 86776, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5864, "text": "Sonra onlar C\u0259h\u0259nn\u0259m\u0259 gir\u0259c\u0259kl\u0259r"}, {"id": 86777, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5865, "text": "v\u0259 onlara deyil\u0259c\u0259k: \u201cBudur sizin yalan hesab etdiyiniz C\u0259h\u0259nn\u0259m!\u201d"}, {"id": 86778, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5866, "text": "Xeyr! H\u0259qiq\u0259t\u0259n, ita\u0259tkarlar\u0131n kitab\u0131 \u0130lliyyundadir!"}, {"id": 86779, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5867, "text": "S\u0259n haradan bil\u0259s\u0259n ki, \u0130lliyyun n\u0259dir?!"}, {"id": 86780, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5868, "text": "O, el\u0259 bir yaz\u0131l\u0131 kitabd\u0131r ki,"}, {"id": 86781, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5869, "text": "onu yaln\u0131z Allaha yax\u0131n olanlar g\u00f6r\u00fcrl\u0259r!"}, {"id": 86782, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5870, "text": "H\u0259qiq\u0259t\u0259n, ita\u0259tkarlar nem\u0259tl\u0259r i\u00e7ind\u0259 olacaqlar."}, {"id": 86783, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5871, "text": "Onlar taxtlar \u00fcst\u00fcnd\u0259 seyr\u0259 dalacaqlar."}, {"id": 86784, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5872, "text": "S\u0259n onlar\u0131n \u00e7\u00f6hr\u0259sind\u0259 s\u0259ad\u0259t parlaql\u0131\u011f\u0131 g\u00f6r\u0259c\u0259ks\u0259n."}, {"id": 86785, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5873, "text": "Onlara m\u00f6h\u00fcrl\u00fc \u015f\u0259rabdan i\u00e7irdil\u0259c\u0259kdir."}, {"id": 86786, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5874, "text": "Onun m\u00f6h\u00fcr\u00fc m\u00fc\u015fkd\u00fcr. Qoy \u00e7al\u0131\u015fanlar onun \u00fc\u00e7\u00fcn s\u0259y g\u00f6st\u0259rsinl\u0259r!"}, {"id": 86787, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5875, "text": "Onun qat\u0131\u015f\u0131\u011f\u0131 is\u0259 t\u0259snim bula\u011f\u0131ndand\u0131r."}, {"id": 86788, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5876, "text": "O t\u0259snim el\u0259 bir bulaqd\u0131r ki, ondan Allaha yax\u0131n olanlar i\u00e7\u0259rl\u0259r."}, {"id": 86789, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5877, "text": "Do\u011frudan da, g\u00fcnah ed\u0259nl\u0259r iman g\u0259tir\u0259nl\u0259r\u0259 g\u00fcl\u00fcrd\u00fcl\u0259r,"}, {"id": 86790, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5878, "text": "onlar\u0131n yan\u0131ndan ke\u00e7dikd\u0259 bir-birin\u0259 qa\u015f-g\u00f6z edirdil\u0259r,"}, {"id": 86791, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5879, "text": "\u00f6z ail\u0259l\u0259rinin yan\u0131na f\u0259r\u0259hl\u0259 qay\u0131d\u0131rd\u0131lar,"}, {"id": 86792, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5880, "text": "m\u00f6minl\u0259ri g\u00f6rd\u00fckd\u0259 is\u0259 deyirdil\u0259r: \u201cH\u0259qiq\u0259t\u0259n, bunlar azm\u0131\u015flard\u0131r!\u201d"}, {"id": 86793, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5881, "text": "Halbuki bu g\u00fcnahkarlar onlara g\u00f6z\u0259t\u00e7i qoyulmam\u0131\u015fd\u0131lar."}, {"id": 86794, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5882, "text": "Bu g\u00fcn m\u0259hz iman g\u0259tir\u0259nl\u0259r kafirl\u0259r\u0259 g\u00fcl\u0259c\u0259kl\u0259r."}, {"id": 86795, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5883, "text": "Onlar taxtlar \u00fcst\u00fcnd\u0259 \u0259trafa baxacaqlar."}, {"id": 86796, "identifier": "az.musayev", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5884, "text": "Kafirl\u0259r t\u00f6r\u0259tdikl\u0259rinin c\u0259zas\u0131n\u0131 ald\u0131larm\u0131?!"}]