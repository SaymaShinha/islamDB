[{"id": 86301, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5376, "text": "\u0130st\u0259y\u0259n kims\u0259 ba\u015f ver\u0259c\u0259k \u0259zab\u0131 ist\u0259di \u2013"}, {"id": 86302, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5377, "text": "bunu kafirl\u0259r \u00fc\u00e7\u00fcn dil\u0259di. O \u0259zab\u0131n qar\u015f\u0131s\u0131n\u0131 alan tap\u0131lmayacaq."}, {"id": 86303, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5378, "text": "O, d\u0259r\u0259c\u0259l\u0259r sahibi olan Allahdand\u0131r."}, {"id": 86304, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5379, "text": "M\u0259l\u0259kl\u0259r v\u0259 Ruh (C\u0259brail) Ona do\u011fru say\u0131 \u0259lli min il\u0259 b\u0259rab\u0259r bir g\u00fcn \u0259rzind\u0259 qalx\u0131rlar."}, {"id": 86305, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5380, "text": "S\u0259n t\u0259mkinl\u0259 s\u0259bir et!"}, {"id": 86306, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5381, "text": "Do\u011frusu, kafirl\u0259r o \u0259zab\u0131n uzaq oldu\u011funu g\u00fcman edirl\u0259r."}, {"id": 86307, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5382, "text": "Biz is\u0259 onun yax\u0131n oldu\u011funu g\u00f6r\u00fcr\u00fck."}, {"id": 86308, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5383, "text": "O g\u00fcn g\u00f6y \u0259ridilmi\u015f metal kimi olacaqd\u0131r."}, {"id": 86309, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5384, "text": "Da\u011flar da didilmi\u015f yuna b\u0259nz\u0259y\u0259c\u0259kl\u0259r."}, {"id": 86310, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5385, "text": "Dost dostunun hal\u0131n\u0131 x\u0259b\u0259r almayacaq."}, {"id": 86311, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5386, "text": "Halbuki onlar bir-birini g\u00f6r\u0259c\u0259kl\u0259r. G\u00fcnahkar o g\u00fcn\u00fcn \u0259zab\u0131ndan qurtarmaq \u00fc\u00e7\u00fcn fidy\u0259 verm\u0259k ist\u0259y\u0259r \u2013 \u00f6z o\u011fullar\u0131n\u0131,"}, {"id": 86312, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5387, "text": "h\u0259yat yolda\u015f\u0131n\u0131 v\u0259 qarda\u015f\u0131n\u0131,"}, {"id": 86313, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5388, "text": "ona s\u0131\u011f\u0131nacaq vermi\u015f n\u0259slini"}, {"id": 86314, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5389, "text": "v\u0259 yer \u00fcz\u00fcnd\u0259 olanlar\u0131n ham\u0131s\u0131n\u0131 \u2013 t\u0259ki bu onu xilas etsin."}, {"id": 86315, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5390, "text": "Xeyr! Bu, alovu \u015f\u00f6l\u0259l\u0259n\u0259n Oddur \u2013"}, {"id": 86316, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5391, "text": "ba\u015f\u0131n d\u0259risini s\u0131y\u0131r\u0131b \u00e7\u0131xaran bir od."}, {"id": 86317, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5392, "text": "O od \u00e7a\u011f\u0131r\u0131r haqqa arxa \u00e7evir\u0259ni, ondan \u00fcz d\u00f6nd\u0259r\u0259ni"}, {"id": 86318, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5393, "text": "v\u0259 mal-d\u00f6vl\u0259t y\u0131\u011f\u0131b saxlayan\u0131."}, {"id": 86319, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5394, "text": "H\u0259qiq\u0259t\u0259n, insan k\u0259mh\u00f6vs\u0259l\u0259 yarad\u0131lm\u0131\u015fd\u0131r."}, {"id": 86320, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5395, "text": "Ona bir pislik toxunduqda \u00f6z\u00fcnd\u0259n \u00e7\u0131x\u0131r,"}, {"id": 86321, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5396, "text": "bir xeyir n\u0259sib olduqda is\u0259 x\u0259sislik edir."}, {"id": 86322, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5397, "text": "Namaz q\u0131lanlar istisnad\u0131r."}, {"id": 86323, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5398, "text": "O k\u0259sl\u0259r ki, namazlar\u0131n\u0131 h\u0259mi\u015f\u0259 q\u0131l\u0131rlar."}, {"id": 86324, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5399, "text": "O k\u0259sl\u0259r ki, onlar\u0131n mallar\u0131nda m\u0259lum bir haqq vard\u0131r-"}, {"id": 86325, "identifier": "az.musayev", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\