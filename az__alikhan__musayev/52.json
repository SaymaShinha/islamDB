[{"id": 85686, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4736, "text": "And olsun da\u011fa!"}, {"id": 85687, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4737, "text": "And olsun yaz\u0131lm\u0131\u015f Kitaba \u2013"}, {"id": 85688, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4738, "text": "nazik d\u0259ri \u00fcz\u0259rind\u0259 olan Kitaba!"}, {"id": 85689, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4739, "text": "And olsun Beytulm\u0259mura!"}, {"id": 83859, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4740, "text": "And olsun y\u00fcks\u0259k tavana!"}, {"id": 84176, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4741, "text": "And olsun alovlanan d\u0259niz\u0259!"}, {"id": 84559, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4742, "text": "R\u0259bbinin \u0259zab\u0131 h\u00f6km\u0259n vaqe olacaqd\u0131r."}, {"id": 84722, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4743, "text": "He\u00e7 n\u0259 onun qar\u015f\u0131s\u0131n\u0131 ala bilm\u0259z."}, {"id": 84811, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4744, "text": "O g\u00fcn g\u00f6y \u015fidd\u0259tl\u0259 sars\u0131lacaq,"}, {"id": 84933, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4745, "text": "da\u011flar da h\u0259r\u0259k\u0259t\u0259 g\u0259l\u0259c\u0259kdir."}, {"id": 85129, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4746, "text": "O g\u00fcn vay hal\u0131na haqq\u0131 yalan sayanlar\u0131n!"}, {"id": 85178, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4747, "text": "O k\u0259sl\u0259r ki, yalan bataql\u0131\u011f\u0131nda \u0259yl\u0259nirl\u0259r."}, {"id": 85690, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4748, "text": "O g\u00fcn onlar amans\u0131zcas\u0131na s\u00fcr\u00fcd\u00fcl\u0259r\u0259k C\u0259h\u0259nn\u0259m oduna at\u0131lacaqlar."}, {"id": 85691, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4749, "text": "Onlara deyil\u0259c\u0259k: \u201cBu sizin yalan sayd\u0131\u011f\u0131n\u0131z C\u0259h\u0259nn\u0259m odudur!"}, {"id": 85692, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4750, "text": "B\u0259lk\u0259 bu da sehrdir, siz g\u00f6rm\u00fcrs\u00fcn\u00fcz?"}, {"id": 85693, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4751, "text": "Yan\u0131n orada! D\u00f6zs\u0259niz d\u0259, d\u00f6zm\u0259s\u0259niz d\u0259 sizin \u00fc\u00e7\u00fcn eynidir. Siz\u0259 ancaq etdiyiniz \u0259m\u0259ll\u0259rin c\u0259zas\u0131 verilir\u201d."}, {"id": 85694, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4752, "text": "\u015e\u00fcbh\u0259siz ki, m\u00fctt\u0259qil\u0259r C\u0259nn\u0259t ba\u011flar\u0131nda v\u0259 nem\u0259t i\u00e7ind\u0259 ya\u015fayacaqlar."}, {"id": 85695, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4753, "text": "R\u0259bbinin onlara b\u0259x\u015f etdiyin\u0259 sevin\u0259c\u0259kl\u0259r. R\u0259bbi onlar\u0131 C\u0259h\u0259nn\u0259m \u0259zab\u0131ndan xilas etmi\u015fdir."}, {"id": 85696, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4754, "text": "Onlara deyil\u0259c\u0259k: \u201cXeyirxah \u0259m\u0259ll\u0259rinizin \u0259v\u0259zi olaraq nu\u015fcanl\u0131qla yeyin, i\u00e7in!\u201d"}, {"id": 85697, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4755, "text": "Onlar s\u0259fl\u0259rl\u0259 sal\u0131nm\u0131\u015f m\u00fct\u0259kk\u0259l\u0259r\u0259 dirs\u0259kl\u0259n\u0259c\u0259kl\u0259r. Biz onlar\u0131 iri qarag\u00f6zl\u00fc huril\u0259rl\u0259 evl\u0259ndir\u0259c\u0259yik."}, {"id": 85698, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4756, "text": "M\u00f6minl\u0259rin imanda onlar\u0131n arxas\u0131nca ged\u0259n n\u0259sill\u0259rini d\u0259 C\u0259nn\u0259td\u0259 \u00f6zl\u0259rin\u0259 qovu\u015fduraca\u011f\u0131q. Onlar\u0131n \u0259m\u0259ll\u0259rind\u0259n he\u00e7 bir \u015fey \u0259skiltm\u0259y\u0259c\u0259yik. H\u0259r k\u0259s qazand\u0131\u011f\u0131n\u0131n girovudur."}, {"id": 85699, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4757, "text": "Onlara ist\u0259dikl\u0259ri meyv\u0259d\u0259n v\u0259 \u0259td\u0259n bol-bol ver\u0259c\u0259yik."}, {"id": 85700, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4758, "text": "Onlar orada bir-birin\u0259 \u015f\u0259rab dolu bad\u0259 uzadacaqlar. Bu \u015f\u0259rab onlar\u0131 n\u0259 bo\u015fbo\u011fazl\u0131\u011fa, n\u0259 d\u0259 g\u00fcnaha t\u0259hrik etm\u0259z."}, {"id": 85701, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4759, "text": "Onlar\u0131n \u0259traf\u0131nda xidm\u0259tl\u0259rin\u0259 verilmi\u015f, qorunub saxlan\u0131lan mirvari kimi yeniyetm\u0259 o\u011flanlar dolanacaqlar."}, {"id": 85618, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4760, "text": "Onlar bir-birin\u0259 \u00fcz tutub soru\u015facaqlar."}, {"id": 85649, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4761, "text": "Onlar dey\u0259c\u0259kl\u0259r: \u201cBiz \u0259vv\u0259ll\u0259r ail\u0259miz i\u00e7ind\u0259 olark\u0259n Allahdan qorxurduq."}, {"id": 85702, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4762, "text": "Allah da biz\u0259 m\u0259rh\u0259m\u0259t g\u00f6st\u0259rdi v\u0259 bizi q\u0131zmar yel \u0259zab\u0131ndan qorudu."}, {"id": 85703, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4763, "text": "Biz \u0259vv\u0259ll\u0259r Ona dua edirdik. H\u0259qiq\u0259t\u0259n, O, yax\u015f\u0131l\u0131q ed\u0259ndir, R\u0259hmlidir!\u201d"}, {"id": 85704, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4764, "text": "Onlara xat\u0131rlat! S\u0259n \u00f6z R\u0259bbinin nem\u0259ti say\u0259sind\u0259 n\u0259 kahins\u0259n, n\u0259 d\u0259 d\u0259li!"}, {"id": 85705, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Yoxsa onlar: \u201cO bir \u015fairdir. Biz onun \u00f6l\u00fcm\u00fcn\u00fc g\u00f6zl\u0259yirik!\u201d\u2013 deyirl\u0259r."}, {"id": 85706, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4766, "text": "De: \u201cG\u00f6zl\u0259yin, m\u0259n d\u0259 sizinl\u0259 birlikd\u0259 g\u00f6zl\u0259y\u0259nl\u0259rd\u0259n\u0259m!\u201d"}, {"id": 85707, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Yoxsa onlar\u0131n a\u011f\u0131llar\u0131 bunu onlara \u0259mr edir? Yaxud onlar az\u011f\u0131n adamlard\u0131r?"}, {"id": 85708, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Yoxsa onlar: \u201cBunu \u00f6z\u00fcnd\u0259n uydurdun!\u201d\u2013 deyirl\u0259r. Xeyr, onlar iman g\u0259tirmirl\u0259r."}, {"id": 85709, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4769, "text": "\u018fg\u0259r do\u011fru s\u00f6yl\u0259yirl\u0259rs\u0259, qoy ona (Qurana) b\u0259nz\u0259r bir k\u0259lam g\u0259tirsinl\u0259r."}, {"id": 85710, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4770, "text": "B\u0259lk\u0259 onlar \u00f6z-\u00f6z\u00fcn\u0259 yaran\u0131blar? Yaxud onlar yaradanlard\u0131r?"}, {"id": 85711, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Yoxsa onlar g\u00f6yl\u0259ri v\u0259 yeri yarad\u0131blar? Xeyr, onlar q\u0259tiyy\u0259n inanm\u0131rlar."}, {"id": 85712, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Yoxsa s\u0259nin R\u0259bbinin x\u0259zin\u0259l\u0259ri onlardad\u0131r? Yaxud onlar h\u00f6km sahibidirl\u0259r?"}, {"id": 85713, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Yoxsa onlar\u0131n, \u00fcst\u00fcn\u0259 \u00e7\u0131x\u0131b m\u0259l\u0259kl\u0259rin s\u00f6hb\u0259tl\u0259rini dinl\u0259dikl\u0259ri bir n\u0259rdivan\u0131 vard\u0131r? Onda qoy onlar\u0131n m\u0259l\u0259kl\u0259ri dinl\u0259y\u0259nl\u0259ri anla\u015f\u0131ql\u0131 bir d\u0259lil g\u0259tirsinl\u0259r!"}, {"id": 85714, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Yoxsa q\u0131zlar Onun, o\u011flanlar is\u0259 sizindir?"}, {"id": 85715, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Yoxsa s\u0259n onlardan bir m\u00fckafat ist\u0259yirs\u0259n v\u0259 onlar da borclu qald\u0131qlar\u0131na g\u00f6r\u0259 a\u011f\u0131r y\u00fck alt\u0131ndad\u0131rlar?"}, {"id": 85716, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Yoxsa onlar qeybi bilir v\u0259 onu yaz\u0131rlar?"}, {"id": 85717, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Yoxsa bir hiyl\u0259 qurmaq ist\u0259yirl\u0259r? Amma kafirl\u0259r \u00f6zl\u0259ri o hiyl\u0259nin qurban\u0131 olacaqlar."}, {"id": 85718, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Yoxsa onlar\u0131n Allahdan ba\u015fqa bir m\u0259budu var? Allah onlar\u0131n qo\u015fduqlar\u0131 \u015f\u0259rikl\u0259rd\u0259n uzaqd\u0131r."}, {"id": 85719, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4779, "text": "\u018fg\u0259r onlar g\u00f6y par\u00e7alar\u0131n\u0131n d\u00fc\u015fd\u00fcy\u00fcn\u00fc g\u00f6rs\u0259l\u0259r: \u201cBulud topas\u0131d\u0131r!\u201d\u2013 dey\u0259rl\u0259r."}, {"id": 85720, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4780, "text": "S\u0259n onlar\u0131 sars\u0131lacaqlar\u0131 g\u00fcn\u0259 qovu\u015fana q\u0259d\u0259r t\u0259rk et!"}, {"id": 85721, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4781, "text": "O g\u00fcn hiyl\u0259l\u0259ri onlara he\u00e7 bir fayda verm\u0259y\u0259c\u0259k v\u0259 onlara k\u00f6m\u0259k g\u00f6st\u0259rilm\u0259y\u0259c\u0259kdir."}, {"id": 85722, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4782, "text": "H\u0259qiq\u0259t\u0259n, zal\u0131mlar \u00fc\u00e7\u00fcn bundan ba\u015fqa \u0259zab da haz\u0131rlanm\u0131\u015fd\u0131r. Lakin onlar\u0131n \u00e7oxu bunu bilmir."}, {"id": 85723, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4783, "text": "S\u0259n \u00f6z R\u0259bbinin h\u00f6km\u00fcn\u0259 s\u0259bir et! H\u0259qiq\u0259t\u0259n, s\u0259n Bizim G\u00f6zl\u0259rimiz \u00f6n\u00fcnd\u0259s\u0259n. Yuxudan qalxd\u0131qda R\u0259bbini h\u0259md il\u0259 zikr et!"}, {"id": 85724, "identifier": "az.musayev", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 4784, "text": "Gec\u0259nin bir vaxt\u0131nda, bir d\u0259 ulduzlar batandan sonra Onun \u015f\u0259nin\u0259 t\u0259rifl\u0259r de!"}]