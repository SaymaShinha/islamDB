, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5980, "text": "Orada h\u00fcnd\u00fcr taxtlar,"}, {"id": 86891, "identifier": "az.musayev", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5981, "text": "d\u00fcz\u00fclm\u00fc\u015f piyal\u0259l\u0259r,"}, {"id": 86892, "identifier": "az.musayev", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5982, "text": "yan-yana qoyulmu\u015f yast\u0131qlar,"}, {"id": 86893, "identifier": "az.musayev", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musa