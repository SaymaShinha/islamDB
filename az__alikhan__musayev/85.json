[{"id": 81108, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5910, "text": "And olsun b\u00fcrcl\u00fc g\u00f6y\u0259!"}, {"id": 81112, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5911, "text": "And olsun v\u0259d\u0259 verilmi\u015f g\u00fcn\u0259!"}, {"id": 86822, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5912, "text": "And olsun \u015fahidlik ed\u0259n\u0259 v\u0259 edil\u0259n\u0259!"}, {"id": 86823, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5913, "text": "M\u0259hv olsun x\u0259nd\u0259k k\u0259nar\u0131nda y\u0131\u011f\u0131\u015fanlar!"}, {"id": 86824, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Odunla al\u0131\u015fd\u0131r\u0131lm\u0131\u015f x\u0259nd\u0259k."}, {"id": 86825, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5915, "text": "Onlar o x\u0259nd\u0259yin q\u0131ra\u011f\u0131nda oturub,"}, {"id": 86826, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5916, "text": "m\u00f6minl\u0259r\u0259 \u00f6z verdikl\u0259ri i\u015fg\u0259nc\u0259l\u0259r\u0259 \u015fahidlik edirdil\u0259r."}, {"id": 86827, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5917, "text": "Kafirl\u0259r onlardan yaln\u0131z Q\u00fcdr\u0259tli, T\u0259rif\u0259layiq Allaha iman g\u0259tirdikl\u0259rin\u0259 g\u00f6r\u0259 intiqam al\u0131rd\u0131lar."}, {"id": 86828, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Halbuki, g\u00f6yl\u0259rd\u0259 v\u0259 yerd\u0259 m\u00fctl\u0259q hakimiyy\u0259t Ona m\u0259xsusdur. Allah h\u0259r \u015fey\u0259 \u015eahiddir."}, {"id": 86829, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5919, "text": "O k\u0259sl\u0259r ki, m\u00f6min ki\u015fi v\u0259 qad\u0131nlar\u0131 yand\u0131rmaqla s\u0131na\u011fa \u00e7\u0259kmi\u015f, sonra da t\u00f6vb\u0259 etm\u0259mi\u015fl\u0259r, onlara C\u0259h\u0259nn\u0259m \u0259zab\u0131 v\u0259 onlar\u0131 yand\u0131r\u0131b-yaxacaq od \u0259zab\u0131 haz\u0131rlanm\u0131\u015fd\u0131r."}, {"id": 86830, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5920, "text": "\u015e\u00fcbh\u0259siz ki, iman g\u0259tirib yax\u015f\u0131 i\u015fl\u0259r g\u00f6r\u0259nl\u0259r \u00fc\u00e7\u00fcn a\u011faclar\u0131 alt\u0131ndan \u00e7aylar axan C\u0259nn\u0259t ba\u011flar\u0131 vard\u0131r. Bu, b\u00f6y\u00fck m\u00fcv\u0259ff\u0259qiyy\u0259tdir."}, {"id": 86831, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Do\u011frudan da, R\u0259bbinin g\u00fcnahkarlar\u0131 yaxalamas\u0131 \u00e7ox \u015fidd\u0259tlidir!"}, {"id": 86832, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5922, "text": "H\u0259qiq\u0259t\u0259n, O, h\u0259r \u015feyi ilkin yarad\u0131r v\u0259 onu yenid\u0259n b\u0259rpa edir!"}, {"id": 86833, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Ba\u011f\u0131\u015flayan da, Sev\u0259n d\u0259 Odur!"}, {"id": 86834, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5924, "text": "\u018fr\u015fin sahibidir, \u015ean-\u015f\u00f6hr\u0259tlidir!"}, {"id": 86835, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5925, "text": "\u0130st\u0259diyini ed\u0259ndir."}, {"id": 86836, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Qo\u015funlar haqq\u0131nda \u0259hvalat s\u0259n\u0259 \u00e7at\u0131bm\u0131?"}, {"id": 86837, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5927, "text": "Firon v\u0259 S\u0259mud qo\u015funlar\u0131n\u0131n."}, {"id": 86838, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Xeyr, k\u00fcfr ed\u0259nl\u0259r haqq\u0131 yalan saymaqdad\u0131rlar."}, {"id": 86839, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5929, "text": "Allah onlar\u0131 arxadan \u0259hat\u0259 etmi\u015fdir."}, {"id": 86840, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5930, "text": "B\u0259li, bu \u015fanl\u0131 Qurand\u0131r."}, {"id": 86841, "identifier": "az.musayev", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "az", "name": "Musayev", "transelator_en_name": "Alikhan Musayev", "format": "text", "type": "translation", "ayah_number": 5931, "text": "O, m\u00fchafiz\u0259 edil\u0259n l\u00f6vh\u0259d\u0259dir."}]