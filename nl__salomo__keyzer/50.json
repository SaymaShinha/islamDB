[{"id": 409840, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4631, "text": "Kaf Bij den glorierijken Koran;"}, {"id": 409842, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4632, "text": "Waarlijk, gij verwondert u, dat een prediker uit hun midden, tot hen is gekomen, en de ongeloovigen zeggen: Dit is eene wonderlijke zaak."}, {"id": 409843, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4633, "text": "Nadat wij dood en tot stof zullen wezen, zullen wij in het leven terugkeeren. Dit is een ver van de gedachte verwijderde terugkeer."}, {"id": 409844, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4634, "text": "Nu weten wij wat de aarde van hen verteert, en wij bezitten een boek dat ons daarvan onderricht."}, {"id": 409845, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4635, "text": "Maar zij beladen de waarheid met leugen, nadat de eerste tot hen is gekomen; daarom zijn zij in een verwarde zaak gestort"}, {"id": 409846, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4636, "text": "Zien zij niet op, tot den hemel boven hen; en overwegen zij niet, hoe wij dien verheven en opgetooid hebben, en dat daarin geene gebreken zijn?"}, {"id": 409847, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4637, "text": "Wij hebben ook de aarde uitgespreid, en daarop vastgewortelde bergen geworpen, en wij doen elke schoone soort van planten daarop voortspruiten."}, {"id": 409848, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4638, "text": "Als een onderwerp ter overweging, en eene vermaning voor iederen mensch, die zich tot ons wendt."}, {"id": 409849, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4639, "text": "En wij zenden den regen als eene zegening van den hemel neder; waardoor wij tuinen doen voortspruiten en het graan dat men oogst."}, {"id": 409850, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4640, "text": "En de rijzige palmboomen met takken vol dadels, die boven elkander hangen."}, {"id": 409851, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4641, "text": "Als een voorraad voor den mensch. Wij verkwikken daardoor een dood land; zoo zal de opstanding der dooden uit hunne graven wezen."}, {"id": 409852, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4642, "text": "Het volk van Noach, en zij die te Al Rass woonden, en Thamoed en Ad en Pharao beschuldigden de profeten van bedrog voor de bewoners van Mekka."}, {"id": 409853, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4643, "text": "Alsmede de broeders van Loth"}, {"id": 409854, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4644, "text": "En de bewoners van het woud nabij Midian en het volk van Tobba; die allen beschuldigden de profeten van bedrog; daarom werden de vonnissen, waarmede ik dreigde, hun rechtvaardig opgelegd."}, {"id": 409855, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4645, "text": "Is onze kracht door de eerste schepping uitgeput? daar zij verbaasd zijn, omdat hun eene nieuwe schepping is voorgesteld: namelijk de opwekking der dooden."}, {"id": 409856, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4646, "text": "Wij schiepen den mensch en weten wat zijne ziel hem influistert, en wij zijn hem nader dan zijne strotader."}, {"id": 409857, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4647, "text": "Als de twee engelen welke hij afvaardigt, om rekenschap te vragen van het gedrag van een mensch, dit verrichten, terwijl de een aan de rechter- en de andere aan de linkerhand zit."}, {"id": 409858, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4648, "text": "Uit hij niet een woord, of er is een bespieder bij hem, gereed om het op te schrijven."}, {"id": 409859, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4649, "text": "En de bewusteloosheid des doods zal in waarheid komen: dat is, o mensch! wat gij getracht hebt te ontgaan."}, {"id": 409860, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4650, "text": "En de trompet zal klinken: dit zal de dag zijn, waarmede gedreigd werd."}, {"id": 409861, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4651, "text": "En iedere ziel zal komen; en bij haar zal een geleider en een getuige zijn."}, {"id": 409862, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4652, "text": "En de eerste zal tot den ongeloovige zeggen: Gij waart vroeger achteloos omtrent dezen dag; maar wij hebben uwen sluier van u afgenomen, en uw gezicht is heden helderziende geworden."}, {"id": 409863, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4653, "text": "En zijne makkers zullen zeggen: Dit is wat gereed is, verklaard te worden."}, {"id": 409864, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4654, "text": "En God zal zeggen: Werp elken ongeloovige en ieder verdorven mensch in de hel."}, {"id": 409865, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4655, "text": "En iedereen die het goede verbood, en iederen zondaar en twijfelaar omtrent het geloof;"}, {"id": 409866, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4656, "text": "Die een anderen god naast den waren God oprichtte. Doe hem eene gestrenge marteling ondergaan."}, {"id": 409867, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4657, "text": "Zijn makker zal zeggen: O Heer! ik heb hem niet verleid; maar hij verkeerde in eene groote dwaling."}, {"id": 409868, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4658, "text": "God zal zeggen: Twist niet in mijne tegenwoordigheid, nu ik u vooraf heb bedreigd met de martelingen, welke gij thans voor u ziet gereed gemaakt."}, {"id": 409869, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4659, "text": "Het vonnis is niet bij mij veranderd; even weinig behandel ik mijne dienaren onrechtvaardig."}, {"id": 409821, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4660, "text": "Op dien dag zullen wij tot de hel zeggen: Zijt gij vol? en zij zal antwoorden: Moet er nog iets bijgevoegd worden?"}, {"id": 409841, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4661, "text": "En het paradijs zal den vromen nader gebracht worden."}, {"id": 409870, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4662, "text": "En men zal tot hen zeggen: Dat is wat u beloofd werd; en een iedereen die zich tot God wendde, en zijne bevelen volgde;"}, {"id": 409871, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4663, "text": "Die den Barmhartige heimelijk vreesde, en met een bekeerd hart tot hem kwam."}, {"id": 409872, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4664, "text": "Treedt hier in vrede binnen; dit is de dag der eeuwigheid."}, {"id": 409873, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4665, "text": "Daar zullen zij alles hebben wat zij begeeren, en daar zal eene meer dan overvloedige bijvoeging van gelukzaligheid met ons zijn."}, {"id": 409874, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4666, "text": "Hoevele geslachten hebben wij v\u00f3\u00f3r de bewoners van Mekka verdelgd, die machtiger dan zij in sterkte waren? Trekt daarom door de streken der aarde, en ziet of er eene schuilplaats tegen mijne wraak is."}, {"id": 409875, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4667, "text": "Waarlijk, hierin is eene vermaning voor hem, die een hart heeft om te begrijpen, of zijn oor leent, en met een aandachtig gemoed aanwezig is."}, {"id": 409876, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4668, "text": "Wij schiepen de hemelen en de aarde en alles wat er tusschen is, in zes dagen, en geene vermoeienis bereikte ons."}, {"id": 409877, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4669, "text": "Verdraag dus geduldig wat zij zeggen, en verkondig den lof van uwen Heer voor zonsopgang en voor zonsondergang."}, {"id": 409878, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4670, "text": "Prijs hem in een gedeelte van den nacht, en volbreng de toegevoegde deelen der vereering."}, {"id": 409879, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4671, "text": "Luister naar den dag, waarop de uitroeper de menschen, van eene nabijgelegen plaats, tot het oordeel zal oproepen."}, {"id": 409880, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4672, "text": "De dag, waarop zij den klank der trompet in waarheid zullen hooren, zal de dag zijn, waarop de menschen uit hunne graven zullen voortkomen."}, {"id": 409881, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4673, "text": "Wij geven leven en doen sterven. Tot ons zullen alle schepselen terugkeeren."}, {"id": 409882, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4674, "text": "De dag, waarop de aarde plotseling zal worden gespleten, zal de dag der verzameling zijn. Het is gemakkelijk voor ons, hen te verzamelen."}, {"id": 409883, "identifier": "nl.keyzer", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4675, "text": "Wij weten wel, wat de ongeloovigen zeggen; en gij zijt niet gezonden, om hen met geweld tot het geloof te dwingen. Waarschuw dus door den Koran hem, die mijne bedreiging vreest."}]