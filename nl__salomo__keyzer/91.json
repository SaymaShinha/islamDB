[{"id": 411196, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6044, "text": "Ik zweer bij de zon en haren opgaanden glans,"}, {"id": 411197, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6045, "text": "Bij de maan, als zij deze volgt,"}, {"id": 411198, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6046, "text": "Bij den dag, als hij zijn glans vertoont,"}, {"id": 411199, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6047, "text": "Bij den nacht, als die alles met duisternis bedekt;"}, {"id": 411200, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6048, "text": "Bij den hemel en bij Hem, die dien heeft gebouwd,"}, {"id": 411201, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6049, "text": "Bij de aarde en bij Hem die haar uitspreidde,"}, {"id": 411202, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6050, "text": "Bij de ziel en bij Hem die haar volkomen vormde,"}, {"id": 411203, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6051, "text": "En haar het vermogen van onderscheiding ingaf, en de macht, tusschen zonde en godsvrucht te kiezen."}, {"id": 411204, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6052, "text": "Hij die haar zuiver bewaart, is gelukkig:"}, {"id": 411205, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6053, "text": "Maar hij die haar heeft verdorven, is ellendig."}, {"id": 411206, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6054, "text": "Thamoed beschuldigde hunnen profeet Saleh van bedrog, door de groote mate hunner zonden."}, {"id": 411207, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6055, "text": "Toen de meest verdorvene onder hen werd gezonden, om den wijfjes-kameel te dooden."}, {"id": 411208, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6056, "text": "En Gods gezant tot hen zeide: Laat Gods (wijfjes-)kameel met vrede en verhindert haar niet te drinken."}, {"id": 411209, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6057, "text": "Maar zij beschuldigden hem van bedrog, en doodden het dier. Daarom verdelgde hun Heer hen, om hunne misdaad, en hij maakte hunne straf voor hen allen gelijk,"}, {"id": 411210, "identifier": "nl.keyzer", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6058, "text": "En hij vreest daarvan de gevolgen niet."}]