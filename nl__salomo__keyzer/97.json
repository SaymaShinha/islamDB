[{"id": 411267, "identifier": "nl.keyzer", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6126, "text": "Waarlijk, wij hebben den Koran in den nacht van al Kadr nedergezonden."}, {"id": 411268, "identifier": "nl.keyzer", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6127, "text": "En wat zal u doen begrijpen, hoe uitstekend de nacht van al Kadr is?"}, {"id": 411269, "identifier": "nl.keyzer", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6128, "text": "Beter dan duizend maanden."}, {"id": 411270, "identifier": "nl.keyzer", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_t