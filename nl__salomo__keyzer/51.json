[{"id": 409884, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Bij de winden, die het stof verspreiden en verstrooien."}, {"id": 409885, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4677, "text": "En bij de wolken, die een last van regen dragen;"}, {"id": 409886, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4678, "text": "Bij de schepen, die de zee snel doorklieven."}, {"id": 409887, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4679, "text": "En bij de engelen, die dingen uitdeelen, noodig voor het onderhoud van alle schepselen"}, {"id": 409888, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Inderdaad, datgene waarmede gij bedreigd zijt, is zekerlijk waar,"}, {"id": 409889, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4681, "text": "En het laatste oordeel zal gewis komen."}, {"id": 409890, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4682, "text": "Bij den hemel met paden voorzien."}, {"id": 409891, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Gij verschilt zeer in hetgeen gij zegt."}, {"id": 409892, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Men zal zich afwenden van dengeen, die van het ware geloof is afgekeerd!"}, {"id": 409893, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Vervloekt mogen de leugenaars zijn."}, {"id": 409894, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4686, "text": "Die in diepe wateren van onwetendheid waden, terwijl zij hun heil verwaarloozen."}, {"id": 409895, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Zij vragen: Wanneer zal de dag des oordeels komen?"}, {"id": 409896, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4688, "text": "Op dien dag zullen zij in het hellevuur verbrand worden."}, {"id": 409897, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4689, "text": "En men zal tot hen zeggen: Proeft uwe straf; dit is hetgeen gij verlangd hebt, dat verhaast zou worden."}, {"id": 409898, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Maar de vromen zullen tusschen tuinen en fonteinen wonen."}, {"id": 409899, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4691, "text": "Datgene ontvangende, wat hun Heer hun zal geven, omdat zij v\u00f3\u00f3r dezen dag rechtvaardigen waren."}, {"id": 405370, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4692, "text": "Zij slapen slechts gedurende een klein gedeelte van den nacht."}, {"id": 405371, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4693, "text": "En vroeg in den ochtend vragen zij vergiffenis van God."}, {"id": 409900, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Een voegzaam deel van hunne welvaart werd hem gegeven, die vroeg, en aan hem, die door schaamte teruggehouden werd te vragen."}, {"id": 409901, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4695, "text": "Er zijn teekenen van goddelijke macht en goedheid op de aarde, voor de menschen van goed begrip."}, {"id": 409902, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4696, "text": "Ook in u zelven: zult gij dus niet overwegen?"}, {"id": 409903, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4697, "text": "Uw onderhoud is in den hemel; en evenzeer bevat hij datgene, wat u werd beloofd."}, {"id": 409904, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4698, "text": "Daarom zweer ik bij den Heer van hemel en aarde, dat dit zekerlijk de waarheid is; overeenkomstig datgene, wat gij zelf zegt."}, {"id": 409905, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Is de geschiedenis van Abraham's geachte gasten niet tot uwe kennis gekomen?"}, {"id": 409906, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Toen zij tot hem ingingen en zeiden: Vrede? antwoordde hij: Vrede! bij zich zelven zeggende: Dit zijn onbekende menschen."}, {"id": 409907, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4701, "text": "En hij ging heimelijk tot zijn gezin, en bracht een gemest kalf."}, {"id": 409908, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4702, "text": "Hij zette het voor hen neder, en toen hij zag, dat zij het niet aanraakten, zeide hij: Eet gij niet?"}, {"id": 409909, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4703, "text": "En hij begon vrees voor hen te koesteren. Zij zeiden: Vrees niet, en zij verklaarden hem de belofte van een wijzen zoon."}, {"id": 409910, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Zijne vrouw kwam nader; zij gaf een gil, sloeg zich in het aangezicht, en zeide ik ben een oude vrouw en onvruchtbaar!"}, {"id": 409911, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4705, "text": "De engelen zeiden: Dit zeide uw Heer; en waarlijk, hij is de Wijze, de Alwetende."}, {"id": 409912, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4706, "text": "En Abraham zeide tot hen: wat is dus uwe boodschap, o gezanten van God?"}, {"id": 409913, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Zij antwoordden: waarlijk, wij worden tot een zondig volk gezonden."}, {"id": 409914, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4708, "text": "Opdat wij steenen van gebakken klei op hen zouden nederzenden."}, {"id": 409915, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4709, "text": "Gemerkt door uwen Heer, ter verdelging der zondaren."}, {"id": 409916, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4710, "text": "En wij telden de ware geloovigen, die in de stad waren."}, {"id": 409917, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4711, "text": "Maar wij vonden niet meer, dan \u00e9\u00e9n gezin van Moslems."}, {"id": 409918, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Wij verwoesten hen, en lieten een teeken aldaar, voor hen, die de ernstige kastijding van God vreezen."}, {"id": 409919, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4713, "text": "In Mozes was mede een teeken, toen Hij hem met duidelijke macht tot Pharao zond."}, {"id": 409920, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Maar deze wendde zich met zijne vorsten af, zeggende: Deze man is een toovenaar of een bezetene."}, {"id": 409921, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Daarom grepen wij hem en zijne soldaten en wierpen hen in de zee: en hij was waard gestrafd te worden."}, {"id": 409922, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4716, "text": "En in den stam van Ad was mede een teeken, toen wij een verwoestenden wind tegen hen zonden."}, {"id": 409923, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4717, "text": "Die niets aanraakte, waar hij nederkwam, of hij verwoeste het, als een verrot voorwerp, en maakte het tot stof."}, {"id": 409924, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4718, "text": "In Thamoed was eveneens een teeken toen er tot hem werd gezegd: Geniet alles gedurende eenigen tijd."}, {"id": 409925, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Maar zij schonden onbeschaamd het bevel van hunnen Heer, waardoor hen een vreeselijk onweder van den hemel overviel, terwijl zij daarheen blikten."}, {"id": 409926, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4720, "text": "Zij waren niet in staat op hunne voeten te staan, evenmin als zij zich van de verdediging konden redden."}, {"id": 409927, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4721, "text": "En het volk van Noach verdelgden wij voor dezen; want het was een volk, dat vreeselijk zondigde."}, {"id": 409928, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4722, "text": "Wij hebben den hemel met macht gebouwd, en dien eene groote uitgebreidheid gegeven."}, {"id": 409929, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Wij hebben de aarde daaronder uitgebreid, en hoe gelijkmatig hebben wij dit gedaan."}, {"id": 409930, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4724, "text": "En van alle dingen hebben wij twee soorten geschapen, opdat gij wellicht zoudt overwegen."}, {"id": 409931, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4725, "text": "Vlucht dus tot God; waarlijk, ik ben een openlijk waarschuwer van Hem onder u."}, {"id": 409932, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Aanbidt geene andere goden behalve uwen Heer. Ik bericht u dit duidelijk uit zijn naam"}, {"id": 409933, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4727, "text": "Op dezelfde wijze kwam er geen gezant tot hunne voorgangers of zij zeiden: Deze man is een toovenaar of een bezetene."}, {"id": 409934, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Hebben zij dit gedrag achtervolgens elkander als erfdeel vermaakt? Ja; zij zondigen vreeselijk."}, {"id": 409935, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4729, "text": "Houdt u dus van hen af, en gij zult vrij van blaam zijn, indien gij aldus handelt."}, {"id": 409936, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Maar ga voort met vermanen; want vermaning is den waren geloovigen van voordeel."}, {"id": 409937, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Ik heb de geniussen en menschen met geen ander doel geschapen, dan opdat zij mij zouden dienen."}, {"id": 409938, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4732, "text": "Ik eisch geenerlei onderhoud van hen; evenmin verlang ik, dat zij mij zullen voeden."}, {"id": 409939, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Waarlijk, God is degene, die alle schepselen voorziet, en die een aanzienlijke macht bezit."}, {"id": 409940, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Aan hen die onzen gezant beleedigden, zal een deel gegeven worden, gelijk aan het deel van hen, die zich in vroegere tijden, evenals zij hebben gedragen; en zij zullen niet wenschen, dat dit verhaast worde."}, {"id": 409941, "identifier": "nl.keyzer", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Wee dus over de ongeloovigen, om hunnen dag, waarmede zij zijn bedreigd!"}]