[{"id": 410053, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4847, "text": "Het uur des oordeels nadert en de maan is gespleten."}, {"id": 410054, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Maar als de ongeloovigen een teeken zien, wenden zij zich af, zeggende: dit is eene machtige betoovering."}, {"id": 410055, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4849, "text": "En zij beschuldigen u, o Mahomet! van bedrog, en volgen hunne eigene lusten: maar ieder ding zal onveranderlijk bepaald wezen."}, {"id": 410056, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4850, "text": "En nu is eene zending tot hen gekomen, waarin eene afschrikking voor hardnekkig ongeloof ligt opgesloten."}, {"id": 410057, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4851, "text": "Deze wijsheid is volkomen; maar waarschuwers helpen bij hen niet."}, {"id": 410058, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Wend u dus van hen af! Den dag waarop de dagvaardende engel den mensch tot eene verschrikkelijke zaak zal oproepen."}, {"id": 410059, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4853, "text": "Zullen zij met nedergeslagen blikken uit hunne graven komen, talrijk, als verspreide sprinkhanen."}, {"id": 410060, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4854, "text": "Zich met schrik naar den dagvaarder spoedende. De ongeloovigen zullen zeggen: Dit is een dag van droefheid."}, {"id": 410061, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4855, "text": "Het volk van Noach beschuldigde dien profeet, alvorens uw volk u verwierp, het beschuldigde onzen dienaar van bedrog; zeggende: Hij is een bezetene, en hij werd met verwijtingen verworpen."}, {"id": 410062, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4856, "text": "Hij riep daarom zijn Heer aan, zeggende: Waarlijk, ik ben overweldigd: wreek mij dus."}, {"id": 410063, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4857, "text": "Daarop openden wij de poorten des hemels, waaruit het water stroomde."}, {"id": 410064, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4858, "text": "Wij deden de aarde waterstralen uitwerpen, zoodat het water van hemel en aarde zich vereenigde, overeenkomstig het vastgestelde besluit."}, {"id": 410065, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4859, "text": "Wij droegen hem, op een schip, uit planken en spijkers samengesteld."}, {"id": 410066, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4860, "text": "Dat zich voor onze oogen voortbewoog, als eene belooning voor hem, die ondankbaar was verworpen."}, {"id": 410067, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4861, "text": "Wij lieten dat schip tot een teeken dienen. Maar is iemand daardoor gewaarschuwd?"}, {"id": 410068, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4862, "text": "En hoe gestreng was mijne wraak en mijne bedreiging!"}, {"id": 410069, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4863, "text": "Nu hebben wij den Koran gemakkelijk tot eene waarschuwing gemaakt; maar is iemand daardoor gewaarschuwd?"}, {"id": 410070, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4864, "text": "De stam van Ad beschuldigde hunnen profeet van bedrog; maar hoe ernstig was mijne wraak en mijne bedreiging!"}, {"id": 410071, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4865, "text": "Waarlijk, wij zonden, op een dag van voortdurend ongeluk een brullenden wind tegen hen."}, {"id": 410072, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4866, "text": "Die de menschen wegvoerde, als waren zij met kracht uitgescheurde wortels van palmboomen."}, {"id": 410073, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4867, "text": "En hoe ernstig was mijne wraak en mijne bedreiging!"}, {"id": 410074, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4868, "text": "Thans hebben wij den Koran gemakkelijk ter waarschuwing gemaakt; maar is iemand daardoor gewaarschuwd?"}, {"id": 410075, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4869, "text": "Die van Thamoed beschuldigden de vermaningen van hunnen profeet van valschheid."}, {"id": 410076, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4870, "text": "En zij zeiden: Zullen wij een enkel man als wij, onder ons volgen? Waarlijk, wij zouden aan dwaling en ongerijmde dwaasheid schuldig zijn."}, {"id": 410077, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4871, "text": "Zou de taak van waarschuwing hem, boven het overige gedeelte van ons, opgedragen zijn? Neen, hij is een leugenaar en een onbeschaamde bedrieger."}, {"id": 410078, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4872, "text": "Maar God zeide tot Saleh: Morgen zullen zij weten wie een leugenaar en een onbeschaamde is;"}, {"id": 410079, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4873, "text": "Want wij zullen zekerlijk de wijfjes-kameel zenden, om hen te beproeven; en gij, sla hen gade, en verdraag hunne beleedigingen met geduld."}, {"id": 410080, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4874, "text": "Voorspel hun, dat het water der putten tusschen hen zal worden verdeeld, en ieder deel zal beurtelings nedergezet worden."}, {"id": 410081, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4875, "text": "Zij riepen hunnen makker, en hij nam een zwaard en doodde haar,"}, {"id": 410082, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4876, "text": "Maar hoe ernstig was mijne wraak en mijne bedreiging!"}, {"id": 410083, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4877, "text": "Want wij zonden hun een enkelen kreet van den engel Gabri\u00ebl te gemoet, en zij werden als de droge stokken, die gebruikt worden door dengeen, welke een kooi voor het vee bouwt."}, {"id": 410084, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4878, "text": "En thans hebben wij den Koran gemakkelijk ter waarschuwing gemaakt; maar is iemand daardoor gewaarschuwd?"}, {"id": 410085, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Het volk van Lot beschuldigde zijne prediking van valschheid."}, {"id": 410086, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4880, "text": "Maar wij zonden een wind tegen hen, die eene regenbui van steenen voortdreef, welke hen allen verdelgde, behalve het gezin van Lot, dat wij vroeg in den ochtend bevrijdden."}, {"id": 410087, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4881, "text": "Dit was door onze gunst. Zoo beloonen wij hen, die dankbaar zijn."}, {"id": 410088, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4882, "text": "En Lot had hen gewaarschuwd voor onze gestrenge kastijding; maar zij twijfelden aan die waarschuwing."}, {"id": 410089, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4883, "text": "Zij eischten zijne gasten, opdat zij hen zouden misbruiken; maar wij staken hunne oogen uit, zeggende: Proeft mijne wraak en mijne bedreiging."}, {"id": 410090, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4884, "text": "En vroeg in den ochtend verraste hen eene zware straf."}, {"id": 410091, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4885, "text": "Proeft dus mijne wraak en mijne bedreiging."}, {"id": 410092, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4886, "text": "Thans hebben wij den Koran gemakkelijk ter waarschuwing, gemaakt; maar is iemand daardoor gewaarschuwd?"}, {"id": 410093, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4887, "text": "De vermaning van Mozes kwam mede tot het volk van Pharao,"}, {"id": 410094, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4888, "text": "Maar zij beschuldigden al onze teekenen van bedrog; daarom kastijdden wij hem met eene machtige en onwederstaanbare kastijding."}, {"id": 410095, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4889, "text": "O bewoners van Mekka! zijn uwe ongeloovigen beter dan deze? Is u in de schriften vrijstelling van straf beloofd?"}, {"id": 410096, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4890, "text": "Zeggen zij: wij vormen een lichaam van menschen, die in staat zijn onze vijanden te bemeesteren?"}, {"id": 410097, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4891, "text": "De menigte zal zekerlijk op de vlucht worden gejaagd en zij zullen hunne ruggen omkeeren."}, {"id": 410098, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4892, "text": "Maar het uur des oordeels is hun bedreigde straftijd, en dat uur zal droeviger en bitterder zijn, dan hunne droefheden in dit leven."}, {"id": 410099, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4893, "text": "Waarlijk, de zondaar doolt in dwaling rond, en zal hier namaals in brandende vlammen worden gemarteld."}, {"id": 410100, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4894, "text": "Op dien dag zullen zij met hunne aangezichten in het vuur worden geworpen, en men zal hun zeggen: Proeft de aanraking der hel."}, {"id": 410101, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4895, "text": "Alle dingen hebben wij geschapen, aan een bepaald besluit gebonden."}, {"id": 410102, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4896, "text": "En ons bevel bestaat slechts in een enkel woord, aan een oogwenk gelijk."}, {"id": 410103, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4897, "text": "Wij hebben vroeger volken verdelgd, die u gelijk waren; maar is iemand uwer door hun voorbeeld gewaarschuwd?"}, {"id": 410104, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4898, "text": "Alles wat gij doet, is in het boek vermeld, dat door de wakende engelen wordt bewaard."}, {"id": 410105, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4899, "text": "Elke daad, klein of groot, is op de welbewaarde tafel nedergeschreven."}, {"id": 410106, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4900, "text": "De vromen zullen echter te midden van tuinen en meren wonen."}, {"id": 410107, "identifier": "nl.keyzer", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 4901, "text": "In de vergadering der waarheid, in tegenwoordigheid van den machtigsten koning."}]