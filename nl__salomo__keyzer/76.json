[{"id": 410768, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5592, "text": "Ging er niet eene groote tijdruimte over den mensch, gedurende welke hij eene nietswaardige zaak was?"}, {"id": 410769, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5593, "text": "Waarlijk, wij hebben den mensch geschapen uit het gemengde zaad van beide seksen, opdat wij hem zouden beproeven, en wij hebben hem doen hooren en zien."}, {"id": 405912, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5594, "text": "Wij hebben hem zeker op den weg geleid, of hij dankbaar, dan wel ondankbaar zou zijn."}, {"id": 410770, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5595, "text": "Waarlijk, wij hebben voor de ongeloovigen ketenen en halskragen en brandend vuur gereed gemaakt."}, {"id": 410771, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5596, "text": "Maar de rechtvaardigen zullen uit een beker wijn drinken, gemengd met het water van Ca oer."}, {"id": 410772, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5597, "text": "Eene fontein waarvan Gods dienaren zullen drinken: zij zullen die door kanalen leiden (werwaarts het hun behaagt)."}, {"id": 410773, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5598, "text": "Deze vervullen hunne gelofte en vreezen den dag, waarvan rampen zich zeer ver uitstrekken."}, {"id": 410774, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5599, "text": "Zij, schoon zelven nooddruftig, geven voedsel aan de armen, aan den wees en aan den balling voor zijne zaak."}, {"id": 410775, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5600, "text": "Zeggende: Wij voeden u alleen voor Gods zaak; wij begeeren belooning noch dankzegging van u."}, {"id": 410776, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5601, "text": "Waarlijk, wij vreezen van onzen Heer een schrikbarenden en rampvollen dag."}, {"id": 410777, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5602, "text": "Daarom zal God hen van de ramp van dien dag bevrijden, en hij zal hunne voorhoofden doen schitteren, en hun vreugde geven."}, {"id": 410778, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5603, "text": "En hij zal hun voor hunne geduldige volharding beloonen, met een tuin en zijden kleederen."}, {"id": 410779, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5604, "text": "Daarin zullen zij op zetels rusten; zij zullen daar zon noch maan zien."}, {"id": 410780, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5605, "text": "De schaduwen der boomen zullen zich over hen uitspreiden, en de vruchten daarvan zullen laag nederkomen, zoodat die gemakkelijk zullen kunnen worden ingezameld."}, {"id": 410781, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5606, "text": "Hunne dienaren zullen bij hen rondgaan met zilveren vaatwerk en bekers."}, {"id": 410782, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5607, "text": "De flesschen zullen flesschen van zilver zijn (blinkend als glas); zij zullen de maat daarvan bepalen."}, {"id": 410783, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5608, "text": "Daar zal hun te drinken worden gegeven, uit bekers (wijn), gemengd met het water van Zedjebil"}, {"id": 410784, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5609, "text": "Uit eene fontein in het paradijs, Selsebil genaamd."}, {"id": 410785, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5610, "text": "En kinderen die eeuwig jong zullen blijven zullen rondgaan om hen te bedienen; als gij hen ziet, zult gij denken dat zij verspreide paarlen zijn."}, {"id": 410786, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5611, "text": "En als gij dit ziet, zult gij geneugten aanschouwen en een groot koninkrijk."}, {"id": 410787, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5612, "text": "Zij zullen bedekt zijn met kleederen van fijne, groene zijde en van gouden weefsels, en zij zullen versierd zijn met zilveren armbanden, en hun Heer zal hun van het zuiverste vocht te drinken geven."}, {"id": 410788, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5613, "text": "(Hij zal tot hen zeggen:) Waarlijk, dit is uwe belooning, en uwe pogingen zijn dankbaar aangenomen."}, {"id": 410789, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5614, "text": "Waarlijk, wij hebben u den Koran door eene (trapsgewijze) openbaring nedergezonden."}, {"id": 410790, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5615, "text": "Wacht dus geduldig het oordeel van uwen Heer af, en geloof geen zondaar of ongeloovige onder hen."}, {"id": 410791, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5616, "text": "Gedenk den naam van uwen Heer, des ochtends en des avonds."}, {"id": 410792, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5617, "text": "En aanbid hem gedurende (een deel van den nacht); en prijs hem gedurende een groot deel des nachts."}, {"id": 410793, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5618, "text": "Waarlijk, deze menschen beminnen het voorbijgaande leven, en veronachtzamen den zwaren dag des oordeels."}, {"id": 410794, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5619, "text": "Wij hebben hen geschapen en hunne ledematen gesterkt, en als het ons behaagt, stellen wij anderen aan hen gelijk, in hunne plaats."}, {"id": 410795, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5620, "text": "Waarlijk, dit is eene waarschuwing; en hij die wil, kieze den weg tot zijn Heer."}, {"id": 410796, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5621, "text": "Maar gij zult niet willen tenzij God wil; want God is alwetend en wijs"}, {"id": 410797, "identifier": "nl.keyzer", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5622, "text": "Hij leidt in zijne genade die hem behagen; maar voor den onrechtvaardige heeft hij eene gestrenge straf gereed gemaakt."}]