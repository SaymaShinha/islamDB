[{"id": 411280, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6139, "text": "Als de aarde door een aardbeving zal geschud worden,"}, {"id": 411281, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6140, "text": "En de aarde haren last zal wegwerpen."}, {"id": 411282, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6141, "text": "En de mensch zal zeggen: Wat schort haar?"}, {"id": 411283, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6142, "text": "Op dien dag zal zij hare tijdingen verklaren."}, {"id": 411284, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6143, "text": "Volgens hetgeen uw Heer haar zal ingeven."}, {"id": 411285, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6144, "text": "Op dien dag zullen de menschen in onderscheiden klassen voorwaarts gaan, om hunne werken te aanschouwen."}, {"id": 411286, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6145, "text": "En wie slechts goed zal gedaan hebben, ter zwaarte van een atoom, zal dat aanschouwen."}, {"id": 411287, "identifier": "nl.keyzer", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 6146, "text": "En wie slechts kwaad zal gedaan hebben ter zwaarte van een atoom, zal dat aanschouwen."}]