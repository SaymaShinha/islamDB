[{"id": 411131, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5968, "text": "Heeft het nieuws van den overvallenden dag des oordeels u bereikt."}, {"id": 411132, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5969, "text": "Die sommigen de aangezichten zal doen buigen?"}, {"id": 411133, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5970, "text": "Werkende en afgemat van vermoeienis."}, {"id": 411134, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5971, "text": "Zullen zij in het gloeiende vuur geworpen worden, om geroosterd te worden."}, {"id": 411135, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5972, "text": "Men zal hun uit eene kokende fontein geven te drinken."}, {"id": 411136, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5973, "text": "Zij zullen geen voedsel hebben, dan droge doornen en distels (al Dari)."}, {"id": 411137, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5974, "text": "Dat voeden, noch den honger stillen zal."}, {"id": 411138, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5975, "text": "Maar de aangezichten van anderen zullen op dien dag vroolijk zijn."}, {"id": 411139, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5976, "text": "Voldaan over hetgeen zij vroeger zullen hebben verricht."}, {"id": 411140, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5977, "text": "Zij zullen in een sierlijken tuin worden geplaatst."}, {"id": 411141, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5978, "text": "Waar zij geene ijdele gesprekken zullen hooren."}, {"id": 411142, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5979, "text": "Daar zal eene springende fontein wezen;"}, {"id": 411143, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5980, "text": "Daar zullen verheven zetels opgericht wezen."}, {"id": 411144, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5981, "text": "En bekers zullen voor hen geplaatst zijn,"}, {"id": 411145, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5982, "text": "En kussens in orde gelegd,"}, {"id": 411146, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5983, "text": "En tapijten geheel uitgespreid."}, {"id": 411147, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5984, "text": "Overwegen zij niet hoe de kameelen geschapen zijn"}, {"id": 411148, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5985, "text": "En hoe de hemel verheven is;"}, {"id": 411149, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5986, "text": "En hoe de bergen zijn bevestigd."}, {"id": 411150, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5987, "text": "En hoe de aarde is uitgespreid?"}, {"id": 411151, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5988, "text": "Daarom, waarschuw uw volk, want gij zijt slechts een waarschuwer,"}, {"id": 411152, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5989, "text": "Eene onbepaalde macht is u niet over hen opgedragen."}, {"id": 411153, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5990, "text": "Maar wie zich afwenden, en niet gelooven zal,"}, {"id": 411154, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5991, "text": "Dien zal God met de groote straf van het volgende leven straffen."}, {"id": 411155, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5992, "text": "Waarlijk, tot ons zullen zij terugkeeren."}, {"id": 411156, "identifier": "nl.keyzer", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "nl", "name": "Keyzer", "transelator_en_name": "Salomo Keyzer", "format": "text", "type": "translation", "ayah_number": 5993, "text": "En dan is het mijne taak, hun rekenschap af te vragen."}]