[{"id": 180470, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Nay! I swear by this city"}, {"id": 180471, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6025, "text": "\u2013 this city wherein you have been rendered violable\u2013"}, {"id": 180472, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6026, "text": "and I swear by the parent and his offspring:"}, {"id": 180473, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Verily We have created man into toil and hardship."}, {"id": 180474, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6028, "text": "Does he think that no one can overpower him?"}, {"id": 180475, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6029, "text": "He says: \u201cI have squandered enormous wealth.\u201d"}, {"id": 180476, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6030, "text": "Does he believe that none has seen him?"}, {"id": 180477, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Did We not grant him two eyes,"}, {"id": 180478, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6032, "text": "and a tongue and two lips?"}, {"id": 180479, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6033, "text": "And did We not show him the two highroads (of good and evil)?"}, {"id": 180480, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6034, "text": "But he did not venture to scale the difficult steep."}, {"id": 180481, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6035, "text": "And what do you know what that difficult steep is?"}, {"id": 174655, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6036, "text": "It is freeing someone's neck from slavery;"}, {"id": 174656, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6037, "text": "or giving food on a day of hunger"}, {"id": 174642, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6038, "text": "to an orphan near of kin;"}, {"id": 174657, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6039, "text": "or to a destitute lying in dust;"}, {"id": 180482, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6040, "text": "and, then besides this, he be one of those who believed, and enjoined upon one another steadfastness and enjoined upon one another compassion."}, {"id": 180483, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6041, "text": "These are the People of the Right Hand."}, {"id": 180484, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6042, "text": "As for those who rejected Our Signs, they are the People of the Left Hand."}, {"id": 180485, "identifier": "en.maududi", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Upon them shall be a Fire that will hem them in."}]