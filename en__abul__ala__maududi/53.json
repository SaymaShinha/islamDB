[{"id": 179271, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4785, "text": "By the star when it sets:"}, {"id": 179272, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4786, "text": "your companion has neither strayed nor is he deluded;"}, {"id": 179273, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4787, "text": "nor does he speak out of his desire."}, {"id": 179274, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4788, "text": "This is nothing but a revelation that is conveyed to him,"}, {"id": 179275, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4789, "text": "something that a very powerful one has imparted to him,"}, {"id": 179215, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4790, "text": "one endowed with immense wisdom. He came forth and stood poised,"}, {"id": 175466, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4791, "text": "being on the higher horizon."}, {"id": 176849, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Then he drew near and hung above suspended,"}, {"id": 177065, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4793, "text": "until he was two bows' length away, or nearer."}, {"id": 179276, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Then he revealed to Allah's servant whatever he had to reveal."}, {"id": 179277, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4795, "text": "His heart added no untruth to what he saw."}, {"id": 179278, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Are you, then, going to contend with him regarding what he sees with his eyes?"}, {"id": 179279, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Indeed he saw him a second time,"}, {"id": 179280, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4798, "text": "by the lote-tree at the farthest boundary,"}, {"id": 179281, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4799, "text": "near which is the Garden of Abode."}, {"id": 179282, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4800, "text": "(This was) when the lote-tree was covered with that which covered it."}, {"id": 179283, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4801, "text": "His eye did not waver, nor did it stray,"}, {"id": 179284, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4802, "text": "and he certainly saw some of the greatest Signs of His Lord."}, {"id": 179285, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Have you ever thought about al-Lat and al-Uzza,"}, {"id": 179286, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4804, "text": "and about the third deity, al-Manat?"}, {"id": 179287, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Shall you have the male issues, and He the female issues?"}, {"id": 179288, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4806, "text": "That is indeed an unfair division!"}, {"id": 179290, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4807, "text": "These are nothing but names that you and your forefathers have invented, for which Allah has sent down no authority. They are merely following their conjectures and their carnal desires although guidance has come to them from their Lord."}, {"id": 179291, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Does man imagine that whatever he wishes for is right for him?"}, {"id": 179292, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4809, "text": "To Allah belong both the Next World and the present."}, {"id": 179293, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4810, "text": "Numerous are the angels in the heavens; yet their intercession shall be of no avail, except in regard to those whom He grants the leave of intercession and whose plea He is pleased to accept."}, {"id": 179294, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Those who do not believe in the Hereafter give angels the names of females,"}, {"id": 179295, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4812, "text": "although they have no knowledge regarding that. They only follow their conjecture and conjecture can never take the place of the Truth."}, {"id": 179296, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4813, "text": "So leave alone those who turn away from the remembrance of Us and who seek nothing but the life of the world \u2013"}, {"id": 179297, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4814, "text": "that being the utmost of their knowledge. Surely your Lord fully knows those who have strayed away from His Path and He also fully knows those who are rightly guided."}, {"id": 179298, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4815, "text": "To Allah alone belongs whatever is in the heavens and whatever is in the earth. He will requite the evil-doers for their deeds and bestow a goodly reward on those who have done good,"}, {"id": 179299, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4816, "text": "on those who avoid grave sins and shameful deeds, even if they may sometimes stumble into lesser offences. Surely your Lord is abounding in His Forgiveness. Very well is He aware of you since He produced you from the earth, and while you were still in your mothers' wombs and not yet born. So do not boastfully claim yourselves to be purified. He fully knows those that are truly Godfearing."}, {"id": 179300, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4817, "text": "(O Prophet), did you see him who turned away (from the Path of Allah),"}, {"id": 179301, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4818, "text": "who gave a little, and then stopped?"}, {"id": 179302, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Does he have any knowledge of the world beyond the ken of sense-perception, and therefore, clearly sees (the Truth)?"}, {"id": 179303, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Has he not been informed of what is in the Scrolls of Moses,"}, {"id": 179304, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4821, "text": "and of Abraham, who lived up to the trust?"}, {"id": 179305, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4822, "text": "\u201cThat no bearer of a burden shall bear the burden of another,"}, {"id": 179306, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4823, "text": "and that man shall have nothing but what he has striven for,"}, {"id": 179307, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4824, "text": "and that (the result of) his striving shall soon be seen,"}, {"id": 179308, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4825, "text": "and that he shall then be fully recompensed,"}, {"id": 179309, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4826, "text": "and that the final end is with your Lord,"}, {"id": 179310, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4827, "text": "and that He it is Who causes people to laugh and to cry,"}, {"id": 179311, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4828, "text": "and that He it is Who causes death and grants life,"}, {"id": 179312, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4829, "text": "and He it is Who created the two kinds, the male and the female,"}, {"id": 179313, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4830, "text": "from a drop of sperm when it was emitted,"}, {"id": 179314, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4831, "text": "and that it is for Him to grant the second life,"}, {"id": 179315, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4832, "text": "that He it is Who bestowed wealth and riches,"}, {"id": 179316, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4833, "text": "that He is the Lord of Sirius,"}, {"id": 179317, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4834, "text": "that He it is Who destroyed the ancient 'Ad,"}, {"id": 179318, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4835, "text": "and Thamud, leaving no trace of them,"}, {"id": 179319, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4836, "text": "and that He it is Who destroyed the people of Noah before for they were much given to iniquity and transgression."}, {"id": 174617, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4837, "text": "And He brought perdition upon the subverted cities"}, {"id": 179289, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4838, "text": "and caused them to be covered with that which He covered them with."}, {"id": 174615, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4839, "text": "So, which of your Lord's bounties will you doubt?\u201d"}, {"id": 174619, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4840, "text": "This is a warning among the warnings of yore."}, {"id": 174620, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4841, "text": "The imminent Hour has drawn near,"}, {"id": 174618, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4842, "text": "and none but Allah can avert it."}, {"id": 174621, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Will you, then, wonder at this?"}, {"id": 174624, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4844, "text": "Will you laugh at it rather than weep?"}, {"id": 174623, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4845, "text": "Will you occupy yourselves simply in merriment?"}, {"id": 174625, "identifier": "en.maududi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4846, "text": "Prostrate yourselves before Allah, and serve Him."}]