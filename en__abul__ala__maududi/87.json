[{"id": 180400, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5949, "text": "Glorify the name of your Lord, the Most High,"}, {"id": 180401, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5950, "text": "Who created all things and fashioned them in good proportion;"}, {"id": 180402, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5951, "text": "Who determined and guided them,"}, {"id": 180403, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5952, "text": "Who brought forth the pasture,"}, {"id": 180404, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5953, "text": "and then made it into a blackish straw."}, {"id": 180405, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5954, "text": "We shall make you recite and then you will not forget,"}, {"id": 180406, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5955, "text": "except what Allah should wish. He knows all that is manifest and all that is hidden."}, {"id": 180407, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5956, "text": "We shall ease you to follow the way of Ease."}, {"id": 180408, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5957, "text": "So render good counsel if good counsel will avail."}, {"id": 180409, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5958, "text": "He who fears (Allah) shall heed it,"}, {"id": 180410, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5959, "text": "but the wretched will turn away from it."}, {"id": 180411, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5960, "text": "He will be cast into the Great Fire."}, {"id": 180412, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5961, "text": "Then he will neither die in it, nor live."}, {"id": 180413, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5962, "text": "He who purified himself shall prosper,"}, {"id": 174654, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5963, "text": "remembering his Lord's name and praying."}, {"id": 174640, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5964, "text": "No; but you prefer the present life,"}, {"id": 180414, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5965, "text": "whereas the Hereafter is better and more enduring."}, {"id": 180415, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5966, "text": "This, indeed, was in the ancient Scrolls,"}, {"id": 180416, "identifier": "en.maududi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5967, "text": "the Scrolls of Abraham and Moses."}]