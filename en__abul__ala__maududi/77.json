[{"id": 180090, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5623, "text": "By the (winds) sent forth in quick succession,"}, {"id": 180091, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5624, "text": "which then blow tempestuously"}, {"id": 180092, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5625, "text": "and raise (clouds) and scatter them around,"}, {"id": 180093, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5626, "text": "then winnow them thoroughly,"}, {"id": 180094, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5627, "text": "and then cast (Allah's) remembrance (in people's hearts),"}, {"id": 180095, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5628, "text": "to serve as an excuse or a warning."}, {"id": 180096, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5629, "text": "Surely what you are promised shall come to pass."}, {"id": 180097, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5630, "text": "So when the stars are extinguished,"}, {"id": 180098, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5631, "text": "and the sky is rent asunder,"}, {"id": 180099, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5632, "text": "and the mountains are blown away,"}, {"id": 180100, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5633, "text": "and the appointed time to bring the Messengers together arrives, (then shall the promised event come to pass)."}, {"id": 180101, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5634, "text": "To which Day has this task been deferred?"}, {"id": 180102, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5635, "text": "To the Day of Judgement."}, {"id": 180103, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5636, "text": "What do you know what the Day of Judgement is?"}, {"id": 180104, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5637, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180105, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5638, "text": "Did We not destroy many a nation of the earlier times?"}, {"id": 180106, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5639, "text": "And We shall cause those of later times to follow them."}, {"id": 180107, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5640, "text": "Thus do We deal with the guilty."}, {"id": 180108, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5641, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180109, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5642, "text": "Did We not create you of a mean fluid,"}, {"id": 180110, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5643, "text": "which We then placed in a secure repository"}, {"id": 180111, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5644, "text": "until an appointed time?"}, {"id": 180112, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5645, "text": "See that We had the power to do so. Great indeed is Our power to do what We will."}, {"id": 180113, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5646, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180114, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5647, "text": "Did We not make the earth a receptacle,"}, {"id": 180115, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5648, "text": "for the living and the dead,"}, {"id": 180116, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5649, "text": "and did We not firmly fix towering mountains on it and give you sweet water to drink?"}, {"id": 180117, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5650, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180118, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5651, "text": "Proceed now towards that which you were wont to deny as false;"}, {"id": 180119, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5652, "text": "proceed towards the three-pronged shadow,"}, {"id": 180120, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5653, "text": "which neither provides (cooling) shade nor protection against the flames;"}, {"id": 180121, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5654, "text": "it indeed throws up sparks like castles,"}, {"id": 180122, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5655, "text": "which seem as though they are yellow-coloured camels."}, {"id": 180123, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5656, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180124, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5657, "text": "That will be the Day on which they will not (be able to) utter a word,"}, {"id": 180125, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5658, "text": "nor will they be allowed to proffer excuses."}, {"id": 180126, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5659, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180127, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5660, "text": "That is the Day of Judgement on which We have assembled you as well as all those who went before you."}, {"id": 180128, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5661, "text": "So if you have any ploy, try it against Me!"}, {"id": 180129, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5662, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180130, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5663, "text": "Behold, today the God-fearing will be amidst shades and springs,"}, {"id": 180131, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5664, "text": "and the fruits that they desire (will be ready at hand)."}, {"id": 180132, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5665, "text": "Eat and drink and may every joy attend you as a reward for your deeds."}, {"id": 180133, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5666, "text": "Thus do We reward those that do good."}, {"id": 180134, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5667, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180135, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5668, "text": "Eat and enjoy yourselves for a while. Surely you are evil-doers."}, {"id": 180136, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5669, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180137, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5670, "text": "When it is said to them: \u201cBow down (before Allah),\u201d they do not bow down."}, {"id": 180138, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5671, "text": "Woe on that Day to those that give the lie to the Truth!"}, {"id": 180139, "identifier": "en.maududi", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5672, "text": "In what discourse after this (Qur'an) will they, then, believe?"}]