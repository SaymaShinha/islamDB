[{"id": 180549, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6107, "text": "Recite in the name of your Lord Who created,"}, {"id": 180550, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6108, "text": "created man from a clot of congealed blood."}, {"id": 174658, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6109, "text": "Recite: and your Lord is Most Generous,"}, {"id": 174659, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6110, "text": "Who taught by the pen,"}, {"id": 174660, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6111, "text": "taught man what he did not know."}, {"id": 174643, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Nay, surely man transgresses;"}, {"id": 174661, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6113, "text": "for he believes himself to be self-sufficient."}, {"id": 174644, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6114, "text": "Surely to your Lord is your return."}, {"id": 174662, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6115, "text": "Did you see him who forbids"}, {"id": 174645, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6116, "text": "a servant (of Allah) when he prays?"}, {"id": 174646, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6117, "text": "Did you consider: what if he is on the Right Way,"}, {"id": 174663, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6118, "text": "and enjoins piety?"}, {"id": 180551, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6119, "text": "Did you consider: what if he gives the lie (to the Truth) and turns away (from it)?"}, {"id": 180552, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6120, "text": "Does he not know that Allah sees everything?"}, {"id": 180553, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6121, "text": "No indeed; if he does not desist, We shall drag him by the forelock;"}, {"id": 180554, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6122, "text": "by the lying forelock steeped in sin."}, {"id": 180555, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6123, "text": "So let him summon his helpmates;"}, {"id": 180556, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6124, "text": "We too shall summon the guards of Hell."}, {"id": 180557, "identifier": "en.maududi", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 6125, "text": "No, not at all. Never obey him. But prostrate yourself and become nigh (to your Lord)."}]