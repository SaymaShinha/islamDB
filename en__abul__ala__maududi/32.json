[{"id": 178024, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3504, "text": "Alif. Lam. Mim."}, {"id": 178025, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3505, "text": "This Book, beyond all doubt, was revealed by the Lord of the Universe."}, {"id": 178026, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3506, "text": "Or do they say: \u201cHe has fabricated it?\u201d Nay, it is the Truth from your Lord so that you may warn a people to whom no warner came before you; perhaps they will be guided to the Right Way."}, {"id": 178027, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3507, "text": "It is Allah Who created the heavens and the earth and all that is between the two, in six days, and then He established Himself on the Throne. You have no guardian or intercessor other than He. Will you, then, not take heed?"}, {"id": 178028, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3508, "text": "He governs from the heaven to the earth and then the record (of this governance) goes up to Him in a day whose measure is a thousand years in your reckoning."}, {"id": 178029, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3509, "text": "He knows all that is beyond as well as all that is within a creature's sense-perception. He is the Most Mighty, the Most Compassionate,"}, {"id": 178030, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3510, "text": "He Who excelled in the creation of all that He created. He originated the creation of man from clay,"}, {"id": 178031, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3511, "text": "then made his progeny from the extract of a mean fluid,"}, {"id": 178032, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3512, "text": "then He duly proportioned him, and breathed into him of His spirit, and bestowed upon you ears and eyes and hearts. And yet, little thanks do you give."}, {"id": 178009, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3513, "text": "They say: \u201cShall we be created afresh after we have become lost in the earth?\u201d Nay, the fact is that they deny that they will meet their Lord."}, {"id": 178033, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3514, "text": "Tell them: \u201cThe angel of death who has been charged with your souls shall gather you, and then you shall be brought back to your Lord.\u201d"}, {"id": 178034, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3515, "text": "Would that you could see the guilty standing before their Lord with their heads downcast, (saying to Him): \u201cOur Lord, we have now seen and heard, so send us back (to the world) that we might act righteously. For now we have come to have firm faith.\u201d"}, {"id": 178035, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3516, "text": "(They will be told): \u201cIf We had so willed, We could have bestowed guidance on every person. But the Word from Me that I will fill Hell with men and jinn, all together, has been fulfilled."}, {"id": 178036, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3517, "text": "So taste the chastisement on account of your forgetting the encounter of this Day. We, too, have forgotten you. Taste the eternal chastisement as a requital for your misdeeds.\u201d"}, {"id": 178037, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": true, "sajda_id": 10, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3518, "text": "None believes in Our Signs except those who, when they are given good counsel through Our verses, fall down prostrate and celebrate the praise of their Lord and do not wax proud."}, {"id": 178039, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3519, "text": "Their sides forsake their beds, and they call upon their Lord in fear and hope, and expend (in charity) out of the sustenance We have granted them."}, {"id": 178040, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3520, "text": "No one knows what delights of the eyes are kept hidden for them as a reward for their deeds."}, {"id": 178041, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3521, "text": "Would a true believer be like him who was an evil-doer? Surely they are not equal."}, {"id": 178042, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3522, "text": "As for those who believe and act righteously, theirs shall be Gardens to dwell in, a hospitality to reward them for their deeds."}, {"id": 178043, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3523, "text": "As for the evil-doers, their refuge shall be the Fire. Every time they want to escape from it they shall be driven back and shall be told: \u201cTaste the chastisement of the Fire which you used to reject as a lie.\u201d"}, {"id": 178044, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3524, "text": "We shall certainly have them taste some chastisement in this world in addition to the greater chastisement (of the Hereafter); perhaps they will retract (from their transgression)."}, {"id": 178045, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3525, "text": "And who is more unjust than he who is given good counsel through the Signs of his Lord and yet he turns away from them? Surely We will exact full retribution from such criminals."}, {"id": 178046, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3526, "text": "Verily We bestowed the Book upon Moses. So entertain no doubt if (the Prophet Muhammad) received the same. We had made that Book a guidance for the Children of Israel,"}, {"id": 178047, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3527, "text": "and when they remained steadfast and firmly believed in Our Signs, We created among them leaders who guided people by Our command."}, {"id": 178048, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3528, "text": "Surely your Lord will judge among them on the Day of Resurrection concerning the matters about which the Children of Israel used to differ."}, {"id": 178049, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3529, "text": "Did (these historical events) not make them realise that We destroyed many nations before them amidst whose dwellings they now move about? Surely there are many Signs in this. Are they unable to hear?"}, {"id": 178050, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3530, "text": "Have they not seen that We drive water to the parched land, thereby bringing forth crops which they and their cattle eat? Are they unable to see?"}, {"id": 178051, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3531, "text": "They say: \u201cIf you are truthful, (tell us) when will the Judgement come?\u201d"}, {"id": 178052, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3532, "text": "Tell them: \u201cIf the unbelievers were to believe on the Day of Judgement that will not avail them. For then they will be granted no respite.\u201d"}, {"id": 178053, "identifier": "en.maududi", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 3533, "text": "So (leave them to themselves and) turn away from them and wait; they too are waiting."}]