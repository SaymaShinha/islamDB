[{"id": 179374, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4902, "text": "The Merciful One"}, {"id": 179375, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4903, "text": "has taught the Qur'an,"}, {"id": 179376, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4904, "text": "has created man,"}, {"id": 179377, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4905, "text": "and has taught him articulate speech."}, {"id": 179378, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4906, "text": "The sun and the moon follow a reckoning,"}, {"id": 179379, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4907, "text": "and the stars and the trees all prostrate themselves,"}, {"id": 179380, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4908, "text": "and He has raised up the heaven and has set a balance"}, {"id": 179381, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4909, "text": "that you may not transgress in the balance,"}, {"id": 179382, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4910, "text": "but weigh things equitably and skimp not in the balance."}, {"id": 179383, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4911, "text": "And He has set up the earth for all beings."}, {"id": 179384, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4912, "text": "Therein are fruit and palm-trees with their dates in sheaths,"}, {"id": 179385, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4913, "text": "and a variety of corn with both husk and grain."}, {"id": 179386, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4914, "text": "Which of the bounties of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179387, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4915, "text": "He has created man from dry, rotten clay like the potter's,"}, {"id": 179388, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4916, "text": "and has created the jinn from the flame of fire."}, {"id": 179389, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4917, "text": "Which of the wonders of your Lord's power will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179390, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4918, "text": "Lord of the two easts and of the two wests is He."}, {"id": 179391, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4919, "text": "Which of the powers of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179392, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4920, "text": "He unleashed the two seas so that they merge together,"}, {"id": 179393, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4921, "text": "and yet there is a barrier between them which they may not overstep."}, {"id": 179394, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4922, "text": "Which of the wonders of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179395, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4923, "text": "From these seas come forth pearls and coral."}, {"id": 179396, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4924, "text": "Which of the wonders of your Lord's power will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179397, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4925, "text": "His are the ships, towering on the sea like mountains."}, {"id": 179398, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4926, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179399, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4927, "text": "All that is on earth will perish,"}, {"id": 179400, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4928, "text": "only the Person of your Lord, full of majesty and splendour, will endure."}, {"id": 179401, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4929, "text": "So which of the wonders of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179402, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4930, "text": "All in the heavens and the earth entreat Him for their needs; a new, mighty task engages Him each day."}, {"id": 179403, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4931, "text": "Which of your Lord's laudable attributes will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179404, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4932, "text": "O you twain, who are a burden (on the earth), We shall attend to you and call you to account."}, {"id": 179405, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4933, "text": "(We shall then see), which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179406, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4934, "text": "O company of jinn and men, if you have the power to go beyond the bounds of the heavens and the earth, go beyond them! Yet you will be unable to go beyond them for that requires infinite power."}, {"id": 179407, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4935, "text": "Which of your Lord's powers will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179408, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4936, "text": "(If you so venture) a flame of fire and smoke shall be lashed at you, which you shall be unable to withstand."}, {"id": 179409, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4937, "text": "Which of your Lord's powers will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179410, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4938, "text": "(What will happen) when the heaven will be split asunder and will become crimson like leather?"}, {"id": 179411, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4939, "text": "Which of your Lord's powers will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179412, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4940, "text": "On that Day there will be no need to ask either men or jinn about their sins."}, {"id": 179413, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4941, "text": "(We shall see) which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179414, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4942, "text": "The culprits shall be known by their marks, and shall be seized by their forelocks and their feet."}, {"id": 179415, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4943, "text": "Which of the powers of your Lord, will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179416, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4944, "text": "(It will be said): \u201cThis is the Hell that the culprits had cried lies to."}, {"id": 179417, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4945, "text": "They will keep circling around between Hell and boiling water."}, {"id": 179418, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4946, "text": "Which of your Lord's powers will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179419, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4947, "text": "For any who fears to stand before his Lord are two Gardens."}, {"id": 179420, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4948, "text": "Which of your Lord's favours will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179421, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4949, "text": "These Gardens will abound in green, blooming branches."}, {"id": 179422, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4950, "text": "Which of your Lord's favours will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179423, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4951, "text": "In each of the two Gardens are two flowing springs."}, {"id": 179424, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4952, "text": "Which of your Lord's favours will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179425, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4953, "text": "In both these is a pair of every fruit."}, {"id": 179426, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4954, "text": "Which of your Lord's favours will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179427, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4955, "text": "They shall recline on couches lined with brocade, and within reach shall hang the fruits of the two Gardens."}, {"id": 179428, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4956, "text": "Which of your Lord's favours will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179429, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4957, "text": "In the midst of these shall be maidens with modest, restrained glances; maidens whom no man or jinn has ever touched before."}, {"id": 179430, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4958, "text": "Which of your Lord's favours will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179431, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4959, "text": "Lovely as rubies and pearls."}, {"id": 179432, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4960, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179433, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4961, "text": "Can the reward of goodness be any other than goodness?"}, {"id": 179434, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4962, "text": "Which of the laudable attributes of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179435, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4963, "text": "And besides these two there shall be two other Gardens."}, {"id": 179436, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4964, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179437, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4965, "text": "Two Gardens, dark green and fresh."}, {"id": 179438, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4966, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179439, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4967, "text": "In them will be two gushing springs."}, {"id": 179440, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4968, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179441, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4969, "text": "Therein will be fruits and dates and pomegranates."}, {"id": 179442, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4970, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179443, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4971, "text": "In the midst of these will be maidens, good and comely."}, {"id": 179444, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4972, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179445, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4973, "text": "There shall be maidens sheltered in tents."}, {"id": 179446, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4974, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179447, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4975, "text": "No man or jinn ever touched them before."}, {"id": 179448, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 4976, "text": "Which of the favours of your Lord will you twain \u2013 you men and jinn \u2013 then deny?"}, {"id": 179449, "identifier": "en.maududi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translati