[{"id": 180251, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5801, "text": "When the sun shall be folded up,"}, {"id": 180252, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5802, "text": "when the stars shall scatter away,"}, {"id": 180253, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5803, "text": "when the mountains shall be set in motion,"}, {"id": 180254, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5804, "text": "when the ten-months pregnant camels shall be abandoned,"}, {"id": 180255, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5805, "text": "when the savage beasts shall be brought together,"}, {"id": 180256, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5806, "text": "when the seas shall be set boiling,"}, {"id": 180257, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5807, "text": "when the souls shall be rejoined (with their bodies),"}, {"id": 180258, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5808, "text": "and when the girl-child buried alive shall be asked:"}, {"id": 180259, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5809, "text": "for what offence was she killed?"}, {"id": 180260, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5810, "text": "and when the scrolls of (men's) deeds shall be unfolded,"}, {"id": 180261, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5811, "text": "and when Heaven is laid bare;"}, {"id": 180262, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5812, "text": "and Hell is stoked,"}, {"id": 180263, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5813, "text": "and Paradise brought nigh:"}, {"id": 180264, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5814, "text": "then shall each person know what he has brought along."}, {"id": 180265, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5815, "text": "No indeed; I swear by the alternating stars"}, {"id": 180266, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5816, "text": "that hide,"}, {"id": 180267, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5817, "text": "and by the night as it recedes,"}, {"id": 180268, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5818, "text": "and the morn as it breathes."}, {"id": 180269, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5819, "text": "Verily this is the word of a noble message-bearer;"}, {"id": 180270, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5820, "text": "one mighty and held in honour with the Lord of the Throne;"}, {"id": 180271, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5821, "text": "there he is obeyed and held trustworthy."}, {"id": 180272, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5822, "text": "(O people of Makkah), your companion is not mad;"}, {"id": 180273, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5823, "text": "he indeed saw the message-bearer on the clear horizon;"}, {"id": 180274, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5824, "text": "nor does he grudge (conveying this knowledge about) the Unseen;"}, {"id": 180275, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5825, "text": "nor is it a word of an accursed Satan."}, {"id": 180276, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5826, "text": "Where to are you then heading?"}, {"id": 180277, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5827, "text": "It is nothing but Good Counsel for everyone in the world,"}, {"id": 180278, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5828, "text": "for everyone of you who wishes to follow the Straight Way;"}, {"id": 180280, "identifier": "en.maududi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Maududi", "transelator_en_name": "Abul Ala Maududi", "format": "text", "type": "translation", "ayah_number": 5829, "text": "but your wishing will not avail unless Allah, the Lord of the Universe, so wishes."}]