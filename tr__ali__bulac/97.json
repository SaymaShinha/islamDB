[{"id": 560933, "identifier": "tr.bulac", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6126, "text": "Ger\u00e7ek \u015fu ki, Biz onu kadir gecesinde indirdik."}, {"id": 560934, "identifier": "tr.bulac", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6127, "text": "Kadir gecesinin ne oldu\u011funu sana bildiren nedir?"}, {"id": 560935, "identifier": "tr.bulac", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6128, "text": "Kadir gecesi, bin aydan daha hay\u0131rl\u0131d\u0131r."}, {"id": 560936, "identifier": "tr.bulac", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6129, "text": "Melekler ve ruh, onda Rablerinin izniyle her bir i\u015f i\u00e7in inerler."}, {"id": 560937, "identifier": "tr.bulac", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6130, "text": "Fecrin \u00e7\u0131k\u0131\u015f\u0131na kadar bir esenliktir (selamd\u0131r) o."}]