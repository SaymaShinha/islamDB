[{"id": 560906, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6099, "text": "\u0130ncire ve zeytine andolsun,"}, {"id": 560907, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6100, "text": "Sina da\u011f\u0131na,"}, {"id": 560908, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6101, "text": "Ve \u015fu emin beldeye (g\u00fcvenilir \u015fehre)."}, {"id": 560909, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6102, "text": "Do\u011frusu, Biz insan\u0131 en g\u00fczel bir bi\u00e7imde yaratt\u0131k."}, {"id": 560910, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6103, "text": "Sonra a\u015fa\u011f\u0131lar\u0131n a\u015fa\u011f\u0131s\u0131na \u00e7evirdik."}, {"id": 560911, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6104, "text": "Ancak iman edip salih amellerde bulunanlar ba\u015fka; onlar i\u00e7in kesintisi olmayan bir ecir vard\u0131r."}, {"id": 560912, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6105, "text": "\u00d6yleyse bundan sonra, hangi \u015fey sana dini yalanlatabilir?"}, {"id": 560913, "identifier": "tr.bulac", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6106, "text": "Allah h\u00fckmedenlerin hakimi de\u011fil midir?"}]