[{"id": 560988, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6180, "text": "Arkadan \u00e7eki\u015ftirip duran, ka\u015f g\u00f6z hareketleriyle alay eden her ki\u015finin vay haline;"}, {"id": 560989, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6181, "text": "Ki o, mal y\u0131\u011f\u0131p biriktiren ve onu sayd\u0131k\u00e7a sayand\u0131r."}, {"id": 560990, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6182, "text": "Ger\u00e7ekten mal\u0131n\u0131n kendisini ebedi k\u0131laca\u011f\u0131n\u0131 san\u0131yor."}, {"id": 560991, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Hay\u0131r; andolsun o, 'hutame'ye at\u0131lacakt\u0131r."}, {"id": 560992, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6184, "text": "\"Hutame\"nin ne oldu\u011funu sana bildiren nedir?"}, {"id": 560993, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6185, "text": "Allah'\u0131n tutu\u015fturulmu\u015f ate\u015fidir."}, {"id": 560994, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6186, "text": "Ki o, y\u00fcreklerin \u00fcst\u00fcne t\u0131rman\u0131p \u00e7\u0131kar."}, {"id": 560995, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6187, "text": "O, onlar\u0131n \u00fczerine kilitlenecektir;"}, {"id": 560996, "identifier": "tr.bulac", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6188, "text": "(Kendileri de) Dikilip-y\u00fckseltilmi\u015f s\u00fctunlarda (ba\u011flanacaklard\u0131r)."}]