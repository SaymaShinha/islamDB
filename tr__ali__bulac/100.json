[{"id": 560955, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6147, "text": "Soluk solu\u011fa ko\u015fan (at)lara andolsun,"}, {"id": 560956, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6148, "text": "(T\u0131rnaklar\u0131yla) Ate\u015f sa\u00e7anlara,"}, {"id": 560957, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6149, "text": "Sabah vakti bask\u0131n yapanlara."}, {"id": 560958, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6150, "text": "Derken, orada tozu dumana katanlara,"}, {"id": 560959, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6151, "text": "Bununla bir (d\u00fc\u015fman) toplulu\u011fun orta yerine kadar dalanlara."}, {"id": 560960, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6152, "text": "Ger\u00e7ekten insan, Rabbine kar\u015f\u0131 nank\u00f6rd\u00fcr."}, {"id": 560961, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6153, "text": "Ve ger\u00e7ekten, kendisi buna \u015fahiddir."}, {"id": 560962, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6154, "text": "Muhakkak o, mal sevgisinden dolay\u0131 (bencil ve cimri tutumundan) \u00e7ok kat\u0131d\u0131r."}, {"id": 560963, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6155, "text": "Yine de bilmeyecek mi? Kabirlerde olanlar\u0131n 'de\u015filip d\u0131\u015fa at\u0131ld\u0131\u011f\u0131,'"}, {"id": 560964, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6156, "text": "G\u00f6\u011f\u00fcslerde olanlar\u0131n derlenip-dev\u015firildi\u011fi zaman\u0131?"}, {"id": 560965, "identifier": "tr.bulac", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 6157, "text": "\u015e\u00fcphesiz, o g\u00fcn Rableri, kendilerinden ger\u00e7ekten haberdard\u0131r."}]