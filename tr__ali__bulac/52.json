[{"id": 559572, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4736, "text": "Tur'a andolsun."}, {"id": 559573, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4737, "text": "Sat\u0131r (sat\u0131r) dizili kitaba,"}, {"id": 559574, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4738, "text": "Yay\u0131lm\u0131\u015f ince deri \u00fczerine;"}, {"id": 559575, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4739, "text": "Ma'mur eve,"}, {"id": 559576, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4740, "text": "Y\u00fckseltilmi\u015f tavana,"}, {"id": 559577, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4741, "text": "Kabar\u0131p, tutu\u015fan denize,"}, {"id": 559578, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4742, "text": "\u015e\u00fcphesiz senin Rabbinin azab\u0131 kesin olarak ger\u00e7ekle\u015fecektir."}, {"id": 559579, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4743, "text": "Onu uzakla\u015ft\u0131r\u0131p-engel olacak yoktur."}, {"id": 559580, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4744, "text": "O g\u00fcn g\u00f6k, sars\u0131l\u0131p \u00e7alkalan\u0131r."}, {"id": 559581, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4745, "text": "Ve da\u011flar (yerlerinden oynatan) bir y\u00fcr\u00fcy\u00fc\u015fle y\u00fcr\u00fcr."}, {"id": 559582, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4746, "text": "\u0130\u015fte o g\u00fcn, yalanlayanlar\u0131n vay haline,"}, {"id": 559583, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4747, "text": "Ki onlar, 'dald\u0131klar\u0131 sa\u00e7ma bir u\u011fra\u015f\u0131' i\u00e7inde oynayan-oyalananlard\u0131r."}, {"id": 559584, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4748, "text": "Cehennem ate\u015fine, 'k\u00fc\u00e7\u00fclt\u00fcc\u00fc bir s\u00fcr\u00fcklenme ile ' s\u00fcr\u00fcklenecekleri g\u00fcn;"}, {"id": 559585, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4749, "text": "(Onlara \u015f\u00f6yle denir:) \"\u0130\u015fte sizin yalanlad\u0131\u011f\u0131n\u0131z ate\u015f budur.\""}, {"id": 559586, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4750, "text": "\"Bu da bir b\u00fcy\u00fc m\u00fc, yoksa siz mi g\u00f6rm\u00fcyorsunuz.\""}, {"id": 559587, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4751, "text": "\"Girin ona; art\u0131k ister sabredin, ister sabretmeyin. Sizin i\u00e7in birdir. Siz ancak, yapt\u0131klar\u0131n\u0131zla cezaland\u0131r\u0131l\u0131yorsunuz.\""}, {"id": 559588, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4752, "text": "Hi\u00e7 \u015f\u00fcphesiz muttakiler, cennetlerde ve nimet i\u00e7indedirler;"}, {"id": 559589, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4753, "text": "Rablerinin verdikleriyle 'sevin\u00e7li ve mutludurlar'. Rableri, kendilerini '\u00e7\u0131lg\u0131nca yanan cehennemin' azab\u0131ndan korumu\u015ftur."}, {"id": 559590, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4754, "text": "\"Yapt\u0131klar\u0131n\u0131zdan dolay\u0131 afiyetle yiyin ve i\u00e7in.\""}, {"id": 559591, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4755, "text": "\u00d6zenle dizilmi\u015f tahtlar \u00fczerinde yaslanm\u0131\u015flard\u0131r. Ve Biz onlar\u0131 iri-ceylan g\u00f6zl\u00fc hurilerle evlendirmi\u015fiz."}, {"id": 559592, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4756, "text": "\u0130man edenler ve soylar\u0131 kendilerini imanda izleyenler; Biz onlar\u0131n soylar\u0131n\u0131 da kendilerine kat\u0131p-ekledik. Onlar\u0131n amellerinden hi\u00e7bir \u015feyi eksiltmedik. Her ki\u015fi kendi kazand\u0131\u011f\u0131na kar\u015f\u0131l\u0131k bir rehindir."}, {"id": 559593, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4757, "text": "Onlara, istek duyup-arzulad\u0131klar\u0131 meyvelerden ve etten bol bol verdik."}, {"id": 559594, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4758, "text": "Orada bir kadeh kap\u0131\u015f\u0131r-\u00e7eki\u015firler ki, onda ne 'bo\u015f ve sa\u00e7ma bir s\u00f6z', ne g\u00fcnaha sokma yoktur."}, {"id": 559595, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4759, "text": "Kendileri i\u00e7in (hizmet eden) civanlar, etraf\u0131nda d\u00f6n\u00fcp dola\u015f\u0131rlar; sanki (her biri) 'sedefte sakl\u0131 inci gibi tertemiz, p\u0131r\u0131l p\u0131r\u0131l.'"}, {"id": 559596, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4760, "text": "Kimi kimine d\u00f6n\u00fcp sorarlar;"}, {"id": 559597, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4761, "text": "Dediler ki: \"Biz do\u011frusu daha \u00f6nce, ailemiz (yak\u0131n akrabalar\u0131m\u0131z) i\u00e7inde endi\u015fe edip-korkard\u0131k.\""}, {"id": 559598, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4762, "text": "\"\u015eimdi Allah, bize l\u00fctufta bulundu ve 'h\u00fccrelere kadar i\u015fleyen kavurucu' azaptan korudu.\""}, {"id": 559599, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4763, "text": "\"\u015e\u00fcphesiz, biz bundan \u00f6nce O'na dua (kulluk) ederdik. Ger\u00e7ekten O, iyili\u011fi bol, esirgemesi \u00e7ok olan\u0131n ta Kendisi'dir.\""}, {"id": 559600, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4764, "text": "\u015eu halde sen, \u00f6\u011f\u00fct verip-hat\u0131rlat; \u00e7\u00fcnk\u00fc sen, Rabbinin nimetiyle ne kahinsin, ne mecnun."}, {"id": 559601, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Yoksa onlar: \"Bir \u015fairdir, biz ona zaman\u0131n (getirece\u011fi) felaketleri g\u00f6zl\u00fcyoruz\" mu diyorlar?"}, {"id": 559602, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4766, "text": "De ki: \"Siz g\u00f6zetleyedurun; \u00e7\u00fcnk\u00fc ben de sizinle birlikte g\u00f6zetleyenlerdenim.\""}, {"id": 559603, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Yoksa bunu kendilerine sa\u00e7ma-ak\u0131llar\u0131 m\u0131 emrediyor? Yoksa onlar azg\u0131n bir kavim midir?"}, {"id": 559604, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Yoksa: \"Onu kendisi uydurup-s\u00f6yledi\" mi diyorlar? Hay\u0131r; onlar iman etmiyorlar."}, {"id": 559605, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4769, "text": "\u015eu halde, e\u011fer do\u011fru s\u00f6zl\u00fcler iseler, benzeri bir s\u00f6z getirsinler."}, {"id": 559606, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4770, "text": "Yoksa onlar, hi\u00e7bir \u015fey olmaks\u0131z\u0131n m\u0131 yarat\u0131ld\u0131lar? Yoksa yarat\u0131c\u0131lar kendileri mi?"}, {"id": 559607, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Yoksa g\u00f6kleri ve yeri onlar m\u0131 yaratt\u0131lar? Hay\u0131r; onlar, kesin bir bilgiyle inanm\u0131yorlar."}, {"id": 559608, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Yoksa Rabbinin hazineleri onlar\u0131n yan\u0131nda m\u0131d\u0131r? Yoksa \u00fcst\u00fcn g\u00fc\u00e7 (her\u015feyin denetim ve y\u00f6netim) sahipleri kendileri midir?"}, {"id": 559609, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Yoksa onlar\u0131n bir merdivenleri mi var (ki) onunla (y\u00fckselip en y\u00fcce makamda konu\u015fulanlar\u0131) dinliyorlar? \u00d6yleyse, dinleyenleri a\u00e7\u0131k bir delil getirsin."}, {"id": 559610, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Yoksa k\u0131zlar O'nun da, erkek-\u00e7ocuklar sizin mi?"}, {"id": 559611, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Yoksa sen onlardan bir \u00fccret mi istiyorsun ki, haks\u0131z bir bor\u00e7tan dolay\u0131 a\u011f\u0131r bir y\u00fck alt\u0131ndalar?"}, {"id": 559612, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Yoksa gayb (bilgisi) onlar\u0131n kat\u0131nda m\u0131d\u0131r, b\u00f6ylece yaz\u0131p-duruyorlar?"}, {"id": 559613, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Yoksa hileli-bir d\u00fczen mi kurmak istiyorlar? Fakat (as\u0131l) \u2018o inkar edenler hileli-d\u00fczene d\u00fc\u015fecek olanlard\u0131r."}, {"id": 559614, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Yoksa onlar\u0131n, Allah'\u0131n d\u0131\u015f\u0131nda ba\u015fka bir ilahlar\u0131 m\u0131 var? Allah, onlar\u0131n \u015firk ko\u015ftuklar\u0131ndan Y\u00fccedir."}, {"id": 559615, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4779, "text": "E\u011fer g\u00f6kten bir par\u00e7an\u0131n d\u00fc\u015fmekte oldu\u011funu g\u00f6rseler bile: \"\u00dcst \u00fcste y\u0131\u011f\u0131lm\u0131\u015f bir buluttur.\" derler."}, {"id": 559616, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4780, "text": "\u00d6yleyse sen onlar\u0131 (en dayan\u0131lmaz azapla) \u00e7arp\u0131lacaklar\u0131 g\u00fcnlerine kavu\u015funcaya kadar b\u0131rak."}, {"id": 559617, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4781, "text": "O g\u00fcn, ne hileli-d\u00fczenleri kendilerine herhangi bir \u015feyle yarar sa\u011flayacak, ne yard\u0131m g\u00f6recekler."}, {"id": 559618, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4782, "text": "\u015e\u00fcphesiz zulmedenlere bundan \u00f6nce de bir azap vard\u0131r; ancak onlar\u0131n \u00e7o\u011fu bilmiyorlar."}, {"id": 559619, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4783, "text": "Art\u0131k, Rabbinin h\u00fckm\u00fcne sabret; \u00e7\u00fcnk\u00fc ger\u00e7ekten sen, Bizim g\u00f6zlerimizin \u00f6n\u00fcndesin. Ve her kalk\u0131\u015f\u0131nda Rabbini hamd ile tesbih et."}, {"id": 559620, "identifier": "tr.bulac", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 4784, "text": "Gecenin bir b\u00f6l\u00fcm\u00fcnde ve y\u0131ld\u0131zlar\u0131n bat\u0131\u015f\u0131n\u0131n ard\u0131nda da O'nu tesbih et."}]