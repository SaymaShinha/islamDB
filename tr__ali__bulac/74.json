[{"id": 560319, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5496, "text": "Ey b\u00fcr\u00fcn\u00fcp \u00f6rt\u00fcnen,"}, {"id": 560320, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5497, "text": "Kalk (ve) bundan b\u00f6yle uyar."}, {"id": 560321, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5498, "text": "Rabbini tekbir et (y\u00fccelt)"}, {"id": 560322, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5499, "text": "Elbiseni temizle."}, {"id": 560323, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5500, "text": "Pislikten ka\u00e7\u0131n\u0131p-uzakla\u015f."}, {"id": 560324, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5501, "text": "Daha \u00e7ok istekte bulunmak i\u00e7in iyilik yapma."}, {"id": 560325, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5502, "text": "Rabbin i\u00e7in sabret."}, {"id": 560326, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5503, "text": "\u00c7\u00fcnk\u00fc o boruya (sur'a) \u00fcf\u00fcr\u00fcld\u00fc\u011f\u00fc zaman,"}, {"id": 560327, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5504, "text": "\u0130\u015fte o g\u00fcn, zorlu bir g\u00fcnd\u00fcr;"}, {"id": 560328, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5505, "text": "Kafirler i\u00e7inse hi\u00e7 kolay de\u011fildir."}, {"id": 560329, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5506, "text": "Kendisini tek olarak (ve yapayaln\u0131z) yaratt\u0131\u011f\u0131m (\u015fu adam)\u0131 Bana b\u0131rak;"}, {"id": 560330, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5507, "text": "Ki Ben ona, 'alabildi\u011fine geni\u015f kapsaml\u0131 bir mal\u2019 (servet) verdim."}, {"id": 560331, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5508, "text": "G\u00f6z \u00f6n\u00fcnde-haz\u0131r \u00e7ocuklar (verdim)."}, {"id": 560332, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5509, "text": "Ve say\u0131s\u0131z imkan ve f\u0131rsatlar\u0131 \u00f6n\u00fcne serdim."}, {"id": 560333, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5510, "text": "Sonra, daha artt\u0131rmam i\u00e7in tamah eder (doyumsuz istekte bulunur)."}, {"id": 560334, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5511, "text": "Hay\u0131r; \u00e7\u00fcnk\u00fc o, Bizim ayetlerimize kar\u015f\u0131 'kesin bir inat\u00e7\u0131d\u0131r.\""}, {"id": 560335, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5512, "text": "Onu alabildi\u011fine sarp bir yoku\u015fa s\u00fcrece\u011fim."}, {"id": 560336, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5513, "text": "\u00c7\u00fcnk\u00fc o, d\u00fc\u015f\u00fcnd\u00fc ve bir \u00f6l\u00e7\u00fc tespit etti."}, {"id": 560337, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5514, "text": "Kahrolas\u0131, nas\u0131l bir \u00f6l\u00e7\u00fc koydu?"}, {"id": 560338, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5515, "text": "Yine kahrolas\u0131, nas\u0131l bir \u00f6l\u00e7\u00fc koydu?"}, {"id": 560339, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5516, "text": "Sonra bir bakt\u0131."}, {"id": 560340, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5517, "text": "Sonra ka\u015flar\u0131n\u0131 \u00e7att\u0131 ve y\u00fcz\u00fcn\u00fc ek\u015fitti."}, {"id": 560341, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5518, "text": "Sonra da s\u0131rt \u00e7evirdi ve b\u00fcy\u00fckl\u00fck taslad\u0131 (istikbar)."}, {"id": 560342, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5519, "text": "B\u00f6ylece: \"Bu, yaln\u0131zca 'aktar\u0131larak \u00f6\u011frenilen' bir b\u00fcy\u00fcd\u00fcr\" dedi."}, {"id": 560343, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5520, "text": "\"Bu, bir be\u015fer s\u00f6z\u00fcnden ba\u015fkas\u0131 de\u011fildir.\""}, {"id": 560344, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5521, "text": "Onu Ben, cehenneme s\u00fcr\u00fckleyip-ataca\u011f\u0131m."}, {"id": 560345, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5522, "text": "Cehennem (sakar) nedir, sen bilir misin?"}, {"id": 560346, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5523, "text": "Ne al\u0131koyar, ne b\u0131rak\u0131r."}, {"id": 560347, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5524, "text": "Be\u015fere delicesine susam\u0131\u015ft\u0131r."}, {"id": 560348, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5525, "text": "Onun \u00fczerinde ondokuz vard\u0131r."}, {"id": 560349, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5526, "text": "Biz o ate\u015fin koruyucular\u0131n\u0131 meleklerden ba\u015fkas\u0131n\u0131 k\u0131lmad\u0131k. Ve onlar\u0131n say\u0131s\u0131n\u0131 inkar edenler i\u00e7in yaln\u0131zca bir fitne (konusu) yapt\u0131k ki, kendilerine kitap verilenler, kesin bir bilgiyle inans\u0131n, iman edenlerin de imanlar\u0131 arts\u0131n; kendilerine kitap verilenler ve iman edenler (b\u00f6ylece) ku\u015fkuya kap\u0131lmas\u0131n. Kalplerinde bir hastal\u0131k olanlar ile kafirler de \u015f\u00f6yle desin: \"Allah, bu \u00f6rnekle neyi anlatmak istedi?\" \u0130\u015fte Allah, diledi\u011fini b\u00f6yle \u015fa\u015f\u0131rt\u0131p-sapt\u0131r\u0131r, diledi\u011fini b\u00f6yle hidayete erdirir. Rabbinin ordular\u0131n\u0131 Kendisi'nden ba\u015fka (hi\u00e7 kimse) bilmez. Bu ise, be\u015fer (insan) i\u00e7in yaln\u0131zca bir \u00f6\u011f\u00fctt\u00fcr."}, {"id": 560350, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5527, "text": "Hay\u0131r; Ay'a andolsun,"}, {"id": 560351, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5528, "text": "D\u00f6n\u00fcp gitti\u011fi zaman geceye,"}, {"id": 560352, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5529, "text": "A\u011fard\u0131\u011f\u0131 zaman sabaha,"}, {"id": 560353, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5530, "text": "Ger\u00e7ekten o, b\u00fcy\u00fck (musibet)lerden biridir."}, {"id": 560354, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5531, "text": "Be\u015fer (insan) i\u00e7in bir uyar\u0131d\u0131r."}, {"id": 560355, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5532, "text": "Sizlerden \u00f6ne ge\u00e7mek veya geride kalmak isteyenler i\u00e7in."}, {"id": 560356, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5533, "text": "Her nefis, kazand\u0131klar\u0131na kar\u015f\u0131l\u0131k bir rehinedir."}, {"id": 560357, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5534, "text": "Ancak Ashab-\u0131 Yemin (sa\u011f ehli) hari\u00e7."}, {"id": 560358, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5535, "text": "Onlar cennetlerdedirler; birbirlerine sorarlar."}, {"id": 560359, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5536, "text": "Su\u00e7lu-g\u00fcnahkarlar\u0131;"}, {"id": 560360, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5537, "text": "\"Sizi \u015fu cehenneme s\u00fcr\u00fckleyip-iten nedir?\""}, {"id": 560361, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5538, "text": "Onlar: \"Biz namaz k\u0131lanlardan de\u011fildik\" dediler."}, {"id": 560362, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5539, "text": "\"Yoksula yedirmezdik.\""}, {"id": 560363, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5540, "text": "\"(Bat\u0131la ve tutkulara) Dal\u0131p gidenlerle biz de dalar giderdik.\""}, {"id": 560364, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5541, "text": "\"Din (hesap ve ceza) g\u00fcn\u00fcn\u00fc yalan say\u0131yorduk.\""}, {"id": 560365, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5542, "text": "\"Sonunda yak\u00een (kesin bir ger\u00e7ek olan \u00f6l\u00fcm) gelip bize \u00e7att\u0131.\""}, {"id": 560366, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5543, "text": "Art\u0131k, \u015fefaat edenlerin \u015fefaati onlara bir yarar sa\u011flamaz."}, {"id": 560367, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5544, "text": "Buna ra\u011fmen, bunlara ne oluyor ki \u00f6\u011f\u00fctten y\u00fcz \u00e7evirip duruyorlar?"}, {"id": 560368, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5545, "text": "Sanki onlar, \u00fcrkm\u00fc\u015f yaban e\u015fekleri gibidirler;"}, {"id": 560369, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5546, "text": "Arslandan korkup-ka\u00e7m\u0131\u015flar."}, {"id": 560370, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5547, "text": "Hay\u0131r; her biri, kendisine a\u00e7\u0131lm\u0131\u015f sahifelerin verilmesini ister."}, {"id": 560371, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5548, "text": "Hay\u0131r; onlar \u015f\u00fcphesiz ahiretten korkmuyorlar."}, {"id": 560372, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5549, "text": "Ger\u00e7ek (\u015fu ki), o (Kur'an,) elbette bir \u00f6\u011f\u00fctt\u00fcr."}, {"id": 560373, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5550, "text": "Art\u0131k kim dilerse, \u00f6\u011f\u00fct al\u0131p-d\u00fc\u015f\u00fcn\u00fcr."}, {"id": 560375, "identifier": "tr.bulac", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5551, "text": "Allah dilemedik\u00e7e onlar \u00f6\u011f\u00fct almazlar; takvan\u0131n sahibi (onu kabul etmeye ehil olan) O'dur, ma\u011ffiretin sahibi (ba\u011f\u0131\u015flamaya ehil olan da) O'dur."}]