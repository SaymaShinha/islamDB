[{"id": 560376, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5552, "text": "Hay\u0131r, kalk\u0131\u015f (k\u0131yamet) g\u00fcn\u00fcne and ederim."}, {"id": 560377, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5553, "text": "Ve yine hay\u0131r; kendini k\u0131nay\u0131p duran nefse de and ederim."}, {"id": 560378, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5554, "text": "\u0130nsan, onun kemiklerini Bizim kesin olarak biraraya getirmeyece\u011fimizi mi san\u0131yor?"}, {"id": 560379, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Evet; onun parmak u\u00e7lar\u0131n\u0131 dahi derleyip-(yeniden) d\u00fczene koymaya g\u00fc\u00e7 yetirenleriz."}, {"id": 560380, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5556, "text": "Ancak insan, \u00f6n\u00fcndeki (sonsuz gelece\u011fi)ni de 'f\u00fccurla s\u00fcrd\u00fcrmek ister.'"}, {"id": 560381, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5557, "text": "\"K\u0131yamet g\u00fcn\u00fc ne zamanm\u0131\u015f\" diye sorar."}, {"id": 560382, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5558, "text": "Ama g\u00f6z 'kama\u015f\u0131p da kayd\u0131\u011f\u0131,'"}, {"id": 560383, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5559, "text": "Ay karard\u0131\u011f\u0131,"}, {"id": 560384, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5560, "text": "G\u00fcne\u015f ve ay birle\u015ftirildi\u011fi zaman;"}, {"id": 560385, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5561, "text": "\u0130nsan o g\u00fcn: \"Ka\u00e7\u0131\u015f nereye?\" der."}, {"id": 560386, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5562, "text": "Hay\u0131r, s\u0131\u011f\u0131nacak herhangi bir yer yok."}, {"id": 560387, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5563, "text": "O g\u00fcn, 'sonunda var\u0131l\u0131p karar k\u0131l\u0131nacak yer (m\u00fcstakar)' yaln\u0131zca Rabbinin kat\u0131d\u0131r."}, {"id": 560388, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5564, "text": "\u0130nsana o g\u00fcn, \u00f6nceden takdim ettikleri ve erteledikleri \u015feylerle haber verilir."}, {"id": 560389, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5565, "text": "Hay\u0131r; insan, kendi nefsine kar\u015f\u0131 bir basirettir."}, {"id": 560390, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5566, "text": "Kendi mazeretlerini ortaya atsa bile."}, {"id": 560391, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5567, "text": "Onu (Kur'an'\u0131, kavray\u0131p belletmek i\u00e7in) aceleye kap\u0131l\u0131p dilini onunla hareket ettirip-durma."}, {"id": 560392, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5568, "text": "\u015e\u00fcphesiz, onu (kalbinde) toplamak ve onu (sana) okutmak Bize ait (bir i\u015f)tir."}, {"id": 560393, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5569, "text": "\u015eu halde, Biz onu okudu\u011fumuz zaman, sen de onun okunu\u015funu izle."}, {"id": 560394, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5570, "text": "Sonra muhakkak onu a\u00e7\u0131klamak Bize ait (bir i\u015f)tir."}, {"id": 560395, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5571, "text": "Hay\u0131r; siz \u00e7ar\u00e7abuk ge\u00e7mekte olan\u0131 (d\u00fcnyay\u0131) seviyorsunuz."}, {"id": 560396, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5572, "text": "Ve ahireti terk edip-b\u0131rak\u0131yorsunuz."}, {"id": 560397, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5573, "text": "O g\u00fcn y\u00fczler \u0131\u015f\u0131l \u0131\u015f\u0131l parlar."}, {"id": 560398, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5574, "text": "Rablerine bak\u0131p-durur."}, {"id": 560399, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5575, "text": "O g\u00fcn, \u00f6yle y\u00fczler vard\u0131r ki kararm\u0131\u015f-ek\u015fimi\u015ftir."}, {"id": 560400, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5576, "text": "Kendisine, beli b\u00fcken i\u015flerin yap\u0131laca\u011f\u0131n\u0131 anlamaktad\u0131r."}, {"id": 560401, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5577, "text": "Hay\u0131r; can, k\u00f6pr\u00fcc\u00fck kemi\u011fine gelip dayand\u0131\u011f\u0131 zaman,"}, {"id": 560402, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5578, "text": "\"Son m\u00fcdahaleyi yapacak kim\" denir."}, {"id": 560403, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5579, "text": "Art\u0131k ger\u00e7ekten, kendisi de bir ayr\u0131l\u0131k oldu\u011funu anlam\u0131\u015ft\u0131r."}, {"id": 560404, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5580, "text": "(\u00d6l\u00fcm korkusundan) Ayaklar birbirine dola\u015ft\u0131\u011f\u0131nda;"}, {"id": 560405, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5581, "text": "O g\u00fcn sevk, yaln\u0131zca Rabbinedir."}, {"id": 560406, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5582, "text": "Fakat o, ne do\u011frulam\u0131\u015f ne de namaz k\u0131lm\u0131\u015ft\u0131."}, {"id": 560407, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5583, "text": "Ancak o, yalanlam\u0131\u015f ve y\u00fcz \u00e7evirmi\u015fti."}, {"id": 560408, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5584, "text": "Sonra \u00e7al\u0131m satarak yak\u0131nlar\u0131na gitmi\u015fti."}, {"id": 560409, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5585, "text": "Sen buna m\u00fcstahaks\u0131n, dahas\u0131na m\u00fcstahaks\u0131n."}, {"id": 560410, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5586, "text": "Yine m\u00fcstahaks\u0131n, dahas\u0131na da m\u00fcstahaks\u0131n."}, {"id": 560411, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5587, "text": "\u0130nsan, 'kendi ba\u015f\u0131na ve sorumsuz' b\u0131rak\u0131laca\u011f\u0131n\u0131 m\u0131 san\u0131yor?"}, {"id": 560412, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5588, "text": "Kendisi, ak\u0131t\u0131lan meniden bir damla su de\u011fil miydi?"}, {"id": 560413, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Sonra bir alak (embriyo) oldu, derken (Allah, onu) yaratt\u0131 ve bir 'd\u00fczen i\u00e7inde bi\u00e7im verdi.'"}, {"id": 560414, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5590, "text": "B\u00f6ylece ondan, erkek ve di\u015fi olmak \u00fczere \u00e7ift k\u0131ld\u0131."}, {"id": 560415, "identifier": "tr.bulac", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5591, "text": "(\u00d6yleyse Allah,) \u00d6l\u00fcleri diriltmeye g\u00fc\u00e7 yetiren de\u011fil midir?"}]