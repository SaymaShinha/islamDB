[{"id": 560577, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5759, "text": "Surat ast\u0131 ve y\u00fcz \u00e7evirdi;"}, {"id": 560578, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5760, "text": "Kendisine o k\u00f6r geldi diye."}, {"id": 560579, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5761, "text": "Nerden biliyorsun; belki o, temizlenip-ar\u0131nacak?"}, {"id": 560580, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5762, "text": "Veya \u00f6\u011f\u00fct alacak; b\u00f6ylelikle bu \u00f6\u011f\u00fct kendisine yarar sa\u011flayacak."}, {"id": 560581, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5763, "text": "Fakat kendini m\u00fcsta\u011fni g\u00f6ren (hi\u00e7bir \u015feye ihtiyac\u0131 olmad\u0131\u011f\u0131n\u0131 sanan) ise,"}, {"id": 560582, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5764, "text": "\u0130\u015fte sen, onda 'yank\u0131 uyand\u0131rmaya\u2019 \u00e7al\u0131\u015f\u0131yorsun."}, {"id": 560583, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5765, "text": "Oysa, onun temizlenip-ar\u0131nmas\u0131ndan sana ne?"}, {"id": 560584, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5766, "text": "Ama ko\u015farak sana gelen ise,"}, {"id": 560585, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5767, "text": "Ki o, 'i\u00e7i titreyerek korkar' bir durumdad\u0131r;"}, {"id": 560586, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5768, "text": "Sen ona ald\u0131r\u0131\u015f etmeden oyalan\u0131yorsun."}, {"id": 560587, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5769, "text": "Hay\u0131r; \u00e7\u00fcnk\u00fc o (Kur'an), bir \u00f6\u011f\u00fctt\u00fcr."}, {"id": 560588, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5770, "text": "Art\u0131k dileyen, onu 'd\u00fc\u015f\u00fcn\u00fcp-\u00f6\u011f\u00fct als\u0131n.'"}, {"id": 560589, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5771, "text": "O (Kur'an), '\u015ferefli-\u00fcst\u00fcn' sahifelerdedir."}, {"id": 560590, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5772, "text": "Y\u00fcceltilmi\u015f, tertemiz (mutahhar) k\u0131l\u0131nm\u0131\u015f."}, {"id": 560591, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5773, "text": "Katiplerin ellerinde."}, {"id": 560592, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5774, "text": "(Ki onlar,) \u00dcst\u00fcn de\u011ferli, 'iyilik ve d\u00fcr\u00fcstl\u00fck sembol\u00fc.'"}, {"id": 560593, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5775, "text": "Kahrolas\u0131 insan, ne kadar nank\u00f6rd\u00fcr."}, {"id": 560594, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5776, "text": "(Allah) Onu hangi \u015feyden yaratt\u0131?"}, {"id": 560595, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5777, "text": "Bir damla sudan yaratt\u0131 da onu 'bir \u00f6l\u00e7\u00fcyle bi\u00e7ime soktu.'"}, {"id": 560596, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5778, "text": "Sonra ona yolu kolayla\u015ft\u0131rd\u0131."}, {"id": 560597, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5779, "text": "Sonra onu \u00f6ld\u00fcrd\u00fc, b\u00f6ylece kabre g\u00f6md\u00fcrd\u00fc."}, {"id": 560598, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5780, "text": "Sonra diledi\u011fi zaman onu diriltir."}, {"id": 560599, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5781, "text": "Hay\u0131r; ona (Allah'\u0131n) emretti\u011fini yerine getirmedi."}, {"id": 560600, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5782, "text": "Bir de insan, yedi\u011fine bir bak\u0131versin;"}, {"id": 560601, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5783, "text": "Biz \u015f\u00fcphesiz, suyu ak\u0131tt\u0131k\u00e7a ak\u0131tt\u0131k,"}, {"id": 557950, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5784, "text": "Sonra yeri yard\u0131k\u00e7a yard\u0131k;"}, {"id": 558030, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5785, "text": "B\u00f6ylece onda taneler bitirdik,"}, {"id": 558194, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5786, "text": "\u00dcz\u00fcmler, yoncalar,"}, {"id": 558289, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5787, "text": "Zeytinler, hurmalar,"}, {"id": 560602, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5788, "text": "Boylar\u0131 birbiriyle yar\u0131\u015fan ve i\u00e7i\u00e7e girmi\u015f a\u011fa\u00e7l\u0131 bah\u00e7eler."}, {"id": 560603, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5789, "text": "Meyveler ve otlakl\u0131klar,"}, {"id": 560604, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5790, "text": "Size ve hayvanlar\u0131n\u0131za bir yarar (meta) olmak \u00fczere."}, {"id": 560605, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5791, "text": "Fakat 'kulaklar\u0131 patlat\u0131rcas\u0131na olan o g\u00fcrleme' geldi\u011fi zaman,"}, {"id": 560606, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5792, "text": "Ki\u015fi o g\u00fcn, kendi karde\u015finden ka\u00e7ar;"}, {"id": 560607, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5793, "text": "Annesinden ve babas\u0131ndan,"}, {"id": 560608, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5794, "text": "E\u015finden ve \u00e7ocuklar\u0131ndan,"}, {"id": 560609, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5795, "text": "O g\u00fcn, onlardan her birisinin kendine yetecek bir i\u015fi vard\u0131r."}, {"id": 560610, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5796, "text": "O g\u00fcn, \u00f6yle y\u00fczler vard\u0131r ki apayd\u0131nl\u0131kt\u0131r;"}, {"id": 560611, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5797, "text": "G\u00fcler ve sevin\u00e7 i\u00e7indedir."}, {"id": 560612, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5798, "text": "Ve o g\u00fcn, \u00f6yle y\u00fczler de vard\u0131r ki \u00fczerini toz b\u00fcr\u00fcm\u00fc\u015ft\u00fcr."}, {"id": 560613, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5799, "text": "Bir karart\u0131 sar\u0131p-kaplam\u0131\u015ft\u0131r."}, {"id": 560614, "identifier": "tr.bulac", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5800, "text": "\u0130\u015fte onlar da, kafir, facir olanlard\u0131r."}]