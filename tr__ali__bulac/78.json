[{"id": 560497, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5673, "text": "Birbirlerine hangi \u015feyi sorup duruyorlar?"}, {"id": 560498, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5674, "text": "O b\u00fcy\u00fck haberi mi?"}, {"id": 560499, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5675, "text": "Ki kendileri hakk\u0131nda anla\u015fmazl\u0131k i\u00e7indedirler."}, {"id": 560500, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5676, "text": "Hay\u0131r; yak\u0131nda bileceklerdir."}, {"id": 560501, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5677, "text": "Yine hay\u0131r; yak\u0131nda bileceklerdir."}, {"id": 560374, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5678, "text": "Biz, yery\u00fcz\u00fcn\u00fc bir d\u00f6\u015fek k\u0131lmad\u0131k m\u0131?"}, {"id": 556069, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5679, "text": "Da\u011flar\u0131 da birer kaz\u0131k?"}, {"id": 556302, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5680, "text": "Sizi \u00e7ift \u00e7ift yaratt\u0131k."}, {"id": 557097, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5681, "text": "Uykunuzu bir dinlenme yapt\u0131k."}, {"id": 557502, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5682, "text": "Geceyi bir \u00f6rt\u00fc yapt\u0131k."}, {"id": 557582, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5683, "text": "G\u00fcnd\u00fcz\u00fc bir ge\u00e7im-vakti k\u0131ld\u0131k."}, {"id": 560502, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5684, "text": "Sizin \u00fcst\u00fcn\u00fcze sapasa\u011flam yedi-g\u00f6k bina ettik."}, {"id": 560503, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5685, "text": "Par\u0131ldad\u0131k\u00e7a par\u0131ldayan bir kandil (g\u00fcne\u015f) k\u0131ld\u0131k."}, {"id": 560504, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5686, "text": "S\u0131k\u0131p suyu \u00e7\u0131karan (bulut)lardan 'bardaktan bo\u015fan\u0131rcas\u0131na su' indirdik."}, {"id": 560505, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5687, "text": "Bununla taneler ve bitkiler bitirip-\u00e7\u0131karal\u0131m diye."}, {"id": 560506, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5688, "text": "Ve birbirine sarma\u015f-dola\u015f bah\u00e7eleri de."}, {"id": 560507, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5689, "text": "\u015e\u00fcphesiz o h\u00fck\u00fcm (fasl) g\u00fcn\u00fc, belirlenmi\u015f bir vakittir."}, {"id": 560508, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5690, "text": "Sur'a \u00fcf\u00fcr\u00fclece\u011fi g\u00fcn, art\u0131k siz dalga dalga geleceksiniz."}, {"id": 560509, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5691, "text": "O s\u0131rada g\u00f6k a\u00e7\u0131lm\u0131\u015f ve kap\u0131 kap\u0131 olmu\u015ftur."}, {"id": 560510, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5692, "text": "Da\u011flar y\u00fcr\u00fct\u00fclm\u00fc\u015f, art\u0131k bir serab oluvermi\u015ftir."}, {"id": 560511, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5693, "text": "Ger\u00e7ekten cehennem, bir g\u00f6zetleme yeridir."}, {"id": 560512, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5694, "text": "Ta\u015fk\u0131nl\u0131k edip-azanlar i\u00e7in son bir var\u0131\u015f yeridir."}, {"id": 560513, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5695, "text": "B\u00fct\u00fcn zamanlar boyunca i\u00e7inde kalacaklard\u0131r."}, {"id": 560514, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5696, "text": "Orada ne serinlik tadacaklar, ne bir i\u00e7ecek."}, {"id": 560515, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5697, "text": "Kaynar sudan ve irinden ba\u015fka."}, {"id": 560516, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5698, "text": "(\u0130\u015flediklerine) Uygun olan bir ceza olarak,"}, {"id": 560517, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5699, "text": "Do\u011frusu onlar, hesaba \u00e7ekileceklerini ummuyorlard\u0131."}, {"id": 560518, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5700, "text": "Bizim ayetlerimizi yalanlayabildikleri kadar yalanl\u0131yorlard\u0131."}, {"id": 560519, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5701, "text": "Oysa Biz, her\u015feyi yaz\u0131p saym\u0131\u015f\u0131zd\u0131r."}, {"id": 560520, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5702, "text": "\u015eimdi tad\u0131n. Size art\u0131k azaptan ba\u015fkas\u0131n\u0131 artt\u0131rmayaca\u011f\u0131z;"}, {"id": 560521, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5703, "text": "Ger\u00e7ek \u015fu ki, muttakiler i\u00e7in 'bir kurtulu\u015f ve mutluluk' vard\u0131r."}, {"id": 560522, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5704, "text": "Nice bah\u00e7eler ve \u00fcz\u00fcm ba\u011flar\u0131."}, {"id": 560523, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5705, "text": "G\u00f6\u011f\u00fcsleri hen\u00fcz tomurcuklanm\u0131\u015f ya\u015f\u0131t k\u0131zlar."}, {"id": 560524, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5706, "text": "Dopdolu kadehler."}, {"id": 560525, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5707, "text": "\u0130\u00e7inde, ne 'bo\u015f ve sa\u00e7ma bir s\u00f6z' i\u015fitirler, ne bir yalan."}, {"id": 560526, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5708, "text": "Rabbinden bir kar\u015f\u0131l\u0131k olmak \u00fczere yeterli bir ba\u011f\u0131\u015f(t\u0131r bu)."}, {"id": 560527, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5709, "text": "G\u00f6klerin, yerin ve ikisi aras\u0131nda bulunanlar\u0131n Rabbi Rahman olan (Allah); O\u2019na hitap etmeye g\u00fc\u00e7 yetiremezler."}, {"id": 560528, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5710, "text": "Ruh ve meleklerin saflar halinde duracaklar\u0131 g\u00fcn; Rahman'\u0131n kendilerine izin verdikleri d\u0131\u015f\u0131nda olanlar konu\u015fmazlar. (Konu\u015facak olan da,) Do\u011fruyu s\u00f6yleyecektir."}, {"id": 560529, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5711, "text": "\u0130\u015fte bu, hak g\u00fcnd\u00fcr. \u015eu halde dileyen Rabbine bir d\u00f6n\u00fc\u015fyolu edinsin."}, {"id": 560530, "identifier": "tr.bulac", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Al\u0130 Bula\u00e7", "transelator_en_name": "Al\u0130 Bula\u00e7", "format": "text", "type": "translation", "ayah_number": 5712, "text": "Ger\u00e7ekten Biz sizi yak\u0131n bir azab ile uyard\u0131k. Ki\u015finin kendi ellerinin \u00f6nceden takdim ettiklerine bakaca\u011f\u0131 g\u00fcn, kafir olan da: \"Ah, ke\u015fke ben bir toprak oluverseydim\" diyecek."}]