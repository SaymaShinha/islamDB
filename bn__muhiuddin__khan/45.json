ifier": "bn.bengali", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bn", "name": "\u09ae\u09c1\u09b9\u09bf\u0989\u09a6\u09cd\u09a6\u09c0\u09a8 \u0996\u09be\u09a8", "transelator_en_name": "Muhiuddin Khan", "format": "text", "type": "translation", "ayah_number": 4505, "text": "\u09af\u0996\u09a8 \u09ac\u09b2\u09be \u09b9\u09a4, \u0986\u09b2\u09cd\u09b2\u09be\u09b9\u09b0 \u0993\u09df\u09be\u09a6\u09be \u09b8\u09a4\u09cd\u09af \u098f\u09ac\u0982 \u0995\u09c7\u09df\u09be\u09ae\u09a4\u09c7 \u0995\u09c7\u09be\u09a8 \u09b8\u09a8\u09cd\u09a6\u09c7\u09b9 \u09a8\u09c7\u0987, \u09a4\u0996\u09a8 \u09a4\u09c7\u09be\u09ae\u09b0\u09be \u09ac\u09b2\u09a4\u09c7 \u0986\u09ae\u09b0\u09be \u099c\u09be\u09a8\u09bf \u09a8\u09be \u0995\u09c7\u09df\u09be\u09ae\u09a4 \u0995\u09bf ? \u0986\u09ae\u09b0\u09be \u0995\u09c7\u09ac\u09b2 \u09a7\u09be\u09b0\u09a3\u09be\u0987 \u0995\u09b0\u09bf \u098f\u09ac\u0982 \u098f \u09ac\u09bf\u09b7\u09df\u09c7 \u0986\u09ae\u09b0\u09be \u09a8\u09bf\u09b6\u09cd\u099a\u09bf\u09a4 \u09a8\u0987\u0964"}, {"id": 104216, "identifier": "bn.bengali", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bn", "name": "\u09ae\u09c1\u09b9\u09bf\u0989\u09a6\u09cd\u09a6\u09c0\u09a8 \u0996\u09be\u09a8", "transelator_en_name": "Muhiuddin Khan", "format": "text", "type": "translation", "ayah_number": 4506, "text": "\u09a4\u09be\