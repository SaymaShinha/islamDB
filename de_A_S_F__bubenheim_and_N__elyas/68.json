[{"id": 154799, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5272, "text": "Nun. Beim Schreibrohr und dem, was sie in Zeilen niederschreiben."}, {"id": 154800, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5273, "text": "Du bist durch die Gunst deines Herrn kein Besessener."}, {"id": 154801, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5274, "text": "Es wird f\u00fcr dich wahrlich Lohn geben, der nicht aufh\u00f6rt."}, {"id": 154802, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5275, "text": "Und du bist wahrlich von gro\u00dfartiger Wesensart."}, {"id": 154803, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5276, "text": "Du wirst sehen, und (auch) sie werden sehen,"}, {"id": 154804, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5277, "text": "wer von euch (durch Besessenheit) der Versuchung ausgesetzt ist."}, {"id": 154805, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5278, "text": "Gewi\u00df, dein Herr wei\u00df sehr wohl, wer von Seinem Weg abirrt, und Er kennt sehr wohl die Rechtgeleiteten."}, {"id": 154806, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5279, "text": "So gehorche nicht den Leugnern (der Botschaft)."}, {"id": 154807, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5280, "text": "Sie m\u00f6chten gern, da\u00df du schmeichelst, so da\u00df (auch) sie schmeicheln (k\u00f6nnen)."}, {"id": 154808, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5281, "text": "Und gehorche keinem ver\u00e4chtlichen Schw\u00f6rer,"}, {"id": 154809, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5282, "text": "Stichler und Verbreiter von Verleumdungen,"}, {"id": 154810, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5283, "text": "Verweigerer des Guten, der \u00dcbertretungen begeht und ein S\u00fcnder ist,"}, {"id": 154811, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5284, "text": "einem von grobem Benehmen und dar\u00fcber hinaus einem Eindringling"}, {"id": 154812, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5285, "text": "(nur deshalb), weil er Besitz und S\u00f6hne hat."}, {"id": 154813, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5286, "text": "Wenn ihm Unsere Zeichen verlesen werden, sagt er: \"(Es sind) Fabeln der Fr\u00fcheren.\""}, {"id": 154814, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5287, "text": "Wir werden ihn mit einem Merkmal auf der Nase zeichnen."}, {"id": 154815, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5288, "text": "Wir haben sie gepr\u00fcft, wie Wir die Besitzer des Gartens pr\u00fcften, als sie schworen, sie w\u00fcrden ihn bei Tagesanbruch abpfl\u00fccken,"}, {"id": 154816, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5289, "text": "und nicht(s) ausnahmen'."}, {"id": 154817, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5290, "text": "Da ging darin Verheerendes von deinem Herrn umher, w\u00e4hrend sie schliefen,"}, {"id": 154818, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5291, "text": "und er war dann am Morgen wie abgepfl\u00fcckt."}, {"id": 154819, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5292, "text": "Da riefen sie bei Tagesanbruch einander zu:"}, {"id": 154820, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5293, "text": "\"Geht in der Morgenfr\u00fche zu eurer Pflanzung, wenn ihr pfl\u00fccken wollt.\""}, {"id": 154821, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5294, "text": "Da zogen sie los und fl\u00fcsterten dabei einander zu:"}, {"id": 154822, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5295, "text": "\"In ihn soll heute ja kein Armer zu euch hineingelangen.\""}, {"id": 154823, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5296, "text": "Und sie gingen hin in der Morgenfr\u00fche, zum Verwehren bereit."}, {"id": 154824, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5297, "text": "Als sie ihn aber sahen, sagten sie: \"Wir haben uns f\u00fcrwahr geirrt."}, {"id": 150746, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5298, "text": "Nein! Vielmehr entbehren wir alles.\""}, {"id": 154660, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5299, "text": "Der Mittlere von ihnen sagte: \"Habe ich nicht zu euch gesagt: W\u00fcrdet ihr doch Allah preisen!\""}, {"id": 154679, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5300, "text": "Sie sagten: \"Preis sei unserem Herrn! Wir waren gewi\u00df ungerecht.\""}, {"id": 154697, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5301, "text": "Da wandten sie sich einander zu, sich gegenseitig tadelnd."}, {"id": 154625, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5302, "text": "Sie sagten: \"O wehe uns! Wir pflegten ja das Ma\u00df (an Frevel) zu \u00fcberschreiten."}, {"id": 154825, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5303, "text": "Vielleicht wird unser Herr uns zum Tausch daf\u00fcr einen besseren (Garten) als ihn geben! Gewi\u00df, nach Allah steht unser Begehren.\""}, {"id": 154826, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5304, "text": "So ist die Strafe; aber die Strafe des Jenseits ist wahrlich gr\u00f6\u00dfer, wenn sie nur w\u00fc\u00dften!"}, {"id": 154827, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5305, "text": "Gewi\u00df, f\u00fcr die Gottesf\u00fcrchtigen wird es bei ihrem Herrn die G\u00e4rten der Wonne geben."}, {"id": 154828, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5306, "text": "Sollen Wir etwa die Gottergebenen den \u00dcbelt\u00e4tern gleichstellen?"}, {"id": 154739, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5307, "text": "Was ist mit euch? Wie urteilt ihr?"}, {"id": 154829, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5308, "text": "Oder habt ihr eine Schrift, in der ihr lernt (und sucht);"}, {"id": 154830, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5309, "text": "ihr sollt darin wahrlich das haben, was ihr euch ausw\u00e4hlt."}, {"id": 154831, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5310, "text": "Oder habt ihr von Uns verbindliche Eide, die bis zum Tag der Auferstehung reichen; ihr bekommt wahrlich das, was ihr urteilt."}, {"id": 154832, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5311, "text": "Frage sie, wer von ihnen daf\u00fcr B\u00fcrge ist."}, {"id": 154833, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5312, "text": "Oder haben sie (etwa) Teilhaber? So sollen sie doch ihre Teilhaber beibringen, wenn sie wahrhaftig sind."}, {"id": 154834, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5313, "text": "Am Tag, da (manch) ein Schenkel entbl\u00f6\u00dft wird und sie aufgerufen werden, sich (anbetend) niederzuwerfen, aber da werden sie es nicht k\u00f6nnen,"}, {"id": 154835, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5314, "text": "w\u00e4hrend ihre Blicke dem\u00fctig sind und Erniedrigung sie bedeckt. Dabei waren sie doch bereits aufgefordert worden, sich (anbetend) niederzuwerfen, als sie (noch) heil waren."}, {"id": 154836, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5315, "text": "Lasse Mich (allein) mit denjenigen, die diese Aussage f\u00fcr L\u00fcge erkl\u00e4ren; Wir werden sie stufenweise (dem Verderben) n\u00e4herbringen, von wo sie nicht wissen."}, {"id": 154837, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5316, "text": "Und Ich gew\u00e4hre ihnen Aufschub. (Aber) gewi\u00df, Meine List ist fest."}, {"id": 154838, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5317, "text": "Oder verlangst du (etwa) von ihnen einen Lohn, so da\u00df sie mit Schulden belastet w\u00e4ren?"}, {"id": 154839, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5318, "text": "Oder besitzen sie (das Wissen \u00fcber) das Verborgene, so da\u00df sie (es) aufschreiben (k\u00f6nnen)?"}, {"id": 154840, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5319, "text": "So sei standhaft in Bezug auf das Urteil deines Herrn'. Und sei nicht wie der Gef\u00e4hrte des (gro\u00dfen) Fisches, als er voller Gram (zu Allah) rief."}, {"id": 154841, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5320, "text": "Wenn ihn nicht eine Gunst von seinem Herrn rechtzeitig erreicht h\u00e4tte, w\u00e4re er wahrlich auf das kahle Land geworfen worden und h\u00e4tte sich dabei Vorw\u00fcrfe zugezogen."}, {"id": 154842, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5321, "text": "Da erw\u00e4hlte ihn sein Herr und machte ihn zu einem der Rechtschaffenen."}, {"id": 154843, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5322, "text": "Diejenigen, die ungl\u00e4ubig sind, w\u00fcrden dich, wenn sie die Ermahnung h\u00f6ren, mit ihren Blicken wahrlich beinahe ins Straucheln bringen. Und sie sagen: \"Er ist ja f\u00fcrwahr besessen.\""}, {"id": 154844, "identifier": "de.bubenheim", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5323, "text": "Doch es ist nur eine Ermahnung f\u00fcr die Weltenbewohner."}]