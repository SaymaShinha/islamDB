[{"id": 155188, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5673, "text": "Wonach fragen sie sich?"}, {"id": 155189, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5674, "text": "Nach der gewaltigen Kunde,"}, {"id": 155190, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5675, "text": "\u00fcber die sie sich uneinig sind."}, {"id": 155191, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5676, "text": "Keineswegs! Sie werden (es noch) erfahren."}, {"id": 155192, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5677, "text": "Abermals: Keineswegs! Sie werden (es noch) erfahren."}, {"id": 155193, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5678, "text": "Haben Wir nicht die Erde zu einer Lagerstatt gemacht"}, {"id": 155194, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5679, "text": "und die Berge zu Pf\u00e4hlen?"}, {"id": 155195, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5680, "text": "Und Wir haben euch als Paare erschaffen."}, {"id": 155196, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5681, "text": "Und Wir haben euren Schlaf zum Ausruhen gemacht."}, {"id": 155197, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5682, "text": "Und Wir haben die Nacht zum Kleid gemacht."}, {"id": 155198, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5683, "text": "Und Wir haben den Tag zur Lebensf\u00fchrung gemacht."}, {"id": 155199, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5684, "text": "Und Wir haben \u00fcber euch sieben feste (Himmel) aufgebaut."}, {"id": 155200, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5685, "text": "Und Wir haben einen hell gl\u00fchenden Leuchtk\u00f6rper gemacht."}, {"id": 155201, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5686, "text": "Und Wir haben von den Regenwolken sich ergie\u00dfendes Wasser herabkommen lassen,"}, {"id": 155202, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5687, "text": "um damit Korn und Gew\u00e4chse hervorzubringen"}, {"id": 155203, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5688, "text": "und dichtbestandene G\u00e4rten."}, {"id": 155204, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5689, "text": "Gewi\u00df, der Tag der Entscheidung ist eine festgesetzte Zeit,"}, {"id": 155205, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5690, "text": "der Tag, da ins Horn geblasen wird und ihr in Scharen herkommt"}, {"id": 155206, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5691, "text": "und da der Himmel ge\u00f6ffnet und dann zu Toren wird"}, {"id": 155207, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5692, "text": "und da die Berge versetzt und dann zu einer Luftspiegelung werden."}, {"id": 155208, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5693, "text": "Gewi\u00df, die H\u00f6lle ist ein Hinterhalt,"}, {"id": 155209, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5694, "text": "f\u00fcr diejenigen, die das Ma\u00df (an Frevel) \u00fcberschreiten, eine Heimstatt,"}, {"id": 155210, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5695, "text": "lange Zeiten darin zu verweilen;"}, {"id": 155211, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5696, "text": "sie werden darin weder K\u00fchlung noch Getr\u00e4nk kosten,"}, {"id": 155212, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5697, "text": "au\u00dfer hei\u00dfem Wasser und stinkender Br\u00fche,"}, {"id": 155213, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5698, "text": "als angemessene Vergeltung."}, {"id": 155214, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5699, "text": "Sie erwarteten ja niemals eine Abrechnung"}, {"id": 155215, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5700, "text": "und erkl\u00e4rten Unsere Zeichen hartn\u00e4ckig f\u00fcr L\u00fcge."}, {"id": 155216, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5701, "text": "Doch alles haben Wir schriftlich erfa\u00dft."}, {"id": 155217, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5702, "text": "So kostet; Wir werden euch nur die Strafe mehren."}, {"id": 155218, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5703, "text": "Gewi\u00df, f\u00fcr die Gottesf\u00fcrchtigen wird es einen Ort des Erfolgs geben,"}, {"id": 155219, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5704, "text": "umfriedete G\u00e4rten und Rebst\u00f6cke"}, {"id": 155220, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5705, "text": "und pr\u00e4chtige, gleichaltrige (weibliche Wesen)"}, {"id": 155221, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5706, "text": "und ein (stets) voller Becher."}, {"id": 155222, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5707, "text": "Weder h\u00f6ren sie darin unbedachte Rede noch L\u00fcge."}, {"id": 155223, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5708, "text": "(Dies) als Belohnung von deinem Herrn, als hinreichende Gabe,"}, {"id": 155224, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5709, "text": "dem Herrn der Himmel und der Erde und dessen, was dazwischen ist, dem Allerbarmer. Sie verf\u00fcgen nicht dar\u00fcber, vor Ihm zu reden,"}, {"id": 155225, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5710, "text": "am Tag, da der Geist und die Engel in Reihe stehen. Sie werden nicht sprechen, au\u00dfer wem der Allerbarmer es erlaubt und wer Richtiges sagt."}, {"id": 155226, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5711, "text": "Das ist der wahrhaftig (eintreffend)e Tag. Wer nun will, nimmt zu seinem Herrn eine Heimkehr."}, {"id": 155227, "identifier": "de.bubenheim", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5712, "text": "Wir warnen euch ja vor naher Strafe, am Tag, da der Mensch schauen wird, was seine H\u00e4nde vorausgeschickt haben, und der Ungl\u00e4ubige sagen wird: \"O w\u00e4re ich doch Erde!\""}]