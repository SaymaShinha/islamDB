[{"id": 155690, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6180, "text": "Wehe jedem Stichler und N\u00f6rgler,"}, {"id": 155691, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6181, "text": "der Besitz zusammentr\u00e4gt und ihn z\u00e4hlt und immer wieder z\u00e4hlt,"}, {"id": 155692, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6182, "text": "wobei er meint, da\u00df sein Besitz ihn ewig leben lie\u00dfe!"}, {"id": 155693, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Keineswegs! Er wird ganz gewi\u00df in al-Hutama geworfen werden."}, {"id": 155694, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6184, "text": "Was l\u00e4\u00dft dich wissen, was al-Hutama ist?"}, {"id": 155695, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6185, "text": "(Sie ist) Allahs entfachtes Feuer,"}, {"id": 155696, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6186, "text": "das Einblick in die Herzen gewinnt."}, {"id": 155697, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6187, "text": "Gewi\u00df, es wird sie einschlie\u00dfen"}, {"id": 155698, "identifier": "de.bubenheim", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6188, "text": "in langgestreckten S\u00e4ulen."}]