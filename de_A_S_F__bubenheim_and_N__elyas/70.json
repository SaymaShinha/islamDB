[{"id": 154895, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5376, "text": "Es fragt ein Fragesteller nach einer Strafe, die hereinbrechen wird"}, {"id": 154896, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5377, "text": "f\u00fcr die Ungl\u00e4ubigen, (einer Strafe,) die niemand abwehren kann;"}, {"id": 154897, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5378, "text": "von Allah, dem Besitzer der Aufstiegswege."}, {"id": 154898, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5379, "text": "Es steigen die Engel und der Geist zu Ihm auf an einem Tag, dessen (Aus)ma\u00df f\u00fcnfzigtausend Jahre ist."}, {"id": 154899, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5380, "text": "Darum sei standhaft in sch\u00f6ner Geduld."}, {"id": 154900, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5381, "text": "Gewi\u00df, sie sehen sie weit entfernt,"}, {"id": 154901, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5382, "text": "Wir aber sehen sie nahe."}, {"id": 154902, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5383, "text": "Am Tag, da der Himmel wie siedendes \u00d6l"}, {"id": 154903, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5384, "text": "und die Berge wie gef\u00e4rbte Wolle sein werden"}, {"id": 154904, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5385, "text": "und kein warmherziger Freund seinen Freund (irgend etwas) fragt."}, {"id": 154905, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5386, "text": "obwohl sie ihrem Anblick ausgesetzt sein werden. Der \u00dcbelt\u00e4ter h\u00e4tte es gern, wenn er sich von der Strafe jenes Tages loskaufte mit seinen S\u00f6hnen"}, {"id": 154906, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5387, "text": "und seiner Gef\u00e4hrtin und seinem Bruder"}, {"id": 154907, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5388, "text": "und seiner Familie, die ihn aufgenommen hat,"}, {"id": 154908, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5389, "text": "und allen, die auf der Erde sind. (Er w\u00fcnscht,) dies m\u00f6ge ihn hierauf retten."}, {"id": 154909, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5390, "text": "Keineswegs! Es ist doch ein loderndes Feuer,"}, {"id": 154910, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5391, "text": "das die Kopfhaut abzieht',"}, {"id": 154911, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5392, "text": "das (den) rufen wird, wer den R\u00fccken kehrt und sich abkehrt,"}, {"id": 154912, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5393, "text": "(Besitz) zusammentr\u00e4gt und dann in Beh\u00e4ltern hortet."}, {"id": 154913, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5394, "text": "Gewi\u00df, der Mensch ist als kleinm\u00fctig erschaffen."}, {"id": 154914, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5395, "text": "Wenn ihm Schlechtes widerf\u00e4hrt, ist er sehr mutlos;"}, {"id": 154915, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5396, "text": "und wenn ihm Gutes widerf\u00e4hrt, (ist er) ein stetiger Verweigerer,"}, {"id": 154916, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5397, "text": "au\u00dfer den Betenden,"}, {"id": 154917, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5398, "text": "diejenigen, die in ihrem Gebet beharrlich sind"}, {"id": 154918, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5399, "text": "und die ein festgesetztes Recht an ihrem Besitz (zugestehen)"}, {"id": 154919, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5400, "text": "dem Bettler und dem Unbemittelten"}, {"id": 154920, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5401, "text": "und die den Tag des Gerichts f\u00fcr wahr halten"}, {"id": 154921, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5402, "text": "und diejenigen, die wegen der Strafe ihres Herrn besorgt sind,"}, {"id": 154922, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5403, "text": "- denn gewi\u00df, vor der Strafe ihres Herrn (kann) sich niemand sicher glauben,"}, {"id": 154923, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5404, "text": "- und diejenigen, die ihre Scham h\u00fcten,"}, {"id": 154924, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5405, "text": "au\u00dfer gegen\u00fcber ihren Gattinnen oder was ihre rechte Hand (an Sklavinnen) besitzt, denn sie sind (hierin) nicht zu tadeln,"}, {"id": 154925, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5406, "text": "- wer aber dar\u00fcber hinaus (etwas) begehrt, das sind die \u00dcbertreter,"}, {"id": 154926, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5407, "text": "- und diejenigen, die auf die ihnen anvertrauten G\u00fcter und ihre Verpflichtung achtgeben,"}, {"id": 154927, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5408, "text": "und diejenigen, die ihr Zeugnis in Aufrichtigkeit ablegen,"}, {"id": 154928, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5409, "text": "und diejenigen, die ihr Gebet einhalten."}, {"id": 154929, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5410, "text": "Jene werden sich in G\u00e4rten befinden und (darin) geehrt."}, {"id": 154930, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5411, "text": "Was ist mit denjenigen, die ungl\u00e4ubig sind, da\u00df sie hastig auf dich zukommen"}, {"id": 154931, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5412, "text": "rechts und links, in Gruppen aufgeteilt?"}, {"id": 154932, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5413, "text": "Begehrt (etwa) jedermann von ihnen, in einen Garten der Wonne eingelassen zu werden?"}, {"id": 154933, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5414, "text": "Keineswegs! Wir haben sie doch aus dem erschaffen, was sie wissen."}, {"id": 154934, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5415, "text": "Nein! Ich schw\u00f6re beim Herrn der Osten und der Westen, Wir haben f\u00fcrwahr die Macht dazu,"}, {"id": 154935, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5416, "text": "da\u00df Wir sie durch bessere als sie austauschen. Und niemand kann Uns zuvorkommen."}, {"id": 154936, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5417, "text": "So lasse sie nur schweifende Gespr\u00e4che f\u00fchren und ihr Spiel treiben, bis sie ihrem Tag begegnen, der ihnen angedroht ist,"}, {"id": 154937, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5418, "text": "dem Tag, da sie aus den Gr\u00e4bern eilig herauskommen werden, als w\u00fcrden sie hastig zu einem aufgerichteten Opferstein laufen,"}, {"id": 154938, "identifier": "de.bubenheim", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 5419, "text": "mit dem\u00fctigen Blicken, bedeckt mit Erniedrigung. Das ist der Tag, der ihnen immer wieder angedroht wurde."}]