[{"id": 155636, "identifier": "de.bubenheim", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6126, "text": "Wir haben ihn ja in der Nacht der Bestimmung hinabgesandt."}, {"id": 155637, "identifier": "de.bubenheim", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6127, "text": "Und was l\u00e4\u00dft dich wissen, was die Nacht der Bestimmung ist?"}, {"id": 155638, "identifier": "de.bubenheim", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6128, "text": "Die Nacht der Bestimmung ist besser als tausend Monate."}, {"id": 155639, "identifier": "de.bubenheim", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6129, "text": "Es kommen die Engel und der Geist in ihr mit der Erlaubnis ihres Herrn mit jeder Angelegenheit herab."}, {"id": 155640, "identifier": "de.bubenheim", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6130, "text": "Frieden ist sie bis zum Anbruch der Morgend\u00e4mmerung."}]