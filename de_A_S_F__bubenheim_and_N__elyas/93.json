[{"id": 155590, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6080, "text": "Bei der Morgenhelle"}, {"id": 155591, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6081, "text": "und der Nacht, wenn sie (alles) umh\u00fcllt!"}, {"id": 155592, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Dein Herr hat sich weder von dir verabschiedet noch ha\u00dft Er (dich)."}, {"id": 155593, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6083, "text": "Und das Jenseits ist wahrlich besser f\u00fcr dich als das Diesseits."}, {"id": 155594, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6084, "text": "Und dein Herr wird dir wahrlich geben, und dann wirst du zufrieden sein."}, {"id": 155595, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Hat Er dich nicht als Waise gefunden und (dir) dann Zuflucht verschafft"}, {"id": 155596, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6086, "text": "und dich irregehend gefunden und dann rechtgeleitet"}, {"id": 155597, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6087, "text": "und dich arm gefunden und dann reich gemacht?"}, {"id": 155598, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6088, "text": "Was nun die Waise angeht, so unterjoche (sie) nicht,"}, {"id": 155599, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6089, "text": "und was den Bettler angeht, so fahre (ihn) nicht an,"}, {"id": 155600, "identifier": "de.bubenheim", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 6090, "text": "und was die Gunst deines Herrn angeht, so erz\u00e4hle (davon)."}]