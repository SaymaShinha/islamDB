[{"id": 154311, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Bei dem Stern, wenn er sinkt!"}, {"id": 154312, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Nicht in die Irre geht euer Gef\u00e4hrte, und auch nicht einem Irrtum ist er erlegen,"}, {"id": 154313, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4787, "text": "und er redet nicht aus (eigener) Neigung."}, {"id": 154314, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4788, "text": "Es ist nur eine Offenbarung, die eingegeben wird."}, {"id": 154315, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4789, "text": "Belehrt hat ihn der Besitzer starker Kr\u00e4fte,"}, {"id": 154316, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4790, "text": "der (geistige) Macht besitzt. So stand Er da"}, {"id": 154317, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4791, "text": "und war am obersten Gesichtskreis."}, {"id": 154318, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Hierauf kam er n\u00e4her und stieg herunter,"}, {"id": 154319, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4793, "text": "so da\u00df er (nur) zwei Bogenl\u00e4ngen entfernt war oder noch n\u00e4her."}, {"id": 154320, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Da gab Er Seinem Diener (als Offenbarung) ein, was Er eingab."}, {"id": 154321, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Nicht hat sein Herz erlogen, was es sah."}, {"id": 154322, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Wollt ihr denn mit ihm streiten \u00fcber das, was er sieht?"}, {"id": 154323, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Und er sah ihn ja ein anderes Mal herabkommen,"}, {"id": 154324, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4798, "text": "beim Sidr-Baum des Endziels,"}, {"id": 154325, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4799, "text": "bei dem der Garten der Zuflucht ist."}, {"id": 154326, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4800, "text": "Als den Sidr-Baum \u00fcberdeckte, was (ihn) \u00fcberdeckte,"}, {"id": 154327, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4801, "text": "da wich der Blick nicht ab, noch \u00fcberschritt er das Ma\u00df."}, {"id": 154328, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Wahrlich, er sah von den Zeichen seines Herrn die gr\u00f6\u00dften."}, {"id": 154299, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Was meint ihr wohl zu al-Lat und al-'Uzza"}, {"id": 150403, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4804, "text": "und auch zu Manat, dieser anderen, der dritten?"}, {"id": 154330, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Ist denn f\u00fcr euch das, was m\u00e4nnlich ist, und f\u00fcr Ihn das, was weiblich ist (bestimmt)?"}, {"id": 154331, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Das w\u00e4re dann eine ungerechte Verteilung."}, {"id": 154332, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Das sind nur Namen, die ihr genannt habt, ihr und eure V\u00e4ter, f\u00fcr die Allah (aber) keine Erm\u00e4chtigung herabgesandt hat. Sie folgen nur Vermutungen und dem, wozu sie selbst neigen, wo doch bereits von ihrem Herrn die Rechtleitung zu ihnen gekommen ist."}, {"id": 154333, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Oder soll (etwa) der Mensch haben, was (immer) er (sich) w\u00fcnscht?"}, {"id": 154334, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4809, "text": "So geh\u00f6rt Allah das Jenseits und das Diesseits."}, {"id": 154335, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4810, "text": "Und wie viele Engel gibt es in den Himmeln, deren F\u00fcrsprache nichts n\u00fctzt, au\u00dfer nachdem Allah es erlaubt hat, wem Er will und wer Sein Wohlgefallen findet."}, {"id": 154336, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Diejenigen, die an das Jenseits nicht glauben, benennen die Engel f\u00fcrwahr mit Namen, wie weibliche Wesen benannt werden."}, {"id": 154337, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Sie haben jedoch kein Wissen hier\u00fcber. Sie folgen nur Vermutungen; die Vermutungen n\u00fctzen aber nichts gegen\u00fcber der Wahrheit."}, {"id": 154338, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4813, "text": "La\u00df nun ab von jemandem, der sich von Unserer Ermahnung abkehrt und nur das diesseitige Leben will."}, {"id": 154339, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Das ist ihr erreichter Wissensstand. Gewi\u00df, dein Herr kennt sehr wohl wer von Seinem Weg abirrt, und Er kennt sehr wohl die Rechtgeleiteten."}, {"id": 154340, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Allah geh\u00f6rt (alles), was in den Himmeln und was auf der Erde ist, auf da\u00df Er denjenigen, die B\u00f6ses tun, das vergelte, was sie tun, und da\u00df Er denjenigen, die Gutes tun, mit dem (Allerbesten vergelte."}, {"id": 154341, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Diejenigen, die schwerwiegende S\u00fcnden und Abscheulichkeiten meiden, au\u00dfer leichten Verfehlungen... Gewi\u00df, dein Herr ist Allumfassend in (Seiner) Vergebung. Er wei\u00df sehr wohl \u00fcber euch Bescheid, als Er euch aus der Erde hervorgebracht hat und als ihr Keimlinge in den Leibern eurer M\u00fctter gewesen seid. So erkl\u00e4rt nicht euch selbst f\u00fcr lauter. Er wei\u00df sehr wohl, wer gottesf\u00fcrchtig ist."}, {"id": 154342, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Was meinst du wohl zu demjenigen, der sich abkehrt"}, {"id": 154343, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4818, "text": "und (nur) wenig gibt und dann (ganz) aussetzt?"}, {"id": 154344, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Besitzt er (etwa) Wissen \u00fcber das Verborgene, da\u00df er sehen k\u00f6nnte?"}, {"id": 154345, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Oder wurde ihm nicht kundgetan, was auf den Bl\u00e4ttern Musas steht"}, {"id": 154346, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4821, "text": "und Ibrahims, der (seine Pflichten) erf\u00fcllte?"}, {"id": 154347, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4822, "text": "(N\u00e4mlich,) da\u00df keine lasttragende (Seele) die Last einer anderen auf sich nehmen wird"}, {"id": 154348, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4823, "text": "und da\u00df es f\u00fcr den Menschen nichts anderes geben wird als das, worum er sich (selbst) bem\u00fcht,"}, {"id": 154349, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4824, "text": "da\u00df sein Bem\u00fchen gesehen werden wird,"}, {"id": 154350, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4825, "text": "da\u00df ihm hierauf nach vollem Ma\u00df vergolten wird"}, {"id": 154351, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4826, "text": "und da\u00df zu deinem Herrn das Endziel sein wird"}, {"id": 154352, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4827, "text": "und da\u00df Er es ist, Der lachen und weinen l\u00e4\u00dft,"}, {"id": 154353, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4828, "text": "und da\u00df Er es ist, Der sterben l\u00e4\u00dft und wieder lebendig macht,"}, {"id": 154354, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4829, "text": "da\u00df Er beide Teile des Paares, das M\u00e4nnliche und das Weibliche, erschafft"}, {"id": 154355, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4830, "text": "aus einem Samentropfen, wenn er ausgespritzt wird,"}, {"id": 154356, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4831, "text": "und da\u00df Ihm die letzte Sch\u00f6pfung obliegt"}, {"id": 154357, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4832, "text": "und da\u00df Er es ist, Der reich macht und Der gen\u00fcgsam macht,"}, {"id": 154358, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4833, "text": "und da\u00df Er es ist, Der der Herr des Hundssterns ist,"}, {"id": 154131, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4834, "text": "da\u00df Er die fr\u00fcheren 'Ad vernichtete"}, {"id": 154275, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4835, "text": "und (auch) die Tamud; so lie\u00df Er nichts \u00fcbrig;"}, {"id": 154359, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4836, "text": "und zuvor das Volk Nuhs - gewi\u00df, sie waren ja noch ungerechter und \u00fcberm\u00e4\u00dfiger (in ihrem Frevel)."}, {"id": 154360, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4837, "text": "Und die umgest\u00fcrzte Stadt lie\u00df Er st\u00fcrzen."}, {"id": 154361, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4838, "text": "Und so \u00fcberdeckte sie, was (sie dann) \u00fcberdeckte."}, {"id": 154362, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4839, "text": "Welche der Wohltaten deines Herrn willst du nun bestreiten?"}, {"id": 154363, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Das ist ein Warner wie die fr\u00fcheren Warner."}, {"id": 154364, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Es steht bevor die immer n\u00e4her kommende (Stunde des Gerichts);"}, {"id": 154365, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4842, "text": "niemand au\u00dfer Allah kann sie beheben."}, {"id": 154366, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Wundert ihr euch denn \u00fcber diese Aussage"}, {"id": 154367, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4844, "text": "und lacht ihr, und weint ihr nicht,"}, {"id": 154368, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4845, "text": "und seid ihr noch belustigt?"}, {"id": 154369, "identifier": "de.bubenheim", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "de", "name": "Bubenheim & Elyas", "transelator_en_name": "A. S. F. Bubenheim and N. Elyas", "format": "text", "type": "translation", "ayah_number": 4846, "text": "Werft euch doch vor Allah nieder und dient (Ihm)."}]