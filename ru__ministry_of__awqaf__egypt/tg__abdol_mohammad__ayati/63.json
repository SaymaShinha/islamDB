[{"id": 541368, "identifier": "tg.ayati", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 488, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "AbdolMohammad Ayati", "format": "text", "type": "translation", "ayah_number": 5189, "text": "\u0427\u0443\u043d \u043c\u0443\u043d\u043e\u0444\u0438\u049b\u043e\u043d \u043d\u0430\u0437\u0434\u0438 \u0442\u0443 \u043e\u044f\u043d\u0434, \u0433\u04ef\u044f\u043d\u0434: \u00ab\u0428\u0430\u04b3\u043e\u0434\u0430\u0442 (\u0433\u0443\u0432\u043e\u04b3\u04e3) \u043c\u0435\u0434\u0438\u04b3\u0435\u043c, \u043a\u0438 \u0442\u0443 \u043f\u0430\u0451\u043c\u0431\u0430\u0440\u0438 \u0425\u0443\u0434\u043e \u04b3\u0430\u0441\u0442\u04e3\u00bb. \u0425\u0443\u0434\u043e \u043c\u0435\u0434\u043e\u043d\u0430\u0434, \u043a\u0438 \u0442\u0443 \u043f\u0430\u0451\u043c\u0431\u0430\u0440\u0430\u0448 \u04b3\u0430\u0441\u0442\u04e3 \u0432\u0430 \u0425\u0443\u0434\u043e \u0448\u043e\u04b3\u0438\u0434\u04e3 \u043c\u0435\u0434\u0438\u04b3\u0430\u0434, \u043a\u0438 \u043c\u0443\u043d\u043e\u0444\u0438\u049b\u043e\u043d \u0434\u0443\u0440\u04ef\u0493\u0433\u04ef\u044f\u043d\u0434."}, {"id": 541370, "identifier": "tg.ayati", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 488, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "AbdolMohamm