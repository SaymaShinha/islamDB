[{"id": 541357, "identifier": "tg.ayati", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "AbdolMohammad Ayati", "format": "text", "type": "translation", "ayah_number": 5178, "text": "\u0425\u0443\u0434\u043e\u0440\u043e \u0442\u0430\u0441\u0431\u0435\u04b3 \u0433\u0443\u044f\u043d\u0434 \u04b3\u0430\u0440 \u0447\u04e3 \u0434\u0430\u0440 \u043e\u0441\u043c\u043e\u043d\u04b3\u043e\u0432\u0443 \u04b3\u0430\u0440 \u0447\u04e3 \u0434\u0430\u0440 \u0437\u0430\u043c\u0438\u043d \u0430\u0441\u0442, \u043e\u043d \u0444\u0430\u0440\u043c\u043e\u043d\u0440\u0430\u0432\u043e\u0438 \u043f\u043e\u043a \u0430\u0437 \u0430\u0439\u0431\u0440\u043e, \u043e\u043d \u0493\u043e\u043b\u0438\u0431\u0438 \u04b3\u0430\u043a\u0438\u043c\u0440\u043e!"}, {"id": 541358, "identifier": "tg.ayati", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "AbdolMohammad Ayati", "format": "text", "type": "translation", "ayah_number": 5179, "text": "\u04ee\u0441\u0442 \u0425\u0443\u0434\u043e\u0435, \u043a\u0438 \u0431\u0430 \u043c\u0438\u0451\u043d\u0438 \u043c\u0430\u0440\u0434\u0443\u043c\u0435 \u0431\u0435\u0441\u0430\u0432\u043e\u0434 \u043f\u0430\u0451\u043c\u0431\u0430\u0440\u0435 \u0430\u0437 \u0445\u0443\u0434\u0430\u0448\u043e\u043d \u0444