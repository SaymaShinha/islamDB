2, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "AbdolMohammad Ayati", "format": "text", "type": "translation", "ayah_number": 4747, "text": "\u043e\u043d\u043e\u043d, \u043a\u0438 \u0441\u0430\u0440\u0433\u0430\u0440\u043c\u0438 \u043a\u043e\u0440\u0434\u043e\u043d \u0431\u043e\u0442\u0438\u043b\u0438 \u0445\u0443\u0434\u0430\u043d\u0434!"}, {"id": 540931, "identifier": "tg.ayati", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "AbdolMohammad Ayati", "format": "text", "type": "translation", "ayah_number": 4748, "text": "\u0420\u04ef\u0437\u0435, \u043a\u0438 \u043e\u043d\u04b3\u043e\u0440\u043e \u0431\u0430 \u049b\u0430\u04b3\u0440 \u0431\u0430 \u04b7\u043e\u043d\u0438\u0431\u0438 \u04b7\u0430\u04b3\u0430\u043d\u043d\u0430\u043c \u043a\u0430\u0448\u0430\u043d\u0434."}, {"id": 540932, "identifier": "tg.ayati", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tg", "name": "\u041e\u044f\u0442\u04e3", "transelator_en_name": "Abdol