043b\u0430\u0445 \u0431\u044b\u043b \u043c\u0438\u043b\u043e\u0441\u0442\u0438\u0432 \u043a \u043d\u0435\u043c\u0443, \u043d\u0438\u0441\u043f\u043e\u0441\u043b\u0430\u0432 \u041a\u043e\u0440\u0430\u043d; \u0435\u043c\u0443 \u043f\u043e\u0432\u0435\u043b\u0435\u043d\u043e \u043f\u0440\u043e\u044f\u0432\u043b\u044f\u0442\u044c \u0442\u0435\u0440\u043f\u0435\u043d\u0438\u0435 \u0438 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u043f\u043e\u0432\u0438\u043d\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0410\u043b\u043b\u0430\u0445\u0443. \u0412 \u0441\u0443\u0440\u0435 \u0442\u0430\u043a\u0436\u0435 - \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0435\u0440\u0435\u0436\u0435\u043d\u0438\u0435 \u0442\u0435\u043c, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043b\u044e\u0431\u044f\u0442 \u044d\u0442\u0443 \u0436\u0438\u0437\u043d\u044c, \u043f\u0440\u0435\u0434\u043f\u043e\u0447\u0438\u0442\u0430\u044f \u0435\u0451 \u0436\u0438\u0437\u043d\u0438 \u0431\u0443\u0434\u0443\u0449\u0435\u0439, \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043e \u043d\u0430\u0437\u0438\u0434\u0430\u043d\u0438\u0438, \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u0449\u0435\u043c\u0441\u044f \u0432 \u044d\u0442\u0438\u0445 \u0430\u0439\u0430\u0442\u0430\u0445, \u0438 \u0443\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f, \u0447\u0442\u043e \u043b\u044e\u0434\u044f\u043c \u043d\u0435 \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u044d\u0442\u043e \u043d\u0430\u0437\u0438\u0434\u0430\u043d\u0438\u0435, \u0435\u0441\u043b\u0438 \u043d\u0430 \u0442\u043e \u043d\u0435 \u0431\u0443\u0434\u0435\u0442 \u0432\u043e\u043b\u0438 \u0410\u043b\u043b\u0430\u0445\u0430 \u0412\u0441\u0435\u0432\u044b\u0448\u043d\u0435\u0433\u043e, \u0438 \u0447\u0442\u043e \u043c\u0438\u043b\u043e\u0441\u0435\u0440\u0434\u0438\u0435 \u0410\u043b\u043b\u0430\u0445\u0430 \u0438 \u0415\u0433\u043e \u043d\u0430\u043a\u0430\u0437\u0430\u043d\u0438\u0435 \u043f\u043e\u0434\u044