[{"id": 130456, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5932, "text": "P\u0159i nebi a HV\u011aZD\u011a NO\u010cN\u00cd!"}, {"id": 130457, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5933, "text": "A co pov\u00ed ti, co jest hv\u011bzda no\u010dn\u00ed?"}, {"id": 130458, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5934, "text": "Hv\u011bzda pronikav\u011b (z\u00e1\u0159iv\u00e1)."}, {"id": 130459, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5935, "text": "Zajist\u00e9 ka\u017ed\u00e1 du\u0161e nad sebou str\u00e1\u017ece m\u00e1."}, {"id": 130460, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5936, "text": "Nech\u0165 pohl\u00e9dne jen \u010dlov\u011bk na to, z \u010deho stvo\u0159en byl."}, {"id": 130461, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5937, "text": "Stvo\u0159en byl z vody vyst\u0159ikl\u00e9,"}, {"id": 130462, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5938, "text": "z prost\u0159ed ledv\u00ed a kost\u00ed hrudn\u00edch vy\u0161l\u00e9."}, {"id": 130463, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5939, "text": "Pro\u010de\u017e jist\u011b (B\u016fh) moc m\u00e1; by vr\u00e1til jej (k \u017eivotu),"}, {"id": 130464, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5940, "text": "v den, kdy zkoum\u00e1na budou tajemstv\u00ed,"}, {"id": 130465, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5941, "text": "a kdy s\u00edly m\u00edti (\u010dlov\u011bk) nebude, ani\u017e pomocn\u00edka."}, {"id": 130466, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5942, "text": "P\u0159i nebi, je\u017e se ot\u00e1\u010d\u00ed;"}, {"id": 130467, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5943, "text": "p\u0159i zemi, je\u017e se rozpuk\u00e1v\u00e1,"}, {"id": 130468, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5944, "text": "(p\u0159\u00eds\u00e1m, \u017ee Kor\u00e1n) zajist\u00e9 jest slovo rozhodn\u00e9,"}, {"id": 130469, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5945, "text": "nen\u00ed Jen \u017eertem (m\u00edn\u011bno)."}, {"id": 130470, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Oni pak kuj\u00ed l\u00e9\u010dky sv\u00e9 \u2014"}, {"id": 130471, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5947, "text": "a j\u00e1 t\u00e9\u017e kuji l\u00e9\u010dky sv\u00e9!"}, {"id": 130472, "identifier": "cs.nykl", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5948, "text": "Pomalu jednej s nev\u011brci: poshov jim (je\u0161t\u011b) chvilenku!"}]