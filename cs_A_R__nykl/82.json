[{"id": 130354, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5830, "text": "Kdy\u017e nebesa se ROZPOLT\u00cd;"}, {"id": 130355, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5831, "text": "a hv\u011bzdy budou rozpr\u00e1\u0161eny;"}, {"id": 130356, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5832, "text": "a mo\u0159e splynou do sebe;"}, {"id": 130357, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5833, "text": "a hroby zdola navrch budou p\u0159evr\u00e1ceny:"}, {"id": 130358, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5834, "text": "(tehdy) zv\u00ed du\u0161e, co byla u\u010dinila, i co byla zame\u0161kala."}, {"id": 130359, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5835, "text": "\u00d3 \u010dlov\u011b\u010de! co zm\u00e1milo t\u011b ohledn\u011b P\u00e1na tv\u00e9ho vz\u00e1cn\u00e9ho,"}, {"id": 130360, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5836, "text": "jen\u017e stvo\u0159il t\u011b a vyrovnal a spr\u00e1vnou (podobu) ti dal;"}, {"id": 130361, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5837, "text": "v jak\u00e9koliv pak podob\u011b cht\u011bl, t\u011b sestavil?"}, {"id": 130362, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5838, "text": "Nikoliv! naopak, vylhan\u00fdm naz\u00fdvaj\u00ed soud (posledn\u00ed)!"}, {"id": 130363, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5839, "text": "V\u0161ak zajist\u00e9 nad v\u00e1mi str\u00e1\u017eci stoj\u00ed,"}, {"id": 130364, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5840, "text": "vz\u00e1cn\u00ed, (v\u0161e) p\u00ed\u0161\u00edc\u00ed;"}, {"id": 130365, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5841, "text": "co kon\u00e1te (dob\u0159e) v\u011bd\u00ed!"}, {"id": 130366, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5842, "text": "Ctnostn\u00ed v rozko\u0161n\u00e9m budou (bydlen\u00ed),"}, {"id": 130367, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5843, "text": "zavil\u00ed pak v pekla plameni,"}, {"id": 130368, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5844, "text": "p\u00e9ci budou se v den soudu (posledn\u00ed):"}, {"id": 130369, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5845, "text": "z n\u011bj nebudou se moci vzd\u00e1liti!"}, {"id": 130370, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5846, "text": "Kdo pov\u00ed ti, co jest den soudu (posledn\u00ed)?"}, {"id": 130371, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5847, "text": "A znovu: kdo pov\u00ed ti, co jest den soudu (posledn\u00ed)?"}, {"id": 130372, "identifier": "cs.nykl", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5848, "text": "Den, kdy, nebude du\u0161e moci u\u010diniti pro jinou du\u0161i prani\u010deho: rozhodnut\u00ed v den ten pat\u0159iti bude Bohu !"}]