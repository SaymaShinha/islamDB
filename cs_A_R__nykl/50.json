[{"id": 129169, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4631, "text": "Q\u00c1F. P\u0159i Kor\u00e1nu vzne\u0161en\u00e9m!"}, {"id": 129170, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4632, "text": "Naopak, div\u00ed se, \u017ee p\u0159i\u0161el jim varovatel ze st\u0159edu jejich; i \u0159\u00edkaj\u00ed nev\u011b\u0159\u00edc\u00ed; \u201eToto\u0165 v\u011bc jest divn\u00e1!"}, {"id": 129171, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4633, "text": "\u017de kdy\u017e zem\u0159eme a budeme v prach obr\u00e1ceni? \u2014 To v\u011bru n\u00e1vrat jest z daleka!\u201c"}, {"id": 129172, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4634, "text": "My (dob\u0159e) v\u00edme, kolik pohltila jich zem\u011b, neb u n\u00e1s jest Kniha, (v\u0161e) uschov\u00e1vaj\u00edc\u00ed."}, {"id": 129173, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4635, "text": "Naopak, l\u017e\u00ed nazvali pravdu, kdy\u017e p\u0159i\u0161la jim: i v postaven\u00ed jsou spleten\u00e9m."}, {"id": 129174, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4636, "text": "Co\u017e nepohl\u00e9dli k nebes\u016fm nad sebou, aby vid\u011bli, jak vybudovali jsme je a zkr\u00e1\u0161lili tak, \u017ee nen\u00ed skulin v nich?"}, {"id": 129175, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4637, "text": "A zemi rozprost\u0159eli jsme a rozh\u00e1zeli po n\u00ed temena hor; a dali vzr\u016fsti na n\u00ed v\u0161eho druhu (rostlin) rozko\u0161n\u00fdch:"}, {"id": 129176, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4638, "text": "k nahl\u00e9dnut\u00ed a napomenuti v\u0161em slu\u017eebn\u00edk\u016fm kaj\u00edcn\u00fdm."}, {"id": 129177, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4639, "text": "A seslali jsme s nebe vodu po\u017eehnanou, j\u00ed\u017e vzr\u016fsti d\u00e1v\u00e1me zahrad\u00e1m a zrn\u00ed obiln\u00e9mu;"}, {"id": 129178, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4640, "text": "a datlovn\u00edk\u016fm vysok\u00fdm s vrcholy obt\u011b\u017ekan\u00fdmi,"}, {"id": 129179, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4641, "text": "k v\u00fd\u017eiv\u011b slu\u017eebn\u00edk\u016fm na\u0161im a vodou o\u017eivujeme zemi zmrtv\u011blou. Takov\u00e9 bude i z mrtv\u00fdch vzk\u0159\u00ed\u0161en\u00ed."}, {"id": 129180, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4642, "text": "P\u0159ed nimi ji\u017e lh\u00e1\u0159i zvali proroky: lid Noem\u016fv a obyvatel\u00e9 Ar-Rassu a (kmen) Tsem\u00fad;"}, {"id": 129181, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4643, "text": "a (kmen) '\u00c1d a Farao a brat\u0159\u00ed Lotovi;"}, {"id": 129182, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4644, "text": "a obyvatel\u00e9 hvozdu a lid Tubba; ti v\u0161ichni lh\u00e1\u0159i nazvali proroky i uskute\u010dnil se (nad nimi) trest sl\u00edben\u00fd."}, {"id": 129183, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4645, "text": "Co\u017e znaveni jsme prvn\u00edm stvo\u0159en\u00edm? Naopak: ale oni p\u0159ec jsou v pochyb\u00e1ch ohledn\u011b stvo\u0159en\u00ed nov\u00e9ho!"}, {"id": 129184, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4646, "text": "My stvo\u0159ili jsme \u010dlov\u011bka a v\u00edme, co na\u0161ept\u00e1v\u00e1 mu du\u0161e jeho: a my bli\u017e\u0161\u00edmi jsme mu \u017e\u00edly hrdeln\u00e9."}, {"id": 129185, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4647, "text": "Kdy\u017e p\u0159ij\u00edmati budou oba p\u0159ij\u00edma\u010di (v\u00fdpov\u011bdi), jeden z prava, druh\u00fd z leva sed\u00edc\u00ed:"}, {"id": 129186, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4648, "text": "nepronese slova, ani\u017e by pozorovatel vedle n\u011bho nebyl pohotov\u011b (je zapsati)."}, {"id": 129187, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4649, "text": "A p\u0159ijde (na\u0148) mr\u00e1kota smrti skute\u010dn\u00e9: \u201eToto jest, \u010demu cht\u011bl ses vyhnouti!\u201c"}, {"id": 129188, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4650, "text": "A zatroubeno bude v roh: toto\u0165 jest den trestu sl\u00edben\u00fd!"}, {"id": 129189, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4651, "text": "I p\u0159ijde jedna ka\u017ed\u00e1 du\u0161e: s ka\u017edou pak honec a sv\u011bdek jej\u00ed."}, {"id": 129190, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4652, "text": "\u201e\u017dil's v lhostejnosti k tomuto dnu: nu\u017e, sejmuli jsme ti clonu a zrak tv\u016fj dnes ostr\u00fd jest!\u201c"}, {"id": 129191, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4653, "text": "A \u0159ekne druh jeho v\u011brn\u00fd: \u201eToto jest co m\u00e1m (pro tebe) p\u0159ipraveno.\u201c"}, {"id": 129162, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4654, "text": "\u201eUvrcte, vy dva, v peklo v\u0161echny nev\u011brce vzdorovit\u00e9:"}, {"id": 129095, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4655, "text": "zabra\u0148ova\u010de dobra, p\u0159estupn\u00edky, pochybova\u010de;"}, {"id": 129192, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4656, "text": "kte\u0159\u00ed stav\u011bli vedle Boha bohy druh\u00e9: uvrcte je v trest p\u0159\u00edsn\u00fd!\u201c"}, {"id": 129193, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4657, "text": "\u0158ekne druh jeho v\u011brn\u00fd: \u201ePane n\u00e1\u0161, j\u00e1\u0165 nesv\u00e1d\u011bl jsem jej, n\u00fdbr\u017e on (s\u00e1m) byl v bloud\u011bn\u00ed d\u00e1ln\u00e9m.\u201c"}, {"id": 129194, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4658, "text": "(\u0158ekne B\u016fh): \u201eNeve\u010fte sporu p\u0159ede mnou: dal jsem v\u00e1m ji\u017e p\u0159ed t\u00edm slib v\u00fdhr\u016f\u017en\u00fd:"}, {"id": 129195, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4659, "text": "nem\u011bn\u00ed se pak slovo m\u00e9: a nejsem utiskovatelem slu\u017eebn\u00edk\u016f sv\u00fdch.\u201c"}, {"id": 129196, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4660, "text": "V den ten \u0159ekneme peklu: \u201eZda\u017e napln\u011bno jsi?\u201c a \u0159ekne: \u201eJest (je\u0161t\u011b) v\u00edce jich?\u201c"}, {"id": 129197, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4661, "text": "P\u0159ibl\u00ed\u017eena pak bude k bohabojn\u00fdm zahrada (r\u00e1je) nedalek\u00e1:"}, {"id": 129198, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4662, "text": "\u201eToto\u0165 jest, co sl\u00edbeno bylo v\u00e1m, v\u0161em kaj\u00edcn\u00fdm a (z\u00e1kony) zachov\u00e1vaj\u00edc\u00edm:"}, {"id": 129199, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4663, "text": "t\u011bm, kdo\u017e b\u00e1li se milosrdn\u00e9ho v skrytosti a p\u0159ich\u00e1zej\u00ed se srdcem kaj\u00edcn\u00fdm."}, {"id": 129200, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4664, "text": "Vstupte do n\u00ed v pokoji: tento\u0165 jest v\u011b\u010dnosti den.\u201c"}, {"id": 129201, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4665, "text": "V n\u00ed budou m\u00edti, co\u017ekoliv cht\u00edti budou a u n\u00e1s bude (toho) rozhojn\u011bn\u00ed."}, {"id": 129202, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4666, "text": "A kolik zahladili jsme p\u0159ed nimi pokolen\u00ed, je\u017e mocnej\u0161\u00ed jich byla silou: a prohledejte zemi, je-li v n\u00ed p\u0159ed n\u00e1mi \u00fato\u010di\u0161t\u011b!"}, {"id": 129203, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4667, "text": "Zajist\u00e9 v tomto jest napomenut\u00ed tomu, jen\u017e m\u00e1 srdce a pop\u0159\u00e1v\u00e1 sluchu a (s\u00e1m) jest sv\u011bdkem."}, {"id": 129204, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4668, "text": "Stvo\u0159ili jsme pak nebesa i zemi a v\u0161e, co\u017e mezi ob\u011bma jest v \u0161esti dnech a nedotkla se n\u00e1s \u00fanava."}, {"id": 129205, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4669, "text": "Trp\u011bliv\u011b sn\u00e1\u0161ej tedy, co \u0159\u00edkaj\u00ed a vyvy\u0161uj P\u00e1na sv\u00e9ho ve chv\u00e1le jeho p\u0159ed slunce v\u00fdchodem a z\u00e1padem."}, {"id": 129206, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4670, "text": "A za noci t\u00e9\u017e chval jej a v posledn\u00edch dvou na tv\u00e1\u0159 padnut\u00edch."}, {"id": 129207, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4671, "text": "A p\u0159islouchej ke dnu, kdy zavol\u00e1 provolavatel z m\u00edsta bl\u00edzk\u00e9ho."}, {"id": 129208, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4672, "text": "V den, kdy zaslechnou zav\u0159esknut\u00ed v pravd\u011b, to\u0165 den bude vystoupen\u00ed (z hrob\u016f):"}, {"id": 129209, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4673, "text": "My\u0165 zajist\u00e9 k\u0159\u00eds\u00edme i umrtvujeme a k n\u00e1m vede (cesta) n\u00e1vratu."}, {"id": 129210, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4674, "text": "V den, kdy rozpolt\u00ed se zem\u011b nad nimi \u2014 to\u0165 shrom\u00e1\u017ed\u011bn\u00ed bude pro n\u00e1s snadn\u00e9."}, {"id": 129211, "identifier": "cs.nykl", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 4675, "text": "My nejl\u00e9pe v\u00edme, co \u0159\u00edkaj\u00ed a ty nejsi nad nimi n\u00e1siln\u00edkem. Tud\u00ed\u017e napom\u00ednej Kor\u00e1nem ty, kdo\u017e boj\u00ed se v\u00fdhr\u016f\u017en\u00fdch slib\u016f m\u00fdch."}]