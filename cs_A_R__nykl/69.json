[{"id": 129859, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5324, "text": "NEVYHNUTELN\u00c1!"}, {"id": 129860, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5325, "text": "Co jest nevyhnuteln\u00e1?"}, {"id": 129861, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5326, "text": "A co objasn\u00ed ti, co jest nevyhnuteln\u00e1?"}, {"id": 129862, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5327, "text": "Vylhan\u00fdm nazvaly (kmeny) Tsemm\u00fad i \u2018\u00c1d \u00fader (rozhodn\u00fd)."}, {"id": 129863, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5328, "text": "A pokud se t\u00fd\u010de (kmene) Tsem\u00fad, zahlazeni byli ranou h\u0159m\u011bj\u00edc\u00ed:"}, {"id": 129864, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5329, "text": "a pokud se t\u00fd\u010de kmene \u2018\u00c1d, zahlazeni byli vichrem bou\u0159n\u00fdm, b\u011bsn\u00edc\u00edm."}, {"id": 129865, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5330, "text": "(B\u016fh) pod\u0159\u00eddil si jej proti nim po sedm noc\u00ed a osm dn\u00ed po sob\u011b n\u00e1sleduj\u00edc\u00edch: tehdy byl bys vid\u011bl lid jeho k zemi sra\u017een\u00fd, jako by byli pni datlovn\u00edk\u016f vykotlam\u00fdmi."}, {"id": 129866, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5331, "text": "Zda\u017e pak vid\u011bl's, \u017ee n\u011bkdo z nich zbyl?"}, {"id": 129867, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5332, "text": "Farao, a kdo\u017e byli p\u0159ed n\u00edm a m\u011bsta vyvr\u00e1cen\u00e1 - t\u00e9\u017e h\u0159\u00edchu se dopustili:"}, {"id": 129868, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5333, "text": "neb neposlu\u0161ni byli proroka P\u00e1na sv\u00e9ho i postihl je trestem z\u00faro\u010den\u00fdm."}, {"id": 129869, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5334, "text": "Zajist\u00e9, kdy\u017e voda v\u0161e zaplavila, nesli jsme v\u00e1s v (lodi) pluj\u00edc\u00ed,"}, {"id": 129870, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5335, "text": "abychom u\u010dinili v\u00e1m ji upom\u00ednkou a aby chovalo ji (ucho) pozorn\u00e9."}, {"id": 129871, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5336, "text": "A\u017e zatroubeno bude na roh jedn\u00edm trouben\u00edm,"}, {"id": 129872, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5337, "text": "a vznesena bude zem\u011b i hory a (ob\u00e9) zhrouceno jedn\u00edm zhroucen\u00edm:"}, {"id": 129873, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5338, "text": "v den onen pak ud\u00e1 se ud\u00e1lost nezvratn\u00e1:"}, {"id": 129874, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5339, "text": "i roz\u0161t\u011bp\u00ed se nebe, neb den ten bude miziv\u00fdm:"}, {"id": 129875, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5340, "text": "and\u011bl\u00e9 budou po stran\u00e1ch jeho; a ponese osm jich tr\u016fn P\u00e1na tv\u00e9ho v den onen nad sebou."}, {"id": 129876, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5341, "text": "V den onen p\u0159edvedeni budete (p\u0159ed Boha) a nic skryto nebude z va\u0161ich (skutk\u016f) skryt\u00fdch."}, {"id": 129877, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5342, "text": "A pokud se t\u00fd\u010de toho; jemu\u017e d\u00e1na bude Kniha jeho do pravice, \u0159ekne: \u201eZde \u010dt\u011bte v Knize m\u00e9."}, {"id": 129878, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5343, "text": "J\u00e1\u0165 v\u017edy jsem myslil, \u017ee (jednou) setk\u00e1m se s \u00fa\u010dtem sv\u00fdm.\u201c"}, {"id": 129879, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5344, "text": "Ten\u0165 nalezne se v \u017eit\u00ed p\u0159\u00edjemn\u00e9m,"}, {"id": 129880, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5345, "text": "v zahrad\u011b zv\u00fd\u0161en\u00e9,"}, {"id": 129881, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5346, "text": "jej\u00ed\u017e plody bl\u00edzky budou:"}, {"id": 129882, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5347, "text": "\u201eJezte a pijte ve zdrav\u00ed v odm\u011bnu za to, co p\u0159edeslali jste ve dnech minul\u00fdch!\u201c"}, {"id": 129883, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5348, "text": "A pokud se t\u00fd\u010de toho, jemu\u017e d\u00e1na bude kniha jeho do levice, \u0159ekne: \u201eK\u00e9\u017e by nikdy nebyla mi b\u00fdvala d\u00e1na kniha moje,"}, {"id": 129884, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5349, "text": "a nikdy nebyl bych zv\u011bd\u011bl, jak stoj\u00ed si \u00fa\u010det m\u016fj!"}, {"id": 129885, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5350, "text": "K\u00e9\u017e (smrt) byla by ukon\u010dila (\u017eit\u00ed moje)!"}, {"id": 129886, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5351, "text": "Nic platno nen\u00ed mi jm\u011bn\u00ed moje!"}, {"id": 129887, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5352, "text": "Odplata ode mne moc moje.\u201c"}, {"id": 129888, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5353, "text": "\u201eChopte se ho a \u0159et\u011bzy sva\u017ete,"}, {"id": 129889, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5354, "text": "pak v ohni (pekeln\u00e9m) jej p\u0159ip\u00e9kejte:"}, {"id": 129890, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5355, "text": "pak v \u0159et\u011bzy sedmdes\u00e1t loket zd\u00e9l\u00ed jej navl\u00e9kn\u011bte;"}, {"id": 129891, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5356, "text": "neb nebyl v\u011b\u0159il v Boha mocn\u00e9ho,"}, {"id": 129892, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5357, "text": "ani\u017e krmiti sna\u017eil se nuzn\u00e9ho."}, {"id": 129893, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5358, "text": "Pro\u010de\u017e nem\u00e1 zde dnes p\u0159\u00edtele v\u0159el\u00e9ho,"}, {"id": 129894, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5359, "text": "ani\u017e pokrmu, vyjma hnisu smrdut\u00e9ho,"}, {"id": 129895, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5360, "text": "z n\u011bj\u017e po\u017e\u00edvati budou pouze h\u0159\u00ed\u0161n\u00edci.\u201c"}, {"id": 129896, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5361, "text": "Nebudu p\u0159isahati p\u0159i tom, co vid\u00edte,"}, {"id": 129897, "identifier": "cs.nykl", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translati