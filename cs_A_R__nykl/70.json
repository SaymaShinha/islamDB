id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5388, "text": "a p\u0159\u00edbuzn\u00fdmi, ji\u017e chr\u00e1n\u00ed ho,"}, {"id": 129922, "identifier": "cs.nykl", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5389, "text": "a obyvateli zem\u011b v\u0161echn\u011bmi, kdyby pak spasilo jej to."}, {"id": 129923, "identifier": "cs.nykl", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5390, "text": "V\u0161ak nikoliv! Neb v\u00fdhe\u0148,"}, {"id": 129924, "identifier": "cs.nykl", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5391, "text": "chopiv\u0161i (je) za lebku,"}, {"id": 129925, "identifier": "cs.nykl", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5392, "text": "zv\u00e1t bude ty, kdo\u017e z\u00e1dy obr\u00e1tili se a odch\u00e1zeli,"}, {"id": 129926, "identifier": "cs.nykl", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5393, "text": "a (statky) hromadili a schra\u0148ovali."}, {"id": 129927, "identifier": "cs.nykl", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Nykl", "transelator_en_name": "A. R. Nykl", "format": "text", "type": "translation", "ayah_number": 5394, "text": "Zajis\u0165 \u010dlov\u011bk stvo\u0159en byl prchl\u00fdm:"}, {"id": 129928, "id