[{"id": 166866, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4902, "text": "The Compassionate."}, {"id": 166867, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4903, "text": "Hath taught the Qur'an."}, {"id": 166868, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4904, "text": "He created man."}, {"id": 166869, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4905, "text": "He taught distinctness."}, {"id": 166870, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4906, "text": "The sun and the moon are in a reckoning."}, {"id": 166871, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4907, "text": "And the herbs and the trees do obe isance.\""}, {"id": 166872, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4908, "text": "And the heaven! He hath elevated it, and He hath set the balance."}, {"id": 166873, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4909, "text": "That ye should not trespass in respect of the balance."}, {"id": 166874, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4910, "text": "And observe the weight with equity, and make not deficient the balance."}, {"id": 166875, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4911, "text": "And the earth! He hath lain it out for the creatures."}, {"id": 166876, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4912, "text": "Therein are fruit and palm-trees sheathed."}, {"id": 166877, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4913, "text": "And grain chaffed and other food."}, {"id": 166878, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4914, "text": "Which, then, of the benefits of your Lord, will ye twain belie?"}, {"id": 166879, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4915, "text": "He created man of clay like, Unto pottery."}, {"id": 166880, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4916, "text": "And He created the Jinn of a flame of fire."}, {"id": 166881, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4917, "text": "Which, then, of the benefits of your Lord will ye twain belie"}, {"id": 166882, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4918, "text": "He is Lord of the two easts and Lord of the two wests."}, {"id": 166883, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4919, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166884, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4920, "text": "He hath let loose the two seas."}, {"id": 166885, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4921, "text": "In-between the twain is a barrier which they pass not."}, {"id": 166886, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4922, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166887, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4923, "text": "There come forth from the twain the pearl and the coral."}, {"id": 166888, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4924, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166889, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4925, "text": "His are the ships with elevated sails upon the sea like mountains."}, {"id": 166890, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4926, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166891, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4927, "text": "Everyone that is thereon will Pass away."}, {"id": 166892, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4928, "text": "And there will remain the countenance of thine Lord, Owner of Majesty and Beneficence."}, {"id": 166860, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4929, "text": "Which, then, of the benefits of your Lord, will ye twain belie?"}, {"id": 166893, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4930, "text": "Of Him beggeth whosoever is in the heavens and the earth; every day He is in a new affair."}, {"id": 166894, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4931, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166895, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4932, "text": "Anon We shall direct Our selves to you, O ye two classes."}, {"id": 166896, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4933, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166897, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4934, "text": "O assembly of Jinn and man! if ye be able to pass out of the regions of the heavens and the earth, then pass out; ye can not pass out except with authority."}, {"id": 166898, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4935, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166899, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4936, "text": "There shall be sent against you both flame of fire and smoke, and ye shall not be able to defend yourselves."}, {"id": 166900, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4937, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166901, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4938, "text": "And when the heaven will be rent in sunder and will become rosy like Unto red hide."}, {"id": 166902, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4939, "text": "Whfch, then, of the benefits of your Lord will ye twain belie!"}, {"id": 166903, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4940, "text": "Of his sin will be questioned that Day neither man nor jinn."}, {"id": 166904, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4941, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166905, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4942, "text": "The culprits will be recognised by their marks, and will be lain hold of by the forelocks and the feet."}, {"id": 166906, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4943, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166907, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4944, "text": "Yonder is the Hell which the culprits belied."}, {"id": 166908, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4945, "text": "Going round between it and balling water fierce."}, {"id": 166909, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4946, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166910, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4947, "text": "And for him who dreadeth the standing before his Lord will be two Gardens."}, {"id": 166911, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4948, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166912, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4949, "text": "With spreading branches."}, {"id": 166913, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4950, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166914, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4951, "text": "In which, will be two fountains running."}, {"id": 166915, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4952, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166916, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4953, "text": "In which will be of every fruit two kinds."}, {"id": 166917, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4954, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166918, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4955, "text": "Reclining on carpets whereof the linings will be of brocade; and the fruit of the two Gardens shall be near at hand."}, {"id": 166919, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4956, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166920, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4957, "text": "Therein shall be those of refraining looks whom before them hath deflowered neither man nor jinn."}, {"id": 166921, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4958, "text": "Which, then, of the benefits of Your Lord will ye twain belie?"}, {"id": 166922, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4959, "text": "As though they are jacinth and coral."}, {"id": 166923, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4960, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166924, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4961, "text": "Shall the recompense of kindness be aught save kindness?"}, {"id": 166925, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4962, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166926, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4963, "text": "And beside the two there will be two other Gardens."}, {"id": 166927, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4964, "text": "Which, then, of the benefits of your Lord will ye twain beli?"}, {"id": 166928, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4965, "text": "Dark-green."}, {"id": 166929, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4966, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166930, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4967, "text": "In which will be two fountains gushing forth."}, {"id": 166931, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4968, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166932, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4969, "text": "In which will be fruit, the date-palm and pomegranate."}, {"id": 166933, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4970, "text": "Which, then, of the benefits of Your Lord will ye twain belie?"}, {"id": 166934, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4971, "text": "Therein will be damsels agreeable and beauteous."}, {"id": 166935, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4972, "text": "Which, then, of the benefits of your Lord will ye twain belie?"}, {"id": 166936, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4973, "text": "Fair ones, confined in tents."}, {"id": 166937, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4974, "text": "Which, then, of the benefits of Your Lord will ye twain belie?"}, {"id": 166938, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4975, "text": "There hath deflowered them neither man nor jinn."}, {"id": 166939, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4976, "text": "Which, then, of the benefits of Your Lord will ye twain belie?"}, {"id": 166940, "identifier": "en.daryabadi", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4977, "text": "Reclining upon cushions green and carpets be