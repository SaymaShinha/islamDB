[{"id": 167553, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5592, "text": "Surely there hath come upon man a space of time when he was not a thing worth mentioning."}, {"id": 167554, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5593, "text": "Verily We! We created man from a sperm of mixtures, that We might prove him, wherefore We made him hearing, seeing."}, {"id": 167555, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5594, "text": "Verily We! We shewed him the way; then he becometh either thankful or ingrate."}, {"id": 167556, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5595, "text": "Verily We! We have gotten ready for the infidels chains and collars and a Blaze."}, {"id": 167557, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5596, "text": "Verily the pious shall drink of a cup whereof the admixture is like Unto camphor."}, {"id": 167558, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5597, "text": "From a fountain whence the bondmen of Allah will drink, causing it to gush abundantly."}, {"id": 167559, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5598, "text": "They fulfill their vow and dread a Day the evil whereof shall be widespreading."}, {"id": 167560, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5599, "text": "And they feed with food, for love of Him, the needy, and the orphan and the captive."}, {"id": 167562, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5600, "text": "Saying: we feed you only for the sake of Allah; we desire not from you any recompense or thanks."}, {"id": 167563, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5601, "text": "Verily we dread from our Lord a Day grim and distressful."}, {"id": 167564, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5602, "text": "Wherefore Allah shall preserve them from the evil of that Day, and shall cause them to meet brightness and joy."}, {"id": 167565, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5603, "text": "And shall recompense them for that which they patiently bare with a Garden and silken garment."}, {"id": 167566, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5604, "text": "Reclining therein upon couches, they shall behold therein neither sun nor hurting cold."}, {"id": 167567, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5605, "text": "And close upon them will be the shades thereof, and low will hang the clusters thereof greatly."}, {"id": 167568, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5606, "text": "And brought round amongst them will be vessels of silver and also goblets of glass."}, {"id": 167569, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5607, "text": "Godblets of silver, they shall have filled them to exact measure."}, {"id": 167570, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5608, "text": "And therein they shall be given to drink of a cup whereof the admixture will be ginger."}, {"id": 167571, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5609, "text": "From a fountain therein, named Salsabil."}, {"id": 167572, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5610, "text": "And there shall go round Unto them youths ever-young. When thou seest them thou wouldst deem them pearls unstrung."}, {"id": 167573, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5611, "text": "And when thou lookest them thou shalt behold delight and a dominion magnificent."}, {"id": 167574, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5612, "text": "Upon them shall be garments of fine green silk and of brocades. And adorned they shall be with bracelets of silver: and their Lord shall give them drink a beverage pure."}, {"id": 167575, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5613, "text": "Verily this is for you by way of recompense, and your endeavour hath been accepted."}, {"id": 167576, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5614, "text": "Verily We! it is We Who have revealed Unto thee the Qur'an, a gradual revelation."}, {"id": 167577, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5615, "text": "Wherefore persevere thou with the commandment of thy Lord, and obey not thou of them, any sinner or ingrate."}, {"id": 167578, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5616, "text": "And remember thou the name of thy Lord in the morning and in the evening."}, {"id": 167579, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5617, "text": "And during the night - worship Him; and hallow Him the livelong night."}, {"id": 167580, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5618, "text": "Verily those love the Herein, and leave in front of them a heavy day."}, {"id": 167581, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5619, "text": "It it We Who creared them and made them firm of make. And whenever We list, We can replace them with others like Unto them."}, {"id": 167582, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5620, "text": "Verily this is an admonition; then whosoever Will, may choose a way Unto his Lord."}, {"id": 167583, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5621, "text": "And ye will not, unless Allah willeth. Verily Allah is ever Knowing, Wise.'"}, {"id": 167584, "identifier": "en.daryabadi", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5622, "text": "He maketh whomsoever He listeth to enter His mercy. And the wrong-doers! for them He hath gotten ready a torment afflective."}]