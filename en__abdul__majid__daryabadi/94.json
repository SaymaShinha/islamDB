ajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6097, "text": "Then when thou becometh relieved, toil;"}, {"id": 168053, "identifier": "en.daryabadi", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6098, "text": "And Unto thine Lord, attend."}]