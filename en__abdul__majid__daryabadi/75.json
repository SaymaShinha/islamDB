[{"id": 167513, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5552, "text": "I swear by the Day of Resurrection."}, {"id": 167514, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5553, "text": "And I swear by the self-reproaching soul."}, {"id": 167515, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5554, "text": "Deemest man that We shall not assemble his bones?"}, {"id": 167516, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Yea! WE are Able to put together evenly his finger-tips."}, {"id": 167517, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5556, "text": "Aye! man desireth that he may sin before him."}, {"id": 167518, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5557, "text": "He asketh: when will the Day of Resurrection be?"}, {"id": 167519, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5558, "text": "When, then, the sight shall be confounded."}, {"id": 167520, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5559, "text": "And the moon shall be ecliped."}, {"id": 167521, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5560, "text": "And the sun and the moon shall be joined."}, {"id": 167522, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5561, "text": "On that Day shall man say: whither is the fleeing."}, {"id": 167523, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5562, "text": "By no means! NO refuge!"}, {"id": 167524, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5563, "text": "Unto thy Lord that Day is the recourse."}, {"id": 167525, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5564, "text": "To man will be declared on that Day that which he hath sent on and left behind."}, {"id": 167526, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5565, "text": "Aye! man against himself shall be an enlightenment."}, {"id": 167527, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5566, "text": "Although he may put forth his pleas."}, {"id": 167528, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5567, "text": "Move not thy tongue therewith that thou mayest hasten therewith."}, {"id": 167529, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5568, "text": "Verily upon us is the collecting thereof and the reciting thereof."}, {"id": 167530, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5569, "text": "Wherefore when We reciteit, follow thou the reciting thereof."}, {"id": 167531, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5570, "text": "And thereafter verily upon us is is the expounding thereof."}, {"id": 167532, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5571, "text": "By no means! Verily ye love the Herein."}, {"id": 167533, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5572, "text": "And leave the Hereafter."}, {"id": 167534, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5573, "text": "Countenances on that Day shall be resplendent,"}, {"id": 167535, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5574, "text": "Looking toward their Lord."}, {"id": 167536, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5575, "text": "And countenances on that Day shall be scowling."}, {"id": 167537, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5576, "text": "Imagining that there will befall them a waiste-breaking calomity."}, {"id": 167538, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5577, "text": "By no means! When it cometh up to he collar-bone."}, {"id": 167539, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5578, "text": "And it is cried. who can charm?"}, {"id": 167540, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5579, "text": "And he bethinketh that it is the time of parting."}, {"id": 167541, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5580, "text": "And one shank is entangled with the other shank."}, {"id": 167542, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5581, "text": "Unto thy Lord that Day is the drive."}, {"id": 167543, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5582, "text": "He neither believed nor prayed,"}, {"id": 167544, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5583, "text": "But he belied and turned away."}, {"id": 167545, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5584, "text": "Then he departed Unto his house hold conceitedly."}, {"id": 167546, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5585, "text": "Woe Unto thee; woe!"}, {"id": 167547, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5586, "text": "Again, woe Unto thee, woe!"}, {"id": 167548, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5587, "text": "Deemest man that he is to be left uncontrolled?"}, {"id": 167549, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5588, "text": "Was he not a sperm of emission emitted?"}, {"id": 167550, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Then he became a clot; then He created him and formed him."}, {"id": 167551, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5590, "text": "And made of him the two sexes, male and female."}, {"id": 167552, "identifier": "en.daryabadi", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5591, "text": "Is not That One then Able to quicken the dead?"}]