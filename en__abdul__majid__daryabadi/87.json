[{"id": 167906, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5949, "text": "Hallow thou the name of thine Lord, the Most High,"}, {"id": 167907, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5950, "text": "Who hath created and then proportioned,"}, {"id": 167908, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5951, "text": "And Who hath disposed and then guided,"}, {"id": 167909, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5952, "text": "And Who bringeth forth the pasturage,"}, {"id": 163233, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5953, "text": "Then maketh it to stubble dusky."}, {"id": 167910, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5954, "text": "We shall enable thee to recite, and then thou shalt not forget,"}, {"id": 167911, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5955, "text": "Save that which Allah may will. Verily He knoweth the public and that which is hidden."}, {"id": 167912, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5956, "text": "And We shall make easy Unto thee the easy way."}, {"id": 167913, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5957, "text": "Wherefore admonish thou; admonition hath surely profited,"}, {"id": 167914, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Admonished is he indeed who feareth,"}, {"id": 167915, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5959, "text": "And the wretched shunneth it-."}, {"id": 167916, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5960, "text": "He Who shall roast into the Great Fire,"}, {"id": 167917, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5961, "text": "Wherein he shall neither die nor live,"}, {"id": 167918, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5962, "text": "He indeed hath attained bliss who hath cleansed himself,"}, {"id": 167919, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5963, "text": "And who remembereth the name of his Lord, and then prayeth."}, {"id": 167920, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5964, "text": "But ye prefer the life of this world,"}, {"id": 167921, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5965, "text": "Whereas the Hereafter is better and more lasting."}, {"id": 167922, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5966, "text": "Verily this is in Writs ancient"}, {"id": 167923, "identifier": "en.daryabadi", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5967, "text": "Writs of Ibrahim and Musa."}]