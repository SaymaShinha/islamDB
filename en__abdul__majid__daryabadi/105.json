[{"id": 168141, "identifier": "en.daryabadi", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6189, "text": "Hast thou not observed what wise thy Lord dealt with the fellows of the elephant?"}, {"id": 168142, "identifier": "en.daryabadi", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6190, "text": "Put He not their stratagem to straying."}, {"id": 168143, "identifier": "en.daryabadi", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6191, "text": "And He sent against them birds in flocks."}, {"id": 168144, "identifier": "en.daryabadi", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_