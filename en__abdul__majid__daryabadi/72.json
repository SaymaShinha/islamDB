[{"id": 167411, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5448, "text": "Say thou: it hath been revealed Unto me that a company of the Jinn listened, and said, verily we have listened to a Recitation wondrous."}, {"id": 167412, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5449, "text": "Guiding Unto rectitude; wherefore we have believed therein, and we shall by no means associate with our Lord anyone."}, {"id": 167413, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5450, "text": "And He - exalted be the majesty of our Lord!-hath taken neither a spouse nor a son."}, {"id": 167414, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5451, "text": "And the foolish among us were wont to forge lie against Allah exceedingly."}, {"id": 167415, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5452, "text": "And verily we! we had imagined that humankind and Jinn would never forge against Allah a lie."}, {"id": 167416, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5453, "text": "And persons among humankind have been seeking refuge with persons of the Jinn, so that they increased them in evil disposition."}, {"id": 167417, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5454, "text": "And indeed they imagined, even as ye imagined, that Allah will not raise any one."}, {"id": 167418, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5455, "text": "And we sought to reach the heaven; then we found it filled with a strong guard and darting meteors."}, {"id": 167419, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5456, "text": "And we were wont to sit on seats therein to listen; but whosoever listeneth now findeth for him a dartin meteor in wait."}, {"id": 167420, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5457, "text": "And we know not whether evil is boded for those who are on the earth, or whether their Lord intendeth for them a right direction."}, {"id": 167421, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5458, "text": "And of us there are some righteous, and of us are some otherwise; we have been following very diverse."}, {"id": 167422, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5459, "text": "And we known that we cannot frustrate Allah in the earth, nor can we frustrate Him by flight."}, {"id": 167423, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5460, "text": "And when we heard the Message of guidance, we believed therein; and whosoever believeth in his Lord, he shall fear neither diminution nor wrong."}, {"id": 167424, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5461, "text": "And of us some are Muslims, and of us some are deviators. Then whosoever hath embraced Islam - such have endeavoured after a Path of rectitude."}, {"id": 167425, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5462, "text": "And as for the deviators, for Hell they shall be fuel."}, {"id": 167426, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5463, "text": "And had they kept to the path surely We would have watered them with rain plenteous."}, {"id": 167427, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5464, "text": "That We might try them there by. And whosoever turneth aside from the remembrance of his Lord, him He shall thrust into a torment vehement."}, {"id": 167428, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5465, "text": "And the prostrations are for Allah; wherefore call not along with Allah anyone."}, {"id": 167429, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5466, "text": "And when the bondman of Allah stood calling upon Him, they well nigh pressed on him stifling."}, {"id": 167430, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5467, "text": "Say thou: I simply call upon Allah, and I associate not with Him any-one."}, {"id": 167431, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5468, "text": "Say thou: 'verily own not for you power of hurt nor of benefit"}, {"id": 167432, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5469, "text": "Say thou: 'verily none can protect me from Allah, nor can I find beside Him any refuge."}, {"id": 167433, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5470, "text": "Mine is but the publishing from Allah and His messages; and whosoever disobeyeth Allah and His apostle, his portion verily is the Hell-Fire as abiders therein for evermore."}, {"id": 167434, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5471, "text": "They shall go on denying until they; behold that which they are promised wherefore then they shall know who were weaker in protectors and fewer in number."}, {"id": 167435, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5472, "text": "Say thou: know not whather that which ye are promised is nigh, or whether my Lord hath appointed for it a distant term."}, {"id": 167436, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5473, "text": "He is the Knower of the unseen, and He discloseth not His unseen Unto anyone."}, {"id": 167379, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5474, "text": "Save Unto an apostle chosen. And then He causeth to go before him and behind him a guard."}, {"id": 167437, "identifier": "en.daryabadi", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5475, "text": "That He may know that they have preached the message of their Lord. And He comprehendeth whatever is with them, and He keepeth count of everything numbered."}]