[{"id": 167438, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5476, "text": "O thou enwrapped!"}, {"id": 167439, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5477, "text": "Stay up the night long save a little -"}, {"id": 167440, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5478, "text": "A half thereof, or abate a little thereof,"}, {"id": 167441, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5479, "text": "Or increase thereto. And intone the Qur'an with a measured intontion. s"}, {"id": 167442, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5480, "text": "Verily We! anon We shall cast upon thee a weighty word."}, {"id": 167443, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5481, "text": "Verily the rising by night! it is most curbing and most conducive to right speech."}, {"id": 167444, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5482, "text": "Verily there is for thee by day occupation prolonged."}, {"id": 167445, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5483, "text": "And remember thou the name of thy Lord, and devote thyself to Him with an exclusive devotion."}, {"id": 167446, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5484, "text": "Lord of the east and the west! No God is there but He! Wherefore take Him for thy trustee."}, {"id": 167447, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5485, "text": "And bear thou with patience whatsoever they say, and depart from them with a becoming departure."}, {"id": 167448, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5486, "text": "And let Me alone with the beliers, owners of ease: and respite thou them a little."}, {"id": 167449, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5487, "text": "Verily with us are heavy fetters and Scorch."}, {"id": 167450, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5488, "text": "And a food that choketh and a torment afflictive."}, {"id": 167451, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5489, "text": "On a Day whereon the earth and the mountains shall quake, and the mountains shall become a sand-heap poured forth."}, {"id": 167452, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5490, "text": "Verily We! We have sent Unto you an apostle, a witness over you, even as We sent Unto Fir'awn an apostle."}, {"id": 167453, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5491, "text": "Then Fir'awn disobeyed the apostle, wherefore We laid hold of him with a painful hold."}, {"id": 167454, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5492, "text": "How then, if ye disbelieve, shall ye escape, on a Day which shall make children grey-headed."}, {"id": 167455, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5493, "text": "And the heaven shall be split there in. His promise needs must be fulfilled."}, {"id": 167456, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5494, "text": "Verily this is an admonition; let him therefore, who will, choose a way Unto his Lord."}, {"id": 167457, "identifier": "en.daryabadi", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 509, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5495, "text": "Verily thy Lord knoweth that thou stayest up near two- thirds of the night, Or half thereof, or a third thereof, and also a party of those who are with thee. And Allah measureth the night and the day. He knoweth that ye can compute it not, He hath relented toward you. Wherefore recite now of the Qur'an, so much as is easy. He knoweth that there will be among you some diseased, and others shall be travelling in the land seeking grace of Allah, and others shall be fighting in the way of Allah. Wherefore recite thereof so much as is easy; and establish the prayer and give the poor-rate and lend Unto Allah a goodly loan. Whatsoever good ye shall send on for your souls, ye shall find it with Allah, better and greater in reward. And ask forgiveness of Allah; verily Allah is Forgiving, Merciful."}]