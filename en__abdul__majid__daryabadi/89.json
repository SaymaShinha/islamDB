[{"id": 167950, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5994, "text": "By the dawn,"}, {"id": 167951, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5995, "text": "And by ten nights,"}, {"id": 167952, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5996, "text": "And by the even and the odd,"}, {"id": 167953, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5997, "text": "And by the night when it departeth,"}, {"id": 167954, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5998, "text": "Indeed in that there is an oath for a man of sense."}, {"id": 167955, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5999, "text": "Bethinkest thou not in what wise thy Lord did with the 'A'ad:"}, {"id": 167956, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6000, "text": "The people of many-columned lram."}, {"id": 167957, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6001, "text": "The like of which was not built in the cities,"}, {"id": 167958, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6002, "text": "And With Thamud who hewed out rocks in the vale,"}, {"id": 167959, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6003, "text": "And with Fir'awn, owner of the stakes -"}, {"id": 167960, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6004, "text": "Who all waxed exorbitant in the cities,"}, {"id": 167961, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6005, "text": "And multiplied therein corruption."}, {"id": 167962, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6006, "text": "Wherefore thy Lord poured on them the scourge of His torment."}, {"id": 167963, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6007, "text": "Verily thy Lord is in an ambuscade,"}, {"id": 167964, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6008, "text": "As for man - when his Lord proveth him and so honoureth him and is bounteous Unto him, then he saith: 'my Lord hath honoured me,"}, {"id": 167965, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6009, "text": "And when He proveth him, and so stinteth Unto him his provision, he saith: 'my Lord hath despised me."}, {"id": 167966, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6010, "text": "By no means! But ye honour not the orphan,"}, {"id": 167967, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6011, "text": "Nor urge upon each other the feeding of the poor,"}, {"id": 167968, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6012, "text": "And devour the inheritance devouring greedily,"}, {"id": 167969, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6013, "text": "And ye love riches with exceeding love."}, {"id": 167970, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6014, "text": "By no means! when the earth shall be ground with grinding, grinding,"}, {"id": 167971, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6015, "text": "And thy Lord shall come and the angels, rank on rank."}, {"id": 167972, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6016, "text": "And the Hell on that Day shall be brought nigh. On that Day man shall remember, but how will remembrance avail him?"}, {"id": 167973, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6017, "text": "He will say. 'would that I had sent before for this life of mine!"}, {"id": 167974, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6018, "text": "Wherefore on that Day none shall torment with His torment."}, {"id": 167975, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6019, "text": "Nor shall any bind with His bond."}, {"id": 164333, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6020, "text": "O thou peaceful soul!"}, {"id": 167976, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6021, "text": "Return Unto thine Lord well pleased and well-pleasing."}, {"id": 167977, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6022, "text": "Enter thou among My righteous bondmen!"}, {"id": 167978, "identifier": "en.daryabadi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6023, "text": "And enter thou My Garden."}]