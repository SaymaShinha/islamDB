[{"id": 166586, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4613, "text": "O ye who believe! be not forward in the presence of Allah and His apostle, and fear Allah; verily Allah is Hearing, Knowing."}, {"id": 166587, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4614, "text": "Ye who believe raise not your voices above the voice of the prophet, nor shout loud Unto him in discourse as ye shout loud Unto one another, lest your works may be rendered of non-effect, while ye perceive not."}, {"id": 166570, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4615, "text": "Verily those who lower their voices in the presence of the apostle of Allah -those are they whose hearts Allah hath disposed Unto piety: theirs will be forgiveness and a mighty hire."}, {"id": 166588, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4616, "text": "Verily those who call aloud Unto thee from without the inner apartments, most of them reflect not.\""}, {"id": 166525, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4617, "text": "And had they had patience till thou camest forth Unto them, it had surely been best for them; and Allah is Forgiving, Merciful."}, {"id": 166589, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4618, "text": "ye who believe! if an evil-doer Came Unto you with a report, then inquire strictly, lest ye hurt a people in ignorance and repent thereafter of that which ye have done."}, {"id": 166590, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4619, "text": "And know that verily among you is the apostle of Allah. Were he to obey you in many affairs, ye would surely be in trouble but Allah hath endeared belief to you and hath made it fair-seeming in your hearts, and hath rendered detestable Unto you infidelity and wickedness and disobedience. These! they are the men of rectitude."}, {"id": 166591, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4620, "text": "Through grace from Allah and His favour; and Allah is Knowing, Wise."}, {"id": 166592, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4621, "text": "And if two parties of the believers fall to mutual fighting, then make reconciliation between the twain. Then if one of them rebelleth against the other, fight that Party which rebelleth till it returneth Unto the affair of Allah; then if it returneth, make reconciliation between the twain with justice and be equitable; verily Allah loveth the equitable."}, {"id": 166593, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4622, "text": "The believers are but brethren; wherefore make reconciliation between your brethren and fear Allah, that haply ye may be shewn mercy."}, {"id": 166594, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4623, "text": "O ye who believe! let not one group scoff at another group, belike they may be better than they are, nor let some women scoff at other women, belike they may be better than they are. And traduce not one another, nor revile one another by odious appellations! ill is the name of sin after belief. And whosoever will not repent, then those, they are the wrong-doers."}, {"id": 166596, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4624, "text": "O ye who believe! avoid much suspicion; verily some suspicion is a sin. And espy not, nor backbite one another: would any of you love to eat the flesh of his dead brother? Ye detest that. And fear Allah verily Allah is Relenting, Merciful."}, {"id": 166597, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4625, "text": "O Humankind verily We! We have created you Of a male and female, and We have made you nations and tribes that ye might know one another. Verily the noblest of you with Allah is the most God-fearing of you; verily Allah is Knowing, Aware,"}, {"id": 166598, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4626, "text": "The desert Arabs say: we have believed. Say thou: believe not, but rather say: We have submitted; and belief hath not yet entered into your hearts. And if ye obey Allah and His apostle, He shall not diminish from you aught Of you, wOrks verily Allah is Forgiving, Merciful."}, {"id": 166599, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4627, "text": "The believers are only those who believed in Allah and His apostle and thereafter doubted not, and strave with their riches and their persons in the cause of Allah. Those! they are the truthful."}, {"id": 166600, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4628, "text": "Say thou: Would ye appraise Allah of your religion, whereas Allah knoweth whatsoever is in the heavens and whatsoever is in the earth, and Allah is of everything Aware!"}, {"id": 166601, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4629, "text": "They make it a favour Unto thee that they have surrendered. Say thou: deem not your surrender a favour Unto me; nay! Allah hath conferred a favour Unto you inasmuch as He hath guided you to belief, if ye are sincere."}, {"id": 166602, "identifier": "en.daryabadi", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4630, "text": "Verily Allah knoweth the Unseen of the heavens and the earth; and Allah is the Beholder of that which ye work."}]