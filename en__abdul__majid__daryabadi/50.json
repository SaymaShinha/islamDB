[{"id": 166603, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4631, "text": "Qaf. By the Qur'an glorious, We have sent thee as a warner."}, {"id": 166604, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4632, "text": "Aye! they marvel that there hath come Unto them a warner from among themselves; so the infidels Say: this is a thing wondrous!"}, {"id": 166605, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4633, "text": "Shell We be brought back when we are dead and have become dust? That is a return remote!"}, {"id": 166606, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4634, "text": "Surely We know that which the earth consumeth of them, and with Us is, Book preserved."}, {"id": 166607, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4635, "text": "Aye! they belie the truth when it cometh Unto them; wherefore they are in an affair confused."}, {"id": 166608, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4636, "text": "Have they not looked up to the heaven above them, in what wise We have constructed and bedecked it, and that therein is no rift?"}, {"id": 166609, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4637, "text": "And the earth! We have spread it forth, and have cast therein firm mountains, and have caused to grow therein of every beauteous kind of plants:"}, {"id": 166610, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4638, "text": "An insight and an admonition for every bondman penitent."}, {"id": 166611, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4639, "text": "And We have sent down from the heaven water brest wherewith We have caused gardens to grow and the grain reaped."}, {"id": 166612, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4640, "text": "And tall date-palms laden with clusters ranged."}, {"id": 166613, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4641, "text": "As a provision for our bondmen; and therewith We have quickened a dead land. Even so will be the coming forth."}, {"id": 166614, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4642, "text": "And before them the people of Nuh belied, and so did the dwellers Of Rass and the Thamud."}, {"id": 166615, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4643, "text": "And the A-ad, and Fir'awn. and the brethren of Lut."}, {"id": 166616, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4644, "text": "And the dwellers in the wood, and the people of Tubba. Each one belied the apostles, wherefore fulfilled was My judgement."}, {"id": 166617, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4645, "text": "Are We then wearied with the first creation? Aye! they are in dubiety regarding a new creation."}, {"id": 166618, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4646, "text": "And assuredly We have created man and We know whatsoever his soul whispereth Unto him, and We are nigher Unto him than his jugular vein."}, {"id": 166619, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4647, "text": "Behold! when the two receivers receive-one on the right hand and one on the left a sitter."}, {"id": 166620, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4648, "text": "Not a word he uttereth but there is with him a watcher ready."}, {"id": 166490, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4649, "text": "And the stupor of death will come in truth: this is that which thou hast been shunning."}, {"id": 166511, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4650, "text": "And the trumpet will be blown: this is the Day of the Threatening."}, {"id": 166595, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4651, "text": "And there shall come every soul therewith shall be a driver and a witness."}, {"id": 166621, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4652, "text": "Assuredly thou wast in neglect thereof; now We have removed from off thee thy veil, so thy sight Today is piercing."}, {"id": 166622, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4653, "text": "And his companion will say: this is that which with me is ready."}, {"id": 166623, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4654, "text": "Cast ye twain into Hell every Person rebellious, contumacious."}, {"id": 166624, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4655, "text": "Hinderer o fgood, trespasser, doubter"}, {"id": 166625, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4656, "text": "Who set up with Allah another god. So cast him ye twain into the torment severe."}, {"id": 166626, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4657, "text": "His companion will say: O our Lord! caused him not to transgress but he was himself in error far off."}, {"id": 166627, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4658, "text": "Allah will say: wrangle not in My presence, and had already proferred Unto you the threat."}, {"id": 166628, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4659, "text": "The Word will not be changed in My presence, nor am an oppressor at all Unto My bondmen."}, {"id": 166629, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4660, "text": "Mention the Day whereon We shall say Unto the Hell: art thou filled? and it will say: is there yet any addition?"}, {"id": 166630, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4661, "text": "And brought nigh will be the Garden Unto the God-fearing, not far-off."}, {"id": 166631, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4662, "text": "This is that which ye were promised: for every oft- returning heedful one."}, {"id": 166632, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4663, "text": "That feareth the Compassionate in the unseen and cometh to Him with a heart penitent:"}, {"id": 166633, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4664, "text": "Enter it in peace. This is the Day of Abidence."}, {"id": 166634, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4665, "text": "Theirs therein will be whatsoever they list; and with Us will be yet more."}, {"id": 166635, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4666, "text": "And how many a generation have We destroyed before them, who were mightier in power than they, and they traversed the cities! No place of refuge could they find."}, {"id": 166636, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4667, "text": "Verily herein is an admonition Unto him who hath a heart, or giveth ear while he is heedful."}, {"id": 166637, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4668, "text": "And assuredly We created the heavens and the earth and whatsoever is in between the twain in six days, and there touched Us naught of weariness."}, {"id": 166638, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4669, "text": "So bear thou patiently with that which they say, and hallow the praise of thine Lord before the rising of the sun and before its setting."}, {"id": 166639, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4670, "text": "And in the night-time hallow Him, and also after the prescribed prostration."}, {"id": 166548, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4671, "text": "And hearken thou: the Day whereon the caller will call from a place quite near."}, {"id": 166503, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4672, "text": "The Day whereon they will surely hear the shout - that is the Day of coming forth."}, {"id": 166640, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4673, "text": "Verily We! it is We Who give life and cause death, and Unto Us is the journeying."}, {"id": 166641, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4674, "text": "That shall be the Day whereon the earth will be cleft from off them as they hasten forth. That shall be a gathering Unto Us easy."}, {"id": 166642, "identifier": "en.daryabadi", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 4675, "text": "We are the best Knower of that which they say, and thou art not over them over them tyrant. Wherefore admonish thou by the Qur'an him who feareth My threat."}]