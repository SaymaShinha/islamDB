[{"id": 168084, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6131, "text": "Those who disbelieved from among the People of the Book and the associaters could not break off, until there had come Unto them the evidence."}, {"id": 168085, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6132, "text": "An apostle from Allah rehearsing Writs cleansed."}, {"id": 168086, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6133, "text": "Wherein are discourses eternal."}, {"id": 168087, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6134, "text": "And those who are vouchsafed the Book divided not save after there had come Unto them the evidence."}, {"id": 168088, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6135, "text": "And they were commanded not but that they should worship Allah, keeping religion pure for him, as upright men, and that they should establish prayer and give the poor-rate, And that is the right religion."}, {"id": 168089, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6136, "text": "Verily those who disbelieve from among the people of the Book and the associaters shall be cast into the Hell-fire, as abiders therein. These! they are the worst of the creation."}, {"id": 168090, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6137, "text": "Verily those who believe and work righteous works - these! they are the best of the creation."}, {"id": 168091, "identifier": "en.daryabadi", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6138, "text": "Their recompense with their Lord shall be Gardens Everlasting, whereunder rivers flow - as abiders therein for evermore. Well-pleases will be Allah with them and well-pleased will be they with Him, That is for him who feareth his Lord."}]