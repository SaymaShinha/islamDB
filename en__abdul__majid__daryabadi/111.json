[{"id": 168169, "identifier": "en.daryabadi", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6217, "text": "Perish the two hands of Abu Lahab, and perish he!"}, {"id": 168170, "identifier": "en.daryabadi", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6218, "text": "His substance availed him not, nor that which he earned."}, {"id": 168171, "identifier": "en.daryabadi", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 6219, "text": "Anon he shall roast into Fire having flame."}, {"id": 168172, "identifier": "en.daryabadi", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_n