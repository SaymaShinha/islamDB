[{"id": 167210, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5242, "text": "Blest be He in whose hand is the dominion, and He is over everything Potent."}, {"id": 167211, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5243, "text": "Who hath created death and life that He might prove you, as to which of you is excellent in work. And He is the Mighty, the Forgiver."}, {"id": 167212, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5244, "text": "Who hath created seven heavens in storeys. Thou shalt not behold in the Compassionates creation any over-sight; then repeat thy look, beholdest thou any crack?"}, {"id": 167213, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5245, "text": "Then repeat thy look twice over, and thy look will return Unto thee dim and It will have become wearied out."}, {"id": 167214, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5246, "text": "And assuredly We have bedecked the nearest heaven with lamps, and We have made them missiles for satans: and for them We have gotten ready the torment of the Blaze."}, {"id": 167215, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5247, "text": "And for those who disbelieve in their Lord will be the torment of Hell; and a hapless destination!"}, {"id": 167216, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5248, "text": "When they will be cast thereinto, they will hear thereof a braying even as it balleth up,"}, {"id": 167217, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5249, "text": "Well-nigh it bursteth with rage. So oft as a company is cast thereinto, the keepers thereof will ask them: came there not Unto you a warner?"}, {"id": 167218, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5250, "text": "They will say: yea! surely there came a warner Unto us, but we belied, and said: God hath not sent down aught, are naught but in a great error."}, {"id": 167219, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5251, "text": "And they will say: had we been wont to hearken or to reflect, we had not been among the fellows of the Blaze."}, {"id": 167220, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5252, "text": "So they shall confess their sin. Far away they-be, the fellows of the Blaze!"}, {"id": 167221, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5253, "text": "Verily those who dread their Lord unseen, theirs shall be forgiveness and a great hire."}, {"id": 167222, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5254, "text": "And whather ye keep your discourse secret or publish it, verily He is the Knower of that which is in the breasts."}, {"id": 167223, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5255, "text": "Shall not He Who hath created know? And He is the Subtile, the Aware."}, {"id": 167224, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5256, "text": "He it is Who hath made the earth Unto you subservient, so go forth in the regions thereof, and eat of His provision. And Unto Him is the Resurrection."}, {"id": 167225, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5257, "text": "Are ye secure that He who is in the heaven will not sink the earth with you and then it should quake?"}, {"id": 167226, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5258, "text": "Or are ye secure that He Who is in the heaven will not send against you a whirlwind? Anon ye shall know what wise hath been My warning."}, {"id": 167227, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5259, "text": "And assuredly those before them belied; then what wise hath been My wrath!"}, {"id": 167228, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5260, "text": "Behold they not the birds above them, outstretching their wings and they also withdraw them? Naught holdeth them except the Compassionate. Verily He is of everything Beholder."}, {"id": 167229, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5261, "text": "Who. is he that can be an army Unto you and succour you, beside the Compassionate! The infidelsjb are but in delusion."}, {"id": 167230, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5262, "text": "Who is he that can provide for you, should He withhold His provision? Aye! they persists in perverseness and aversion."}, {"id": 167105, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5263, "text": "Is he, then, who goeth about grovelling upon his face better directed, or he who walketh evenly on a straight path?"}, {"id": 167125, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5264, "text": "Say thou: He it is who hath brought you forth and hath endowed you with hearing and sights and hearts. Little thanks it is ye give!"}, {"id": 167206, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5265, "text": "Say thou: He it is Who hath spread you over the earth, and Unto Him ye shall be gathered."}, {"id": 167231, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5266, "text": "And they say: when will this promise be fulfilled, if ye say sooth?"}, {"id": 167232, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5267, "text": "Say thou: the knowledge is only with Allah, and but a warner manifest."}, {"id": 167233, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5268, "text": "But when they will behold it proximating sad will be the countenances of those who disbelieve, and it will be said: this is that which ye have been calling for."}, {"id": 167234, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5269, "text": "Say thou: bethink ye if Allah destroy me and those with me; or have mercy on us, who will protect the infidels from a torment afflictive?"}, {"id": 167235, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5270, "text": "Say thou: He is the Compassionate; in Him we have believed, and in Him we have put our trust. -And anon ye will know who it is that is in error manifest."}, {"id": 167236, "identifier": "en.daryabadi", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5271, "text": "Say thou: bethink ye, were your water to be sunk away, who then could bring you water welling-up?"}]