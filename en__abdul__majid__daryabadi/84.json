[{"id": 167842, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5885, "text": "When the heaven is sundered."}, {"id": 167843, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5886, "text": "And it hearkeneth to its Lord, and is duteous,"}, {"id": 167844, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5887, "text": "And when the earth shall be stretched forth."}, {"id": 167845, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5888, "text": "And shall cast forth that which is therein, and shall become empty."}, {"id": 167846, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5889, "text": "And it hearkeneth to its Lord, and is duteous."}, {"id": 167847, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5890, "text": "O man! verily thou art toiling toward thy Lord a painful toiling, and art about to meet Him."}, {"id": 167848, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5891, "text": "Then as to him who shall be given his book in his right hand -"}, {"id": 167849, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5892, "text": "His account shall presently be taken by an easy reckoning."}, {"id": 167850, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5893, "text": "And he shall return Unto his people joyfully."}, {"id": 167851, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5894, "text": "And as to him who shall be given his book from behind his back-"}, {"id": 167852, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5895, "text": "He shall presently call for death,"}, {"id": 167853, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5896, "text": "And he shall roast into a Blaze."}, {"id": 167854, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5897, "text": "Verily he was among his people joyous."}, {"id": 167855, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5898, "text": "Verily he imagined that he would not be back."}, {"id": 167856, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5899, "text": "Yea! his Lord had ever been beholding him."}, {"id": 167857, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5900, "text": "I swear by the afterglow of sunset,"}, {"id": 167858, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5901, "text": "And by the night and that which it driveth together,"}, {"id": 167859, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5902, "text": "And by the moon when she becometh full."}, {"id": 167860, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5903, "text": "Surely ye shall ride layer upon layer."}, {"id": 167861, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5904, "text": "What aileth them then, that they believe not,"}, {"id": 167862, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": true, "sajda_id": 14, "sajda_obligatory": false, "sajda_recommended": true, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5905, "text": "And that, when the Qur'an is read Unto them, they prostrate not them-selves!"}, {"id": 167863, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5906, "text": "Yea those who disbelieve belie."}, {"id": 167864, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5907, "text": "Whereas Allah knoweth best that which they cherish."}, {"id": 167865, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5908, "text": "Wherefore announce thou Unto them a torment afflictive."}, {"id": 167866, "identifier": "en.daryabadi", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Daryabadi", "transelator_en_name": "Abdul Majid Daryabadi", "format": "text", "type": "translation", "ayah_number": 5909, "text": "But those who believe and work righteous works, theirs shall be a hire unending."}]