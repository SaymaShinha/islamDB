[{"id": 261497, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5994, "text": "\u00a1CONSIDERA el alba"}, {"id": 261498, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5995, "text": "y las diez noches!"}, {"id": 261499, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5996, "text": "\u00a1Considera lo m\u00faltiple y el Uno!"}, {"id": 255679, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5997, "text": "\u00a1Considera la noche cuando transcurre!"}, {"id": 261500, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5998, "text": "Considerando todo esto --\u00bfpodr\u00eda darse una evidencia [m\u00e1s] solemne de la verdad para alguien dotado de entendimiento?"}, {"id": 261501, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5999, "text": "\u00bfNO HAS VISTO lo que tu Sustentador hizo con [la tribu de] Aad,"}, {"id": 261502, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6000, "text": "[la gente] de Iram, la de numerosas columnas,"}, {"id": 261503, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6001, "text": "como no ha sido creada otra igual en toda esa tierra? \u2013"}, {"id": 261504, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6002, "text": "\u00bfy con [la tribu de] Zamud, que excavaron la roca en el valle?\u2014"}, {"id": 261505, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6003, "text": "\u00bfy con Fara\u00f3n, el de numerosos postes?"}, {"id": 261506, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6004, "text": "[Fueron ellos los] que excedieron todos los l\u00edmites de la equidad en sus territorios,"}, {"id": 261507, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6005, "text": "sembrando all\u00ed gran corrupci\u00f3n:"}, {"id": 261508, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6006, "text": "por lo que tu Sustentador descarg\u00f3 sobre ellos un azote de castigo:"}, {"id": 261509, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6007, "text": "\u00a1ciertamente, tu Sustentador est\u00e1 siempre al acecho!"}, {"id": 261510, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6008, "text": "PERO EL HOMBRE, cuando su Sustentador le prueba con Su generosidad, y le permite disfrutar de una vida confortable, dice: \u201cMi Sustentador ha sido [justamente] generoso conmigo\u201d;"}, {"id": 261511, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6009, "text": "en cambio, cuando le prueba restringiendo sus medios de subsistencia, dice: \u201c\u00a1Mi Sustentador me ha despreciado!\u201d"}, {"id": 261512, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6010, "text": "\u00a1Pero no! \u00a1Al contrario, [Oh hombres, considerad todo lo que hac\u00e9is y dej\u00e1is de hacer:] no sois generosos con el hu\u00e9rfano,"}, {"id": 261513, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6011, "text": "ni os inst\u00e1is unos a otros a alimentar al necesitado,"}, {"id": 261514, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6012, "text": "y devor\u00e1is con voracidad la herencia [de otros],"}, {"id": 261515, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6013, "text": "y am\u00e1is la riqueza con un amor desmedido!"}, {"id": 261516, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6014, "text": "\u00a1Pero no! \u00bf[Qu\u00e9 ser\u00e1 de vosotros en el D\u00eda del Juicio,] cuando la tierra sea pulverizada, golpe a golpe,"}, {"id": 261517, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6015, "text": "y se revele [la majestad de] tu Sustentador, y los \u00e1ngeles [se formen], fila tras fila?"}, {"id": 261518, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6016, "text": "Y ese D\u00eda ser\u00e1 acercado el infierno [hasta quedar a la vista]; ese D\u00eda el hombre recordar\u00e1 [todo lo que hizo y dej\u00f3 de hacer]: pero, \u00bfde qu\u00e9 le servir\u00e1 ese recuerdo?"}, {"id": 261519, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6017, "text": "Dir\u00e1: \u201c\u00a1Ojal\u00e1 hubiera prove\u00eddo de antemano para mi [otra] vida!\u201d"}, {"id": 261520, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6018, "text": "Pues, nadie puede castigar como castigar\u00e1 \u00c9l ese D\u00eda [a los pecadores],"}, {"id": 261521, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6019, "text": "y nadie puede atar con ligaduras como las Suyas."}, {"id": 261522, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6020, "text": "[Pero a los justos Dios les dir\u00e1:] \u201c\u00a1Oh t\u00fa, alma sosegada!"}, {"id": 261523, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6021, "text": "\u00a1Vuelve a tu Sustentador, complacida [y] digna de [Su] complacencia:"}, {"id": 261524, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6022, "text": "entra, pues, con Mis [verdaderos] siervos \u2013"}, {"id": 261525, "identifier": "es.asad", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6023, "text": "si, entra en Mi para\u00edso!\u201d"}]