[{"id": 261526, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6024, "text": "\u00a1NO! Juro por este territorio \u2013"}, {"id": 261527, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6025, "text": "este territorio en el que t\u00fa puedes vivir libremente--"}, {"id": 261528, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6026, "text": "y [juro] por todo padre y su descendencia:"}, {"id": 261529, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6027, "text": "En verdad, hemos creado al hombre para [una vida de] continuas penalidades y pruebas."}, {"id": 261530, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6028, "text": "\u00bfPiensa, pues, que nadie tiene poder sobre \u00e9l?"}, {"id": 261531, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6029, "text": "Se jacta [diciendo]: \u201c\u00a1He consumido abundantes riquezas!\u201d"}, {"id": 261532, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6030, "text": "\u00bfPiensa, pues, que nadie le ve?"}, {"id": 261533, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6031, "text": "\u00bfAcaso no le hemos dado dos ojos,"}, {"id": 261534, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6032, "text": "y una lengua, y dos labios,"}, {"id": 261535, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6033, "text": "y le hemos mostrado las dos v\u00edas [del bien y el mal]?"}, {"id": 261536, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Pero no est\u00e1 dispuesto a subir por la empinada cuesta...."}, {"id": 261537, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6035, "text": "\u00bfY qu\u00e9 puede hacerte concebir lo que es esa empinada cuesta?"}, {"id": 261538, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6036, "text": "[Es] liberar a un ser humano de la esclavitud,"}, {"id": 261539, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6037, "text": "o alimentar, en tiempos de escasez [propia],"}, {"id": 261540, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6038, "text": "a un pariente hu\u00e9rfano,"}, {"id": 261541, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6039, "text": "o a un pobre [desconocido] tirado en el suelo \u2013"}, {"id": 261542, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6040, "text": "y ser, adem\u00e1s, de los que han llegado a creer y se exhortan mutuamente a la paciencia, y se exhortan mutuamente a la compasi\u00f3n."}, {"id": 261543, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6041, "text": "ESOS SON los que han alcanzado la rectitud;"}, {"id": 261544, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6042, "text": "pero los que se empe\u00f1an en negar la verdad de Nuestros mensajes \u2013\u00e9sos se han hundido en el mal,"}, {"id": 261545, "identifier": "es.asad", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6043, "text": "[y] el fuego se cierra en torno a ellos."}]