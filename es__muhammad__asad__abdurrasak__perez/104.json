[{"id": 261679, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6180, "text": "\u00a1AY de todo aquel que difama, que critica!"}, {"id": 261680, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6181, "text": "\u00a1[Ay de aquel ] que amasa riqueza y la considera como salvaguardia,"}, {"id": 261681, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6182, "text": "creyendo que su riqueza le har\u00e1 vivir eternamente!"}, {"id": 261682, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6183, "text": "\u00a1Que va! \u00a1Ciertamente, [en la Otra Vida] ser\u00e1 arrojado a un tormento demoledor!"}, {"id": 261683, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6184, "text": "\u00bfY qu\u00e9 puede hacerte concebir lo que ser\u00e1 ese tormento demoledor?"}, {"id": 255681, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6185, "text": "Un fuego encendido por Dios,"}, {"id": 261684, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6186, "text": "que se elevar\u00e1 sobre los corazones [de los pecadores]:"}, {"id": 261685, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6187, "text": "\u00a1realmente, se cerrar\u00e1 en torno a ellos"}, {"id": 261686, "identifier": "es.asad", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6188, "text": "en inmensas columnas!"}]