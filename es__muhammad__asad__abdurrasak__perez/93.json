[{"id": 261582, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6080, "text": "CONSIDERA las horas de resplandor matinal,"}, {"id": 261583, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6081, "text": "y la noche cuando se torna quieta y oscura."}, {"id": 261584, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Tu Sustentador no te ha abandonado, ni desde\u00f1ado:"}, {"id": 261585, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6083, "text": "\u00a1pues, en verdad, la Otra Vida ser\u00e1 mejor para ti que esta primera [parte de tu vida]!"}, {"id": 261586, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6084, "text": "Y, en verdad, tu Sustentador habr\u00e1 de darte [cuanto tu coraz\u00f3n desea], y quedar\u00e1s satisfecho."}, {"id": 261587, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6085, "text": "\u00bfNo te encontr\u00f3 hu\u00e9rfano, y te ampar\u00f3?"}, {"id": 261588, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6086, "text": "\u00bfY no te encontr\u00f3 perdido, y te gui\u00f3?"}, {"id": 261589, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6087, "text": "\u00bfY no te encontr\u00f3 necesitado, y te dio lo suficiente?"}, {"id": 261590, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6088, "text": "No seas, pues, injusto con el hu\u00e9rfano,"}, {"id": 261591, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6089, "text": "y al que busca [tu] ayuda no le rech aces,"}, {"id": 261592, "identifier": "es.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 6090, "text": "y proclama siempre las bendiciones de tu Sustentador."}]