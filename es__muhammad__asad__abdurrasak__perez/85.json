[{"id": 261416, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5910, "text": "\u00a1CONSIDERA el firmamento lleno de grandes constelaciones,"}, {"id": 261417, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5911, "text": "y [luego acu\u00e9rdate de] el D\u00eda prometido,"}, {"id": 261418, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5912, "text": "y [de] Aquel que da testimonio [de todo], y [de] lo que es testimoniado [por \u00c9l]!"}, {"id": 261419, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5913, "text": "\u00a1[S\u00d3LO] SE DESTRUYEN a s\u00ed mismos, quienes preparan un foso"}, {"id": 261420, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5914, "text": "de fuego que arde intensamente [para todos los que han llegado a creer]!"}, {"id": 261421, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5915, "text": "Cuando contemplan [alegres] ese [fuego],"}, {"id": 261422, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5916, "text": "plenamente conscientes de lo que hacen a los creyentes,"}, {"id": 261423, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5917, "text": "a los que odian \u00fanicamente por creer en Dios, el Todopoderoso, el Digno de toda alabanza,"}, {"id": 261424, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Aquel a quien pertenece el dominio de los cielos y de la tierra. \u00a1Pero Dios es testigo de todo!"}, {"id": 261425, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Realmente, a quienes persiguen a los creyentes y a las creyentes, y luego no se arrepienten, les aguarda el castigo del infierno: \u00a1si, les aguarda el castigo del fuego!"}, {"id": 261426, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5920, "text": "[Pero,] realmente, quienes llegan a creer y hacen buenas obras tendr\u00e1n [en la Otra Vida] jardines por los que corren arroyos --\u00a1ese es el triunfo supremo!"}, {"id": 261427, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5921, "text": "\u00a1REALMENTE, el rigor de tu Sustentador es sumamente severo!"}, {"id": 261428, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5922, "text": "\u00c9l es, ciertamente, quien crea [al hombre] en un principio, y [es quien] lo suscitar\u00e1 de nuevo."}, {"id": 261370, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Y s\u00f3lo \u00c9l es realmente indulgente, universal en Su amor,"}, {"id": 261429, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5924, "text": "entronizado en sublime omnipotencia,"}, {"id": 261430, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5925, "text": "hacedor soberano de todo cuanto quiere."}, {"id": 261431, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5926, "text": "\u00bfHA LLEGADO a tu conocimiento la historia de los ej\u00e9rcitos [criminales]"}, {"id": 261432, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5927, "text": "de Fara\u00f3n, y [de la tribu] de Zamud?"}, {"id": 261433, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Y, a\u00fan as\u00ed, los que se empe\u00f1an en negar la verdad persisten en desmentirla:"}, {"id": 261434, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5929, "text": "pero Dios les cerca [con Su conocimiento y poder] sin que se den cuenta."}, {"id": 261435, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5930, "text": "\u00a1Bien al contrario: esta [escritura divina que ellos rechazan] es un discurso sublime,"}, {"id": 261436, "identifier": "es.asad", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "es", "name": "Asad", "transelator_en_name": "Muhammad Asad - Abdurrasak P\u00e9rez", "format": "text", "type": "translation", "ayah_number": 5931, "text": "[inscrito] en una tabla imperecedera!"}]