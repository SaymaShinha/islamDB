[{"id": 617226, "identifier": "tt.nugman", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tt", "name": "Yakub Ibn Nugman", "transelator_en_name": "Yakub Ibn Nugman", "format": "text", "type": "translation", "ayah_number": 6194, "text": "\u041a\u0443\u0440\u0430\u0435\u0448 \u043a\u0430\u0431\u0438\u043b\u04d9\u0441\u0435\u043d\u0435\u04a3 \u04e9\u043b\u0444\u04d9\u0442\u043b\u04d9\u043d\u04af\u0435\u043d\u04d9 \u0491\u04d9\u0497\u04d9\u0431\u043b\u04d9\u043d\u0435\u0433\u0435\u0437."}, {"id": 617227, "identifier": "tt.nugman", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tt", "name": "Yakub Ibn Nugman", "transelator_en_name": "Yakub Ibn Nugman", "format": "text", "type": "translation", "ayah_number": 6195, "text": "\u0410\u043b\u0430\u0440\u043d\u044b\u04a3 \u0497\u04d9\u0439 \u04bb\u04d9\u043c \u043a\u044b\u0448 \u043a\u04e9\u043d\u043d\u04d9\u0440\u0435\u043d\u0434\u04d9 \u0441\u04d9\u04af\u0434\u04d9 \u0431\u0435\u043b\u04d9\u043d \u0441\u04d9\u0444\u04d9\u0440\u0434\u04d9 \u0439\u04e9\u0440\u04af\u043b\u04d9\u0440\u0435\u043d\u0434\u04d9 \u04e9\u043b\u0444\u04d9\u0442\u043b\u04d9\u043d\u04af\u043b\u04d9\u0440\u0435\u043d\u04d9 \u0491\u04d9\u0497\u04d9\u0431\u043b\u04d9\u043d\u0435\u0433\u0435\u0437! (\u041a\u0443\u0440\u0430\u0435\u0448 \u0497\u04d9\u0439 \u0431\u0435\u0440 \u043c\u04d9\u0440\u0442\u04d9\u0431\u04d9, \u043a\u044b\u0448 \u0431\u0435\u