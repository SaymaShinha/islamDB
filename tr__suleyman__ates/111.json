[{"id": 604699, "identifier": "tr.ates", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6217, "text": "Ebuleheb'in iki eli kurusun (yok olsun o); zaten yok oldu ya."}, {"id": 604700, "identifier": "tr.ates", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6218, "text": "Ne mal\u0131, ne de kazand\u0131\u011f\u0131 onu (Allah'\u0131n kahr\u0131ndan) kurtaramad\u0131."}, {"id": 604701, "identifier": "tr.ates", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6219, "text": "Alevli bir ate\u015fe girecektir (o)."}, {"id": 604702, "identifier": "tr.ates", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629