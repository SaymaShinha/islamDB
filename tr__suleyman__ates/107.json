[{"id": 604680, "identifier": "tr.ates", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6198, "text": "Din(ahiret cezas\u0131n)\u0131 yalanlayan(adam)\u0131 g\u00f6rd\u00fcn m\u00fc?"}, {"id": 604681, "identifier": "tr.ates", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6199, "text": "\u0130\u015fte o, \u00f6ks\u00fcz\u00fc iter, kakar;"}, {"id": 604682, "identifier": "tr.ates", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6200, "text": "Yoksulu doyurma\u011fa \u00f6nayak olmaz."}, {"id": 604683, "identifier": "tr.ates", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u