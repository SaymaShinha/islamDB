[{"id": 598668, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 1, "text": "Rahman ve Rahim Allah'\u0131n ad\u0131yla"}, {"id": 604884, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 2, "text": "Alemlerin Rabbi (sahibi, yeti\u015ftiricisi) Allah'a hamdolsun."}, {"id": 604885, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 3, "text": "(O) Rahman'd\u0131r, Rahim'dir."}, {"id": 604886, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 4, "text": "Din (ceza ve m\u00fckafat) g\u00fcn\u00fcn\u00fcn sahibidir."}, {"id": 604887, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5, "text": "(Ya Rabbi) Ancak sana kulluk eder, ancak Senden yard\u0131m isteriz!"}, {"id": 604888, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6, "text": "Bizi do\u011fru yola ilet:"}, {"id": 604889, "identifier": "tr.ates", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 7, "text": "-ni'met verdi\u011fin kimselerin yoluna. Kendilerine gazabedilmi\u015f olanlar\u0131n ve sapm\u0131\u015flar\u0131n yoluna de\u011fil. (ya Rabbi)!"}]