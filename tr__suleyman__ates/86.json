[{"id": 604416, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5932, "text": "G\u00f6\u011fe ve tar\u0131k'a andolsun."}, {"id": 604417, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5933, "text": "Tar\u0131k'\u0131n ne oldu\u011funu sen nereden bileceksin?"}, {"id": 604418, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5934, "text": "Parlayan y\u0131ld\u0131zd\u0131r."}, {"id": 604419, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5935, "text": "Hi\u00e7bir can yoktur ki ba\u015f\u0131nda bir koruyucu (bek\u00e7i) olmas\u0131n."}, {"id": 604420, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5936, "text": "\u0130nsan neden yarat\u0131ld\u0131\u011f\u0131na bir baks\u0131n:"}, {"id": 604421, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5937, "text": "At\u0131lan bir sudan yarat\u0131ld\u0131."}, {"id": 604422, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5938, "text": "Bel ile kaburga kemikleri aras\u0131ndan \u00e7\u0131kan (bir sudan)."}, {"id": 604423, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5939, "text": "O (Allah), onu tekrar d\u00f6nd\u00fcr(\u00fcp yarat)ma\u011fa kadirdir."}, {"id": 604424, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5940, "text": "Gizlilerin (ortaya d\u00f6k\u00fcl\u00fcp) yoklanaca\u011f\u0131 g\u00fcn,"}, {"id": 604425, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5941, "text": "\u0130nsan\u0131n ne bir g\u00fcc\u00fc, ne de bir yard\u0131mc\u0131s\u0131 vard\u0131r."}, {"id": 604426, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5942, "text": "D\u00f6n\u00fc\u015fl\u00fc g\u00f6\u011fe andolsun,"}, {"id": 604427, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5943, "text": "(Bitkilerin \u00e7\u0131kmas\u0131 i\u00e7in) \u00c7atlayan yere andolsun ki,"}, {"id": 604428, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5944, "text": "O (Kur'an), elbette (hak ile bat\u0131l\u0131) ay\u0131rdedici bir s\u00f6zd\u00fcr."}, {"id": 604429, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5945, "text": "O, \u015faka de\u011fildir."}, {"id": 604430, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Onlar (onu iptal etmek i\u00e7in) bir tuzak kuruyorlar."}, {"id": 604431, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5947, "text": "Ben de (onlar\u0131 yakalamak i\u00e7in) bir tuzak kuruyorum."}, {"id": 604432, "identifier": "tr.ates", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5948, "text": "Hele sen o kafirlere m\u00fchlet ver, biraz b\u0131rak onlar\u0131 (bildiklerine gitsinler)."}]