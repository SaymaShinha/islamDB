[{"id": 604314, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5830, "text": "G\u00f6k yar\u0131ld\u0131\u011f\u0131 zaman,"}, {"id": 604315, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5831, "text": "Y\u0131ld\u0131zlar sa\u00e7\u0131ld\u0131\u011f\u0131 zaman,"}, {"id": 604316, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5832, "text": "Denizler f\u0131\u015fk\u0131rt\u0131ld\u0131\u011f\u0131 zaman,"}, {"id": 604317, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5833, "text": "Kabirlerin i\u00e7i d\u0131\u015f\u0131na getirildi\u011fi zaman,"}, {"id": 604318, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5834, "text": "Her can, ne (yap\u0131p) \u00f6ne s\u00fcrd\u00fc\u011f\u00fcn\u00fc ve ne (yapmay\u0131p) geride b\u0131rakt\u0131\u011f\u0131n\u0131 bilir."}, {"id": 604319, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5835, "text": "Ey insan, seni engin kerem sahibi Rabbine kar\u015f\u0131 ne aldat\u0131p isyana s\u00fcr\u00fckledi?"}, {"id": 604320, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5836, "text": "O (Rab) ki seni yaratt\u0131, seni d\u00fczenledi, sana \u00f6l\u00e7\u00fcl\u00fc bir bi\u00e7im verdi."}, {"id": 604321, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5837, "text": "Seni(n organlar\u0131n\u0131) diledi\u011fi \u015fekilde birbirine ekledi."}, {"id": 604322, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5838, "text": "Hay\u0131r, (bu gururunuzun sebebi \u015fudur) siz ceza (g\u00f6rme)yi yalanl\u0131yorsunuz."}, {"id": 604323, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5839, "text": "Oysa \u00fczerinizde koruyucu(yapt\u0131klar\u0131n\u0131z\u0131 zabtedici melek)ler vard\u0131r;"}, {"id": 604324, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5840, "text": "De\u011ferli yaz\u0131c\u0131lar,"}, {"id": 604325, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5841, "text": "Yapt\u0131\u011f\u0131n\u0131z her\u015feyi bilirler."}, {"id": 604326, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5842, "text": "\u0130yiler ni'met i\u00e7indedirler."}, {"id": 604327, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5843, "text": "K\u00f6t\u00fcler de yak\u0131c\u0131 ate\u015f i\u00e7indedirler."}, {"id": 604328, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5844, "text": "Ceza g\u00fcn\u00fc oraya girerler."}, {"id": 604329, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5845, "text": "Onlar ondan (hi\u00e7bir yere ka\u00e7\u0131p) kaybolacak de\u011fillerdir."}, {"id": 604330, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5846, "text": "Ceza g\u00fcn\u00fcn\u00fcn ne oldu\u011funu sen nereden bileceksin?"}, {"id": 604331, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5847, "text": "Ve yine ceza g\u00fcn\u00fcn\u00fcn ne oldu\u011funu sen nereden bileceksin?"}, {"id": 604332, "identifier": "tr.ates", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 5848, "text": "O, kimsenin kimseye yard\u0131m edemeyece\u011fi bir g\u00fcnd\u00fcr! O g\u00fcn buyruk, yaln\u0131z Allah'\u0131nd\u0131r."}]