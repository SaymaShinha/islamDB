[{"id": 604506, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Yoo, and i\u00e7erim bu kente,"}, {"id": 604507, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6025, "text": "Ki sen bu \u015fehire girmekte(burada ya\u015famakta)s\u0131n."}, {"id": 604508, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6026, "text": "Ve (and i\u00e7erim) do\u011furucuya ve do\u011furdu\u011funa ki,"}, {"id": 604509, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Biz insan\u0131 zorluk aras\u0131nda yaratt\u0131k."}, {"id": 604510, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6028, "text": "\u0130nsan, hi\u00e7 kimsenin kendisine g\u00fc\u00e7 yetiremeyece\u011fini mi san\u0131yor?"}, {"id": 604511, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6029, "text": "(G\u00f6steri\u015f ve \u00f6v\u00fcnme i\u00e7in) \"Ben bir\u00e7ok mal telef ettim\" diyor."}, {"id": 604512, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6030, "text": "Kimsenin kendisini g\u00f6rmedi\u011fini mi san\u0131yor?"}, {"id": 604513, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Biz ona vermedik mi: \u0130ki g\u00f6z"}, {"id": 604514, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6032, "text": "Bir dil, iki dudak?"}, {"id": 604515, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6033, "text": "Ona iki tepeyi (anas\u0131n\u0131n iki memesini emmenin veya hay\u0131r ve \u015ferrin yolunu) g\u00f6sterdik."}, {"id": 604516, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Fakat o, sarp yoku\u015fa at\u0131lamad\u0131."}, {"id": 604517, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6035, "text": "Sarp yoku\u015fun ne oldu\u011funu sen nereden bileceksin?"}, {"id": 604518, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6036, "text": "Bir boynu (k\u00f6lelik zincirinden) \u00e7\u00f6zmek,"}, {"id": 604519, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6037, "text": "Yahut a\u00e7l\u0131k g\u00fcn\u00fcnde doyurmakt\u0131r:"}, {"id": 604520, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6038, "text": "Akraba olan yetimi,"}, {"id": 604521, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6039, "text": "Yahut hi\u00e7bir \u015feyi olmayan yoksulu."}, {"id": 604522, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6040, "text": "Sonra inan\u0131p birbirlerine sab\u0131r tavsiye eden ve merhamet tavsiye edenlerden olmak."}, {"id": 604523, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6041, "text": "\u0130\u015fte onlar sa\u011f\u0131n adamlar\u0131d\u0131r (Kitab\u0131 sa\u011f\u0131ndan verilen u\u011furlu ki\u015filerdir)."}, {"id": 604524, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6042, "text": "Ayetlerimizi tan\u0131mayanlar ise solun adamlar\u0131d\u0131r (Kitab\u0131 solundan verilen u\u011fursuz ki\u015filerdir)."}, {"id": 604525, "identifier": "tr.ates", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "S\u00fcleyman Ate\u015f", "transelator_en_name": "Suleyman Ates", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Onlara (kap\u0131lar\u0131) \u00fczerlerine kilitlenecek bir ate\u015f vard\u0131r!"}]