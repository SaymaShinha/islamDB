[{"id": 436521, "identifier": "ru.abuadel", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ru", "name": "\u0410\u0431\u0443 \u0410\u0434\u0435\u043b\u044c", "transelator_en_name": "Abu Adel", "format": "text", "type": "translation", "ayah_number": 1, "text": "\u0421 \u0438\u043c\u0435\u043d\u0435\u043c \u0410\u043b\u043b\u0430\u0445\u0430 \u041c\u0438\u043b\u043e\u0441\u0442\u0438\u0432\u043e\u0433\u043e, \u041c\u0438\u043b\u043e\u0441\u0435\u0440\u0434\u043d\u043e\u0433\u043e!"}, {"id": 436522, "identifier": "ru.abuadel", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ru", "name": "\u0410\u0431\u0443 \u0410\u0434\u0435\u043b\u044c", "transelator_en_name": "Abu Adel", "format": "text", "type": "translation", "ayah_number": 2, "text": "(\u0412\u0441\u044f) \u0445\u0432\u0430\u043b\u0430 \u2013 (\u043b\u0438\u0448\u044c \u043e\u0434\u043d\u043e\u043c\u0443) \u0410\u043b\u043b\u0430\u0445\u0443, \u0413\u043e\u0441\u043f\u043e\u0434\u0443 \u043c\u0438\u0440\u043e\u0432 [\u0413\u043e\u0441\u043f\u043e\u0434\u0443 \u0432\u0441\u0435\u0445 \u0442\u0432\u043e\u0440\u0435\u043d\u0438\u0439],"}, {"id": 436523, "identifier": "ru.abuadel", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641