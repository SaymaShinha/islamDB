[{"id": 116806, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4736, "text": "Tako mi Gore,"}, {"id": 116807, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4737, "text": "I Knjige napisane -"}, {"id": 116808, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4738, "text": "Na pergamentu razvijenom -"}, {"id": 116809, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4739, "text": "I Ku\u0107e posje\u0107ivane,"}, {"id": 116687, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4740, "text": "I svoda uzdignutog,"}, {"id": 113294, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4741, "text": "I mora nabujalog,"}, {"id": 116810, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4742, "text": "Uistinu, kazna Gospodara tvog \u0107e se zbiti;"}, {"id": 116811, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4743, "text": "Nema tog ko \u0107e je odbiti:"}, {"id": 116812, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4744, "text": "Na Dan kad se zaljulja nebo talasanjem,"}, {"id": 116813, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4745, "text": "I pokrenu brda kretanjem!"}, {"id": 116814, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4746, "text": "Pa te\u0161ko Tog dana poricateljima,"}, {"id": 116815, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4747, "text": "Onima koji se u upu\u0161tanju zabavljaju,"}, {"id": 116816, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4748, "text": "Na Dan kad budu gurnuti ka vatri D\u017eehennema, (\u017eestokim) guranjem."}, {"id": 116817, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4749, "text": "\"Ovo je ta vatra koju ste poricali,"}, {"id": 116818, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4750, "text": "Pa je li ovo sihr, ili vi ne vidite?"}, {"id": 116819, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4751, "text": "Pr\u017ei\u0107ete se njome, pa strpili se ili ne strpili, isto vam je. Bi\u0107ete pla\u0107eni samo za ono \u0161to ste radili.\""}, {"id": 116820, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4752, "text": "Uistinu, bogobojazni \u0107e biti u ba\u0161\u010dama i u\u017eivanju,"}, {"id": 116821, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4753, "text": "Radova\u0107e se u onom \u0161ta im je dao Gospodar njihov, a sa\u010duva\u0107e ih Gospodar njihov kazne d\u017eehima."}, {"id": 116822, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4754, "text": "\"Jedite i pijte prijatno, zbog onog \u0161to ste radili.\""}, {"id": 116823, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4755, "text": "Bi\u0107e naslonjeni na sofama poredanim, a o\u017eeni\u0107emo ih hurijama lijepih krupnih o\u010diju."}, {"id": 116824, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4756, "text": "A oni koji su vjerovali - i slijedilo ih potomstvo njihovo u vjerovanju - priklju\u010di\u0107emo im potomstvo njihovo, i ne\u0107emo im zakinuti od djela njihovih ni\u0161ta. Svaki \u010dovjek je zalog onog \u0161ta je zaradio."}, {"id": 116825, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4757, "text": "I pru\u017eit \u0107emo im vo\u0107e i meso od \u010dega \u017eele."}, {"id": 116826, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4758, "text": "Dodavat \u0107e u njemu \u010da\u0161u, ne\u0107e u njoj biti nepromi\u0161ljenosti, niti grijeha."}, {"id": 116827, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4759, "text": "I obilazi\u0107e oko njih mladi\u0107i njihovi, kao da su oni biser skriveni."}, {"id": 116828, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4760, "text": "I neki od njih \u0107e prilaziti nekima, pitat \u0107e,"}, {"id": 116829, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4761, "text": "Re\u0107i \u0107e: \"Uistinu, mi smo prije u porodicama na\u0161im bili bri\u017eni,"}, {"id": 116830, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4762, "text": "Pa nam je Allah bio dobar i sa\u010duvao nas kazne vrelog vjetra."}, {"id": 116831, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4763, "text": "Uistinu, mi smo od prije Njega prizivali, uistinu, On, On je Dobrostivi, Milosrdni.\""}, {"id": 116832, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4764, "text": "Zato opominji - pa nisi ti milo\u0161\u0107u Gospodara tvog prorok, niti lu\u0111ak."}, {"id": 116833, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Zar govore: \"Pjesnik! Sa\u010deka\u0107emo za njega sudbinu sumnjivu!\""}, {"id": 116834, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4766, "text": "Reci: \"I\u0161\u010dekujte! Pa zaista sam ja sa vama od \u010dekalaca.\""}, {"id": 116835, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Zar im nare\u0111uju njihovi umovi ovo ili su oni narod koji granice prelazi?"}, {"id": 116836, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Zar govore: \"Izmislio ga je.\" Naprotiv! Ne vjeruju!"}, {"id": 116837, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4769, "text": "Pa neka donesu hadis sli\u010dan njemu, ako istinu govore!"}, {"id": 116838, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4770, "text": "Zar su stvoreni bez i\u010deg, ili su oni stvoritelji?"}, {"id": 116839, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Zar su stvorili nebesa i Zemlju? Naprotiv, nisu uvjereni!"}, {"id": 116840, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Zar su kod njih riznice Gospodara tvog, ili su oni vladari?"}, {"id": 116841, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Zar imaju ljestve - prislu\u0161kuju na njima? Pa neka prislu\u0161kiva\u010d njihov donese dokaz jasan!"}, {"id": 116842, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Zar su Njegove k\u0107eri, a va\u0161i sinovi?"}, {"id": 116843, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Zar ih pita\u0161 za nagradu, pa su oni nametom optere\u0107eni?"}, {"id": 116844, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Zar je kod njih nevidljivo, pa oni pi\u0161u?"}, {"id": 116845, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Zar \u017eele spletku? Pa oni koji ne vjeruju - oni su zapleteni!"}, {"id": 116846, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Zar imaju boga sem Allaha? Slavljen neka je Allah od onog \u0161ta pridru\u017euju!"}, {"id": 116847, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4779, "text": "I ako bi vidjeli komad neba koji pada, rekli bi: \"Oblaci nagomilani!\""}, {"id": 116848, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4780, "text": "Zato ih pusti dok ne sretnu Dan svoj - onaj u kojem \u0107e biti zgromljeni,"}, {"id": 116849, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4781, "text": "Dan kad im ne\u0107e koristiti spletka njihova nimalo, niti \u0107e oni biti pomognuti."}, {"id": 116850, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4782, "text": "A uistinu, oni koji \u010dine zulm, ima\u0107e kaznu mimo toga, ali ve\u0107ina njih ne zna."}, {"id": 116851, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4783, "text": "I strpi se radi presude Gospodara tvog: ta doista si ti pod o\u010dima Na\u0161im; i slavi sa hvalom Gospodara svog u vrijeme kad ustaje\u0161,"}, {"id": 116852, "identifier": "bs.mlivo", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4784, "text": "I no\u0107u - pa slavi Ga i s odstupanjem zvijezda."}]