[{"id": 118107, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6059, "text": "Tako mi no\u0107i kad prekrije,"}, {"id": 118108, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6060, "text": "I dana, kad zasjaji,"}, {"id": 118109, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6061, "text": "I Onog \u0160ta mu\u0161ko i \u017eensko stvara:"}, {"id": 118110, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Uistinu, trud va\u0161 je razli\u010dit."}, {"id": 118111, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6063, "text": "Pa \u0161to se ti\u010de onog ko daje i boji se,"}, {"id": 118112, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6064, "text": "I povjeruje u najbolje,"}, {"id": 118113, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6065, "text": "Pa - olak\u0161a\u0107emo mu (put) za lahko\u0107u;"}, {"id": 118114, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6066, "text": "A \u0161to se ti\u010de onog ko \u0161krtari i osje\u0107a se neovisnim,"}, {"id": 118115, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6067, "text": "I zanije\u010de najbolje,"}, {"id": 118116, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6068, "text": "Pa - olak\u0161a\u0107emo mu za te\u0161ko\u0107u,"}, {"id": 118117, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6069, "text": "I ne\u0107e mu koristiti bogatstvo njegovo kad se strovali."}, {"id": 118118, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6070, "text": "Uistinu! Na Nama je upu\u0107ivanje,"}, {"id": 118119, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6071, "text": "I uistinu, Na\u0161 je drugi i prvi (svijet)."}, {"id": 118120, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6072, "text": "Pa upozoravam vas vatrom plamte\u0107om:"}, {"id": 118121, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6073, "text": "Ne\u0107e se pr\u017eiti njome, sem najnesre\u0107niji,"}, {"id": 118122, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6074, "text": "Koji pori\u010de i okre\u0107e se,"}, {"id": 118123, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6075, "text": "A odstrani\u0107e se od nje najbogobojazniji,"}, {"id": 118124, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6076, "text": "Koji daje imetak svoj, \u010diste\u0107i se,"}, {"id": 118125, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6077, "text": "I kod kojeg nema niko nikakve blagodati da bi bio nagra\u0111en,"}, {"id": 118126, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6078, "text": "Izuzev tra\u017eenja lica Gospodara njegovog Uzvi\u0161enog."}, {"id": 118127, "identifier": "bs.mlivo", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 6079, "text": "A doista \u0107e zadovoljan biti."}]