[{"id": 117298, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5242, "text": "Blagoslovljen neka je Onaj u ruci \u010dijoj je vlast - a On nad svakom stvari ima mo\u0107 -"}, {"id": 117299, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5243, "text": "Koji je stvorio smrt i \u017eivot, da vas isku\u0161a koji je od vas bolji djelom. A On je Mo\u0107ni, Oprosnik -"}, {"id": 117300, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5244, "text": "Koji je stvorio sedam nebesa slojevito. Ne vidi\u0161 u stvaranju Milostivog nikakvog nesklada. Pa povrati pogled, da li vidi\u0161 ikakve napukline?"}, {"id": 117301, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5245, "text": "Ponovo vrati pogled drugi put, povrati\u0107e ti se pogled oslabljen i bi\u0107e on pomu\u0107en."}, {"id": 117302, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5246, "text": "I zaista smo ukrasili nebo najbli\u017ee svjetiljkama i u\u010dinili ih projektilima za \u0161ejtane, a pripremili smo im kaznu seira."}, {"id": 117303, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5247, "text": "A ima\u0107e oni koji ne vjeruju u Gospodara svog, kaznu D\u017eehennema, a lo\u0161e je dolazi\u0161te!"}, {"id": 117304, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5248, "text": "Kad budu ba\u010deni u njega, \u0107ut \u0107e ga, hropta\u0107e i on \u0107e vriti."}, {"id": 117305, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5249, "text": "Skoro da prsne od srd\u017ebe. Kad god se u njega baci skupina, upita\u0107e ih \u010duvari njegovi: \"Zar vam nije dolazio opominja\u010d?\""}, {"id": 117306, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5250, "text": "Re\u0107i \u0107e: \"Svakako; diosta nam je dolazio opominja\u010d, pa smo porekli i rekli: 'Nije Allah objavio ni\u0161ta, vi ste samo u zabludi velikoj!\""}, {"id": 117213, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5251, "text": "I re\u0107i \u0107e: \"Da smo slu\u0161ali ili shvatali, ne bismo bili me\u0111u stanovnicima seira.\""}, {"id": 117283, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5252, "text": "Tad \u0107e priznati grijeh svoj. Pa daleko bili stanovnici seira."}, {"id": 117307, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5253, "text": "Uistinu! Oni koji se boje Gospodara svog nevidljivo, ima\u0107e oni oprost i nagradu veliku."}, {"id": 117308, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5254, "text": "I tajili vi govor va\u0161 ili ga razgla\u0161avali, uistinu, On je Znalac onog \u0161ta je u grudima."}, {"id": 117309, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5255, "text": "Zar ne zna Onaj koji je stvorio? A On je Fini, Obavije\u0161teni."}, {"id": 117310, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5256, "text": "On je Taj koji vam je Zemlju u\u010dinio podlo\u017enom, pa hodajte predjelima njenim i jedite od opskrbe Njegove, a na Njemu je pro\u017eivljenje."}, {"id": 117311, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5257, "text": "Jeste li sigurni od Onog ko je u nebu, da ne\u0107e dati da vas proguta zemlja, tad kad se ona zatalasa?"}, {"id": 117312, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5258, "text": "Ili ste sigurni od Onog ko je u nebu, da ne po\u0161alje na vas oluju? Pa sazna\u0107ete kakva je opomena Moja!"}, {"id": 117313, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5259, "text": "I zaista su poricali oni koji su bili prije njih, pa kakvo je bilo odbijanje Moje!"}, {"id": 117314, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5260, "text": "Zar ne vide ptice iznad sebe - \u0161ire krila i skupljaju ih? Dr\u017ei ih jedino Milostivi. Uistinu, On je svake stvari Vidilac."}, {"id": 117315, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5261, "text": "Ili, ko je taj ko je vojska va\u0161a, da vam pomogne mimo Milostivog? Nevjernici su samo u iluziji."}, {"id": 117316, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5262, "text": "Ili, ko je taj ko \u0107e vas opskrbiti, ako zadr\u017ei opskrbu Svoju? Naprotiv, ustrajavaju u prevr\u0161ivanju i izbjegavanju."}, {"id": 117317, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5263, "text": "Pa da li je ko ide ni\u010dice na licu svom upu\u0107eniji, ili ko ide pravo na putu pravom?"}, {"id": 117318, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5264, "text": "Reci: \"On je Taj koji vas je stvorio, i na\u010dinio vam sluh i vidove i srca. Malo \u0161to zahvaljujete.\""}, {"id": 117319, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5265, "text": "Reci: \"On je Taj koji vas je umno\u017eio na Zemlji, a Njemu \u0107ete biti sabrani.\""}, {"id": 117320, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5266, "text": "I govore: \"Kad \u0107e ta prijetnja, ako istinu govorite?\""}, {"id": 117321, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5267, "text": "Reci: \"Znanje je jedino kod Allaha, a ja sam samo opominja\u010d jasni.\""}, {"id": 117323, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5268, "text": "Pa po\u0161to ga ugledaju blizu, rastu\u017ei\u0107e se lica onih koji ne vjeruju, i bi\u0107e re\u010deno: \"Ovo je to \u0161ta ste prizivali.\""}, {"id": 117324, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5269, "text": "Reci: \"Vidite li? Ako uni\u0161ti Allah mene i onog ko je sa mnom, ili nam se smiluje - pa ko \u0107e za\u0161tititi nevjernike od kazne bolne?\""}, {"id": 117325, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5270, "text": "Reci: \"On je Milostivi. U Njega vjerujemo i u Njega se pouzdavamo. Pa sazna\u0107ete ko je taj ko je u zabludi o\u010ditoj.\""}, {"id": 117326, "identifier": "bs.mlivo", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5271, "text": "Reci: \"Vidite li? Ako voda va\u0161a osvane porinuta, pa ko \u0107e vam donijeti vodu teku\u0107u?\""}]