[{"id": 115478, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3410, "text": "Alif. Lam. Mim."}, {"id": 115479, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3411, "text": "Pora\u017eeni su Bizantinci,"}, {"id": 115480, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3412, "text": "U zemlji najbli\u017eoj, a oni \u0107e poslije poraza svog, pobijediti,"}, {"id": 115481, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3413, "text": "Za nekoliko godina - Allahova je naredba prije i poslije - a tog dana radovat \u0107e se vjernici,"}, {"id": 115482, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3414, "text": "S pomo\u0107i Allahovom. Poma\u017ee koga ho\u0107e, a On je Mo\u0107ni, Milosrdni."}, {"id": 115483, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3415, "text": "Obe\u0107anje je Allahovo; ne\u0107e Allah iznevjeriti obe\u0107anje Svoje, me\u0111utim, ve\u0107ina ljudi ne zna."}, {"id": 115484, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3416, "text": "Znaju vanj\u0161tinu \u017eivota Dunjaa, a oni su o Ahiretu nehajni."}, {"id": 115485, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3417, "text": "Zar ne\u0107e razmisliti u du\u0161ama svojim? Allah je stvorio nebesa i Zemlju i \u0161ta je izme\u0111u njih, samo s Istinom i rokom odre\u0111enim. A uistinu, ve\u0107ina ljudi su u susret Gospodara svog nevjernici."}, {"id": 115486, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3418, "text": "Zar nisu putovali po Zemlji, pa vidjeli kakav je bio kraj onih prije njih? Bili su mo\u0107niji od njih u sili i kopali su zemlju i gradili je vi\u0161e no \u0161to je izgra\u0111uju, a dolazili su im poslanici njihovi s jasnim dokazima. Pa nije Allah u\u010dinio zulm njima, nego su du\u0161ama svojim zulm u\u010dinili."}, {"id": 115487, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3419, "text": "Potom je zlo bilo kraj onih koji su \u010dinili zlo, zato \u0161to su poricali ajete Allahove i njima se izrugivali."}, {"id": 115488, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3420, "text": "Allah je po\u010deo stvaranje, zatim ga ponavlja, potom \u0107ete Njemu biti vra\u0107eni."}, {"id": 115489, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3421, "text": "A na Dan kad nastupi ^as, o\u010dajavat \u0107e prestupnici."}, {"id": 115490, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3422, "text": "I ne\u0107e oni imati od ortaka svojih posrednike, i bi\u0107e u ortake svoje nevjernici."}, {"id": 115491, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3423, "text": "A na Dan kad nastupi ^as, Tog dana \u0107e se rastaviti."}, {"id": 115492, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3424, "text": "Pa \u0161to se ti\u010de onih koji budu vjerovali i dobra djela \u010dinili - pa oni \u0107e se u (bujnom) vrtu veseliti."}, {"id": 115493, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3425, "text": "Pa \u0161to se ti\u010de onih koji ne budu vjerovali i znakove Na\u0161e poricali, i susret Ahireta - pa takvi \u0107e biti u kaznu privedeni."}, {"id": 115494, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3426, "text": "Pa slavljen neka je Allah kad zano\u0107ite i kad osvanete -"}, {"id": 115495, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3427, "text": "I Njemu hvala na nebesima i Zemlji, i po\u010detkom no\u0107i i u podnevu kad ste!"}, {"id": 115496, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3428, "text": "Izvodi \u017eivo iz mrtvog i izvodi mrtvo iz \u017eivog. I o\u017eivljava zemlju poslije mrtvila njenog, a tako \u0107ete biti izvedeni."}, {"id": 115497, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3429, "text": "A od znakova Njegovih je \u0161to vas je stvorio od pra\u0161ine, potom gle! vi smrtnici (koji) se \u0161irite!"}, {"id": 115498, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3430, "text": "I od znakova Njegovih je \u0161to je stvorio za vas od du\u0161a va\u0161ih parice, da se smirite uz njih, i uspostavio izme\u0111u vas ljubav i milost. Uistinu, u tome su znaci za ljude koji razmi\u0161ljaju!"}, {"id": 115499, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3431, "text": "I od znakova Njegovih je stvaranje nebesa i Zemlje, i razli\u010ditost jezika va\u0161ih i boja va\u0161ih. Uistinu, u tome su znaci za u\u010dene!"}, {"id": 115500, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3432, "text": "I od znakova Njegovih je spavanje va\u0161e no\u0107u i danju i tra\u017eenje va\u0161e iz blagodati Njegove. Uistinu, u tome su znaci za ljude koji \u010duju!"}, {"id": 115501, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3433, "text": "I od znakova Njegovih je: pokazuje vam munju strahom i nadom, i spu\u0161ta s neba vodu, pa o\u017eivljava njome zemlju nakon mrtvila njenog. Uistinu, u tome su znaci za ljude koji razumiju!"}, {"id": 115502, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3434, "text": "I od znakova Njegovih je \u0161to postoji nebo i Zemlja po naredbi Njegovoj. Zatim, kad vas pozove zovom iz Zemlje, gle! vi izlazite!"}, {"id": 115503, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3435, "text": "I Njegov je ko je na nebesima i Zemlji. Svi su Njemu pokorni."}, {"id": 115505, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3436, "text": "I On je Taj koji po\u010dinje stvaranje, zatim ga ponavlja, a to je Njemu lak\u0161e. I Njegov primjer je najuzvi\u0161eniji na nebesima i Zemlji, a On je Mo\u0107ni, Mudri."}, {"id": 115506, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3437, "text": "Navodi vam primjer od du\u0161a va\u0161ih: imate li vi me\u0111u onim \u0161ta posjeduju desnice va\u0161e ikakve ortake u onom \u010dim smo vas opskrbili, pa ste vi u tome isti? Bojite li se njih kao va\u0161im strahom du\u0161a va\u0161ih? Tako izla\u017eemo znakove ljudima koji shvataju."}, {"id": 115507, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3438, "text": "Naprotiv, oni koji \u010dine zulm, slijede strasti svoje bez znanja. Pa ko \u0107e uputiti onoga koga je zaveo Allah? A ne\u0107e oni imati nikakvih pomaga\u010da."}, {"id": 115508, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3439, "text": "Zato stani licem svojim vjeri pravoj, fitretu Allahovom, onom po kojem je stvorio ljude - nema promjene stvaranja Allahovog. To je vjera prava, ali ve\u0107ina ljudi ne zna."}, {"id": 115509, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3440, "text": "Budite oni koji se obra\u0107aju Njemu, i bojte Ga se i obavljajte salat i ne budite od mu\u0161rika,"}, {"id": 115510, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3441, "text": "Od onih koji su podijelili vjeru svoju i postali sekte. Svaka stranka se s onim \u0161to je kod njih, raduje."}, {"id": 115511, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3442, "text": "A kad ljude dotakne nesre\u0107a, zovu Gospodara svog obra\u0107aju\u0107i Mu se pokajani\u010dki; zatim, kad oni okuse od Njega milost, gle! skupina njih Gospodaru svom pridru\u017euje,"}, {"id": 115512, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3443, "text": "Da bi bili nezahvalni na onom \u0161ta smo im dali. \"Pa u\u017eivajte, ta sazna\u0107ete!\""}, {"id": 115513, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3444, "text": "Zar smo im spustili dokaz, pa on govori o onom \u0161ta Mu pridru\u017euju?"}, {"id": 115514, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3445, "text": "I kad damo da ljudi okuse milost, raduju se u njoj; a kad ih zadesi zlo zbog onog \u0161to su prethodno uradile ruke njihove, gle! oni o\u010dajavaju!"}, {"id": 115515, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3446, "text": "Zar nisu vidjeli da Allah pru\u017ea opskrbu kome ho\u0107e, i uskra\u0107uje? Uistinu! U tome su znaci za ljude koji vjeruju."}, {"id": 115516, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3447, "text": "Pa daj bli\u017enjem pravo njegovo i siromahu i sinu puta. To je najbolje za one koji \u017eele lice Allahovo, a ti takvi \u0107e biti uspje\u0161ni."}, {"id": 115517, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3448, "text": "A ono \u0161to ste dali od kamate da bi porasla u imecima ljudi - pa ne\u0107e porasti kod Allaha; a ono \u0161to date od zekata \u017eele\u0107i lice Allahovo - pa ti takvi \u0107e imati mnogostruko."}, {"id": 115518, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3449, "text": "Allah je Taj koji vas je stvorio, zatim vas opskrbio, potom \u0107e vas usmrtiti, zatim \u0107e vas o\u017eiviti. Ima li iko od ortaka va\u0161ih koji \u010dini i\u0161ta od toga? Slava Njemu! I uzvi\u0161en neka je od onog \u0161ta pridru\u017euju!"}, {"id": 115519, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3450, "text": "Pojavio se nered na kopnu i moru zbog onog \u0161to \u010dine ruke ljudi, da bi dao da oni iskuse dio onog \u0161ta su uradili, ne bi li se oni povratili."}, {"id": 115520, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3451, "text": "Reci: \"Putujte po Zemlji pa vidite kakav je bio kraj onih od ranije! Bili su ve\u0107ina njih mu\u0161rici.\""}, {"id": 115521, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3452, "text": "Zato stani licem svojim vjeri ispravnoj, prije no do\u0111e od Allaha Dan kojem nema odbijanja. Tog dana \u0107e se razdvojiti."}, {"id": 115522, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3453, "text": "Ko ne vjeruje, pa protiv njega je kufr njegov, a ko radi dobro, pa du\u0161ama svojim pripremaju,"}, {"id": 115523, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3454, "text": "Da bi nagradio one koji vjeruju i rade dobra djela iz dobrote Svoje. Uistinu! On ne voli nevjernike."}, {"id": 115524, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3455, "text": "I od znakova Njegovih je \u0161to \u0161alje vjetrove (kao) radosne vijesti, i da vi ku\u0161ate iz milosti Njegove, i da plovi plovilo po naredbi Njegovoj, i da tra\u017eite iz blagodati Njegove, i da biste vi zahvaljivali."}, {"id": 115525, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3456, "text": "A doista, slali smo prije tebe poslanike narodu njihovom, pa su im dolazili s jasnim dokazima, te smo se osvetili onima koji su skrivili; a pomaganje vjernicima bila je na Nama obaveza."}, {"id": 115526, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3457, "text": "Allah je Taj koji \u0161alje vjetrove pa di\u017eu oblak, te ga rasprostire u nebu kako ho\u0107e i \u010dini ga komadima; tad vidi\u0161 ki\u0161u izlazi iz sred njega! Pa kad pogodi njome koga ho\u0107e od robova Svojih, gle! oni se vesele,"}, {"id": 115527, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3458, "text": "Iako su bili prije no \u0161to je spu\u0161tena na njih - prije toga - o\u010dajnici."}, {"id": 115528, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3459, "text": "Zato pogledaj tragove milosti Allahove, kako o\u017eivljava zemlju nakon mrtvila njenog. Uistinu! To je O\u017eivljava\u010d mrtvih, a On nad svakom stvari ima mo\u0107!"}, {"id": 115529, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3460, "text": "I da po\u0161aljemo vjetar, pa ga vide \u017eutim, sigurno bi poslije toga nastavili da ne vjeruju."}, {"id": 115531, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3461, "text": "Pa uistinu, ti ne\u0107e\u0161 pro\u010duti mrtve, niti pro\u010duti gluhe dozivanjem, kad se okrenu bje\u017ee\u0107i."}, {"id": 115532, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3462, "text": "I nisi ti vodi\u010d slijepcima iz zablude njihove. Pro\u010dut \u0107e\u0161 jedino onog ko vjeruje u ajete Na\u0161e - ta oni su muslimani."}, {"id": 115533, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3463, "text": "Allah je Taj koji vas je stvorio od slabog, zatim je poslije slabosti snagu na\u010dinio, potom je poslije snage slabost na\u010dinio i sijedu kosu. Stvara \u0161ta ho\u0107e, a On je Znalac, Mo\u0107ni."}, {"id": 115534, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3464, "text": "A na Dan kad nastupi ^as, zaklinja\u0107e se prestupnici da nisu ostali sem \u010das; tako su se oni odvratili."}, {"id": 115535, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3465, "text": "I re\u0107i \u0107e oni kojima je dato znanje i vjerovanje: \"Doista ste ostali po Knjizi Allahovoj do Dana podizanja, ta ovo je Dan podizanja, ali vi niste znali!\""}, {"id": 115536, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3466, "text": "Pa Tog dana ne\u0107e koristiti onima koji su \u010dinili zulm, pravdanje njihovo, niti \u0107e im se dati da tra\u017ee naklonost."}, {"id": 115537, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3467, "text": "I doista, naveli smo ljudima u ovom Kur'anu od svakog primjera (pone\u0161to); a da si im donio znak, sigurno bi rekli oni koji ne vjeruju: \"Vi ste samo oni koji iznose neistinu!\""}, {"id": 115538, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3468, "text": "Tako pe\u010dati Allah srca onih koji ne znaju."}, {"id": 115539, "identifier": "bs.mlivo", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3469, "text": "Zato se strpi! Uistinu, obe\u0107anje Allahovo je Istina, i neka te nikako ne smatraju lahkim oni koji nisu uvjereni."}]