[{"id": 117878, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5830, "text": "Kad se nebo rascijepi,"}, {"id": 117879, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5831, "text": "I kad se planete raspr\u0161e,"}, {"id": 117880, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5832, "text": "I kad mora uzavriju,"}, {"id": 117881, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5833, "text": "I kad se kaburi ispreturaju,"}, {"id": 117882, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5834, "text": "Zna\u0107e du\u0161a \u0161ta je unaprijed poslala i (\u0161ta) zapostavila."}, {"id": 117883, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5835, "text": "O \u010dovje\u010de! \u0160ta te je obmanulo o Gospodaru tvom plemenitom,"}, {"id": 117884, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5836, "text": "Koji te je stvorio, pa te uredio, te te simetri\u010dnim u\u010dinio?,"}, {"id": 117885, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5837, "text": "U kojem obliku, kako je htio, sastavio te!"}, {"id": 117886, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5838, "text": "Nikako! Naprotiv, pori\u010dete Sud."}, {"id": 117887, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5839, "text": "A uistinu, nad vama su \u010duvari,"}, {"id": 117888, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5840, "text": "Plemeniti pisari,"}, {"id": 117889, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5841, "text": "Znaju \u0161ta \u010dinite."}, {"id": 117890, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5842, "text": "Uistinu, pravedni \u0107e u zadovoljstvo,"}, {"id": 117891, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5843, "text": "I uistinu, razvratnici \u0107e u d\u017eehim."}, {"id": 117892, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5844, "text": "Pr\u017ei\u0107e se njime Dana sudnjeg,"}, {"id": 117893, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5845, "text": "I ne\u0107e oni iz njega biti odsutni."}, {"id": 117894, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5846, "text": "A \u0161ta zna\u0161 ti \u0161ta je Dan sudnji?"}, {"id": 117895, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5847, "text": "Ponovo, \u0161ta zna\u0161 ti \u0161ta je Dan sudnji?"}, {"id": 117896, "identifier": "bs.mlivo", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5848, "text": "Dan kad ne\u0107e vladati du\u0161a za du\u0161u nimalo, a komanda \u0107e Tog dana biti Allahova."}]