[{"id": 116288, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4219, "text": "Ha. Mim."}, {"id": 116289, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4220, "text": "Objava je od Milostivog, Milosrdnog."}, {"id": 116290, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4221, "text": "Knjiga \u010diji su ajeti razlo\u017eeni, Kur'an na arapskom, za ljude koji znaju."}, {"id": 116291, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4222, "text": "Donosilac je radosne vijesti i Opomene. Pa odvratila se ve\u0107ina njih, pa oni ne \u010duju,"}, {"id": 116292, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4223, "text": "I govore: \"Srca su na\u0161a pod pokrivkama prema onom \u010demu nas poziva\u0161, i u u\u0161ima na\u0161im je gluho\u0107a, a izme\u0111u nas i izme\u0111u tebe je veo; zato radi, uistinu, mi smo radnici.\""}, {"id": 116293, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4224, "text": "Reci: \"Ja sam samo smrtnik sli\u010dan vama. Meni se objavljuje da je uistinu Bog va\u0161, Bog Jedini, zato uzmite put pravi Njemu i tra\u017eite oprost Njegov. A te\u0161ko mu\u0161ricima,"}, {"id": 116294, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4225, "text": "Onima koji ne daju zekat, a oni su u Ahiret nevjernici."}, {"id": 116295, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4226, "text": "Uistinu, oni koji vjeruju i \u010dine dobra djela, ima\u0107e oni nagradu neprekidnu.\""}, {"id": 116296, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4227, "text": "Reci: \"Ne vjerujete li uistinu vi u Onog ko je stvorio Zemlju u dva dana, i pripisujete Mu suparnike? To je Gospodar svjetova!\""}, {"id": 116297, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4228, "text": "I na\u010dinio je u njoj planine stabilne - iznad nje, i blagoslovio je i odmjerio na njoj njene proizvode prehrane u \u010detiri dana jednaka - (informacija) za one koji pitaju."}, {"id": 116298, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4229, "text": "Zatim se usmjerio nebu - a ono je bilo dim - pa rekao njemu i Zemlji: \"Do\u0111ite milom ili silom!\" Reko\u0161e: \"Dolazimo poslu\u0161ni.\""}, {"id": 116299, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4230, "text": "Potom ih je uredio kao sedam nebesa u dva dana i objavio u svakom nebu stvar njegovu. A uljep\u0161ali smo nebo najbli\u017ee svjetiljkama i za\u0161titom. To je diktat Mo\u0107nog, Znalca."}, {"id": 116300, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4231, "text": "Pa ako se odvrate, tad reci: \"Upozoravam vas munjom sli\u010dnoj munji Ada i Semuda\","}, {"id": 116301, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4232, "text": "Kad su im dolazili poslanici ispred njih i iza njih: \"Ne obo\u017eavajte, izuzev Allaha!\" Rekli su: \"Da je htio Gospodar na\u0161, sigurno bi spustio meleke. Pa uistinu, mi smo u ono s \u010dim ste poslati, nevjernici.\""}, {"id": 116302, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4233, "text": "Pa \u0161to se ti\u010de Ada - pa oholili su se na Zemlji bez ikakvog prava i govorili: \"Ko je \u017ee\u0161\u0107i od nas u sili?\" Zar nisu vidjeli da je Allah koji ih je stvorio - On \u017ee\u0161\u0107i od njih u sili; a znakove Na\u0161e su nijekali."}, {"id": 116303, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4234, "text": "Zato smo na njih poslali vjetar olujni u danima nesretnim, da damo da oni iskuse kaznu poni\u017eenja u \u017eivotu Dunjaa; a sigurno je kazna Ahireta sramnija, i oni ne\u0107e biti pomognuti."}, {"id": 116304, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4235, "text": "A \u0161to se ti\u010de Semuda, upu\u0107ivali smo ih, pa su vi\u0161e voljeli sljepilo od Upute, te ih je pogodila munja kazne poni\u017eavaju\u0107e zbog onog \u0161to su zaslu\u017eili,"}, {"id": 116305, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4236, "text": "A spasili smo one koji su vjerovali i bili bogobojazni."}, {"id": 116306, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4237, "text": "A na Dan kad budu sakupljeni neprijatelji Allahovi k vatri - tad \u0107e oni biti raspodijeljeni."}, {"id": 116307, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4238, "text": "Dok, kad joj do\u0111u, svjedo\u010di\u0107e protiv njih sluh njihov i vidovi njihovi i ko\u017ee njihove, o onom \u0161ta su radili."}, {"id": 116308, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4239, "text": "I re\u0107i \u0107e ko\u017eama svojim: \"Za\u0161to svjedo\u010dite protiv nas?\" Re\u0107i \u0107e: \"Dao nam je Allah da govorimo - Onaj koji je govorom obdario svaku stvar\" - a On vas je stvorio prvi put, i Njemu se vra\u0107ate."}, {"id": 116309, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4240, "text": "A niste se krili da ne svjedo\u010di protiv vas sluh va\u0161, niti vidovi va\u0161i, niti ko\u017ee va\u0161e, nego ste mislili da Allah ne zna mnogo od onog \u0161ta ste radili."}, {"id": 116310, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4241, "text": "A to va\u0161e mi\u0161ljenje koje ste pretpostavljali o Gospodaru svom, upropastilo vas je, pa ste postali od gubitnika."}, {"id": 116311, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4242, "text": "Pa ako trpe - pa vatra je boravi\u0161te njihovo; a ako pitaju za naklonost, pa nisu oni od onih kojima je ona data."}, {"id": 116312, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4243, "text": "I dodijelili smo im drugove, pa su im uljep\u0161ali ono \u0161to je ispred njih i \u0161to je iza njih. I obistinila se na njima Rije\u010d u zajednicama ve\u0107 pro\u0161lim prije njih od d\u017einna i ljudi. Uistinu, oni su bili gubitnici."}, {"id": 116313, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4244, "text": "I ka\u017eu oni koji ne vjeruju: \"Ne slu\u0161ajte ovaj Kur'an i brbljajte u njemu, da biste vi nadvladali.\""}, {"id": 116314, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4245, "text": "Pa sigurno \u0107emo dati da oni koji ne vjeruju iskuse kaznu \u017eestoku, i sigurno \u0107emo ih platiti za najgore \u0161ta su radili."}, {"id": 116315, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4246, "text": "To je pla\u0107a neprijateljima Allahovim: Vatra! Ima\u0107e oni u njoj ku\u0107u vje\u010dnosti: pla\u0107a \u0161to su ajete Na\u0161e nijekali."}, {"id": 116316, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4247, "text": "I re\u0107i \u0107e oni koji nisu vjerovali: \"Gospodaru na\u0161! Poka\u017ei nam one od d\u017einna i ljudi koji su nas zaveli, da ih stavimo pod noge na\u0161e, da budu od najdonjih.\""}, {"id": 116317, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4248, "text": "Uistinu, oni koji govore: \"Gospodar na\u0161 je Allah\", zatim budu ispravni, spu\u0161taju se na njih meleci: \"Ne pla\u0161ite se i ne \u017ealostite i radujte se D\u017eennetu koji vam se obe\u0107ava."}, {"id": 116318, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4249, "text": "Mi smo za\u0161titnici va\u0161i u \u017eivotu Dunjaa i na Ahiretu. A ima\u0107ete vi u njemu \u0161ta za\u017eele du\u0161e va\u0161e, i ima\u0107ete vi u njemu \u0161ta zatra\u017eite,"}, {"id": 116319, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4250, "text": "Ugo\u0161\u0107enje od Oprosnika, Milosrdnog."}, {"id": 116320, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4251, "text": "A ko je bolji govorom od onog ko poziva Allahu i radi dobro i ka\u017ee: \"Uistinu! Ja sam od muslimana.\""}, {"id": 116321, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4252, "text": "I nije isto dobro i zlo. Odbij (zlo) s onim \u0161to je bolje, tad gle! onaj \u0161to je izme\u0111u tebe i njega neprijateljstvo - kao da je on prijatelj prisan."}, {"id": 116322, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4253, "text": "A pru\u017ei\u0107e se to samo onima koji se strpe, i ne\u0107e se to pru\u017eiti, izuzev posjedniku sre\u0107e goleme."}, {"id": 116323, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4254, "text": "A ako te navede od \u0161ejtana podsticaj, tad potra\u017ei za\u0161titu u Allaha. Uistinu! On, On je Onaj koji \u010duje, Znalac."}, {"id": 116324, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4255, "text": "I od znakova Njegovih je no\u0107 i dan, i Sunce i Mjesec. Ne padajte na sed\u017edu Suncu, ni Mjesecu, a padajte na sed\u017edu Allahu koji ih je stvorio, ako Njega obo\u017eavate!"}, {"id": 116325, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": true, "sajda_id": 12, "sajda_obligatory": true, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4256, "text": "Pa ako se uzohole, ta oni koji su kod Gospodara tvog, slave Ga no\u0107u i danju i oni se ne umaraju."}, {"id": 116326, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4257, "text": "I od znakova Njegovih je \u0161to ti vidi\u0161 zemlju smirenu, pa kad na nju spustimo vodu, zatreperi i uzbuja. Uistinu, Onaj koji je o\u017eivljava je O\u017eivljava\u010d mrtvih. Uistinu, On nad svakom stvari ima mo\u0107."}, {"id": 116327, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4258, "text": "Uistinu, oni koji iskre\u0107u ajete Na\u0161e, ne\u0107e se od Nas sakriti. Pa je li ko u vatru bude ba\u010den, bolji, ili ko do\u0111e siguran na Dan kijameta? Radite \u0161ta ho\u0107ete! Uistinu, On je onog \u0161ta radite Vidilac."}, {"id": 116328, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4259, "text": "Uistinu, oni koji ne vjeruju u Opomenu po\u0161to im je do\u0161la a uistinu, on je Knjiga silna."}, {"id": 116329, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4260, "text": "Ne prilazi joj neistina ispred nje, niti iza nje; Objava je od Mudrog, Hvaljenog."}, {"id": 116330, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4261, "text": "Govori ti se samo ono \u0161to je ve\u0107 re\u010deno poslanicima prije tebe. Uistinu, Gospodar tvoj je Vlasnik oprosta i Vlasnik kazne bolne."}, {"id": 116331, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4262, "text": "A da smo ga u\u010dinili Kur'anom na stranom (jeziku), sigurno bi rekli: \"\u0160to nisu razja\u0161njeni ajeti njegovi? Zar strani (jezik) a Arap?\" Reci: \"On je onima koji vjeruju Uputa i Lijek.\" A koji ne vjeruju, u u\u0161ima njihovim je gluho\u0107a i on je za njih sljepilo. Takvi \u0107e biti pozivani iz mjesta dalekog."}, {"id": 116332, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4263, "text": "I doista smo Musau dali Knjigu, pa se o njoj prepiralo. A da nije Rije\u010di prethodne od Gospodara tvog, sigurno bi me\u0111u njima bilo presu\u0111eno. A uistinu, oni su o njemu u sumnji ozbiljnoj."}, {"id": 116333, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4264, "text": "Ko radi dobro - pa za du\u0161u je njegovu, a ko \u010dini zlo - pa protiv nje je; i nije Gospodar tvoj nikako zalim robovima."}, {"id": 116334, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4265, "text": "Njemu se vra\u0107a znanje \u010casa. I ne izlaze plodovi iz \u010da\u0161ki svojih, i ne nosi nijedno \u017eensko, niti rodi - osim sa znanjem Njegovim. A na Dan kad ih zovne: \"Gdje su ortaci Moji?\" - re\u0107i \u0107e: \"Objavljujemo Ti: nije od nas nijedan svjedok.\""}, {"id": 116335, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4266, "text": "I nesta\u0107e od njih ono \u0161ta su prizivali ranije i zna\u0107e da oni ne\u0107e imati nikakvog pribje\u017ei\u0161ta."}, {"id": 116336, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4267, "text": "Ne zamara \u010dovjeka nimalo iskanje dobra, a ako ga dodirne zlo, tad je onaj koji zdvaja, o\u010dajan je."}, {"id": 116337, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4268, "text": "A ako damo da on okusi milost od Nas, nakon \u0161to ga je nesre\u0107a dotakla, sigurno govori: \"Ovo je moje i ne mislim da \u0107e \u010cas nastupiti. A ako se vratim Gospodaru svom, uistinu \u0107u ja kod Njega imati dobro.\" Pa sigurno \u0107emo obavijestiti one koji ne vjeruju, o onome \u0161ta su radili, i sigurno \u0107emo dati da oni okuse kaznu grubu."}, {"id": 116338, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4269, "text": "I kad \u010dovjeka obdarimo, odvrati se i udalji u svoju stranu, a kad ga dotakne zlo, tad je dowe \u0161iroke."}, {"id": 116339, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4270, "text": "Reci: \"Da li vidite, ako je od Allaha, potom niste vjerovali u njega, ko je zabludjeliji od onog ko je u protivrje\u010dju dalekom?\""}, {"id": 116340, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4271, "text": "Pokaziva\u0107emo im znakove Na\u0161e na horizontima i u du\u0161ama njihovim, dok im ne bude jasno da je on Istina. Zar nije dovoljno, Gospodar tvoj, \u0161to je On nad svakom stvari svjedok?"}, {"id": 116341, "identifier": "bs.mlivo", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4272, "text": "Uistinu! Oni su u sumnji o susretu Gospodara svog! Besumnje! Uistinu! On je svake stvari Okru\u017eitelj!"}]