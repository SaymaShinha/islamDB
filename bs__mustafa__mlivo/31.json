[{"id": 115540, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3470, "text": "Alif. Lam. Mim."}, {"id": 115541, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3471, "text": "To su znaci Kitaba mudrog,"}, {"id": 115542, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3472, "text": "Upute i milosti za dobro\u010dinitelje,"}, {"id": 115543, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3473, "text": "One koji obavljaju salat i daju zekat, a oni su u Ahiret, oni uvjereni."}, {"id": 115544, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3474, "text": "Takvi su na Uputi Gospodara svog, a ti takvi su uspje\u0161ni."}, {"id": 115545, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3475, "text": "I od ljudi je ko kupuje pri\u010du za zabavu, da bi zavodio s puta Allahovog bez ikakva znanja i uzimao ga ruglom. Ti takvi \u0107e imati kaznu prezrenu."}, {"id": 115546, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3476, "text": "A kad mu se u\u010de ajeti Na\u0161i, okrene se oholo kao da ih nije \u010duo, kao da je u oba njegova uha gluho\u0107a. Zato ga obraduj kaznom bolnom."}, {"id": 115547, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3477, "text": "Uistinu, oni koji vjeruju i \u010dine dobra djela, ima\u0107e oni ba\u0161\u010de u\u017eivanja,"}, {"id": 115548, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3478, "text": "Vje\u010dno \u0107e biti u njima. Obe\u0107anje Allahovo je istinito; a On je Mo\u0107ni, Mudri."}, {"id": 115549, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3479, "text": "Stvorio je nebesa bez stubova koje vidite, i postavio po Zemlji planine stabilne da vas ne trese, i razasuo po njoj svakovrsne \u017eivotinje. I spustili smo s neba vodu, pa u\u010dinili da na njoj iznikne (bilje) svake vrste plemenite."}, {"id": 115550, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 411, "ruku": 355, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3480, "text": "Ovo je stvaranje Allahovo. Pa poka\u017eite Mi \u0161ta su to stvorili oni mimo Njega! Naprotiv, zalimi su u zabludi o\u010ditoj."}, {"id": 115551, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3481, "text": "I doista, dali smo Lukmanu mudrost: \"Zahvaljuj Allahu! A ko zahvaljuje - pa samo zahvaljuje za du\u0161u svoju. A ko ne zahvaljuje - pa uistinu, Allah je Neovisni, Hvaljeni.\""}, {"id": 115552, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3482, "text": "I kad re\u010de Lukman sinu svom - a on ga je savjetovao: \"O sin\u010di\u0107u moj! Ne pridru\u017euj Allahu. Uistinu, \u0161irk je zulm stra\u0161an!\""}, {"id": 115553, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3483, "text": "I wassijetili smo \u010dovjeku za roditelje njegove - nosila ga je majka njegova slabo\u0161\u0107u nad slabosti, a odbijanje njegovo je u dvije godine - \"Zahvaljuj Meni i roditeljima svojim, Meni je dolazi\u0161te!\""}, {"id": 115554, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3484, "text": "A ako se bore s tobom da Mi pridru\u017ei\u0161 ono o \u010demu ti nema\u0161 znanje, tad ne poslu\u0161aj njih dvoje. I dru\u017ei se s njima na Dunjau uljudno, i slijedi put onog ko Mi se obra\u0107a. Zatim je Meni povratak va\u0161, pa \u0107u vas obavijestiti o onom \u0161ta ste radili."}, {"id": 115555, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3485, "text": "\"O sin\u010di\u0107u moj! Uistinu! Bilo to te\u017eine zrna goru\u0161ice, u stijeni ili u nebesima ili u Zemlji, iznije\u0107e ga Allah. Uistinu! On je Fini, Obavije\u0161teni."}, {"id": 115556, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3486, "text": "O sin\u010di\u0107u moj! Obavljaj salat i nare\u0111uj dobro, a zabranjuj nevaljalo, i strpi se nad onim \u0161ta te zadesi. Uistinu, to su odlu\u010duju\u0107e stvari."}, {"id": 115557, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3487, "text": "I ne iskrivljuj obraz svoj ljudima, i ne hodaj po Zemlji likuju\u0107i. Uistinu! Allah ne voli nijednog varalicu, hvalisavca."}, {"id": 115558, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 412, "ruku": 356, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3488, "text": "I budi umjeren u hodu svom i spusti glas svoj. Uistinu, najru\u017eniji glasovi su njakanje magaraca.\""}, {"id": 115559, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3489, "text": "Zar ne vidite da vam je Allah pot\u010dinio \u0161ta je na nebesima i \u0161ta je na Zemlji, i upotpunio vam blagodati Svoje svana i iznutra? A od ljudi je ko raspravlja o Allahu bez znanja i bez Upute, i bez Knjige osvjetljavaju\u0107e."}, {"id": 115560, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3490, "text": "A kad im se ka\u017ee: \"Slijedite ono \u0161ta je objavio Allah\", govore: \"Naprotiv, slijedimo ono na \u010demu smo na\u0161li o\u010deve na\u0161e.\" Zar i kad ih \u0161ejtan poziva kazni seira?"}, {"id": 115561, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3491, "text": "A ko preda lice svoje Allahu i on bude dobro\u010dinitelj, pa doista se uhvatio za hvati\u0161te naj\u010dvr\u0161\u0107e. A Allahov je konac stvari."}, {"id": 115562, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3492, "text": "A ko ne vjeruje, pa nek te ne \u017ealosti nevjerovanje njegovo. Nama je povratak njihov, pa \u0107emo ih obavijestiti o onom \u0161ta su radili. Uistinu! Allah je Znalac grudi."}, {"id": 115563, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3493, "text": "Dajemo im da u\u017eivaju malo, zatim \u0107emo ih natjerati kazni gruboj."}, {"id": 115564, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3494, "text": "A ako ih upita\u0161: \"Ko je stvorio nebesa i Zemlju?\" - sigurno \u0107e re\u0107i: \"Allah.\" Reci: \"Hvala neka je Allahu!\" Naprotiv, ve\u0107ina njih ne zna."}, {"id": 115565, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3495, "text": "Allahovo je \u0161ta je na nebesima i Zemlji. Uistinu! Allah! On je Neovisni, Hvaljeni."}, {"id": 115566, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3496, "text": "A da je svako drvo na Zemlji (pretvoreno u) pera, a more - da se ono produ\u017ei nakon (iscrpljenja) njegovog za sedam mora - ne bi se iscrpile Rije\u010di Allahove. Uistinu! Allah je Mo\u0107ni, Mudri."}, {"id": 115567, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 413, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3497, "text": "Stvaranje va\u0161e i podizanje va\u0161e (iz mrtvih) je samo - kao du\u0161e jedne. Uistinu! Allah je Onaj koji \u010duje, Onaj koji vidi."}, {"id": 115568, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3498, "text": "Zar ne vidi\u0161 da Allah daje da no\u0107 u\u0111e u dan i daje da dan u\u0111e u no\u0107, a pot\u010dinio je Sunce i Mjesec, svakoje plovi roku odre\u0111enom - i da je Allah o onom \u0161ta radite Obavije\u0161teni?"}, {"id": 115569, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3499, "text": "To zato \u0161to je Allah - On Istina, i \u0161to je ono \u0161to prizivaju mimo Njega, neistina, i \u0161to je Allah, On Uzvi\u0161eni, Veliki."}, {"id": 115570, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3500, "text": "Zar ne vidi\u0161 da plovilo plovi morem s blagodati Allahovom? - da bi vam pokazao neke znakove Svoje. Uistinu, u tome su znaci za svakog strpljivog, zahvalnog."}, {"id": 115571, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3501, "text": "I kad ih prekrije talas kao nadstre\u0161ice, prizivaju Allaha odani njegovoj vjeri. Pa po\u0161to ih izbavi na kopno, tad je neki od njih osrednji. A negira znakove Na\u0161e jedino svaki podmukli, nezahvalni."}, {"id": 115572, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3502, "text": "O ljudi! Bojte se Gospodara svog i pla\u0161ite se Dana kad ne\u0107e (mo\u0107i) platiti roditelj za dijete svoje, niti dijete - ono ne\u0107e biti onaj koji pla\u0107a za roditelja svog nimalo. Uistinu, obe\u0107anje Allahovo je Istina. Zato, neka vas nikako ne obmane \u017eivot Dunjaa, i neka vas nikako o Allahu ne obmane garur."}, {"id": 115573, "identifier": "bs.mlivo", "surah_number": 31, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0644\u0642\u0645\u0627\u0646", "surah_en_name": "Luqman", "surah_en_name_translation": "Luqman", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 414, "ruku": 357, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3503, "text": "Uistinu! Allah! Kod Njega je znanje ^asa. A spu\u0161ta ki\u0161u i zna \u0161ta je u matericama. A ne zna du\u0161a \u0161ta \u0107e zaraditi sutra, i ne zna du\u0161a u kojoj zemlji \u0107e umrijeti. Uistinu, Allah je Znalac, Obavije\u0161teni!"}]