[{"id": 117255, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5200, "text": "Allaha slavi \u0161ta je u nebesima i \u0161ta je na Zemlji. Njegova je vlast i Njegova je hvala, a On nad svakom stvari ima mo\u0107."}, {"id": 117256, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5201, "text": "On je Taj koji vas je stvorio, pa je od vas nevjernik i od vas vjernik. A Allah je onog \u0161to radite Vidilac."}, {"id": 117257, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5202, "text": "Stvorio je nebesa i Zemlju s Istinom i oblikovao vas i uljep\u0161ao likove va\u0161e, a Njemu je dolazi\u0161te!"}, {"id": 117258, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5203, "text": "Zna \u0161ta je u nebesima i Zemlji, i zna \u0161ta tajite i \u0161ta otkrivate; a Allah je Znalac grudi."}, {"id": 117259, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5204, "text": "Zar vam nije stigla vijest o onima koji nisu vjerovali prije, pa su iskusili pogubnost postupka svog, a ima\u0107e oni kaznu bolnu."}, {"id": 117260, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5205, "text": "To zato \u0161to su im dolazili poslanici njihovi s dokazima jasnim, pa su govorili: \"Zar da nas smrtnik upu\u0107uje?\" Pa nisu vjerovali i okretali su se. A Allah je Neovisani i Allah je Bogati, Hvaljeni!"}, {"id": 117261, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5206, "text": "Oni koji ne vjeruju tvrde da ne\u0107e biti podignuti. Reci: \"Svakako, tako mi Gospodara mog, sigurno \u0107ete biti podignuti, zatim sigurno obavije\u0161teni \u0161ta ste radili.\" A to je Allahu lahko."}, {"id": 117262, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5207, "text": "Zato vjerujte u Allaha i Poslanika Njegovog i svjetlo koje smo spustili. A Allah je o onom \u0161to radite Obavije\u0161teni."}, {"id": 117263, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5208, "text": "Na Dan kad vas skupi za Dan sastanka, to \u0107e biti Dan me\u0111usobnog obmanjivanja. A ko vjeruje u Allaha i radi dobro, pokri\u0107e mu zla djela njegova i uve\u0161\u0107e ga u ba\u0161\u010de ispod kojih teku rijeke, vje\u010dno \u0107e biti u njima, zauvijek. To je uspjeh veli\u010danstven!"}, {"id": 117264, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5209, "text": "A oni koji ne vjeruju i pori\u010du ajete Na\u0161e, takvi \u0107e biti stanovnici vatre, vje\u010dno \u0107e biti u njoj. A lo\u0161e je dolazi\u0161te!"}, {"id": 117265, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5210, "text": "Ne poga\u0111a nikakva nesre\u0107a, izuzev s dozvolom Allahovom. A ko vjeruje u Allaha - upu\u0107uje srce njegovo. A Allah je o svakoj stvari Znalac."}, {"id": 117266, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5211, "text": "I poslu\u0161ajte Allaha i poslu\u0161ajte Poslanika! Pa ako se okrenete, ta samo je na Poslaniku Na\u0161em dostava jasna."}, {"id": 117267, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5212, "text": "Allah! Samo je On Bog! I u Allaha zato nek se pouzdaju vjernici."}, {"id": 117268, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5213, "text": "O vi koji vjerujete! Uistinu, me\u0111u \u017eenama va\u0161im i djecom va\u0161om neprijatelja imate, zato ih se pri\u010duvajte. A ako izbri\u0161ete i pre\u0111ete i oprostite, pa doista je Allah Oprosnik, Milosrdni!"}, {"id": 117269, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5214, "text": "Bogatstva va\u0161a i djeca va\u0161a samo su isku\u0161enje - a Allah - kod Njega je nagrada veli\u010danstvena."}, {"id": 117270, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5215, "text": "Pa bojte se Allaha koliko mo\u017eete, i slu\u0161ajte i pokorite se i udjeljujte, bolje je za du\u0161e va\u0161e. A ko spasi pohlepe du\u0161u svoju - pa ti takvi su od uspjelih."}, {"id": 117271, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5216, "text": "Ako zajmite Allahu zajam lijep, udvostru\u010di\u0107e vam ga i oprosti\u0107e vam. A Allah je Zahvalni, Blagi,"}, {"id": 117272, "identifier": "bs.mlivo", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 5217, "text": "Znalac nevidljivog i vidljivog, Mo\u0107ni, Mudri!"}]