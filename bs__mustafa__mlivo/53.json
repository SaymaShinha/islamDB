[{"id": 116853, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Tako mi zvijezde kad zalazi,"}, {"id": 116854, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Nije zalutao drug va\u0161, niti skrenuo,"}, {"id": 116855, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4787, "text": "I ne govori po \u017eelji."}, {"id": 116856, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4788, "text": "To je jedino Objava - objavljuje se."}, {"id": 116857, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4789, "text": "U\u010di ga Jedan \u017eestokih sila,"}, {"id": 116858, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4790, "text": "Posjednik snage. Tad se postavio,"}, {"id": 116859, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4791, "text": "I on bio na horizontu najuzvi\u0161enijem,"}, {"id": 116860, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Zatim se pribli\u017eio, pa spustio,"}, {"id": 116861, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4793, "text": "Pa bio na rastojanju dva luka ili bli\u017ei,"}, {"id": 116680, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Te objavio (Allah) robu Svom, \u0161ta je objavio."}, {"id": 112267, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Nije poreklo srce \u0161ta je vidio."}, {"id": 112269, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Pa zar raspravljate s njim o onom \u0161to je vidio?"}, {"id": 112271, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4797, "text": "I zaista ga je vidio pri spu\u0161tanju drugom,"}, {"id": 112270, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4798, "text": "Kod Sidre krajnje"}, {"id": 112268, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4799, "text": "Kod koje je d\u017eennetsko skloni\u0161te,"}, {"id": 112272, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4800, "text": "Kad je Sidru pokrivalo \u0161ta je pokrivalo."}, {"id": 112299, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4801, "text": "Nije skrenuo pogled, niti prekora\u010dio,"}, {"id": 112792, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Doista je vidio od znakova Gospodara svog - najve\u0107e."}, {"id": 112998, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Pa jeste li vidjeli Lata i Uzzaa,"}, {"id": 113187, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4804, "text": "I Menat, tre\u0107u, posljednju?"}, {"id": 114494, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Zar vama mu\u0161ko, a Njemu \u017eensko?"}, {"id": 115504, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4806, "text": "To je onda podjela nepravedna!"}, {"id": 116862, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4807, "text": "To su samo imena - nadjeli ste ih vi i o\u010devi va\u0161i. Nije objavio Allah za njih nikakav dokaz. Slijede samo pretpostavku i ono \u0161ta \u017eele du\u0161e, a ve\u0107 im je do\u0161la Uputa od Gospodara njihovog!"}, {"id": 116863, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Ili: ho\u0107e li imati \u010dovjek \u0161ta za\u017eeli?"}, {"id": 116864, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Pa, Allahov je zadnji i prvi."}, {"id": 116865, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4810, "text": "I koliko je meleka u nebesima \u010dije posredovanje ne\u0107e ni\u0161ta koristiti, izuzev nakon \u0161to dopusti Allah kome ho\u0107e i bude zadovoljan!"}, {"id": 116866, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Uistinu, oni koji ne vjeruju u Ahiret, imenuju meleke imenovanjem \u017eenskim,"}, {"id": 116867, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4812, "text": "A nemaju oni o tome nikakvo znanje. Slijede samo pretpostavku, a uistinu, pretpostavka ni\u0161ta ne koristi Istini."}, {"id": 116868, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Zato se okreni od onog ko se odvra\u0107a od Opomene Na\u0161e i samo \u017eeli \u017eivot Dunjaa."}, {"id": 116869, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4814, "text": "To je njihov domet od znanja. Uistinu, Gospodar tvoj, On je Najbolji znalac ko je odlutao s puta Njegovog, i On je Najbolji znalac onog ko se upu\u0107uje."}, {"id": 116870, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4815, "text": "A Allahovo je \u0161ta je u nebesima i \u0161ta je na Zemlji, da bi platio one koji \u010dine zlo, onim \u0161to rade, i platio one koji rade lijepo - lijepim,"}, {"id": 116871, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4816, "text": "One koji izbjegavaju velike grijehe i razvrate, izuzev neznatnih. Uistinu, Gospodar tvoj je sveobuhvatnog oprosta. On je va\u0161 Najbolji poznavalac otkad vas je stvorio od zemlje, i kad ste vi bili zameci u utrobama majki va\u0161ih. Zato ne pripisujte \u010distotu du\u0161ama svojim. On je Najbolji znalac onog ko se boji."}, {"id": 116872, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Pa jesi li vidio onog koji se okrenuo,"}, {"id": 116873, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4818, "text": "I dao malo i zadr\u017eao,"}, {"id": 116874, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Je li kod njega znanje nevidljivog, pa on vidi?"}, {"id": 116875, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Zar nije obavije\u0161ten o onom \u0161to je u listovima Musaovim,"}, {"id": 116876, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4821, "text": "I Ibrahimovim, koji je ispunio:"}, {"id": 116877, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4822, "text": "Da se ne\u0107e teretiti optere\u0107eni teretom drugog,"}, {"id": 116878, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4823, "text": "I da \u0107e \u010dovjekovo biti samo za \u0161to se potrudi,"}, {"id": 116879, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4824, "text": "I da \u0107e trud njegov vidljiv biti -"}, {"id": 116880, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4825, "text": "Zatim \u0107e za njega biti pla\u0107en pla\u0107om najpotpunijom -"}, {"id": 116881, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4826, "text": "I da je Gospodaru tvom kraj (svega);"}, {"id": 116882, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4827, "text": "I da On, On zasmijava i rasplakuje,"}, {"id": 116883, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4828, "text": "I da On, On usmr\u0107uje i o\u017eivljava,"}, {"id": 116884, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4829, "text": "I da On stvara par: mu\u0161ko i \u017eensko,"}, {"id": 116885, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4830, "text": "Od kapi sjemena kada se izbaci -"}, {"id": 116886, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4831, "text": "I da je na Njemu stvaranje drugo;"}, {"id": 116887, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4832, "text": "I da je On, On Onaj koji oboga\u0107uje i Koji pribavlja,"}, {"id": 116888, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4833, "text": "I da je On, On Gospodar Siriusa,"}, {"id": 116889, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4834, "text": "I da je On uni\u0161tio Ada prvog,"}, {"id": 116890, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4835, "text": "I Semuda tad nije po\u0161tedio,"}, {"id": 116891, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4836, "text": "I narod Nuhov od ranije. Uistinu oni - bili su oni nepravedniji i vi\u0161e pretjeruju\u0107i;"}, {"id": 116892, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4837, "text": "I Mutefiket je poru\u0161io,"}, {"id": 116893, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4838, "text": "Pa ih je pokrilo \u0161ta je pokrilo."}, {"id": 116894, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4839, "text": "Pa o kojoj od blagodati Gospodara svog se raspravlja\u0161?"}, {"id": 116895, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Ovo je Opomena od opomena ranijih."}, {"id": 115530, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Pribli\u017eio se blisko,"}, {"id": 115948, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4842, "text": "Nema za njega mimo Allaha otkrivanja."}, {"id": 116048, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Pa zar se ovom hadisu i\u0161\u010du\u0111ujete,"}, {"id": 116896, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4844, "text": "I smijete se, a ne pla\u010dete,"}, {"id": 116897, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4845, "text": "I vi ste bezbri\u017eni?"}, {"id": 116898, "identifier": "bs.mlivo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4846, "text": "Pa padnite na sed\u017edu Allahu i obo\u017eavajte!"}]