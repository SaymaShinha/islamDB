[{"id": 116483, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4415, "text": "Ha. Mim."}, {"id": 116484, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4416, "text": "Tako mi Knjige jasne!"}, {"id": 116485, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4417, "text": "Uistinu, Mi smo je spustili u no\u0107i blagoslovljenoj - uistinu, Mi smo Opominja\u010di -"}, {"id": 116486, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4418, "text": "U njoj se razdjeljuje svaka stvar mudra,"}, {"id": 116487, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4419, "text": "Naredbom od Nas - uistinu, Mi smo Po\u0161iljaoci -"}, {"id": 116488, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4420, "text": "Milo\u0161\u0107u Gospodara tvog. Uistinu, On, On je Onaj koji \u010duje, Onaj koji zna,"}, {"id": 116489, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4421, "text": "Gospodara nebesa i Zemlje i \u0161ta je izme\u0111u njih - ako ste uvjereni."}, {"id": 116490, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4422, "text": "Samo je On Bog! O\u017eivljava i usmr\u0107uje; Gospodar va\u0161 i Gospodar o\u010deva va\u0161ih ranijih."}, {"id": 116491, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4423, "text": "Naprotiv, oni se u sumnji zabavljaju."}, {"id": 116492, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4424, "text": "Pa sa\u010dekaj Dan kad se nebo pojavi s dimom o\u010ditim,"}, {"id": 116493, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4425, "text": "Prekrit \u0107e ljude! Ovo \u0107e biti kazna bolna."}, {"id": 116494, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4426, "text": "\"Gospodaru na\u0161! Otkloni od nas kaznu. Uistinu, mi smo vjernici.\""}, {"id": 116495, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4427, "text": "Otkud im Opomena, a ve\u0107 im je do\u0161ao Poslanik jasni,"}, {"id": 116496, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4428, "text": "Potom su se od njega odvratili i rekli: \"Pou\u010deni, lu\u0111ak!\""}, {"id": 116497, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4429, "text": "Uistinu! Mi \u0107emo kaznu malo otkloniti. Doista! Vi ste povratnici."}, {"id": 116498, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4430, "text": "Na Dan kad dograbimo grabljenjem najve\u0107im, uistinu, Mi \u0107emo biti Osvetnici."}, {"id": 116499, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4431, "text": "I doista smo isku\u0161ali prije njih narod faraonov, a do\u0161ao im je poslanik plemeniti:"}, {"id": 116500, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4432, "text": "\"Izru\u010dite mi robove Allahove. Uistinu! Ja sam vam poslanik pouzdani."}, {"id": 116501, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4433, "text": "I ne uzvisujte se protiv Allaha. Uistinu! Ja vam dolazim s autoritetom o\u010ditim,"}, {"id": 116502, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4434, "text": "I uistinu, ja tra\u017eim za\u0161titu u Gospodara svog i Gospodara va\u0161eg, da me ne kamenujete."}, {"id": 116503, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4435, "text": "A ako mi ne vjerujete, onda me ostavite.\""}, {"id": 116504, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4436, "text": "Tad pozva (Musa) Gospodara svog: \"Ovi su narod prestupnika.\""}, {"id": 116505, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4437, "text": "\"Pa otputuj s robovima Mojim no\u0107u. Uistinu, vi \u0107ete biti slije\u0111eni."}, {"id": 116506, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4438, "text": "I ostavi more smirenim! Uistinu, oni \u0107e biti vojska potopljenih.\""}, {"id": 116507, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4439, "text": "Koliko ostavi\u0161e ba\u0161\u010da i izvora,"}, {"id": 116508, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4440, "text": "I \u017eitnih polja i mjesta plemenitog,"}, {"id": 116509, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4441, "text": "I blagodati u kojoj su radosni bili."}, {"id": 116510, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4442, "text": "Tako - a dali smo to u naslije\u0111e narodu drugih;"}, {"id": 116511, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4443, "text": "Pa nije plakalo nad njima nebo i Zemlja, i nije (im) bilo odgo\u0111eno."}, {"id": 116512, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4444, "text": "I doista smo spasili sinove Israilove od kazne prezrene,"}, {"id": 116513, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4445, "text": "Od faraona; Uistinu, on je bio nadmen, od onih koji pretjeruju."}, {"id": 116514, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4446, "text": "I doista smo ih odabrali sa znanjem nad svjetovima,"}, {"id": 116515, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4447, "text": "I dali im znakove u kojima je bila ku\u0161nja o\u010dita."}, {"id": 116516, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4448, "text": "Uistinu, ovi govore:"}, {"id": 116517, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4449, "text": "\"To je samo na\u0161a prva smrt, i ne\u0107emo mi biti podignuti."}, {"id": 116518, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4450, "text": "Pa dajte o\u010deve na\u0161e, ako ste iskreni!\""}, {"id": 116519, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4451, "text": "Jesu li oni bolji ili narod Tubba i oni prije njih? Uni\u0161tili smo ih. Uistinu, oni su bili prestupnici!"}, {"id": 116520, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4452, "text": "I nismo stvorili nebesa i Zemlju i \u0161ta je izme\u0111u njih zabavljaju\u0107i se."}, {"id": 116521, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4453, "text": "Stvorili smo ih jedino s Istinom, ali ve\u0107ina njih ne zna."}, {"id": 116522, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4454, "text": "Uistinu, Dan odluke je njihovo vrijeme odre\u0111eno za sve,"}, {"id": 116523, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4455, "text": "Dan kad ne\u0107e prijatelj koristiti prijatelju nimalo, niti \u0107e oni biti pomognuti,"}, {"id": 116524, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4456, "text": "Izuzev kome se smiluje Allah. Uistinu! On, On je Mo\u0107ni, Milosrdni."}, {"id": 116525, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4457, "text": "Uistinu, drvo zekkum \u0107e biti"}, {"id": 116526, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4458, "text": "Hrana grje\u0161nika!"}, {"id": 116527, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4459, "text": "Kao rastopljen metal vri\u0107e u trbusima,"}, {"id": 116528, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4460, "text": "Kao vrenje klju\u010dale vode."}, {"id": 116529, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4461, "text": "\"Zgrabite ga te ga odvucite usred d\u017eehima,"}, {"id": 116530, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4462, "text": "Potom nad glavom njegovom za kaznu izlivajte klju\u010dalu vodu."}, {"id": 116531, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4463, "text": "Ku\u0161aj! Uistinu ti, ti si 'mo\u0107ni, plemeniti!'"}, {"id": 116532, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4464, "text": "Uistinu, ovo je ono u \u0161to ste sumnjali.\""}, {"id": 116533, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4465, "text": "Uistinu, bogobojazni \u0107e biti u mjestu sigurnom,"}, {"id": 116534, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4466, "text": "U ba\u0161\u010dama i izvorima,"}, {"id": 116535, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4467, "text": "Odijevat \u0107e se svilom i brokatom, naspramni \u0107e biti."}, {"id": 116536, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4468, "text": "Tako; i o\u017eeni\u0107emo ih hurijama lijepih krupnih o\u010diju,"}, {"id": 116537, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4469, "text": "Pozivat \u0107e u njima za svako vo\u0107e, sigurni,"}, {"id": 116538, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4470, "text": "Ne\u0107e u njima okusiti smrt, izuzev smrti prve, i sa\u010duva\u0107e ih kazne d\u017eehima,"}, {"id": 116539, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4471, "text": "Dobrotom od Gospodara tvog. To je to, uspjeh veli\u010danstven!"}, {"id": 116540, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4472, "text": "Pa doista smo ga olak\u0161ali na jeziku tvom, da bi se oni pou\u010dili."}, {"id": 116541, "identifier": "bs.mlivo", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 4473, "text": "Zato pri\u010dekaj! Uistinu, oni su \u010dekatelji."}]