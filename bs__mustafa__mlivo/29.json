[{"id": 115411, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3341, "text": "Alif. Lam. Mim."}, {"id": 115412, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3342, "text": "Misle li ljudi da \u0107e biti ostavljeni, ako ka\u017eu: \"Vjerujemo\", i da ne\u0107e biti isku\u0161avani?"}, {"id": 115413, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3343, "text": "A doista smo isku\u0161avali one prije njih. Pa sigurno \u0107e Allah znati one koji su istiniti i sigurno znati la\u0161ce."}, {"id": 115414, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3344, "text": "Zar misle oni koji rade zlodjela, da \u0107e Nas preduhitriti? Zlo je to \u0161to prosu\u0111uju!"}, {"id": 115415, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3345, "text": "Ko se nada susretu s Allahom, pa uistinu, rok Allahov je sigurno nastupaju\u0107i, a On je Onaj koji \u010duje, Znalac."}, {"id": 115416, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3346, "text": "A ko se bori, pa samo se bori za du\u0161u svoju. Uistinu! Allah je Neovisan od svjetova."}, {"id": 115417, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3347, "text": "A oni koji vjeruju i \u010dine dobra djela, - sigurno \u0107emo im pokriti zla djela njihova, i sigurno \u0107emo ih nagraditi (prema) najboljem \u0161ta su radili."}, {"id": 115418, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3348, "text": "I naredili smo \u010dovjeku prema roditeljima njegovim dobro. A ako se budu s tobom borili, da bi ti Meni pridru\u017eio ono o \u010demu ti nema\u0161 znanje, tad ih ne poslu\u0161aj. Meni je povratak va\u0161, pa \u0107u vas obavijestiti o onom \u0161ta ste radili."}, {"id": 115419, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3349, "text": "A oni koji vjeruju i \u010dine dobra djela, sigurno \u0107emo ih uvesti me\u0111u pravedne."}, {"id": 115420, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3350, "text": "I od ljudi je ko ka\u017ee: \"Vjerujemo u Allaha\" - pa kad bude pa\u0107en radi Allaha, smatra ku\u0161nju ljudi kao kaznu Allahovu; a ako do\u0111e pomo\u0107 od Gospodara tvog, sigurno \u0107e re\u0107i: \"Uistinu, mi smo bili s vama.\" Zar nije Allah Najbolji znalac o onom \u0161to je u grudima svjetova?"}, {"id": 115421, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3351, "text": "A sigurno Allah zna one koji vjeruju i sigurno zna munafike."}, {"id": 115422, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3352, "text": "I govore oni koji ne vjeruju, onima koji vjeruju: \"Slijedite put na\u0161 i ponije\u0107emo gre\u0161ke va\u0161e!\" A ne\u0107e oni biti nosioci gre\u0161aka njihovih nimalo. Uistinu, oni su la\u017eljivci."}, {"id": 115423, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3353, "text": "A sigurno \u0107e nositi terete svoje i terete uz terete svoje, i sigurno \u0107e biti pitani na Dan kijameta o onom \u0161ta su izmi\u0161ljali."}, {"id": 115424, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3354, "text": "I doista smo poslali Nuha narodu njegovom, pa je ostao me\u0111u njima hiljadu, izuzev pedeset godina, pa ih je zadesio potop, a oni su bili zalimi."}, {"id": 115293, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3355, "text": "Pa smo spasili njega i saputnike na la\u0111i i u\u010dinili je znakom svjetovima."}, {"id": 115425, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3356, "text": "I Ibrahima, kad re\u010de narodu svom: \"Obo\u017eavajte Allaha i bojte se Njega; to je bolje za vas, kad biste znali."}, {"id": 115426, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3357, "text": "Obo\u017eavate mimo Allaha jedino idole i izmi\u0161ljate la\u017e. Uistinu, oni koje obo\u017eavate mimo Allaha, ne vladaju va\u0161om opskrbom. Zato kod Allaha tra\u017eite opskrbu i obo\u017eavajte Njega i zahvaljujte Mu. Njemu \u0107ete biti vra\u0107eni.\""}, {"id": 115427, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3358, "text": "A ako budete poricali, pa doista su poricale zajednice prije vas. A na Poslaniku je jedino dostava jasna."}, {"id": 115428, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3359, "text": "Zar ne vide kako Allah po\u010dinje stvaranje, potom ga ponavlja? Uistinu! To je Allahu lahko."}, {"id": 115429, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3360, "text": "Reci: \"Putujte po Zemlji pa vidite kako je po\u010deo stvaranje, zatim \u0107e Allah stvoriti stvaranjem drugim. Uistinu! Allah nad svakom stvari ima mo\u0107."}, {"id": 115430, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3361, "text": "Ka\u017enjava koga ho\u0107e, a smiluje se kome ho\u0107e, a Njemu \u0107ete biti vra\u0107eni.\""}, {"id": 115431, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3362, "text": "I niste vi ti koji \u0107e uma\u0107i u Zemlji, niti u nebesima. A nemate vi mimo Allaha nikakva za\u0161titnika, niti pomaga\u010da."}, {"id": 115432, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3363, "text": "A oni koji ne vjeruju u ajete Allahove i susret s Njim, takvi se u o\u010daju spram milosti Moje, a ti takvi \u0107e imati kaznu bolnu."}, {"id": 115433, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3364, "text": "Pa je bio odgovor naroda njegovog, jedino \u0161to su rekli: \"Ubijte ga\", ili \"Spalite ga.\" Tad ga je Allah spasio od vatre. Uistinu, u tome su znaci za ljude koji vjeruju."}, {"id": 115434, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3365, "text": "I re\u010de: \"Mimo Allaha ste uzeli jedino idole ljubavlju me\u0111u vama u \u017eivotu Dunjaa, potom \u0107ete na Dan kijameta poricati jedni druge i proklinjati jedni druge, a skloni\u0161te va\u0161e bi\u0107e vatra, i ne\u0107ete vi imati nikakvih pomaga\u010da.\""}, {"id": 115435, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3366, "text": "Tad njemu povjerova Lut, i re\u010de: \"Uistinu! Ja sam muhad\u017eir Gospodaru svom. Uistinu, On, On je Mo\u0107ni, Mudri.\""}, {"id": 115436, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3367, "text": "I podarili smo njemu Ishaka i Jakuba, i dali potomstvu njegovom poslanstvo i Knjigu, i dali mu nagradu njegovu na Dunjau, a uistinu, on \u0107e na Ahiretu biti od pravednih."}, {"id": 115437, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3368, "text": "I Luta, kad re\u010de narodu svom: \"Uistinu, vi \u010dinite razvrat u kojem vas nije pretekao niko od svjetova."}, {"id": 115438, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3369, "text": "Zar vi uistinu pristupate ljudima i presjecate put, i \u010dinite u sastajali\u0161tu svom ru\u017eno? Tad je bio odgovor naroda njegovog samo \u0161to su rekli: \"Daj nam kaznu Allahovu, ako si od istinitih!\""}, {"id": 115439, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3370, "text": "Re\u010de: \"Gospodaru moj! Pomozi me protiv naroda mufsida.\""}, {"id": 115440, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3371, "text": "I po\u0161to Na\u0161i izaslanici do\u0111o\u0161e Ibrahimu s radosnom vije\u0161\u0107u, reko\u0161e: \"Uistinu, Mi \u0107emo uni\u0161titi stanovnike ovog grada, uistinu su stanovnici njegovi zalimi.\""}, {"id": 115441, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3372, "text": "(Ibrahim) re\u010de: \"Uistinu, u njemu je Lut.\" Reko\u0161e: \"Mi smo Najbolji znalac onog ko je u njemu. Sigurno \u0107emo spasiti njega i porodicu njegovu, izuzev \u017eene njegove, bi\u0107e od onih koji ostaju.\""}, {"id": 115442, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3373, "text": "I po\u0161to poslanici Na\u0161i do\u0111o\u0161e Lutu, sneveseli se zbog njih i bi pritje\u0161njen zbog njih, nemo\u0107an, i reko\u0161e: \"Ne boj se i ne \u017ealosti se! Uistinu, Mi smo Spasitelji tvoji i porodice tvoje, izuzev \u017eene tvoje, bi\u0107e od onih koji ostaju."}, {"id": 115443, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3374, "text": "Uistinu, Mi smo ti koji \u0107e spustiti na stanovnike ovog grada kaznu s neba, zbog toga \u0161to su grije\u0161ili.\""}, {"id": 115444, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3375, "text": "I doista smo od njega ostavili znak jasan za ljude koji razmi\u0161ljaju."}, {"id": 115445, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3376, "text": "A Medjenu, brata njihovog \u0160uajba, pa re\u010de: \"O narode moj! Obo\u017eavajte Allaha i nadajte se Danu posljednjem, i ne pravite zlo na Zemlji, \u010dine\u0107i fesad.\""}, {"id": 115446, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3377, "text": "Tad su ga porekli, pa ih je zahvatio potres, i osvanu\u0161e u ku\u0107i svojoj povaljani."}, {"id": 115447, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3378, "text": "I Ada i Semuda! A doista vam je jasno iz nastamba njihovih. I uljep\u0161ao im je \u0161ejtan djela njihova, te ih odvratio s puta, a bili su pronicljivi."}, {"id": 115448, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3379, "text": "I Karuna i faraona i Hamana! A doista im je donio Musa dokaze jasne, pa su se uzoholili na Zemlji, a nisu (Nas) mogli prete\u0107i."}, {"id": 115450, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3380, "text": "Tad smo svakog dograbili zbog grijeha njegovog. Pa od njih je onaj na koga smo poslali pje\u0161\u010danu oluju, i od njih je onaj kojeg je pogodio krik, i od njih je onaj kojeg smo dali da zemlja proguta, i od njih je onaj kojeg smo potopili. A nije im Allah u\u010dinio zulm, nego su du\u0161ama svojim zulm \u010dinili."}, {"id": 115451, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3381, "text": "Primjer onih koji uzimaju mimo Allaha za\u0161titnike, je kao primjer pauka koji isplete ku\u0107u. A uistinu - od najslabijih ku\u0107a je ku\u0107a paukova - kad bi znali!"}, {"id": 115452, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3382, "text": "Uistinu! Allah zna \u0161ta prizivaju mimo Njega od stvari. A On je Mo\u0107ni, Mudri."}, {"id": 115453, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3383, "text": "A to su primjeri, navodimo ih ljudima, i ne shvataju ih, izuzev u\u010deni."}, {"id": 115454, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3384, "text": "Stvorio je Allah nebesa i Zemlju s Istinom. Uistinu, u tome je znak za vjernike."}, {"id": 115455, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 346, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3385, "text": "Recitiraj \u0161ta ti se objavljuje od Knjige i obavljaj salat. Uistinu, salat odvra\u0107a od razvrata i ru\u017enog, a sigurno je spominjanje Allaha najve\u0107e; a Allah zna \u0161ta radite."}, {"id": 115456, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3386, "text": "I ne raspravljajte sa sljedbenicima Knjige osim s onim \u0161ta je najbolje, izuzev onih od njih koji \u010dine zulm, i recite: \"Vjerujemo u ono \u0161ta je objavljeno nama i \u0161ta je objavljeno vama; a Bog na\u0161 i Bog va\u0161 je Jedini, i mi smo Njemu predani.\""}, {"id": 115457, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3387, "text": "I tako smo ti objavili Knjigu. Pa oni kojima smo dali Knjigu, vjeruju u nju; i od ovih je ko u nju vjeruje. A negiraju ajete Na\u0161e samo nevjernici."}, {"id": 115458, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3388, "text": "I nisi prije nje recitirao nikakvu knjigu, niti si je pisao desnicom svojom. Tad bi posumnjali la\u017eovi."}, {"id": 115459, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3389, "text": "Naprotiv, to su znaci jasni u grudima onih kojima je dato znanje, a negiraju ajete Na\u0161e samo zalimi."}, {"id": 115460, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3390, "text": "I govore: \"Za\u0161to mu se ne spuste znaci od Gospodara njegovog?\" Reci: \"Jedino su znaci kod Allaha, a ja sam samo opominja\u010d jasni.\""}, {"id": 115461, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3391, "text": "Zar im nije dovoljno \u0161to smo ti Mi objavili Knjigu - u\u010di im se? Uistinu, u tome je milost i opomena za narod koji vjeruje."}, {"id": 115462, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3392, "text": "Reci: \"Dovoljan je Allah izme\u0111u mene i izme\u0111u vas Svjedok. Zna \u0161ta je u nebesima i Zemlji. A oni koji vjeruju u neistinu, a ne vjeruju u Allaha, ti takvi su gubitnici.\""}, {"id": 115463, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3393, "text": "I po\u017euruju te s kaznom; a da nije roka odre\u0111enog, sigurno bi im do\u0161la kazna. I sigurno \u0107e im do\u0107i iznenadno, a oni ne\u0107e opaziti."}, {"id": 115464, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3394, "text": "Po\u017euruju te s kaznom, a uistinu, D\u017eehennem \u0107e biti okru\u017eilac nevjernika,"}, {"id": 115465, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3395, "text": "Na Dan kad ih prekrije kazna iznad njih i ispod nogu njihovih i bude re\u010deno: \"Ku\u0161ajte ono \u0161ta ste radili!\""}, {"id": 115466, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3396, "text": "O robovi Moji koji vjerujete! Uistinu, Zemlja Moja je prostrana, zato Mene - ta Mene obo\u017eavajte!"}, {"id": 115467, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3397, "text": "Svaka du\u0161a \u0107e iskusiti smrt, zatim \u0107ete Nama biti povra\u0107eni."}, {"id": 115468, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3398, "text": "A oni koji vjeruju i rade dobra djela, sigurno \u0107emo ih smjestiti u d\u017eennetske odaje ispod kojih teku rijeke, vje\u010dno \u0107e biti u njima. Divna je nagrada radnika,"}, {"id": 115469, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3399, "text": "Onih koji se strpe i u Gospodara svog pouzdaju!"}, {"id": 115470, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3400, "text": "A koliko je \u017eivine koja opskrbu ne snosi? Allah opskrbljuje nju i vas. A On je Onaj koji \u010duje, Znalac."}, {"id": 115471, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3401, "text": "A ako ih upita\u0161: \"Ko je stvorio nebesa i Zemlju i pot\u010dinio Sunce i Mjesec?\" - sigurno \u0107e re\u0107i: \"Allah!\" Pa kako se odvra\u0107aju?"}, {"id": 115472, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3402, "text": "Allah pru\u017ea opskrbu kome ho\u0107e od robova Svojih, i uskra\u0107uje mu. Uistinu! Allah je o svakoj stvari Znalac."}, {"id": 115473, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3403, "text": "I ako ih upita\u0161: \"Ko spu\u0161ta s neba vodu, pa o\u017eivljava njome zemlju nakon mrtvila njenog?\" - sigurno \u0107e re\u0107i: \"Allah!\" Reci: \"Hvala Allahu!\" Naprotiv, ve\u0107ina njih ne razumije."}, {"id": 115474, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3404, "text": "A ovaj \u017eivot Dunjaa je samo igra i zabava. A uistinu, ku\u0107a Ahireta - ona je \u017eivot, kad bi znali!"}, {"id": 115475, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3405, "text": "Pa kad se ukrcaju na plovilo, zazivaju Allaha, odani Njegovom dinu. Pa po\u0161to ih izbavi na kopno, gle! oni (Mu) pridru\u017euju,"}, {"id": 115399, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3406, "text": "Da bi nezahvalni bili na onom \u0161ta smo im dali, i da bi u\u017eivali; pa sazna\u0107e!"}, {"id": 115449, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3407, "text": "Zar nisu vidjeli da smo Mi u\u010dinili Harem sigurnim, a kidnapuju se ljudi oko njih? Pa zar u neistinu vjeruju, a u blagodat Allahovu ne vjeruju?"}, {"id": 115476, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3408, "text": "A ko je nepravedniji od onog ko izmi\u0161lja la\u017e protiv Allaha ili pori\u010de Istinu, po\u0161to mu do\u0111e? Zar nije u D\u017eehennemu boravi\u0161te za nevjernike!"}, {"id": 115477, "identifier": "bs.mlivo", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Mlivo", "transelator_en_name": "Mustafa Mlivo", "format": "text", "type": "translation", "ayah_number": 3409, "text": "A oni koji se bore za Nas, sigurno \u0107emo ih uputiti putevima Na\u0161im. A uistinu, Allah je sa dobro\u010diniteljima."}]