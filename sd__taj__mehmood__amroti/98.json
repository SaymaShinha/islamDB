[{"id": 486153, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6131, "text": "\u06aa\u062a\u0627\u0628 \u0648\u0627\u0631\u0646 \u06fd \u0645\u0634\u0631\u06aa\u0646 \u0645\u0627\u0646 \u062c\u0646 \u0627\u0646\u06aa\u0627\u0631 \u06aa\u064a\u0648 \u0633\u064a (\u0627\u064a\u0633\u064a\u0646 \u062a\u0627\u0626\u064a\u0646 \u067e\u0646\u06be\u0646\u062c\u064a \u0631\u0633\u0645 \u06a9\u0627\u0646) \u0627\u0644\u06b3 \u067f\u064a\u06bb \u0648\u0627\u0631\u0627 \u0646\u0647 \u06be\u0648\u0627 \u062c\u064a\u0633\u064a\u0646 \u062a\u0627\u0626\u064a\u0646 \u0648\u067d\u0646 \u067e\u068c\u0631\u064a \u062d\u064f\u062c\u0651\u062a (\u0646\u0647) \u0627\u0686\u064a."}, {"id": 486154, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6132, "text": "(\u064a\u0639\u0646\u064a) \u0627\u0644\u0644\u0647 \u062c\u0648 \u067e\u064a\u063a\u0645\u0628\u0631 \u062c\u0648 \u067e\u0627\u06aa \u0635\u062d\u064a\u0641\u0627 \u067e\u0699\u06be\u064a."}, {"id": 486155, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6133, "text": "\u062c\u0646 \u06fe \u0633\u068c\u0627 \u0633\u0646\u0648\u0627\u0646 \u0644\u06a9\u064a\u0644 \u062d\u064f\u06aa\u0645 \u0622\u06be\u0646."}, {"id": 486156, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6134, "text": "\u06fd \u062c\u0646 \u06a9\u064a \u06aa\u062a\u0627\u0628 \u068f\u0646\u0648 \u0648\u064a\u0648 \u0633\u064a \u067e\u0627\u06bb \u0648\u067d \u067e\u068c\u0631\u064a \u062d\u064f\u062c\u0651\u062a \u062c\u064a \u0627\u0686\u06bb \u06a9\u0627\u0646\u067e\u0648\u0621\u0650 \u0645\u064f\u062e\u062a\u0644\u0641 \u067f\u064a\u0627."}, {"id": 486157, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6135, "text": "\u06fd \u06be\u0646 \u06a9\u0627\u0646\u0633\u0648\u0627\u0621\u0650 (\u067b\u064a\u0648) \u06aa\u0648 \u062d\u06aa\u0645 \u0646\u0647 \u06aa\u064a\u0648 \u0648\u064a\u0646 \u062a\u0647 \u0628\u0627\u0637\u0644 \u06a9\u0627\u0646 \u0645\u064f\u0646\u06be\u0646 \u0645\u0648\u0699\u064a \u0627\u0644\u0644\u0647 \u062c\u064a \u0639\u0628\u0627\u062f\u062a \u06aa\u0646 \u06fd \u0646\u0645\u0627\u0632 \u067e\u0699\u06be\u0646\u062f\u0627 \u0631\u06be\u0646 \u06fd \u0632\u06aa\u0648\u0670\u0629 \u068f\u064a\u0646\u062f\u0627 \u0631\u06be\u0646 \u06fd \u0627\u06be\u0648 \u0622\u06be\u064a \u0633\u068c\u0648 \u062f\u064a\u0646."}, {"id": 486158, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6136, "text": "\u0628\u064a\u0634\u06aa \u06aa\u062a\u0627\u0628 \u0648\u0627\u0631\u0646 \u06fd \u0645\u0634\u0631\u06aa\u0646 \u0645\u0627\u0646 \u062c\u0646 \u06aa\u0641\u0631 \u06aa\u064a\u0648 \u0633\u064a \u062f\u0648\u0632\u062e \u062c\u064a \u0628\u0627\u06be \u06fe \u067e\u0648\u0646\u062f\u0627 \u0645\u0646\u062c\u06be\u0633 \u0633\u062f\u0627\u0626\u064a\u0646 \u0631\u06be\u0646\u062f\u0699 \u06be\u0648\u0646\u062f\u0627\u060c \u0627\u06be\u064a \u0626\u064a (\u0633\u0627\u0631\u064a\u0621\u064e) \u062e\u0644\u0642 \u06a9\u0627\u0646 (\u062a\u0645\u0627\u0645) \u0628\u0687\u0699\u0627 \u0622\u06be\u0646."}, {"id": 486159, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6137, "text": "\u0628\u064a\u0634\u06aa \u062c\u0646 \u0627\u064a\u0645\u0627\u0646 \u0622\u0646\u062f\u0648 \u06fd \u0686\u06b1\u0627 \u06aa\u0645 \u06aa\u064a\u0627 \u0627\u06be\u064a \u0626\u064a (\u0633\u0627\u0631\u064a\u0621\u064e) \u062e\u064e\u0644\u0642 \u06a9\u0627\u0646 \u0680\u064e\u0644\u0627 \u0622\u06be\u0646."}, {"id": 486160, "identifier": "sd.amroti", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sd", "name": "\u0627\u0645\u0631\u0648\u067d\u064a", "transelator_en_name": "Taj Mehmood Amroti", "format": "text", "type": "translation", "ayah_number": 6138, "text": "\u0633\u0646\u062f\u0646 \u0628\u062f\u0644\u0648 \u0633\u0646\u062f\u0646 \u067e\u0627\u0644\u06bb\u06be\u0627\u0631 \u0648\u067d \u06be\u0645\u064a\u0634\u0647 \u062c\u064a \u0631\u06be\u06bb \u0648\u0627\u0631\u0627 \u0628\u0627\u063a \u0622\u06be\u0646 \u062c\u0646 \u062c\u064a \u06be\u064a\u067a\u0627\u0646 \u0648\u0627\u06be\u064a\u0648\u0646 \u067e\u064a\u0648\u0646 \u0648\u06be\u0646 \u0627\u0646\u06be\u0646 \u06fe \u06be\u0645\u064a\u0634\u0647 \u0633\u062f\u0627\u0626\u064a\u0646 \u0631\u06be\u0646\u062f\u0627\u060c \u0627\u0644\u0644\u0647 \u06a9\u0627\u0626\u0646 \u0631\u0627\u0636\u064a \u067f\u064a\u0648 \u06fd \u0627\u064f\u06be\u064a \u06a9\u0627\u0646\u0626\u0633 \u0631\u0627\u0636\u064a \u067f\u064a\u0627\u060c \u0627\u06be\u0648 (\u0648\u0639\u062f\u0648) \u0627\u0646\u06be\u064a\u0621\u064e \u0644\u0627\u0621\u0650 \u062c\u064a\u06aa\u0648 \u067e\u0646\u06be\u0646\u062c\u064a \u067e\u0627\u0644\u06bb\u06be\u0627\u0631 \u06a9\u0627\u0646 \u068a\u0646\u0648."}]