[{"id": 73700, "identifier": "quran-uthmani-min", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthmani Minimal", "transelator_en_name": "Uthmani Minimal", "format": "text", "type": "quran", "ayah_number": 5242, "text": "\u0628\u0650\u0633\u0645\u0650 \u0627\u0644\u0644\u0651\u064e\u0647\u0650 \u0627\u0644\u0631\u0651\u064e\u062d\u0645\u0670\u0646\u0650 \u0627\u0644\u0631\u0651\u064e\u062d\u064a\u0645\u0650 \u062a\u064e\u0628\u0670\u0631\u064e\u0643\u064e \u0627\u0644\u0651\u064e\u0630\u0649 \u0628\u0650\u064a\u064e\u062f\u0650\u0647\u0650 \u0627\u0644\u0645\u064f\u0644\u0643\u064f \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u064e\u0644\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u0621\u064d \u0642\u064e\u062f\u064a\u0631\u064c"}, {"id": 73701, "identifier": "quran-uthmani-min", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthmani Minimal", "transelator_en_name": "Uthmani Minimal", "format": "text", "type": "quran", "ayah_number": 5243, "text": "\u0627\u0644\u0651\u064e\u0630\u0649 \u062e\u064e\u0644\u064e\u0642\u064e \u0627\u0644\u0645\u064e\u0648\u062a\u064e \u0648\u064e\u0627\u0644\u062d\u064e\u064a\u0648\u0670\u0629\u064e \u0644\u0650\u064a\u064e\u0628\u0644\u064f\u0648\u064e\u0643\u064f\u0645 \u0623\u064e\u064a\u0651\u064f\u0643\u064f\u0645 \u0623\u064e\u062d\u0633\u064e\u0646\