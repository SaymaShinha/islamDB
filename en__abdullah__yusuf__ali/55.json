[{"id": 173115, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4902, "text": "(Allah) Most Gracious!"}, {"id": 173116, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4903, "text": "It is He Who has taught the Qur'an."}, {"id": 173117, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4904, "text": "He has created man:"}, {"id": 173118, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4905, "text": "He has taught him speech (and intelligence)."}, {"id": 173119, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4906, "text": "The sun and the moon follow courses (exactly) computed;"}, {"id": 173120, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4907, "text": "And the herbs and the trees - both (alike) prostrate in adoration."}, {"id": 173121, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4908, "text": "And the Firmament has He raised high, and He has set up the Balance (of Justice),"}, {"id": 173122, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4909, "text": "In order that ye may not transgress (due) balance."}, {"id": 173123, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4910, "text": "So establish weight with justice and fall not short in the balance."}, {"id": 173124, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4911, "text": "It is He Who has spread out the earth for (His) creatures:"}, {"id": 173125, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4912, "text": "Therein is fruit and date-palms, producing spathes (enclosing dates);"}, {"id": 173126, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4913, "text": "Also corn, with (its) leaves and stalk for fodder, and sweet-smelling plants."}, {"id": 173127, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4914, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173128, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4915, "text": "He created man from sounding clay like unto pottery,"}, {"id": 173129, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4916, "text": "And He created Jinns from fire free of smoke:"}, {"id": 173130, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4917, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173131, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4918, "text": "(He is) Lord of the two Easts and Lord of the two Wests:"}, {"id": 173132, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4919, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173133, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4920, "text": "He has let free the two bodies of flowing water, meeting together:"}, {"id": 173134, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4921, "text": "Between them is a Barrier which they do not transgress:"}, {"id": 173135, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4922, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173136, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4923, "text": "Out of them come Pearls and Coral:"}, {"id": 173137, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4924, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173138, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4925, "text": "And His are the Ships sailing smoothly through the seas, lofty as mountains:"}, {"id": 173139, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4926, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173140, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4927, "text": "All that is on earth will perish:"}, {"id": 173141, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4928, "text": "But will abide (for ever) the Face of thy Lord,- full of Majesty, Bounty and Honour."}, {"id": 173142, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4929, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173143, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4930, "text": "Of Him seeks (its need) every creature in the heavens and on earth: every day in (new) Splendour doth He (shine)!"}, {"id": 173144, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4931, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173145, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4932, "text": "Soon shall We settle your affairs, O both ye worlds!"}, {"id": 173146, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4933, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173147, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4934, "text": "O ye assembly of Jinns and men! If it be ye can pass beyond the zones of the heavens and the earth, pass ye! not without authority shall ye be able to pass!"}, {"id": 173148, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4935, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173149, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4936, "text": "On you will be sent (O ye evil ones twain!) a flame of fire (to burn) and a smoke (to choke): no defence will ye have:"}, {"id": 173150, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4937, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173151, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4938, "text": "When the sky is rent asunder, and it becomes red like ointment:"}, {"id": 173152, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4939, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173153, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4940, "text": "On that Day no question will be asked of man or Jinn as to his sin."}, {"id": 173154, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4941, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173155, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4942, "text": "(For) the sinners will be known by their marks: and they will be seized by their forelocks and their feet."}, {"id": 173156, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4943, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173157, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4944, "text": "This is the Hell which the Sinners deny:"}, {"id": 173158, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4945, "text": "In its midst and in the midst of boiling hot water will they wander round!"}, {"id": 173159, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4946, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173160, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4947, "text": "But for such as fear the time when they will stand before (the Judgment Seat of) their Lord, there will be two Gardens-"}, {"id": 173161, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4948, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173162, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4949, "text": "Containing all kinds (of trees and delights);-"}, {"id": 173163, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4950, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173164, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4951, "text": "In them (each) will be two Springs flowing (free);"}, {"id": 173165, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4952, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173166, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4953, "text": "In them will be Fruits of every kind, two and two."}, {"id": 173167, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4954, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173168, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4955, "text": "They will recline on Carpets, whose inner linings will be of rich brocade: the Fruit of the Gardens will be near (and easy of reach)."}, {"id": 173169, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4956, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173170, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4957, "text": "In them will be (Maidens), chaste, restraining their glances, whom no man or Jinn before them has touched;-"}, {"id": 173171, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4958, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173172, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4959, "text": "Like unto Rubies and coral."}, {"id": 173173, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4960, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173174, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4961, "text": "Is there any Reward for Good - other than Good?"}, {"id": 173175, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4962, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173176, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4963, "text": "And besides these two, there are two other Gardens,-"}, {"id": 173177, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4964, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173178, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4965, "text": "Dark-green in colour (from plentiful watering)."}, {"id": 173179, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4966, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173180, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4967, "text": "In them (each) will be two Springs pouring forth water in continuous abundance:"}, {"id": 173181, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4968, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173182, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4969, "text": "In them will be Fruits, and dates and pomegranates:"}, {"id": 173183, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4970, "text": "Then which of the favours of your Lord will ye deny?"}, {"id": 173184, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4971, "text": "In them will be fair (Companions), good, beautiful;-"}, {"id": 173185, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4972, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173186, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4973, "text": "Companions restrained (as to their glances), in (goodly) pavilions;-"}, {"id": 173187, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4974, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173188, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4975, "text": "Whom no man or Jinn before them has touched;-"}, {"id": 173189, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4976, "text": "Then which of the favours of your Lord will ye deny?-"}, {"id": 173190, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 4977, "text": "Reclining on green Cushions and rich Carpets of beauty."}, {"id": 173191, "identifier": "en.yusufali", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "sur