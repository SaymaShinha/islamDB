[{"id": 174379, "identifier": "en.yusufali", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6189, "text": "Seest thou not how thy Lord dealt with the Companions of the Elephant?"}, {"id": 174380, "identifier": "en.yusufali", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6190, "text": "Did He not make their treacherous plan go astray?"}, {"id": 174381, "identifier": "en.yusufali", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6191, "text": "And He sent against them Flights of Birds,"}, {"id": 174382, "identifier": "en.yusufali", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6192, "text": "Striking them with stones of baked clay."}, {"id": 174383, "identifier": "en.yusufali", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6193, "text": "Then did He make them like an empty field of stalks and straw, (of which the corn) has been eaten up."}]