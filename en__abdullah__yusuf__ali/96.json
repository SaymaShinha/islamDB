[{"id": 174298, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6107, "text": "Proclaim! (or read!) in the name of thy Lord and Cherisher, Who created-"}, {"id": 174299, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6108, "text": "Created man, out of a (mere) clot of congealed blood:"}, {"id": 174300, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6109, "text": "Proclaim! And thy Lord is Most Bountiful,-"}, {"id": 174301, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6110, "text": "He Who taught (the use of) the pen,-"}, {"id": 174302, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6111, "text": "Taught man that which he knew not."}, {"id": 174303, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Nay, but man doth transgress all bounds,"}, {"id": 174304, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6113, "text": "In that he looketh upon himself as self-sufficient."}, {"id": 174305, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6114, "text": "Verily, to thy Lord is the return (of all)."}, {"id": 174306, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6115, "text": "Seest thou one who forbids-"}, {"id": 174307, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6116, "text": "A votary when he (turns) to pray?"}, {"id": 174308, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6117, "text": "Seest thou if he is on (the road of) Guidance?-"}, {"id": 174309, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6118, "text": "Or enjoins Righteousness?"}, {"id": 174310, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6119, "text": "Seest thou if he denies (Truth) and turns away?"}, {"id": 174311, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6120, "text": "Knoweth he not that Allah doth see?"}, {"id": 174312, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6121, "text": "Let him beware! If he desist not, We will drag him by the forelock,-"}, {"id": 174313, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6122, "text": "A lying, sinful forelock!"}, {"id": 174314, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6123, "text": "Then, let him call (for help) to his council (of comrades):"}, {"id": 174315, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6124, "text": "We will call on the angels of punishment (to deal with him)!"}, {"id": 174316, "identifier": "en.yusufali", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6125, "text": "Nay, heed him not: But bow down in adoration, and bring thyself the closer (to Allah)!"}]