[{"id": 174317, "identifier": "en.yusufali", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6126, "text": "We have indeed revealed this (Message) in the Night of Power:"}, {"id": 174318, "identifier": "en.yusufali", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6127, "text": "And what will explain to thee what the night of power is?"}, {"id": 174319, "identifier": "en.yusufali", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6128, "text": "The Night of Power is better than a thousand months."}, {"id": 174320, "identifier": "en.yusufali", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah