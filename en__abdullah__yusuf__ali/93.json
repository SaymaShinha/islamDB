[{"id": 174272, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6080, "text": "By the Glorious Morning Light,"}, {"id": 174273, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6081, "text": "And by the Night when it is still,-"}, {"id": 174274, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Thy Guardian-Lord hath not forsaken thee, nor is He displeased."}, {"id": 174275, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6083, "text": "And verily the Hereafter will be better for thee than the present."}, {"id": 174276, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6084, "text": "And soon will thy Guardian-Lord give thee (that wherewith) thou shalt be well-pleased."}, {"id": 174277, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Did He not find thee an orphan and give thee shelter (and care)?"}, {"id": 174278, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6086, "text": "And He found thee wandering, and He gave thee guidance."}, {"id": 174279, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6087, "text": "And He found thee in need, and made thee independent."}, {"id": 174280, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6088, "text": "Therefore, treat not the orphan with harshness,"}, {"id": 168399, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6089, "text": "Nor repulse the petitioner (unheard);"}, {"id": 174281, "identifier": "en.yusufali", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6090, "text": "But the bounty of the Lord - rehearse and proclaim!"}]