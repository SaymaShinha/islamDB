[{"id": 174236, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6044, "text": "By the Sun and his (glorious) splendour;"}, {"id": 174237, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6045, "text": "By the Moon as she follows him;"}, {"id": 174238, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6046, "text": "By the Day as it shows up (the Sun's) glory;"}, {"id": 174239, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6047, "text": "By the Night as it conceals it;"}, {"id": 174240, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6048, "text": "By the Firmament and its (wonderful) structure;"}, {"id": 174241, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6049, "text": "By the Earth and its (wide) expanse:"}, {"id": 174242, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6050, "text": "By the Soul, and the proportion and order given to it;"}, {"id": 174243, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6051, "text": "And its enlightenment as to its wrong and its right;-"}, {"id": 174244, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6052, "text": "Truly he succeeds that purifies it,"}, {"id": 174245, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6053, "text": "And he fails that corrupts it!"}, {"id": 174246, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6054, "text": "The Thamud (people) rejected (their prophet) through their inordinate wrong-doing,"}, {"id": 174247, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6055, "text": "Behold, the most wicked man among them was deputed (for impiety)."}, {"id": 174248, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6056, "text": "But the Messenger of Allah said to them: \"It is a She-camel of Allah! And (bar her not from) having her drink!\""}, {"id": 174249, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6057, "text": "Then they rejected him (as a false prophet), and they hamstrung her. So their Lord, on account of their crime, obliterated their traces and made them equal (in destruction, high and low)!"}, {"id": 174250, "identifier": "en.yusufali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Yusuf Ali", "transelator_en_name": "Abdullah Yusuf Ali", "format": "text", "type": "translation", "ayah_number": 6058, "text": "And for Him is no fear of its consequences."}]