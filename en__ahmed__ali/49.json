[{"id": 185313, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4613, "text": "O YOU WHO believe, do not forestall the judgements of God and His Apostle, and have fear of God. Verily God hears all and knows everything."}, {"id": 185314, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4614, "text": "O you who believe, do not raise your voices above the voice of the Prophet, and do not speak loudly to him as you do with one another lest your deeds are nullified unconsciously."}, {"id": 185315, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4615, "text": "They who speak in a low voice in the presence of God's Apostle, have had their hearts tried by God for reverence. There is forgiveness for them and a great reward."}, {"id": 185316, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4616, "text": "Many of those who call you from outside the apartments are not considerate."}, {"id": 185317, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4617, "text": "If they had waited for you to come out it would have been better for them. But God is forgiving and kind."}, {"id": 185318, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4618, "text": "O you who believe, if a dissolute person brings some news, verify it first lest you attack a people ignorantly and later regret what you had done."}, {"id": 185319, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4619, "text": "Know that the Apostle of God is among you: If he agreed with you in most matters you would surely come to grief. But God has made faith more desirable to you, and attractive to your hearts, and rendered disbelief and sin and disobedience repugnant. They are those who are well directed"}, {"id": 185320, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4620, "text": "By God's benevolence and His grace. God is all-knowing and all-wise."}, {"id": 185321, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4621, "text": "If two groups of believers come to fight one another, promote peace between them. Then if one of them turns aggressive against the other, fight against the aggressive party till it returns to God's authority. If it does so, make peace among them equitably and be impartial. Verily God loves those who are just."}, {"id": 185322, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4622, "text": "The faithful are surely brothers; so restore friendship among your brothers, and fear God that you may be favoured."}, {"id": 185323, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4623, "text": "O you who believe, men should not laugh at other men, for it may be they are better than them; and women should not laugh at other women, for they may perhaps be better than them. Do not slander one another, nor give one another nick-names. After believing, it is bad to give (another) a bad name. Those who do not repent behave wickedly."}, {"id": 185324, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4624, "text": "O you who believe, avoid most suspicions: Some suspicions are indeed sins. So do not pry into others' secrets and do not backbite. Would any of you like to eat a dead brother's flesh? You would surely be revolted by it Then fear God. He is certainly forgiving and kind."}, {"id": 185325, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4625, "text": "O men, We created you from a male and female, and formed you into nations and tribes that you may recognise each other. He who has more integrity has indeed greater honour with God. Surely God is all-knowing and well-informed."}, {"id": 185327, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4626, "text": "The Arabs of the desert say: \"We believe.\" Tell them: \"You do not believe. Better say: 'We submit'; for belief has not yet penetrated your hearts.\" If you begin to obey God and His Apostle, He will not withhold the least (of the recompense) of your labour. Verily God is forgiving and kind.\""}, {"id": 185328, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4627, "text": "They alone are believers who come to believe in God and His Apostle, then never doubt again, and struggle wealth and soul in the way of God. They are the truthful and sincere."}, {"id": 185329, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4628, "text": "Say: \"Are you trying to convince God of your faithfulness? But God knows all there is in the heavens and the earth, for God has knowledge of everything.\""}, {"id": 185330, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4629, "text": "They impress upon you that they have submitted. Tell them: \"Do not favour me with your submission. In fact God has favoured you by showing you the way to belief, if you are men of truth.\""}, {"id": 185331, "identifier": "en.ahmedali", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4630, "text": "Verily God knows the unknown of the heavens and the earth; and God perceives all you do."}]