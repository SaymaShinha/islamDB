[{"id": 185435, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4736, "text": "I CALL TO witness the Mount Sinai,"}, {"id": 185436, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4737, "text": "And the Scripture inscribed"}, {"id": 185437, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4738, "text": "On a parchment scroll unrolled,"}, {"id": 185438, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4739, "text": "The house ever-peopled,"}, {"id": 185439, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4740, "text": "The roof raised high,"}, {"id": 185440, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4741, "text": "And the swollen sea,"}, {"id": 185441, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4742, "text": "The punishment of your Lord is certain to come."}, {"id": 185442, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4743, "text": "There is none who could avert it."}, {"id": 185443, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4744, "text": "The day the sky will tremble,"}, {"id": 185444, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4745, "text": "The mountains move and fly away,"}, {"id": 185445, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4746, "text": "Will be the day of woe for those"}, {"id": 185446, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4747, "text": "Who, ignoring the Reality, engage in pleasantries."}, {"id": 185447, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4748, "text": "The day they are dragged and pushed into Hell,"}, {"id": 185448, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4749, "text": "(And told:) \"This is the fire which you denied."}, {"id": 185449, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4750, "text": "Is it magic, or you cannot see?"}, {"id": 185450, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4751, "text": "Roast in it. Bear it with patience or impatience, it will be the same for you. You will be requited for what you had done.\""}, {"id": 185417, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4752, "text": "Those who fear God and follow the straight path will surely be in gardens and in bliss,"}, {"id": 185451, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4753, "text": "Rejoicing at what their Lord has given them; and their Lord will preserve them from the torment of Hell."}, {"id": 185452, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4754, "text": "\"Eat and drink with relish,\" (they will be told), (as recompense) for what you had done.\""}, {"id": 185453, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4755, "text": "They would recline on couches set in rows, paired with fair companions (clean of thought and) bright of eye."}, {"id": 185454, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4756, "text": "And those who believed, and whose progeny also followed them in their faith, will be united with their offspring. We will not deprive them of their labour in the least. Every man is bound to what he does."}, {"id": 185455, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4757, "text": "We shall give them fruits and meats, and what they desire."}, {"id": 185456, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4758, "text": "They will exchange cups of wine free of (incitement to) pleasantry or sin."}, {"id": 185457, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4759, "text": "And young attendants like pearls within their shells, will go round."}, {"id": 185361, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4760, "text": "They will ask each other questions,"}, {"id": 185458, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4761, "text": "Saying: \"We were also once full of fear at home."}, {"id": 185459, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4762, "text": "But God has been gracious to us, and has saved us from the torment of scorching wind."}, {"id": 185460, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4763, "text": "We used to pray to Him erstwhile; He is the just and merciful."}, {"id": 185461, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4764, "text": "Remind them, therefore, that by the grace of your Lord you are neither a soothsayer nor possessed."}, {"id": 185462, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Do they say: \"He is a poet for whom we expect an adverse turn of fortune?\""}, {"id": 185463, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4766, "text": "Tell them: \"Keep on expecting, I am expecting with you.\""}, {"id": 185464, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Does their reasoning prompt them to this, or they are a people rebellious?"}, {"id": 185465, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Or do they say: \"He has fabricated it?\" In fact, they will never believe."}, {"id": 185466, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4769, "text": "So let them bring a discourse like it, if they are truthful"}, {"id": 185467, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4770, "text": "Or were they created of themselves? Or are they the creators?"}, {"id": 185468, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Or have they created the heavens and the earth? In fact they are certain of nothing."}, {"id": 185469, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Do they possess the treasures of your Lord? Or are they the treasurers?"}, {"id": 185470, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Or do they have a ladder (climbing which) they can hear (the secrets of heaven)? Let one who has heard then bring a clear proof."}, {"id": 185471, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Has (God) daughters and they sons?"}, {"id": 185472, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Or do you demand any wages from them so that they are burdened with expense?"}, {"id": 185473, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Or do they have knowledge of the Unknown which they write down?"}, {"id": 185474, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Or do they want to stage a deception? -- Then only those who do not believe will be the ones who will be tricked."}, {"id": 185475, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Or do they have a god apart from God? He is too exalted for what they associate with Him!"}, {"id": 185476, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4779, "text": "If they should see a segment falling from the sky, they would say: \"It is only a massive cloud.\""}, {"id": 185477, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4780, "text": "So, leave them until they face their day (of doom) when they will be stunned."}, {"id": 185478, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4781, "text": "Their deception will not avail them in the least on that day, nor will they be helped."}, {"id": 185479, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4782, "text": "Surely there are other torments besides this for those who are wicked, though most of them do not know."}, {"id": 185480, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4783, "text": "Await the judgement of your Lord, for you are always before Our eyes, and glorify your Lord with praises when you rise,"}, {"id": 185481, "identifier": "en.ahmedali", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4784, "text": "And glorify Him in the night and when the stars begin to wane."}]