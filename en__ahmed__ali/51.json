[{"id": 185376, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4676, "text": "I CALL TO witness those who scatter (dust) by dispersing,"}, {"id": 185377, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4677, "text": "And those who bear the load (of rain),"}, {"id": 185378, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4678, "text": "And those who move (on the water) gently,"}, {"id": 185379, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4679, "text": "And those who distribute (it) by command,"}, {"id": 185380, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Verily the promise made to you is true:"}, {"id": 185381, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4681, "text": "The Judgement will indeed take place."}, {"id": 185382, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4682, "text": "The heavens webbed-with-tracks is witness"}, {"id": 185383, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4683, "text": "You are surely caught in contradictions.,"}, {"id": 185384, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4684, "text": "From which only he turns who is turned away."}, {"id": 185385, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Perish will those who just guess and speculate,"}, {"id": 185386, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4686, "text": "Who are lost in deception."}, {"id": 185387, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4687, "text": "They ask: \"When is the Day of Judgement?\" --"}, {"id": 185388, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4688, "text": "The day they will be burnt in the Fire,"}, {"id": 185389, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4689, "text": "(And told:) \"Taste your punishment. This is what you were trying to hasten.\""}, {"id": 185390, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Surely those who fear God and follow the straight path will be amidst gardens and fresh springs of water,"}, {"id": 185391, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4691, "text": "Receiving what is given them by their Lord. They were surely the virtuous before this."}, {"id": 185392, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4692, "text": "They slept little in the night,"}, {"id": 185393, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4693, "text": "And every morning asked forgiveness,"}, {"id": 185240, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4694, "text": "In whose wealth the suppliant and the deprived had a share."}, {"id": 185193, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4695, "text": "There are signs in the earth for those who are firm in their faith,"}, {"id": 185394, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4696, "text": "And within yourselves. Can you not perceive?"}, {"id": 185395, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4697, "text": "There is in the heavens your sustenance, and whatever has been promised you."}, {"id": 185396, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4698, "text": "By the Lord of the heavens and the earth, this is certainly true even as you speak."}, {"id": 185397, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Has the story of Abraham's honoured guests come to you?"}, {"id": 185398, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4700, "text": "When they came to him, they said: \"Peace.\" He answered: \"Peace.\" They were a people he did not recognise."}, {"id": 185399, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4701, "text": "So he hurried to the house and brought a fatted calf,"}, {"id": 185400, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4702, "text": "And placing it before them said: \"Won't you eat?\""}, {"id": 185401, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4703, "text": "He felt afraid of them; but they said: \"Have no fear,\" and gave him the good news of a wise son."}, {"id": 185402, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4704, "text": "His wife came out lamenting, striking her forehead, and said: \"I, am old and barren woman?\""}, {"id": 185403, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4705, "text": "They said: \"Thus said your Lord. He is indeed all-wise and all-knowing.\""}, {"id": 185404, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4706, "text": "He said: \"What is then your business, O messengers?\""}, {"id": 185405, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4707, "text": "They said: \"We have been sent to a wicked people"}, {"id": 185406, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4708, "text": "So as to let loose clods of clay on them"}, {"id": 185407, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4709, "text": "Marked by your Lord for those who waste their substance.\""}, {"id": 185408, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4710, "text": "So We evacuated everyone who was a believer there,"}, {"id": 185409, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4711, "text": "But did not find more than a single family of believers."}, {"id": 185410, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4712, "text": "We left a sign in this for those who fear the grievous punishment,"}, {"id": 185411, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4713, "text": "As (there was) in Moses when We sent him to the Pharaoh with clear authority."}, {"id": 185412, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4714, "text": "But he turned to his counsellors, and said: \"He is a magician or lunatic.\""}, {"id": 185413, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4715, "text": "So We seized him and his armies, and threw them into the sea, for he was worthy of blame."}, {"id": 185414, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4716, "text": "In 'Ad (also is a sign), when We sent a blasting wind against them,"}, {"id": 185415, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4717, "text": "Which turned everything it touched to ashes."}, {"id": 185416, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4718, "text": "And in Thamud, when We said to them: \"Enjoy yourselves for a while;\""}, {"id": 185418, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4719, "text": "But they disobeyed the command of their Lord; so they were destroyed by a thunderbolt, and they could only gape,"}, {"id": 185419, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4720, "text": "And neither stand up nor defend themselves."}, {"id": 185420, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4721, "text": "(So had We destroyed) the people of Noah before them: They were surely a sinful people."}, {"id": 185421, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4722, "text": "We built the heavens by Our authority; and We are the Lord of power and expanse."}, {"id": 185422, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4723, "text": "We spread the earth a carpet; what comfort We provide!"}, {"id": 185423, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4724, "text": "And We created pairs of everything that you may contemplate."}, {"id": 185424, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4725, "text": "So turn to God. I give you a clear warning from Him."}, {"id": 185425, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Do not set up another god with God. I give you a clear warning from Him."}, {"id": 185426, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4727, "text": "Even thus no apostle came to those before them but they said: \"He is a sorcerer or a mad man.\""}, {"id": 185427, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Is this the legacy they have passed down from one to the other? In fact, they are a rebellious people."}, {"id": 185428, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4729, "text": "Turn away from them. You will not be blamed."}, {"id": 185429, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4730, "text": "But go on reminding them, as reminding benefits the believers."}, {"id": 185430, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4731, "text": "I have not created the jinns and men but to worship Me."}, {"id": 185431, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4732, "text": "I want no sustenance from them nor do I want them to feed Me."}, {"id": 185432, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4733, "text": "God is certainly the great provider, Lord of strength and power."}, {"id": 185433, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Those who do wrong will indeed come to the same end as their fellows (of old). So let them not ask Me to hasten (the punishment)."}, {"id": 185434, "identifier": "en.ahmedali", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Alas the woe for those who refuse to believe in the Day which has been promised them."}]