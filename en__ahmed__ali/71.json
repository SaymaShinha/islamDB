[{"id": 186107, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5420, "text": "SENT NOAH to his people to warn them before the painful punishment came upon them."}, {"id": 186108, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5421, "text": "He said: \"O my people, I warn you clearly"}, {"id": 186109, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5422, "text": "That you should worship God and fear Him, and follow me"}, {"id": 186110, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5423, "text": "That He may forgive some of your sins and prolong your term till an appointed time. Surely when God's appointed time is come it will not be put off, if only you knew!"}, {"id": 186111, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5424, "text": "He said: \"O Lord, I called my people night and day,"}, {"id": 186112, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5425, "text": "But the more I called, they only ran the farther away."}, {"id": 186113, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5426, "text": "And every time I called them that You may forgive them, they thrust their fingers into their ears, and covered themselves with their garments, and became wayward, and behaved with downright insolence."}, {"id": 186114, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5427, "text": "Then I called them loudly and more openly,"}, {"id": 186115, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5428, "text": "And declared to them in public and in private,"}, {"id": 186116, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5429, "text": "And I told them: 'Ask your Lord to forgive you. He is verily forgiving."}, {"id": 186117, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5430, "text": "He will send you abundant rain from the sky,"}, {"id": 186118, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5431, "text": "And will give you increase of wealth and sons, and give you gardens and springs of water."}, {"id": 186119, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5432, "text": "What has come upon you that you do not fear the majesty of God,"}, {"id": 186120, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5433, "text": "Knowing that He has created you by various stages?"}, {"id": 186121, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5434, "text": "Do you not see how God has fashioned seven skies one above the other,"}, {"id": 186122, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5435, "text": "And has placed the moon therein, an illumination, and has placed the sun, a lighted lamp?"}, {"id": 180911, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5436, "text": "God produced you from the earth like a vegetable growth;"}, {"id": 180913, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5437, "text": "He will then return you back to it, and bring you out again."}, {"id": 180909, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5438, "text": "God has made the earth for you a spreading,"}, {"id": 180919, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5439, "text": "So that you may walk upon its spacious paths.'"}, {"id": 186123, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5440, "text": "But they did not listen to me,\" Noah said, \"and followed him whose wealth and children only added to his ruin."}, {"id": 186124, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5441, "text": "And they contrived a plot of great magnitude,"}, {"id": 186125, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5442, "text": "And said: 'Do not abandon your gods, and do not abandon Wadda or Suwa', or Yaghuth, Ya'uq or Nasr.'"}, {"id": 186126, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5443, "text": "And they misled many. So do not give the evil-doers increase but in error."}, {"id": 186127, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5444, "text": "They were drowned because of their habitual sinfulness, and sent to Hell, and did not find any helper other than God."}, {"id": 186128, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5445, "text": "Noah said: \"O Lord, do not leave a single habitation of unbelievers on the earth."}, {"id": 186129, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5446, "text": "If you leave them, they will lead Your creatures astray, and beget but iniquitous and ungrateful offspring."}, {"id": 186130, "identifier": "en.ahmedali", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5447, "text": "O Lord, forgive me, my parents, and any one who enters my house as a believer, and all believing men and women, and do not give the evil-doers increase save in ruin."}]