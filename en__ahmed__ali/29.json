[{"id": 184034, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3341, "text": "ALIF LAM MIM."}, {"id": 184035, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3342, "text": "Do men think they will get away by saying: \"We believe,\" and will not be tried?"}, {"id": 184036, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3343, "text": "We had tried those who were before them so that God knew who spoke the truth, and who were liars."}, {"id": 184037, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3344, "text": "Do those who do evil think that they will get the better of Us? How bad is the judgement that they make!"}, {"id": 184038, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3345, "text": "He who hopes to meet God (should know) that God's appointed time will surely come. He is all-hearing and all-knowing."}, {"id": 184039, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3346, "text": "He who strives does so for himself. Verily God is independent of the creatures of the world."}, {"id": 184040, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3347, "text": "We shall pardon the sinful deeds of those who believe and do the right, and give them a reward better than their deeds."}, {"id": 184041, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3348, "text": "We have enjoined on man to be good to his parents; but if they try to make you associate with Me that of which you have no knowledge, then do not obey them. You have to come back to Us, when I will tell you what you used to do."}, {"id": 184042, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3349, "text": "We shall admit those who believe and do the right among the righteous."}, {"id": 184043, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3350, "text": "There are among men those who say: \"We believe in God;\" yet if they happen to suffer in the cause of God they take oppression by men as punishment from God. And if help comes to them from your Lord, they say: \"We were with you.\" Does not God know what is hidden in the hearts of men?"}, {"id": 184044, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3351, "text": "God will surely know the believers and know the hypocrites."}, {"id": 184045, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3352, "text": "Those who deny say to those who affirm: \"Follow our way; we shall carry the burden of your sins.\" But they cannot carry the burden of their sins in the least. They are liars indeed."}, {"id": 184046, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3353, "text": "They will carry their own loads and other loads besides their own; and will surely be questioned on the Day of Resurrection about what they contrived."}, {"id": 184047, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3354, "text": "We sent Noah to his people, and he lived with them a thousand years minus fifty. Then they were caught by the deluge for they were evil."}, {"id": 184048, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3355, "text": "But We saved him and those with him in the ark, and made it a sign for the creatures of the world."}, {"id": 184049, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3356, "text": "And (remember) Abraham who said to his people: \"Worship God and be obedient to Him. This is better for you if you understand."}, {"id": 184050, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3357, "text": "You worship idols in place of God and invent lies. Surely those you worship other than God have no power over your means of livelihood. So seek your sustenance from God, and worship Him and give Him thanks. To Him will you be brought back in the end."}, {"id": 184051, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3358, "text": "But if you deny, then many a people have denied before you. The duty of the apostle is to convey the message clearly.\""}, {"id": 184052, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3359, "text": "Do they not see how God originates creation, then reverts it back? This is indeed how inevitably the law of God works;"}, {"id": 184053, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3360, "text": "Say: \"Travel on the earth and see how He originated creation. Then (you will know) how God will raise the last raising (of the dead). Surely God has power over everything,"}, {"id": 184054, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3361, "text": "Punish whom He will, and have mercy on whom He please. And to Him will you be brought back in the end."}, {"id": 184055, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3362, "text": "Escape Him you cannot either in the earth or in the sky; and you have no friend or helper apart from God."}, {"id": 184057, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3363, "text": "Those who deny the signs of God and the meeting with Him, cannot have hope of My mercy. There is a painful punishment for them."}, {"id": 184058, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3364, "text": "The people (of Abraham) had no answer except: \"Kill him or burn him;\" but God saved him from the fire. There are lessons in this for those who believe."}, {"id": 184059, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3365, "text": "He said: \"You have taken to idols through mutual affection in this life, in place of God, but on the Day of Resurrection you will disown and curse each other, and your abode will be Hell, and you will have none to help you.\""}, {"id": 184060, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3366, "text": "Then Lot believed in him; and (Abraham) said: \"I will separate myself and take refuge in my Lord. Surely He is all-mighty and all-wise.\""}, {"id": 184061, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3367, "text": "So We bestowed on him Isaac and Jacob and gave his progeny prophethood and scripture, and rewarded him in this world, and in the next he will be among the upright."}, {"id": 184062, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3368, "text": "And (remember) Lot when he said to his people: \"You indulge in lecherous acts which none of the creatures had done before you."}, {"id": 184063, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3369, "text": "You commit unnatural acts with men and cut off the way (of procreation), and commit obscenities in your gatherings.\" The only answer his people made was: \"Bring the punishment of God, if you are truthful.\""}, {"id": 184064, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3370, "text": "\"O Lord, help me against the wicked people,\"(Lot) prayed."}, {"id": 184065, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3371, "text": "When Our messengers came to Abraham with good news, they said: \"We have to destroy this city as its inhabitants have become sinful.\""}, {"id": 184066, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3372, "text": "He said: \"Surely Lot is there.\" They answered: \"We know who is there. We are to save him and his family except his wife, for she is one of those who will stay behind.\""}, {"id": 184067, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3373, "text": "So, when Our messengers came to Lot, he was worried on their account as he was unable to protect them. They said: \"Have no fear or regret. We will certainly save you and your family except your wife, for she is one of those who will stay behind."}, {"id": 184068, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3374, "text": "We have to bring a scourge from the heavens on the people of this city as they are depraved.\""}, {"id": 184069, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3375, "text": "Verily We have left a clear sign of this for people of sense to see."}, {"id": 184070, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3376, "text": "To Midian We sent their brother Shu'aib. He said: \"O people, worship God and be ready for the Day of Resurrection. Do no evil, and create no mischief in the land.\""}, {"id": 184071, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3377, "text": "But they denied him and were seized by an earthquake, and lay overturned in their homes in the morning."}, {"id": 184072, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3378, "text": "And (remember) 'Ad and Thamud. It will be clear to you from their habitations (how they were destroyed), for Satan had made their deeds look attractive to them, and turned them away from the path; and yet they were a people of acumen."}, {"id": 184073, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3379, "text": "(Remember) Qarun, Pharaoh and Haman to whom Moses came with clear signs; but they were haughty (and oppressed) the land. Yet they could not run away from Us;"}, {"id": 184074, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3380, "text": "And We seized all of them for their crimes. Against some We sent a violent wind hurling stones, and some We seized with a mighty blast, and some We submerged under the earth, and some We drowned. It was not for God to wrong them, they wronged themselves."}, {"id": 184075, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3381, "text": "The semblance of those who take protectors besides God is that of the spider. She arranges a house for herself, but the flimsiest of houses is the spider's. If only they had sense!"}, {"id": 184076, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3382, "text": "Verily God knows what they invoke in His place, for He is all-mighty and all-wise."}, {"id": 184077, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3383, "text": "These are precepts of wisdom We offer to men, but only those who are rational understand."}, {"id": 184078, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3384, "text": "God has created the heavens and the earth with reason. Surely in this is a sign for those who believe."}, {"id": 184079, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 346, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3385, "text": "Recite what has been revealed to you of this Book, and be constant in devotion. Surely prayer keeps you away from the obscene and detestable, but the remembrance of God is greater far; and God knows what you do."}, {"id": 184081, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3386, "text": "Do not argue with the people of the Book unless in a fair way, apart from those who act wrongly, and say to them: \"We believe what has been sent down to us, and we believe what has been sent down to you. Our God and your God is one, and to Him we submit.\""}, {"id": 184082, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3387, "text": "That is how We have revealed this Book to you and those to whom We have sent down the Book will believe in it. Only those who are infidels will deny it."}, {"id": 184083, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3388, "text": "You did not read any Scripture before this, nor wrote one with your right hand, or else these dissemblers would have found a cause to doubt it."}, {"id": 184084, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3389, "text": "In fact, in the minds of those who have intelligence these are clear signs. No one denies Our revelations except those who are unjust."}, {"id": 184085, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3390, "text": "For they say: \"How Is it no signs were sent down to him from his Lord?\" Say: \"The signs are with God. I am only a warner, plain and simple.\""}, {"id": 184086, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3391, "text": "Is it not sufficient for them that We have revealed the Book to you which is read out to them? It is indeed a grace and reminder for people who believe."}, {"id": 184087, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3392, "text": "Say: \"God is sufficient as witness between me and you.\" He knows what is in the heavens and the earth. It is those who believe in falsehood and disbelieve in God who will perish."}, {"id": 184088, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3393, "text": "They want you to hasten the punishment: But for a time already determined the punishment would have come upon them. It will come upon them all too suddenly, and they will be caught unawares."}, {"id": 184089, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3394, "text": "They want you to hasten the punishment: Hell will indeed surround the unbelievers."}, {"id": 184090, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3395, "text": "The day the punishment comes upon them from above and underneath their feet, (God will) say: \"And now taste of what you had done.\""}, {"id": 184091, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3396, "text": "O My creatures who believe, surely My earth has plenty of scope and so worship only Me."}, {"id": 184092, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3397, "text": "Every soul has to know the taste of death. You will then be sent back to Us."}, {"id": 184093, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3398, "text": "We shall admit those who believe and do the right to empyreal gardens with rivers rippling by, where they will abide for ever. How excellent the guerdon of those who toil,"}, {"id": 184094, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3399, "text": "Who persevere and place their trust in their Lord."}, {"id": 184095, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3400, "text": "How many living things there are on the earth that do not Store their food; God provides them as well as you. He is all-hearing and all-knowing."}, {"id": 184096, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3401, "text": "If you ask them: \"Who created the heavens and the earth, and who set the sun and the moon to work?\" They will answer: \"God.\" Why then do they vacillate?"}, {"id": 184097, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3402, "text": "God increases the means of those of His creatures as He please, or limits them for whomsoever He will. He is certainly cognisant of everything."}, {"id": 184098, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3403, "text": "If you ask them: \"Who sends down rain from the sky and quickens the earth when it is dead?\" They will answer: \"God.\" Say: \"All praise be to God.\" But most of them do not understand."}, {"id": 184099, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3404, "text": "The life of this world is only a sport and play. It is surely the home of the Hereafter that will indeed be life extended and new, if only they knew!"}, {"id": 184100, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3405, "text": "When they board a ship they call on God, placing their faith wholly in Him. But when He brings them safely back to shore, they begin to associate others with Him,"}, {"id": 184101, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3406, "text": "And deny what We had given them, in order to go on enjoying themselves. They will come to know soon."}, {"id": 184102, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3407, "text": "Do they not see that We have given them a safe sanctuary, while all around them men are being despoiled? Do they then believe what is false, and deny the bounty of God?"}, {"id": 184104, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3408, "text": "Who is more unjust than he who fabricates a lie about God, or denies the truth when it has come to him? Is there not an abode for unbelievers in Hell?"}, {"id": 184105, "identifier": "en.ahmedali", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 3409, "text": "We shall guide those who strive in Our cause to the paths leading straight to Us. Surely God is with those who do good."}]