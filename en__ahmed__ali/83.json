[{"id": 186520, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5849, "text": "WOE TO THOSE who give short measure,"}, {"id": 186521, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5850, "text": "Who insist on being given full when they take from others,"}, {"id": 186522, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5851, "text": "Whilst when they measure or weigh for them, give less."}, {"id": 186523, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Do they not think they will be raised (to life) again"}, {"id": 186524, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5853, "text": "On a grievous day,"}, {"id": 186525, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5854, "text": "The day all mankind will stand before the Lord of all the worlds?"}, {"id": 186526, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5855, "text": "Indeed the ledger of the wicked will be in (the lowest depths) Sijjin."}, {"id": 186527, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5856, "text": "How will you comprehend what Sijjin is?"}, {"id": 186528, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5857, "text": "It is a (repository of) distinctly written record."}, {"id": 186529, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5858, "text": "Ah the woe that day for those who deny,"}, {"id": 186530, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5859, "text": "Who call the Day of judgement a lie!"}, {"id": 186531, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5860, "text": "None denies it but the sinful transgressors."}, {"id": 186532, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5861, "text": "When Our revelations are recited before him, he says: \"These are fables of long ago.\""}, {"id": 186533, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5862, "text": "No. In fact what they have been doing has rusted their hearts."}, {"id": 186534, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5863, "text": "Therefore they will be screened off from their Lord that day,"}, {"id": 186535, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5864, "text": "Then they will indeed burn in Hell."}, {"id": 186536, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5865, "text": "They will then be told: \"This is what you had denied.\""}, {"id": 186537, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5866, "text": "Surely the ledger of the pious will be in 'Illiyun (heights above the heights)."}, {"id": 186538, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5867, "text": "But how will you comprehend what 'Illiyun is?"}, {"id": 186539, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5868, "text": "It is a (repository of) distinctly written record"}, {"id": 186540, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5869, "text": "Witnessed by those who are honoured."}, {"id": 186541, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5870, "text": "Verily the pious will be in heaven,"}, {"id": 186542, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5871, "text": "On couches face to face."}, {"id": 186543, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5872, "text": "On their faces you will see the glow of beatitude."}, {"id": 186544, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5873, "text": "They will be served the choicest wine, sealed"}, {"id": 186545, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5874, "text": "With a sealing of musk, which those who aspire for the best should desire, --"}, {"id": 186546, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5875, "text": "Blended with the water of Tasnim, (heights ultimate of evolution),"}, {"id": 186547, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5876, "text": "A fountain from which only they who are honoured drink."}, {"id": 186548, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5877, "text": "The sinners indeed laughed at believers"}, {"id": 186549, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5878, "text": "And winked at one another as they passed by them;"}, {"id": 186550, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5879, "text": "And when they went back to their people turned to make fun of them;"}, {"id": 186551, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5880, "text": "And when they saw them, they said: \"They have indeed gone astray.\""}, {"id": 186552, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5881, "text": "But they were not Sent to be guardians over them!"}, {"id": 186553, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5882, "text": "So, the believers will laugh at the infidels on that day,"}, {"id": 186554, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5883, "text": "Regarding them from their cushioned seats."}, {"id": 186555, "identifier": "en.ahmedali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5884, "text": "Will not the infidels pay for what they had done?"}]