[{"id": 186715, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6044, "text": "I CALL TO witness the sun and his early morning splendour,"}, {"id": 186716, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6045, "text": "And the moon as she follows in his wake,"}, {"id": 186717, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6046, "text": "The day when it reveals his radiance,"}, {"id": 186718, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6047, "text": "The night when it covers him over,"}, {"id": 186719, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6048, "text": "The heavens and its architecture,"}, {"id": 186720, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6049, "text": "The earth and its spreading out,"}, {"id": 186721, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6050, "text": "The soul and how it was integrated"}, {"id": 186722, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6051, "text": "And given the faculty of knowing what is disruptive and what is intrinsic to it."}, {"id": 186723, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6052, "text": "He who nourishes it will surely be successful,"}, {"id": 186724, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6053, "text": "And he who confines it will surely come to grief."}, {"id": 186725, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6054, "text": "The Thamud denied (the truth) in their perverseness"}, {"id": 186726, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6055, "text": "When among them the great wretch arose,"}, {"id": 186727, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6056, "text": "And the apostle of God had to tell them: \"This is God's she-camel, let her drink.\""}, {"id": 186728, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6057, "text": "But they called him a liar and hamstrung her. So God obliterated them for their crime, and (destroyed) all of them alike."}, {"id": 186729, "identifier": "en.ahmedali", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 6058, "text": "He does not fear the consequence."}]