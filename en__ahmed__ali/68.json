[{"id": 185972, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5272, "text": "NUN. I CALL to witness the pen and what they inscribe,"}, {"id": 185973, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5273, "text": "You are not demented by the grace of your Lord."}, {"id": 185974, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5274, "text": "There is surely reward unending for you,"}, {"id": 185975, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5275, "text": "For you are verily born of sublime nature."}, {"id": 185976, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5276, "text": "So you will see, and they will realise,"}, {"id": 185977, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5277, "text": "Who is distracted."}, {"id": 185978, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5278, "text": "Verily your Lord knows those who have gone astray from His path, and He knows those who are guided on the way."}, {"id": 185979, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5279, "text": "So do not comply with those who deny:"}, {"id": 185980, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5280, "text": "They only want that you should relent, so that they may come to terms."}, {"id": 185981, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5281, "text": "Do not heed a contemptible swearer,"}, {"id": 185982, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5282, "text": "Or backbiter, calumniator, slanderer,"}, {"id": 185983, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5283, "text": "Who hinders men from (doing) good, the transgressor, the iniquitous,"}, {"id": 185984, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5284, "text": "Crude, and above all, mean and infamous,"}, {"id": 185985, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5285, "text": "Simply because he possesses wealth and children."}, {"id": 185986, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5286, "text": "When you recite Our revelations to him, he says: \"These are fables of long ago.\""}, {"id": 185987, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5287, "text": "We shall brand him on the muzzle."}, {"id": 185988, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5288, "text": "We have tried you as We tried the owners of the garden when they vowed to gather the fruits in the morning"}, {"id": 185989, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5289, "text": "But did not add: \"If God may please.\""}, {"id": 185990, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5290, "text": "Then a calamity from your Lord fell upon it, but they remained fast asleep."}, {"id": 185991, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5291, "text": "So by the morning it seemed as though picked clean."}, {"id": 185865, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5292, "text": "At daybreak they called to each other:"}, {"id": 185890, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5293, "text": "\"If you want to gather the fruits, let us go early to the plantation.\""}, {"id": 180877, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5294, "text": "So they departed, talking in low voices:"}, {"id": 180882, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5295, "text": "\"Let no needy person come to you within it today.\""}, {"id": 180887, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5296, "text": "They left early in the morning bent on this purpose."}, {"id": 185782, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5297, "text": "When they saw (and did not recognise it) they said: \"Surely we have lost the way."}, {"id": 185802, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5298, "text": "No. In fact we have been deprived of it.\""}, {"id": 185992, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5299, "text": "One who was temperate among them, said: \"Did I not say: 'Why don't you priase God?'\""}, {"id": 185993, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5300, "text": "\"Glory to our Lord,\" they said; we were really in the wrong.\""}, {"id": 185994, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5301, "text": "Then they started blaming one another,"}, {"id": 185995, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5302, "text": "Saying: \"Alas the woe, we were iniquitous."}, {"id": 185917, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5303, "text": "Maybe our Lord will give us better than this. We turn to our Lord in supplication.\""}, {"id": 185996, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5304, "text": "Such is Our chastisement; and the punishment of the Hereafter will be greater, if only they knew!"}, {"id": 185997, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5305, "text": "For those who fear God there are pleasure gardens with their Lord."}, {"id": 185998, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5306, "text": "Should We treat those who submit and obey in the same way as those who are culpable?"}, {"id": 185999, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5307, "text": "What has come upon you that you judge in such a wise?"}, {"id": 186000, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5308, "text": "Or have you a Book in which you read"}, {"id": 186001, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5309, "text": "That you can surely have whatever you choose?"}, {"id": 186002, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5310, "text": "Or have you taken a binding promise from Us which would hold till the Day of Judgement, that you will get whatever you demand?"}, {"id": 186003, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5311, "text": "Ask them: \"Which of you is able to vouch for this?\""}, {"id": 186004, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5312, "text": "Or have they any partners? Let them bring their partners then, if what they say is true."}, {"id": 186005, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5313, "text": "On the day the great calamity befalls, and they are called to bow in homage, they will not be able to do so."}, {"id": 186006, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5314, "text": "Lowered will be their eyes, disgrace overwhelming them. They had indeed been called to bow in homage when they were free of blame."}, {"id": 186007, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5315, "text": "So leave those who deny this Discourse to Me. We shall lead them step by step to (ruin) in a way they will not know."}, {"id": 186008, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5316, "text": "Yet I will give them respite: Surely My plan is compact."}, {"id": 186009, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5317, "text": "Do you ask for any compensation from them that they are burdened with want?"}, {"id": 186010, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5318, "text": "Or do they have knowledge of the unknown which they copy down?"}, {"id": 186011, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5319, "text": "So wait with patience for the judgement of your Lord, and do not be like (onah) of the fish who called (to his Lord) when he was choked with anger."}, {"id": 186012, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5320, "text": "Had it not been for a favour from his Lord he would have been cast blame-worthy on a barren plain."}, {"id": 186013, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5321, "text": "Then his Lord chose him and placed him among the upright."}, {"id": 186014, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5322, "text": "But the unbelievers would like to stare you out of balance when they hear the warning, and say: \"Surely he is possessed;\""}, {"id": 186015, "identifier": "en.ahmedali", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Ali", "transelator_en_name": "Ahmed Ali", "format": "text", "type": "translation", "ayah_number": 5323, "text": "Whilst it is no more than reminder for the people of the world."}]