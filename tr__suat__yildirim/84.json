[{"id": 598142, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5885, "text": "G\u00f6k yar\u0131ld\u0131\u011f\u0131 zaman..."}, {"id": 598143, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5886, "text": "Ve hep yapageldi\u011fi gibi, Rabbinin buyru\u011funu dinledi\u011fi zaman..."}, {"id": 598144, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5887, "text": "Yer yay\u0131l\u0131p d\u00fcmd\u00fcz edildi\u011fi, [20,106-107]"}, {"id": 598145, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5888, "text": "\u0130\u00e7indekileri d\u0131\u015far\u0131 at\u0131p bo\u015fald\u0131\u011f\u0131,"}, {"id": 598146, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5889, "text": "Ve hep yapageldi\u011fi gibi, Rabbinin buyru\u011funu dinledi\u011fi zaman... Seyredin siz: neler olacak o zaman!"}, {"id": 598147, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5890, "text": "Ey insan! Sen, t\u00e2 Rabbine kavu\u015funcaya kadar didinip duracaks\u0131n."}, {"id": 598148, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5891, "text": "Hesap defteri sa\u011f eline verilen kimsenin hesab\u0131 kolayca g\u00f6r\u00fcl\u00fcr.Ve ailesine sevin\u00e7 i\u00e7inde d\u00f6ner."}, {"id": 598149, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5892, "text": "Hesap defteri sa\u011f eline verilen kimsenin hesab\u0131 kolayca g\u00f6r\u00fcl\u00fcr.Ve ailesine sevin\u00e7 i\u00e7inde d\u00f6ner."}, {"id": 598150, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5893, "text": "Hesap defteri sa\u011f eline verilen kimsenin hesab\u0131 kolayca g\u00f6r\u00fcl\u00fcr.Ve ailesine sevin\u00e7 i\u00e7inde d\u00f6ner."}, {"id": 598151, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5894, "text": "Hesap defteri arkas\u0131ndan sol eline verilen kimse ise,"}, {"id": 598152, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5895, "text": "Yok olmay\u0131 ister."}, {"id": 598153, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5896, "text": "Alevli ate\u015fe girer."}, {"id": 598154, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5897, "text": "O d\u00fcnyada iken ailesi i\u00e7inde keyifli, \u015f\u0131mar\u0131k idi."}, {"id": 598155, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5898, "text": "Hi\u00e7bir s\u00fbrette Rabbine d\u00f6nmeyece\u011fini san\u0131rd\u0131."}, {"id": 598156, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5899, "text": "Hay\u0131r! O Rabbine d\u00f6necek! Zira Rabbi, devaml\u0131 s\u00fbrette onun yapt\u0131klar\u0131n\u0131 g\u00f6r\u00fcyor, tek tek kontrol ediyordu. (Bu kontrol\u00fcn de elbette b\u00f6yle bir neticesi olacakt\u0131.)"}, {"id": 598157, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5900, "text": "Demek, ger\u00e7ek onun sand\u0131\u011f\u0131 gibi de\u011fildir. \u015eafak hakk\u0131 i\u00e7in!"}, {"id": 598158, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5901, "text": "Gece ve gecenin bar\u0131nd\u0131rd\u0131\u011f\u0131, \u015feyler hakk\u0131 i\u00e7in,"}, {"id": 598159, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5902, "text": "Dolunay halini alan ay hakk\u0131 i\u00e7in:"}, {"id": 598160, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5903, "text": "Siz halden hale ge\u00e7eceksiniz!"}, {"id": 598161, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5904, "text": "\u00d6yleyse onlara ne oluyor ki iman etmiyorlar?"}, {"id": 598162, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": true, "sajda_id": 14, "sajda_obligatory": false, "sajda_recommended": true, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5905, "text": "Kendilerine Kur'\u00e2n okundu\u011funda derin bir sayg\u0131 ile e\u011filmiyorlar?"}, {"id": 598163, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5906, "text": "Bilakis, o k\u00e2firler dini yalan saymaya devam ediyorlar."}, {"id": 598164, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5907, "text": "Allah, onlar\u0131n kalplerinde ne saklad\u0131klar\u0131n\u0131 pek iyi bilir!"}, {"id": 598165, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5908, "text": "Sen de onlara gayet ac\u0131 bir azap m\u00fcjdele!"}, {"id": 598166, "identifier": "tr.yildirim", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5909, "text": "Fakat iman edip makbul ve g\u00fczel i\u015fler yapanlara ise, hi\u00e7 kesintiye u\u011framayan, bitip t\u00fckenmeyen m\u00fck\u00e2fat vard\u0131r."}]