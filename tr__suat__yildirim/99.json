[{"id": 598393, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6139, "text": "Yer o m\u00fcthi\u015f depremiyle sars\u0131ld\u0131\u011f\u0131 zaman..."}, {"id": 598394, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6140, "text": "Ve yer ba\u011fr\u0131ndaki a\u011f\u0131rl\u0131klar\u0131 \u00e7\u0131kard\u0131\u011f\u0131 zaman..."}, {"id": 598395, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6141, "text": "\u0130nsan \u015fa\u015fk\u0131n \u015fa\u015fk\u0131n: \u201cNe oluyor buna!\u201d dedi\u011fi zaman..."}, {"id": 598396, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6142, "text": "\u0130\u015fte o g\u00fcn yer, \u00fcst\u00fcnde olan biten her \u015feyi anlat\u0131r:"}, {"id": 598397, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6143, "text": "\u00c7\u00fcnk\u00fc Rabbin ona bunlar\u0131 vahyeder."}, {"id": 598398, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6144, "text": "\u0130\u015fte o g\u00fcn b\u00f6l\u00fckler halinde insanlar,kabirlerinden \u00e7\u0131k\u0131p y\u00fcce divana dururlar,ta ki yapt\u0131klar\u0131n\u0131n kar\u015f\u0131l\u0131\u011f\u0131n\u0131 g\u00f6r\u00fcp al\u0131rlar."}, {"id": 598399, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6145, "text": "Zerre a\u011f\u0131rl\u0131\u011f\u0131nca hay\u0131r yapan onu bulur,"}, {"id": 598400, "identifier": "tr.yildirim", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6146, "text": "Zerre a\u011f\u0131rl\u0131\u011f\u0131nca \u015fer yapan da onu bulur."}]