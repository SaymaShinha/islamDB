[{"id": 592423, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 1, "text": "Rahm\u00e2n ve rah\u00eem olan Allah'\u0131n ad\u0131yla [59,22-24]"}, {"id": 592424, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 2, "text": "B\u00fct\u00fcn hamdler, \u00f6vg\u00fcler \u00e2lemlerin Rabbi Allah'ad\u0131r."}, {"id": 592425, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 3, "text": "O rahm\u00e2nd\u0131r, rah\u00eemdir."}, {"id": 592426, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 4, "text": "Din g\u00fcn\u00fcn\u00fcn, hesap g\u00fcn\u00fcn\u00fcn tek h\u00e2kimidir. [24,25; 37,53]"}, {"id": 592427, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5, "text": "(Haydi \u00f6yleyse deyiniz): \u201cYaln\u0131z Sana ibadet eder, yaln\u0131z senden medet umar\u0131z.\u201d [73,9; 6,1; 3,64]"}, {"id": 592428, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6, "text": "Bizi do\u011fru yola, Sana do\u011fru varan yola ilet."}, {"id": 592429, "identifier": "tr.yildirim", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 7, "text": "Nimet ve l\u00fctfuna nail ettiklerinin yoluna ilet. Gazaba u\u011frayanlar\u0131n ve sapk\u0131nlar\u0131nkine de\u011fil. [4,69]"}]