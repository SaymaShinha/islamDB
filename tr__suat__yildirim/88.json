[{"id": 598225, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5968, "text": "G\u00e2\u015fiye'nin,deh\u015feti her taraf\u0131 saracak olan o fel\u00e2ketin mahiyeti hakk\u0131nda elbet sen de bilgi sahibi oldun."}, {"id": 598226, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5969, "text": "Y\u00fczler vard\u0131r o g\u00fcn yere e\u011filmi\u015ftir, zelildir!"}, {"id": 598227, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5970, "text": "Yorgundur, bitkin mi bitkindir!"}, {"id": 598228, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5971, "text": "K\u0131zg\u0131n ate\u015fe girerler."}, {"id": 598229, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5972, "text": "Susay\u0131nca kaynar su kaynayan bir \u00e7e\u015fmeden i\u00e7erler."}, {"id": 598230, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5973, "text": "Yiyecekleri sadece bir dikenden ibarettir."}, {"id": 598231, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5974, "text": "Bu diken ne besleyicidir, ne de a\u00e7l\u0131\u011f\u0131 giderir."}, {"id": 598232, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5975, "text": "Ama y\u00fczler vard\u0131r, o g\u00fcn mutludurlar!"}, {"id": 598233, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5976, "text": "Emeklerinin neticesini almadan \u00f6t\u00fcr\u00fc gayet memnundurlar."}, {"id": 598234, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5977, "text": "Pek \u00fcst\u00fcn ve pek muteber bir cennettedirler."}, {"id": 598235, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5978, "text": "Orada hi\u00e7 bo\u015f s\u00f6z i\u015fitmezler. [19,62; 52,23; 56-25-26]"}, {"id": 598236, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5979, "text": "Orada akan berrak p\u0131narlar..."}, {"id": 598237, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5980, "text": "Orada \u00fcst\u00fcn, k\u0131ymetli tahtlar..."}, {"id": 598238, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5981, "text": "Haz\u0131rlanm\u0131\u015f kadehler..."}, {"id": 598239, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5982, "text": "Dizilmi\u015f koltuklar, yast\u0131klar..."}, {"id": 598240, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5983, "text": "Yay\u0131lm\u0131\u015f hal\u0131lar ve d\u00f6\u015femeler..."}, {"id": 598241, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5984, "text": "O k\u00e2firler bak\u0131p d\u00fc\u015f\u00fcnmezler mi: (Mesela) deve nas\u0131l yarat\u0131lm\u0131\u015f?"}, {"id": 598242, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5985, "text": "G\u00f6k nas\u0131l kurulup u\u00e7suz bucaks\u0131z y\u00fckseltilmi\u015f? [50,6; 67,3-4]"}, {"id": 598243, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5986, "text": "Da\u011flar nas\u0131l da yeri tutup, dengeleyen direkler halinde dikilmi\u015f."}, {"id": 598244, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5987, "text": "Yery\u00fcz\u00fc nas\u0131l yay\u0131l\u0131p hayata elveri\u015fli k\u0131l\u0131nm\u0131\u015f?"}, {"id": 598245, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5988, "text": "\u0130\u015fte b\u00f6yle... Sen insanlar\u0131 ir\u015fada devam et! Zaten senin g\u00f6revin sadece ir\u015fad edip d\u00fc\u015f\u00fcnd\u00fcrmektir."}, {"id": 598246, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5989, "text": "Yoksa sen kimseyi zorlayacak de\u011filsin."}, {"id": 598247, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5990, "text": "L\u00e2kin kim ki imana s\u0131rt\u0131n\u0131 d\u00f6ner ve ink\u00e2r eder,"}, {"id": 598248, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5991, "text": "Allah da onu en b\u00fcy\u00fck cezaya \u00e7arpt\u0131r\u0131r."}, {"id": 598249, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5992, "text": "Elbet onlar\u0131n d\u00f6n\u00fc\u015f\u00fc Bize olacakt\u0131r."}, {"id": 598250, "identifier": "tr.yildirim", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 5993, "text": "Elbet hesaplar\u0131n\u0131 g\u00f6rmek de Bizim i\u015fimiz olacakt\u0131r."}]