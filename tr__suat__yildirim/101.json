[{"id": 598412, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6158, "text": "Kari'a,"}, {"id": 598413, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6159, "text": "Nedir o kari'a?"}, {"id": 598414, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6160, "text": "Kari'ay\u0131, o kap\u0131lar\u0131 d\u00f6ven ve deh\u015fetiyle kalplere \u00e7arpan o k\u0131yamet felaketini sen nereden bileceksin ki!"}, {"id": 598415, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6161, "text": "O g\u00fcn insanlar u\u00e7u\u015fan kelebekler gibi \u015furaya buraya f\u0131rlat\u0131l\u0131r."}, {"id": 598416, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6162, "text": "Da\u011flar at\u0131lm\u0131\u015f y\u00fcne d\u00f6ner,"}, {"id": 598417, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6163, "text": "Art\u0131k kimin tart\u0131lar\u0131 a\u011f\u0131r basarsa,"}, {"id": 598418, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6164, "text": "Memnun kalaca\u011f\u0131 bir hayata girer."}, {"id": 598419, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6165, "text": "Kimin tart\u0131lar\u0131 da hafif gelirse,"}, {"id": 598420, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6166, "text": "Onun bar\u0131na\u011f\u0131 da Haviye olur."}, {"id": 598421, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6167, "text": "Onun ne oldu\u011funu bilir misin?"}, {"id": 598422, "identifier": "tr.yildirim", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6168, "text": "Haviye bir ate\u015ftir: k\u0131zg\u0131n m\u0131 k\u0131zg\u0131n!"}]