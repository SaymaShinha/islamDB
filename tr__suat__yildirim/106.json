[{"id": 598448, "identifier": "tr.yildirim", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6194, "text": "Kurey\u015f'in g\u00fcven ve bar\u0131\u015f anla\u015fmalar\u0131ndan faydalanmalar\u0131n\u0131 sa\u011flamak i\u00e7in,"}, {"id": 598449, "identifier": "tr.yildirim", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6195, "text": "K\u0131\u015f ve yaz seferlerinde faydaland\u0131klar\u0131 anla\u015fmalar\u0131n kadrini bilmi\u015f olmak i\u00e7in,"}, {"id": 598450, "identifier": "tr.yildirim", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6196, "text": "Yaln\u0131z bu Ev'in (K\u00e2\u2019benin) Rabbine ibadet etsinler."}, {"id": 598451, "identifier": "tr.yildirim", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6197, "text": "Kendilerini a\u00e7l\u0131ktan kurtar\u0131p doyuran, korkudan emin k\u0131lan Rab'lerine kulluk etsinler!"}]