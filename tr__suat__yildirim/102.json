[{"id": 598423, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6169, "text": "D\u00fcnyal\u0131klarla b\u00f6b\u00fcrlenmek, oyalad\u0131 sizleri."}, {"id": 598424, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6170, "text": "T\u00e2 boylay\u0131ncaya kadar kabirleri!"}, {"id": 598425, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6171, "text": "Hay\u0131r (ge\u00e7ici d\u00fcnya zevklerine ba\u011flanmak do\u011fru de\u011fil, sak\u0131n\u0131n bundan) ileride bileceksiniz!"}, {"id": 598426, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6172, "text": "Evet, evet! \u0130leride bileceksiniz!"}, {"id": 598427, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6173, "text": "Sak\u0131n\u0131n bundan! E\u011fer kesin bir tarzda (ilmelyakin) bilseydiniz b\u00f6yle yapmazd\u0131n\u0131z."}, {"id": 598428, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6174, "text": "Siz cehennemi g\u00f6receksiniz."}, {"id": 598429, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6175, "text": "Evet, evet onu mutlaka g\u00f6zlerinizle g\u00f6receksiniz!"}, {"id": 598430, "identifier": "tr.yildirim", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Suat Y\u0131ld\u0131r\u0131m", "transelator_en_name": "Suat Yildirim", "format": "text", "type": "translation", "ayah_number": 6176, "text": "Sonra o g\u00fcn nimetlerden hesaba \u00e7ekileceksiniz!"}]