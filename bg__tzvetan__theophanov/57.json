[{"id": 98476, "identifier": "bg.theophanov", "surah_number": 57, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062f\u064a\u062f", "surah_en_name": "Al-Hadid", "surah_en_name_translation": "The Iron", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 537, "ruku": 472, "hizb_quarter": 215, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5076, "text": "\u0412\u0441\u0438\u0447\u043a\u043e \u043d\u0430 \u043d\u0435\u0431\u0435\u0441\u0430\u0442\u0430 \u0438 \u0432\u0441\u0438\u0447\u043a\u043e \u043d\u0430 \u0437\u0435\u043c\u044f\u0442\u0430 \u043f\u0440\u043e\u0441\u043b\u0430\u0432\u044f \u0410\u043b\u043b\u0430\u0445. \u0422\u043e\u0439 \u0435 \u0412\u0441\u0435\u043c\u043e\u0433\u044a\u0449\u0438\u044f, \u041f\u0440\u0435\u043c\u044a\u0434\u0440\u0438\u044f."}, {"id": 98477, "identifier": "bg.theophanov", "surah_number": 57, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062f\u064a\u062f", "surah_en_name": "Al-Hadid", "surah_en_name_translation": "The Iron", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 537, "ruku": 472, "hizb_quarter": 215, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5077, "text": "\u041d\u0435\u0433\u043e\u0432\u0430 \u0435 \u0432\u043b\u0430\u0441\u0442\u0442\u0430 \u043d\u0430 \u043d\u0435\u0431\u0435\u0441\u0430\u0442\u0430 \u0438 \u043d\u0430 \u0437\u0435\u043c\u044f\u0442\u0430. \u0422\u043e\u0439 \u0441\u044a\u0436\u0438\u0432\u044f\u0432\u0430 \u0438 \u0422\u043e\u0439 \u0443\u043c\u044a\u0440\u0442\u0432\u044f\u0432\u0430. \u0422\u043e\u0439 \u0437