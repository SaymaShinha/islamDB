[{"id": 98876, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5476, "text": "\u041e, \u0442\u0438, \u043a\u043e\u0439\u0442\u043e \u0441\u0435 \u0437\u0430\u0433\u0440\u044a\u0449\u0430\u0448,"}, {"id": 98877, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5477, "text": "\u0441\u0442\u0430\u0432\u0430\u0439 [\u0437\u0430 \u043c\u043e\u043b\u0438\u0442\u0432\u0430] \u043f\u0440\u0435\u0437 \u043d\u043e\u0449\u0442\u0430, \u043d\u043e \u043d\u0435 \u0446\u044f\u043b\u0430\u0442\u0430,"}, {"id": 98878, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5478, "text": "\u0430 \u043d\u0430\u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0430, \u0438\u043b\u0438 \u043f\u043e-\u043c\u0430\u043b\u043a\u043e \u043e\u0442 \u0442\u043e\u0432\u0430,"}, {"id": 98879, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5479, "text": "\u0438\u043b\u0438 \u043f\u043e\u0432\u0435\u0447\u0435, \u0438 \u0447\u0435\u0442\u0438 \u041a\u043e\u0440\u0430\u043d\u0430 \u043e\u0442\u043c\u0435\u0440\u0435\u043d\u043e!"}, {"id": 98880, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5480, "text": "\u0429\u0435 \u0442\u0438 \u043f\u0440\u0430\u0442\u0438\u043c \u041d\u0438\u0435 \u0442\u0435\u0436\u043a\u043e \u0421\u043b\u043e\u0432\u043e."}, {"id": 98881, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u06