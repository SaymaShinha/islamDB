rter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5483, "text": "\u0418 \u0441\u043f\u043e\u043c\u0435\u043d\u0430\u0432\u0430\u0439 \u0438\u043c\u0435\u0442\u043e \u043d\u0430 \u0441\u0432\u043e\u044f \u0413\u043e\u0441\u043f\u043e\u0434, \u0438 \u0441 \u0432\u0441\u0435\u043e\u0442\u0434\u0430\u0439\u043d\u043e\u0441\u0442 \u0441\u0435 \u043f\u043e\u0441\u0432\u0435\u0442\u0438 \u043d\u0430 \u041d\u0435\u0433\u043e -"}, {"id": 98884, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5484, "text": "\u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430 \u0418\u0437\u0442\u043e\u043a\u0430 \u0438 \u043d\u0430 \u0417\u0430\u043f\u0430\u0434\u0430! \u041d\u044f\u043c\u0430 \u0434\u0440\u0443\u0433 \u0411\u043e\u0433 \u043e\u0441\u0432\u0435\u043d \u041d\u0435\u0433\u043e. \u041d\u0435\u0433\u043e \u0432\u0437\u0435\u043c\u0438 \u0437\u0430 \u043f\u043e\u043a\u0440\u043e\u0432\u0438\u0442\u0435\u043b!"}, {"id": 98885, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5485, "text": "\u0418 \u0431\u044a\u0434\u0438 \u0442\u044a\u0440\u043f\u0435\u043b\u0438\u0432 \u043a\u044a\u043c \u043e\u043d\u043e\u0432\u0430, \u043a\u043e\u0435\u0442\u043e \u0433\u043e\u0432\u043e\u0440\u044f\u0442! \u0418 \u0433\u0438 \u043d\u0430\u043f\u0443\u0441\u043a\u0430\u0439 \u0441 \u0434\u043e\u0441\u0442\u043e\u0439\u043d\u0441\u0442\u0432\u043e!"}, {"id": 98886, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5486, "text": "\u0418 \u043e\u0441\u0442\u0430\u0432\u0438 \u043d\u0430 \u041c\u0435\u043d \u043e\u0442\u0440\u0438\u0447\u0430\u0449\u0438\u0442\u0435, \u043a\u043e\u0438\u0442\u043e \u0436\u0438\u0432\u0435\u044f\u0442 \u0432 \u0431\u043b\u0430\u0433\u043e\u0434\u0435\u043d\u0441\u0442\u0432\u0438\u0435, \u0438 \u0438\u043c \u0434\u0430\u0439 \u043c\u0430\u043b\u043a\u0430 \u043e\u0442\u0441\u0440\u043e\u0447\u043a\u0430!"}, {"id": 98887, "identifier": "bg.theophanov", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "s