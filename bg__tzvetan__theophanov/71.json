[{"id": 98817, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5420, "text": "\u041d\u0438\u0435 \u0438\u0437\u043f\u0440\u0430\u0442\u0438\u0445\u043c\u0435 \u041d\u0443\u0445 \u043f\u0440\u0438 \u043d\u0435\u0433\u043e\u0432\u0438\u044f \u043d\u0430\u0440\u043e\u0434: \u201c\u041f\u0440\u0435\u0434\u0443\u043f\u0440\u0435\u0434\u0438 \u043d\u0430\u0440\u043e\u0434\u0430 \u0441\u0438, \u0434\u043e\u043a\u0430\u0442\u043e \u043d\u0435 \u0433\u043e \u0435 \u0441\u043f\u043e\u043b\u0435\u0442\u044f\u043b\u043e \u0431\u043e\u043b\u0435\u0437\u043d\u0435\u043d\u043e \u043c\u044a\u0447\u0435\u043d\u0438\u0435!\u201d"}, {"id": 98818, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5421, "text": "\u0420\u0435\u0447\u0435: \u201c\u041e, \u043d\u0430\u0440\u043e\u0434\u0435 \u043c\u043e\u0439, \u0430\u0437 \u0441\u044a\u043c \u044f\u0432\u0435\u043d \u043f\u0440\u0435\u0434\u0443\u043f\u0440\u0435\u0434\u0438\u0442\u0435\u043b \u0437\u0430 \u0432\u0430\u0441."}, {"id": 98819, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5422, "text": "\u0421\u043b\u0443\u0436\u0435\u0442\u0435 \u043d\u0430 \u0410\u043b\u043b\u0430\u0445 \u0438 \u0441\u0435 \u0431\u043e\u0439\u0442\u0435 \u043e\u0442 \u041d\u0435\u0433\u043e, \u0438 \u043c\u0438 \u0441\u0435 \u043f\u043e\u043a\u043e\u0440\u0435\u0442\u0435!"}, {"id": 98820, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5423, "text": "\u0422\u043e\u0433\u0430\u0432\u0430 \u0422\u043e\u0439 \u0449\u0435 \u0432\u0438 \u043e\u043f\u0440\u043e\u0441\u0442\u0438 \u0433\u0440\u0435\u0445\u043e\u0432\u0435\u0442\u0435 \u0438 \u0449\u0435 \u0432\u0438 \u0434\u0430\u0434\u0435 \u043e\u0442\u0441\u0440\u043e\u0447\u043a\u0430 \u0437\u0430 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043e \u0432\u0440\u0435\u043c\u0435. \u0421\u0440\u043e\u043a\u044a\u0442 \u043d\u0430 \u0410\u043b\u043b\u0430\u0445, \u043a\u043e\u0433\u0430\u0442\u043e \u0434\u043e\u0439\u0434\u0435, \u043d\u0435 \u0449\u0435 \u0431\u044a\u0434\u0435 \u0437\u0430\u0431\u0430\u0432\u0435\u043d, \u0430\u043a\u043e \u0437\u043d\u0430\u0435\u0442\u0435.\u201d"}, {"id": 98821, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5424, "text": "\u0420\u0435\u0447\u0435: \u201c\u0413\u043e\u0441\u043f\u043e\u0434\u0438 \u043c\u043e\u0439, \u0437\u043e\u0432\u044f\u0445 \u043d\u0430\u0440\u043e\u0434\u0430 \u0441\u0438 \u043d\u043e\u0449\u0435\u043c \u0438 \u0434\u0435\u043d\u0435\u043c,"}, {"id": 98822, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5425, "text": "\u043d\u043e \u0437\u043e\u0432\u044a\u0442 \u043c\u0438 \u0441\u0430\u043c\u043e \u0443\u0432\u0435\u043b\u0438\u0447\u0438 \u0442\u044f\u0445\u043d\u043e\u0442\u043e \u0431\u044f\u0433\u0441\u0442\u0432\u043e."}, {"id": 98823, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5426, "text": "\u0418 \u0432\u0441\u044f\u043a\u043e\u0433\u0430, \u0449\u043e\u043c \u0433\u0438 \u0437\u043e\u0432\u044f\u0445, \u0437\u0430 \u0434\u0430 \u0433\u0438 \u043e\u043f\u0440\u043e\u0441\u0442\u0438\u0448, \u0437\u0430\u043f\u0443\u0448\u0432\u0430\u0445\u0430 \u0441 \u043f\u0440\u044a\u0441\u0442\u0438 \u0443\u0448\u0438\u0442\u0435 \u0441\u0438 \u0438 \u0441\u0435 \u0437\u0430\u0433\u0440\u044a\u0449\u0430\u0445\u0430 \u0432 \u0434\u0440\u0435\u0445\u0438\u0442\u0435 \u0441\u0438, \u0438 \u0443\u043f\u043e\u0440\u0441\u0442\u0432\u0430\u0445\u0430, \u0438 \u0432\u0441\u0435 \u043f\u043e\u0432\u0435\u0447\u0435 \u0441\u0435 \u0432\u044a\u0437\u0433\u043e\u0440\u0434\u044f\u0432\u0430\u0445\u0430."}, {"id": 98824, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5427, "text": "\u041f\u043e\u0441\u043b\u0435 \u0433\u0438 \u0437\u043e\u0432\u044f\u0445 \u043e\u0442\u043a\u0440\u0438\u0442\u043e,"}, {"id": 98825, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5428, "text": "\u043f\u043e\u0441\u043b\u0435 \u0438\u043c \u043f\u0440\u043e\u0433\u043b\u0430\u0441\u044f\u0432\u0430\u0445 \u044f\u0432\u043d\u043e \u0438 \u0438\u043c \u0448\u0435\u043f\u043d\u0435\u0445 \u0442\u0430\u0439\u043d\u043e."}, {"id": 98826, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5429, "text": "\u0418 \u043a\u0430\u0437\u0432\u0430\u0445: \u201c\u041c\u043e\u043b\u0435\u0442\u0435 \u0432\u0430\u0448\u0438\u044f \u0413\u043e\u0441\u043f\u043e\u0434 \u0437\u0430 \u043e\u043f\u0440\u043e\u0449\u0435\u043d\u0438\u0435 - \u0422\u043e\u0439 \u0435 \u043c\u043d\u043e\u0433\u043e\u043e\u043f\u0440\u043e\u0449\u0430\u0432\u0430\u0449,"}, {"id": 98827, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5430, "text": "\u0437\u0430 \u0434\u0430 \u0432\u0438 \u043f\u0440\u0430\u0449\u0430 \u04