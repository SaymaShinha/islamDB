anselator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5440, "text": "\u0420\u0435\u0447\u0435 \u041d\u0443\u0445: \u201c\u0413\u043e\u0441\u043f\u043e\u0434\u0438 \u043c\u043e\u0439, \u0442\u0435 \u043c\u0438 \u0441\u0435 \u0432\u044a\u0437\u043f\u0440\u043e\u0442\u0438\u0432\u0438\u0445\u0430 \u0438 \u043f\u043e\u0441\u043b\u0435\u0434\u0432\u0430\u0445\u0430 \u043e\u043d\u0435\u0437\u0438, \u0447\u0438\u0438\u0442\u043e \u0438\u043c\u043e\u0442\u0438 \u0438 \u0434\u0435\u0446\u0430 \u0441\u0430\u043c\u043e \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0445\u0430 \u0442\u044f\u0445\u043d\u0430\u0442\u0430 \u0437\u0430\u0433\u0443\u0431\u0430."}, {"id": 98838, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5441, "text": "\u0418 \u043b\u0443\u043a\u0430\u0432\u0441\u0442\u0432\u0430\u0445\u0430 \u0442\u0435 \u0441 \u043e\u0433\u0440\u043e\u043c\u043d\u043e \u043b\u0443\u043a\u0430\u0432\u0441\u0442\u0432\u043e."}, {"id": 98839, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5442, "text": "\u0418 \u043a\u0430\u0437\u0432\u0430\u0445\u0430: \u201c\u041d\u0438\u043a\u043e\u0433\u0430 \u043d\u0435 \u0438\u0437\u043e\u0441\u0442\u0430\u0432\u044f\u0439\u0442\u0435 \u0441\u0432\u043e\u0438\u0442\u0435 \u0431\u043e\u0436\u0435\u0441\u0442\u0432\u0430, \u043d\u0435 \u0438\u0437\u043e\u0441\u0442\u0430\u0432\u044f\u0439\u0442\u0435 \u043d\u0438\u0442\u043e \u0423\u0430\u0434, \u043d\u0438\u0442\u043e \u0421\u0443\u0430\u0430, \u043d\u0438\u0442\u043e \u042f\u0433\u0443\u0441 \u0438 \u042f\u0443\u043a, \u0438 \u041d\u0430\u0441\u0440!\u201d"}, {"id": 98840, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5443, "text": "\u0418 \u0432\u0435\u0447\u0435 \u0437\u0430\u0431\u043b\u0443\u0434\u0438\u0445\u0430 \u0442\u0435 \u043c\u043d\u043e\u0437\u0438\u043d\u0430. \u0418 \u043d\u0430\u0434\u0431\u0430\u0432\u044f\u0439 \u043d\u0430 \u0443\u0433\u043d\u0435\u0442\u0438\u0442\u0435\u043b\u0438\u0442\u0435 \u0441\u0430\u043c\u043e \u0437\u0430\u0431\u043b\u0443\u0434\u0430!\u201d"}, {"id": 98841, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5444, "text": "\u0417\u0430\u0440\u0430\u0434\u0438 \u0441\u0432\u043e\u0438\u0442\u0435 \u0433\u0440\u0435\u0445\u043e\u0432\u0435 \u0442\u0435 \u0431\u044f\u0445\u0430 \u0438\u0437\u0434\u0430\u0432\u0435\u043d\u0438 \u0438 \u0432\u044a\u0432\u0435\u0434\u0435\u043d\u0438 \u0432 \u041e\u0433\u044a\u043d\u044f, \u0438 \u043d\u0435 \u043d\u0430\u043c\u0435\u0440\u0438\u0445\u0430 \u0437\u0430 \u0441\u0435\u0431\u0435 \u0441\u0438 \u0434\u0440\u0443\u0433\u0438 \u043f\u043e\u043c\u043e\u0449\u043d\u0438\u0446\u0438 \u0432\u043c\u0435\u0441\u0442\u043e \u0410\u043b\u043b\u0430\u0445."}, {"id": 98842, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5445, "text": "\u0418 \u0440\u0435\u0447\u0435 \u041d\u0443\u0445: \u201c\u0413\u043e\u0441\u043f\u043e\u0434\u0438 \u043c\u043e\u0439, \u043d\u0435 \u043e\u0441\u0442\u0430\u0432\u044f\u0439 \u043f\u043e \u0437\u0435\u043c\u044f\u0442\u0430 \u043d\u0438\u0442\u043e \u0435\u0434\u0438\u043d \u043d\u0435\u0432\u0435\u0440\u043d\u0438\u043a!"}, {"id": 98843, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5446, "text": "\u041e\u0441\u0442\u0430\u0432\u0438\u0448 \u043b\u0438 \u0433\u0438, \u0442\u0435 \u0449\u0435 \u0437\u0430\u0431\u043b\u0443\u0434\u044f\u0442 \u0422\u0432\u043e\u0438\u0442\u0435 \u0440\u0430\u0431\u0438 \u0438 \u0449\u0435 \u0440\u043e\u0434\u044f\u0442 \u0441\u0430\u043c\u043e \u043d\u0435\u0447\u0435\u0441\u0442\u0438\u0432\u0446\u0438, \u043d\u0435\u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u043d\u0438\u0446\u0438."}, {"id": 98844, "identifier": "bg.theophanov", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bg", "name": "\u0422\u0435\u043e\u0444\u0430\u043d\u043e\u0432", "transelator_en_name": "Tzvetan Theophanov", "format": "text", "type": "translation", "ayah_number": 5447, "text": "\u0413\u043e\u0441\u043f\u043e\u0434\u0438 \u043c\u043e\u0439, \u043e\u043f\u0440\u043e\u0441\u0442\u0438 \u043d\u0430 \u043c\u0435\u043d \u0438 \u043d\u0430 \u0440\u043e\u0434\u0438\u0442\u0435\u043b\u0438\u0442\u0435 \u043c\u0438, \u0438 \u043d\u0430 \u0432\u0441\u0435\u043a\u0438 \u0432\u044f\u0440\u0432\u0430\u0449, \u043a\u043e\u0439\u0442\u043e \u0432\u043b\u0435\u0437\u0435 \u0432 \u043c\u043e\u044f \u0434\u043e\u043c, \u0438 \u043d\u0430 \u0432\u044f\u0440\u0432\u0430\u0449\u0438\u0442\u0435 \u043c\u044a\u0436\u0435, \u0438 \u043d\u0430 \u0432\u044f\u0440\u0432\u0430\u0449\u0438\u0442\u0435 \u0436\u0435\u043d\u0438, \u0438 \u043d\u0430\u0434\u0431\u0430\u0432\u044f\u0439 \u043d\u0430 \u0443\u0433\u043d\u0435\u0442\u0438\u0442\u0435\u043b\u0438\u0442\u0435 \u0441\u0430\u043c\u043e \u0433\u0438\u0431\u0435\u043b!\u201d"}]