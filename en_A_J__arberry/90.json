[{"id": 249038, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6024, "text": "No! I swear by this land,"}, {"id": 249039, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6025, "text": "and thou art a lodger in this land;"}, {"id": 249040, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6026, "text": "by the begetter, and that he begot,"}, {"id": 249041, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6027, "text": "indeed, We created man in trouble."}, {"id": 249042, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6028, "text": "What, does he think none has power over him,"}, {"id": 249043, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6029, "text": "saying, 'I have consumed wealth abundant'?"}, {"id": 249044, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6030, "text": "What, does he think none has seen him?"}, {"id": 249045, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Have We not appointed to him two eyes,"}, {"id": 249046, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6032, "text": "and a tongue, and two lips,"}, {"id": 249047, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6033, "text": "and guided him on the two highways?"}, {"id": 249048, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Yet he has not assaulted the steep;"}, {"id": 249049, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6035, "text": "and what shall teach thee what is the steep?"}, {"id": 249050, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6036, "text": "The freeing of a slave,"}, {"id": 249051, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6037, "text": "or giving food upon a day of hunger"}, {"id": 249052, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6038, "text": "to an orphan near of kin"}, {"id": 249053, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6039, "text": "or a needy man in misery;"}, {"id": 249054, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6040, "text": "then that he become of those who believe and counsel each other to be steadfast, and counsel each other to be merciful."}, {"id": 249055, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6041, "text": "Those are the Companions of the Right Hand."}, {"id": 249056, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6042, "text": "And those who disbelieve in Our signs, they are the Companions of the Left Hand;"}, {"id": 249057, "identifier": "en.arberry", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 6043, "text": "over them is a Fire covered down."}]