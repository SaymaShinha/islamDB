[{"id": 247657, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4631, "text": "Qaf. By the glorious Koran!"}, {"id": 247658, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4632, "text": "Nay, but they marvel that a warner has come to them from among them; and the unbelievers say, 'This is a marvellous thing!"}, {"id": 247659, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4633, "text": "What, when we are dead and become dust? That is a far returning!'"}, {"id": 247660, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4634, "text": "We know what the earth diminishes of them; with Us is a book recording."}, {"id": 247661, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4635, "text": "Nay, but they cried lies to the truth when it came to them, and so they are in a case confused."}, {"id": 247662, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4636, "text": "What, have they not beheld heaven above them, how We have built it, and decked it out fair, and it has no cracks?"}, {"id": 247663, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4637, "text": "And the earth -- We stretched it forth, and cast on it firm mountains, and We caused to grow therein of every joyous kind"}, {"id": 247228, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4638, "text": "for an insight and a reminder to every penitent servant."}, {"id": 247570, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4639, "text": "And We sent down out of heaven water blessed, and caused to grow thereby gardens and grain of harvest"}, {"id": 247620, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4640, "text": "and tall palm-trees with spathes compact,"}, {"id": 247664, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4641, "text": "a provision for the servants, and thereby We revived a land that was dead. Even so is the coming forth."}, {"id": 247665, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4642, "text": "Cried lies before them the people of Noah and the men of Er-Rass, and Thamood,"}, {"id": 247666, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4643, "text": "and Ad and Pharaoh, the brothers of Lot,"}, {"id": 247667, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4644, "text": "the men of the Thicket, the people of Tubba'. Every one cried lies to the Messengers, and My threat came true."}, {"id": 247668, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4645, "text": "What, were We wearied by the first creation? No indeed; but they are in uncertainty as to the new creation."}, {"id": 247669, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4646, "text": "We indeed created man; and We know what his soul whispers within him, and We are nearer to him than the jugular vein."}, {"id": 247670, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4647, "text": "When the two angels meet together, sitting one on the right, and one on the left,"}, {"id": 247671, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4648, "text": "not a word he utters, but by him is an observer ready."}, {"id": 247672, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4649, "text": "And death's agony comes in truth; that is what thou wast shunning!"}, {"id": 247673, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4650, "text": "And the Trumpet shall be blown; that is the Day of the Threat."}, {"id": 247674, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4651, "text": "And every soul shall come, and with it a driver and a witness."}, {"id": 247675, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4652, "text": "'Thou wast heedless of this; therefore We have now removed from thee thy covering, and so thy sight today is piercing.'"}, {"id": 247555, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4653, "text": "And his comrade shall say, 'This is what I have, made ready.'"}, {"id": 247579, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4654, "text": "'Cast, you twain, into Gehenna every froward unbeliever,"}, {"id": 247648, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4655, "text": "every hinderer of the good, transgressor, disquieter,"}, {"id": 247676, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4656, "text": "who set up with God another god; therefore, you twain, cast him into the terrible chastisement.'"}, {"id": 247677, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4657, "text": "And his comrade shall say, 'Our Lord, I made him not insolent, but he was in far error.'"}, {"id": 247678, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4658, "text": "He shall say, 'Dispute not before Me! For I sent you beforehand -- the threat."}, {"id": 247679, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4659, "text": "The Word is not changed with Me; I wrong not My servants.'"}, {"id": 247680, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4660, "text": "Upon the day We shall say unto Gehenna, 'Art thou filled?' And it shall say, 'Are there any more to come?'"}, {"id": 247681, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4661, "text": "And Paradise shall be brought forward to the godfearing, not afar:"}, {"id": 247682, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4662, "text": "'This is that you were promised; it is for every mindful penitent.'"}, {"id": 247683, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4663, "text": "Whosoever fears the All-merciful in the Unseen, and comes with a penitent heart:"}, {"id": 247684, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4664, "text": "'Enter it in peace! This is the Day of Eternity.'"}, {"id": 247685, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4665, "text": "Therein they shall have whatever they will; and with Us there is yet more."}, {"id": 247686, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4666, "text": "How many a generation We destroyed before them that was stronger in valour than they, then they searched about in the land; was there any asylum?"}, {"id": 247687, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4667, "text": "Surely in that there is a reminder to him who has a heart, or will give ear with a present mind."}, {"id": 247688, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4668, "text": "We created the heavens and the earth, and what between them is, in six days, and no weariness touched Us."}, {"id": 247689, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4669, "text": "So be thou patient under what they say, and proclaim thy Lord's praise before the rising of the sun, and before its setting,"}, {"id": 247690, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4670, "text": "and proclaim thy Lord's praise in the night, and at the ends of the prostrations."}, {"id": 247691, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4671, "text": "And listen thou for the day when the caller shall call from a near place."}, {"id": 247692, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4672, "text": "On the day they hear the Cry in truth, that is the day of coming forth."}, {"id": 247693, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4673, "text": "It is We who give life, and make to die, and to Us is the homecoming."}, {"id": 247694, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4674, "text": "Upon the day when the earth is split asunder from about them as they hasten forth; that is a mustering easy for Us."}, {"id": 247695, "identifier": "en.arberry", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4675, "text": "We know very well what they say; thou art not a tyrant over them. Therefore remind by the Koran him who fears My threat."}]