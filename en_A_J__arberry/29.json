[{"id": 246360, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3341, "text": "Alif Lam Mim"}, {"id": 246361, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3342, "text": "Do the people reckon that they will be left to say 'We believe,' and will not be tried?"}, {"id": 246362, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3343, "text": "We certainly tried those that were before them, and assuredly God knows those who speak truly, and assuredly He knows the liars."}, {"id": 246363, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3344, "text": "Or do they reckon, those who do evil deeds, that they will outstrip Us? Ill they judge!"}, {"id": 246364, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3345, "text": "Whoso looks to encounter God, God's term is coming; He is the All-hearing, the All-knowing."}, {"id": 246365, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3346, "text": "Whosoever struggles, struggles only to his own gain; surely God is All-sufficient nor needs any being."}, {"id": 246366, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3347, "text": "And those who believe, and do righteous deeds, We shall surely acquit them of their evil deeds, and shall recompense them the best of what they were doing."}, {"id": 246367, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3348, "text": "We have charged man, that he be kind to his parents; but if they strive with thee to make thee associate with Me that whereof thou hast no knowledge, then do not obey them; unto Me you shall return, and I shall tell you what you were doing."}, {"id": 246368, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3349, "text": "And those who believe, and do righteous deeds assuredly We shall admit them among the righteous."}, {"id": 246369, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3350, "text": "Some men there are who say, 'We believe in God,' but when such a man is hurt in God's cause, he makes the persecution of men as it were God's chastisement; then if help comes from thy Lord, he will say 'We were with you.' What, does not God know very well what is in the breasts of all beings?"}, {"id": 246370, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3351, "text": "God surely knows the believers, and He knows the hypocrites."}, {"id": 246371, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3352, "text": "The unbelievers say to the believers, 'Follow our path, and let us carry your offences'; yet they cannot carry anything, even of their own offences; they are truly liars."}, {"id": 246372, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3353, "text": "They shall certainly carry their loads, and other loads along with their loads, and upon the Day of Resurrection they shall surely be questioned concerning that they were forging."}, {"id": 246373, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3354, "text": "Indeed, We sent Noah to his people, and he tarried among them a thousand years, all but fifty; so the Flood seized them, while they were evildoers."}, {"id": 246374, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3355, "text": "Yet We delivered him, and those who were in the ship, and appointed it for a sign unto all beings."}, {"id": 246375, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3356, "text": "And Abraham, when he said to his people, 'Serve God, and fear Him; that is better for you, did you know."}, {"id": 246376, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3357, "text": "You only serve, apart from God, idols and you create a calumny; those you serve, apart from God, have no power to provide for you. So seek after your provision with God, and serve Him, and be thankful to Him; unto Him you shall be returned."}, {"id": 246377, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3358, "text": "'But if you cry me lies, nations cried lies before you; and it is only for the Messenger to deliver the Manifest Message.'"}, {"id": 246378, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3359, "text": "(Have they not seen how God originates creation, then brings it back again? Surely that is an easy matter for God."}, {"id": 246379, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3360, "text": "Say: Journey in the land, then behold how He originated creation; then God causes the second growth to grow; God is powerful over everything,"}, {"id": 246380, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3361, "text": "chastising whom He will, and having mercy on whomsoever He will, and unto Him you shall be turned."}, {"id": 246381, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3362, "text": "You are not able to frustrate Him either in the earth or in heaven; and you have not, apart from God, either protector or helper."}, {"id": 246382, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3363, "text": "And those who disbelieve in God's signs and the encounter with Him -- they despair of My mercy, and there awaits them a painful chastisement.)"}, {"id": 246383, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3364, "text": "But the only answer of his people was that they said, 'Slay him, or burn him!' Then God delivered him from the fire; surely in that are signs for a people who believe."}, {"id": 246384, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3365, "text": "And he said, 'You have only taken to yourselves idols, apart from God, as a mark of mutual love between you in the present life; then upon the Day of Resurrection you will deny one another, and you will curse one another, and your refuge will be the Fire, and you will have no helpers.'"}, {"id": 246385, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3366, "text": "But Lot believed him; and he said, 'I will flee to my Lord; He is the All-mighty, the All-wise.'"}, {"id": 246386, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3367, "text": "And We gave him Isaac and Jacob, and We appointed the Prophecy and the Book to be among his seed; We gave him his wage in this world, and in the world to come he shall be among the righteous."}, {"id": 246387, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3368, "text": "And Lot, when he said to his people 'Surely you commit such indecency as never any being in all the world committed before you."}, {"id": 246388, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3369, "text": "What, do you approach men, and cut the way, and commit in your assembly dishonour?' But the only answer of his people was that they said, 'Then bring us the chastisement of God, if thou speakest truly.'"}, {"id": 246389, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3370, "text": "He said, 'My Lord, help me against the people that work corruption.'"}, {"id": 246390, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3371, "text": "And when Our messengers came to Abraham with the good tidings, they said, 'We shall destroy the people of this city, for its people are evildoers.'"}, {"id": 246391, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3372, "text": "He said, 'Lot is in it.' They said, 'We know very well who is in it; assuredly We shall deliver him and his family, except his wife; she has become of those that tarry.'"}, {"id": 246393, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3373, "text": "When that Our messengers came to Lot he was troubled on their account and distressed for them; but they said, 'Fear not, neither sorrow, for surely we shall deliver thee and thy family, except thy wife; she has become of those that tarry."}, {"id": 246394, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3374, "text": "We shall send down upon the people of this city wrath out of heaven for their ungodliness.'"}, {"id": 246395, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3375, "text": "And indeed, We have left thereof a sign, a clear sign? unto a people who understand."}, {"id": 246396, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3376, "text": "And to Midian their brother Shuaib; he said, 'O my people, serve God, and look you for the Last Day; and do not mischief in the land, working corruption.'"}, {"id": 246397, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3377, "text": "But they cried lies to him; so the earthquake seized them, and morning found them in their habitation fallen prostrate."}, {"id": 246398, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3378, "text": "And Ad, and Thamood -- it has become clear to you from their dwelling-places; and Satan decked out fair to them their works, and barred them from the way, though they saw clearly."}, {"id": 246399, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3379, "text": "And Korah, and Pharaoh, and Haman; Moses came to them with the clear signs, but they waxed proud in the earth, yet they outstripped Us not."}, {"id": 246400, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3380, "text": "Each We seized for his sin; and of them against some We loosed a squall of pebbles and some were seized by the Cry, and some We made the earth to swallow, and some We drowned; God would never wrong them, but they wronged themselves."}, {"id": 246401, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3381, "text": "The likeness of those who have taken to them protectors, apart from God, is as the likeness of the spider that takes to itself a house; and surely the frailest of houses is the house of the spider, did they but know."}, {"id": 246402, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3382, "text": "God knows whatever thing they call upon apart from Him; He is the All-mighty, the All-wise."}, {"id": 246403, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3383, "text": "And those similitudes -- We strike them for the people, but none understands them save those who know."}, {"id": 246404, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3384, "text": "God created the heavens and the earth with the truth; surely in that is a sign to the believers."}, {"id": 246405, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 346, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3385, "text": "Recite what has been revealed to thee of the Book, and perform the prayer; prayer forbids indecency and dishonour. God's remembrance is greater; and God knows the things you work."}, {"id": 246406, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3386, "text": "Dispute not with the People of the Book save in the fairer manner, except for those of them that do wrong; and say, 'We believe in what has been sent down to us, and what has been sent down to you; our God and your God is One, and to Him we have surrendered.'"}, {"id": 246408, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3387, "text": "Even so We have sent down to thee the Book. Those to whom We have given the Book believe in it; and some of these believe in it; and none denies Our signs but the unbelievers."}, {"id": 246409, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3388, "text": "Not before this didst thou recite any Book, or inscribe it with thy right hand, for then those who follow falsehood would have doubted."}, {"id": 246410, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3389, "text": "Nay; rather it is signs, clear signs in the breasts of those who have been given knowledge; and none denies Our signs but the evildoers."}, {"id": 246411, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3390, "text": "They say, 'Why have signs not been gent down upon him from his Lord?' Say: 'The signs are only with God, and I am only a plain warner."}, {"id": 246412, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3391, "text": "What, is it not sufficient for them that We have sent down upon thee the Book that is recited to them? Surely in that is a mercy, and a reminder to a people who believe."}, {"id": 246413, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3392, "text": "Say: 'God suffices as a witness between me and you.' He knows whatsoever is in the heavens and earth. Those who believe in vanity and disbelieve in God -- those, they are the losers."}, {"id": 246414, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3393, "text": "And they demand of thee to hasten the chastisement! But for a stated term the chastisement would have come upon them; but it shall come upon them suddenly, when they are not aware."}, {"id": 246415, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3394, "text": "They demand of thee to hasten the chastisement! Lo, Gehenna encompasses the unbelievers."}, {"id": 246416, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3395, "text": "Upon the day the chastisement shall overwhelm them from above them and from under their feet, and He shall say, 'Taste now what you were doing!'"}, {"id": 246296, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3396, "text": "O My servants who believe, surely My earth is wide; therefore Me do you serve!"}, {"id": 246392, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3397, "text": "Every soul shall taste of death; then unto Us you shall be returned."}, {"id": 246417, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3398, "text": "And those who believe, and do righteous deeds, We shall surely lodge them in lofty chambers of paradise, underneath which rivers flow, therein dwelling forever; and excellent is the wage of those who labour,"}, {"id": 246418, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3399, "text": "such men as are patient, and put their trust in their Lord."}, {"id": 246419, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3400, "text": "How many a beast that bears not its own provision, but God provides for it and you! He is the All-hearer, the All-knower."}, {"id": 246420, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3401, "text": "If thou askest them, 'Who created the heavens and the earth and subjected the sun and the moon?' they will say, 'God.' How then are they perverted?"}, {"id": 246421, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3402, "text": "God outspreads and straitens His provision to whomsoever He will of His servants; God has knowledge of everything."}, {"id": 246422, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3403, "text": "If thou askest them, 'Who sends down out of heaven water, and therewith revives the earth after it is dead?' they will say, 'God.' Say: 'Praise belongs to God.' Nay, but most of them have no understanding."}, {"id": 246423, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3404, "text": "This present life is naught but a diversion and a sport; surely the Last Abode is Life, did they but know."}, {"id": 246424, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3405, "text": "When they embark in the ships, they call on God, making their religion sincerely His; but when He has delivered them to the land, they associate others with Him,"}, {"id": 246425, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3406, "text": "that they may be ungrateful for what We have given them, and take their enjoyment; they will soon know!"}, {"id": 246426, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3407, "text": "Have they not seen that We have appointed a sanctuary secure, while all about them the people are snatched away? What, do they believe in vanity, and do they disbelieve in God's blessing?"}, {"id": 246427, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3408, "text": "And who does greater evil than he who forges against God a lie, or cries lies to the truth when it comes to him? What, is there not in Gehenna a lodging for the unbelievers?"}, {"id": 246428, "identifier": "en.arberry", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 3409, "text": "But those who struggle in Our cause, surely We shall guide them in Our ways; and God is with the good-doers."}]