[{"id": 247435, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4415, "text": "Ha Mim"}, {"id": 247436, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4416, "text": "By the Clear Book."}, {"id": 247437, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4417, "text": "We have sent it down in a blessed night (We are ever warning)"}, {"id": 247438, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4418, "text": "therein every wise bidding"}, {"id": 247439, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4419, "text": "determined as a bidding from Us, (We are ever sending)"}, {"id": 247440, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4420, "text": "as a mercy from thy Lord (surely He is the All-hearing, the All-knowing)"}, {"id": 247441, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4421, "text": "Lord of the heavens and earth, and all that between them is if you have faith."}, {"id": 247442, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4422, "text": "There is no god but He; He gives life and makes to die; your Lord and the Lord of your fathers, the ancients."}, {"id": 247443, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4423, "text": "Nay, but they are in doubt, playing."}, {"id": 247444, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4424, "text": "So be on the watch for a day when heaven shall bring a manifest smoke"}, {"id": 247445, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4425, "text": "covering the people; this is a painful chastisement."}, {"id": 247446, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4426, "text": "'O our Lord, remove Thou from us the chastisement; we are believers.'"}, {"id": 247447, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4427, "text": "How should they have the Reminder, seeing a clear Messenger already came to them,"}, {"id": 247448, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4428, "text": "then they turned away from him and said, 'A man tutored, possessed!'"}, {"id": 247449, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4429, "text": "'Behold, We are removing the chastisement a little; behold, you revert!'"}, {"id": 247450, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4430, "text": "Upon the day when We shall assault most mightily, then We shall take Our vengeance."}, {"id": 247451, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4431, "text": "Already before them We tried the people of Pharaoh, and a noble Messenger came unto them,"}, {"id": 247452, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4432, "text": "saying, 'Deliver to me God's servants; I am for you a faithful Messenger,"}, {"id": 247453, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4433, "text": "and, 'Rise not up against God; behold, I come to you with a clear authority,"}, {"id": 247454, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4434, "text": "and I take refuge in my Lord and your Lord, lest you should stone me."}, {"id": 247455, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4435, "text": "'But if so be that you believe me not, go you apart from me!'"}, {"id": 247456, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4436, "text": "And he called to his Lord, saying, 'These are a sinful people.'"}, {"id": 247457, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4437, "text": "'Then set thou forth with My servants in a watch of the night; surely you will be followed."}, {"id": 247458, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4438, "text": "And leave the sea becalmed; they are a drowned host.'"}, {"id": 247459, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4439, "text": "They left how many gardens and fountains,"}, {"id": 247460, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4440, "text": "sown fields, and how noble a station,"}, {"id": 247461, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4441, "text": "and what prosperity they had rejoiced in!"}, {"id": 247462, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4442, "text": "Even so; and We bequeathed them upon another people."}, {"id": 247463, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4443, "text": "Neither heaven nor earth wept for them, nor were they respited;"}, {"id": 247464, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4444, "text": "and We delivered the Children of Israel from the humbling chastisement,"}, {"id": 247465, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4445, "text": "from Pharaoh; surely he was a high One, of the prodigals;"}, {"id": 247466, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4446, "text": "and We chose them, out of a knowledge, above all beings,"}, {"id": 247467, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4447, "text": "and gave them signs wherein there was a manifest trial."}, {"id": 247468, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4448, "text": "These men do say,"}, {"id": 247469, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4449, "text": "'There is nothing but our first death; we shall not be revived."}, {"id": 247470, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4450, "text": "Bring us our fathers, if you speak truly!'"}, {"id": 247471, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4451, "text": "Are they better, or the people of Tubba' and those before them whom We destroyed? They were surely sinners."}, {"id": 247472, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4452, "text": "We created not the heavens and earth, and all that between them is, in play;"}, {"id": 247473, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4453, "text": "We created them not save in truth; but most of them know it not."}, {"id": 247474, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4454, "text": "Surely the Day of Decision shall be their appointed time, all together,"}, {"id": 247475, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4455, "text": "the day a master shall avail nothing a client, and they shall not be helped,"}, {"id": 247476, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4456, "text": "save him upon whom God has mercy; He is the All-mighty, the All-compassionate."}, {"id": 247477, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4457, "text": "Lo, the Tree of Ez-Zakkoum"}, {"id": 247478, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4458, "text": "is the food of the guilty,"}, {"id": 247086, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4459, "text": "like molten copper, bubbling in the belly"}, {"id": 247479, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4460, "text": "as boiling water bubbles."}, {"id": 247480, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4461, "text": "'Take him, and thrust him into the midst of Hell,"}, {"id": 247481, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4462, "text": "then pour over his head the chastisement of boiling water!'"}, {"id": 247482, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4463, "text": "'Taste! Surely thou art the mighty, the noble."}, {"id": 247483, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4464, "text": "This is that concerning which you were doubting.'"}, {"id": 247484, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4465, "text": "Surely the godfearing shall be in a station secure"}, {"id": 247485, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4466, "text": "among gardens and fountains,"}, {"id": 247486, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4467, "text": "robed in silk and brocade, set face to face."}, {"id": 247487, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4468, "text": "Even so; and We shall espouse them to wide-eyed houris,"}, {"id": 247488, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4469, "text": "therein calling for every fruit, secure."}, {"id": 247489, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4470, "text": "They shall not taste therein of death, save the first death, And He shall guard them against the chastisement of Hell --"}, {"id": 247490, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4471, "text": "a bounty from thy Lord; that is the mighty triumph."}, {"id": 247491, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4472, "text": "Now We have made it easy by thy tongue, that haply they may remember."}, {"id": 247492, "identifier": "en.arberry", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Arberry", "transelator_en_name": "A. J. Arberry", "format": "text", "type": "translation", "ayah_number": 4473, "text": "So be on the watch; they too are on the watch."}]