[{"id": 591295, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5242, "text": "teb\u00e2rake-lle\u1e95\u00ee biyedihi-lm\u00fclk. veh\u00fcve `al\u00e2 k\u00fclli \u015fey'in \u1e33ad\u00eer."}, {"id": 591296, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5243, "text": "elle\u1e95\u00ee \u1e2bale\u1e33a-lmevte vel\u1e25ay\u00e2te liyebl\u00fcvek\u00fcm eyy\u00fck\u00fcm a\u1e25sen\u00fc `amel\u00e2. veh\u00fcve-l`az\u00eez\u00fc-lgaf\u00fbr."}, {"id": 591274, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5244, "text": "elle\u1e95\u00ee \u1e2bale\u1e33a seb`a sem\u00e2v\u00e2tin \u1e6dib\u00e2\u1e33\u00e2. m\u00e2 ter\u00e2 f\u00ee \u1e2bal\u1e33i-rra\u1e25m\u00e2ni min tef\u00e2v\u00fct. ferci`i-lbe\u1e63ara hel ter\u00e2 min f\u00fc\u1e6d\u00fbr."}, {"id": 591297, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5245, "text": "\u015d\u00fcmme-rci`i-lbe\u1e63ara kerrateyni yen\u1e33alib ileyke-lbe\u1e63aru \u1e2b\u00e2siev veh\u00fcve \u1e25as\u00eer."}, {"id": 591298, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5246, "text": "vele\u1e33ad zeyyenne-ssem\u00e2e-dd\u00fcny\u00e2 bime\u1e63\u00e2b\u00ee\u1e25a vece`aln\u00e2h\u00e2 ruc\u00fbmel li\u015f\u015fey\u00e2\u1e6d\u00eeni vea`tedn\u00e2 leh\u00fcm `a\u1e95\u00e2be-sse`\u00eer."}, {"id": 591299, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5247, "text": "velille\u1e95\u00eene kefer\u00fb birabbihim `a\u1e95\u00e2b\u00fc cehennem. vebi'se-lme\u1e63\u00eer."}, {"id": 591300, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5248, "text": "i\u1e95\u00e2 \u00fcl\u1e33\u00fb f\u00eeh\u00e2 semi`\u00fb leh\u00e2 \u015feh\u00ee\u1e33av vehiye tef\u00fbr."}, {"id": 591301, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5249, "text": "tek\u00e2d\u00fc temeyyez\u00fc mine-lgay\u017c. k\u00fcllem\u00e2 \u00fcl\u1e33iye f\u00eeh\u00e2 fevc\u00fcn seeleh\u00fcm \u1e2bazenet\u00fch\u00e2 elem ye'tik\u00fcm ne\u1e95\u00eer."}, {"id": 591302, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5250, "text": "\u1e33\u00e2l\u00fb bel\u00e2 \u1e33ad c\u00e2en\u00e2 ne\u1e95\u00eerun feke\u1e95\u1e95ebn\u00e2 ve\u1e33uln\u00e2 m\u00e2 nezzele-ll\u00e2h\u00fc min \u015fey'. in ent\u00fcm ill\u00e2 f\u00ee \u1e0dal\u00e2lin keb\u00eer."}, {"id": 591303, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5251, "text": "ve\u1e33\u00e2l\u00fb lev k\u00fcnn\u00e2 nesme`u ev na`\u1e33il\u00fc m\u00e2 k\u00fcnn\u00e2 f\u00ee a\u1e63\u1e25\u00e2bi-sse`\u00eer."}, {"id": 591304, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5252, "text": "fa`teraf\u00fb bi\u1e95embihim. fesu\u1e25\u1e33al lia\u1e63\u1e25\u00e2bi-sse`\u00eer."}, {"id": 591305, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5253, "text": "inne-lle\u1e95\u00eene ya\u1e2b\u015fevne rabbeh\u00fcm bilgaybi leh\u00fcm magfirat\u00fcv veecrun keb\u00eer."}, {"id": 591306, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5254, "text": "veesirr\u00fb \u1e33avlek\u00fcm evi-cher\u00fb bih. inneh\u00fb `al\u00eem\u00fcm bi\u1e95\u00e2ti-\u1e63\u1e63ud\u00fbr."}, {"id": 591307, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5255, "text": "el\u00e2 ya`lem\u00fc men \u1e2bale\u1e33. veh\u00fcve-lle\u1e6d\u00eef\u00fc-l\u1e2bab\u00eer."}, {"id": 591308, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5256, "text": "h\u00fcve-lle\u1e95\u00ee ce`ale lek\u00fcm\u00fc-l'ar\u1e0da \u1e95el\u00fblen fem\u015f\u00fb f\u00ee men\u00e2kibih\u00e2 vek\u00fcl\u00fb mir riz\u1e33ih. veileyhi-nn\u00fc\u015f\u00fbr."}, {"id": 591309, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5257, "text": "eemint\u00fcm men fi-ssem\u00e2i ey ya\u1e2bsife bik\u00fcm\u00fc-l'ar\u1e0da fei\u1e95\u00e2 hiye tem\u00fbr."}, {"id": 591310, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5258, "text": "em emint\u00fcm men fi-ssem\u00e2i ey y\u00fcrsile `aleyk\u00fcm \u1e25\u00e2\u1e63ib\u00e2. feseta`lem\u00fbne keyfe ne\u1e95\u00eer."}, {"id": 591311, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5259, "text": "vele\u1e33ad ke\u1e95\u1e95ebe-lle\u1e95\u00eene min \u1e33ablihim fekeyfe k\u00e2ne nek\u00eer."}, {"id": 591312, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5260, "text": "evelem yerav ile-\u1e6d\u1e6dayri fev\u1e33ah\u00fcm \u1e63\u00e2ff\u00e2tiv veya\u1e33bi\u1e0dn. m\u00e2 y\u00fcmsik\u00fch\u00fcnne ille-rra\u1e25m\u00e2n. inneh\u00fb bik\u00fclli \u015fey'im be\u1e63\u00eer."}, {"id": 591313, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5261, "text": "emmen h\u00e2\u1e95e-lle\u1e95\u00ee h\u00fcve c\u00fcnd\u00fcl lek\u00fcm yen\u1e63uruk\u00fcm min d\u00fbni-rra\u1e25m\u00e2n. ini-lk\u00e2fir\u00fbne ill\u00e2 f\u00ee gur\u00fbr."}, {"id": 591288, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5262, "text": "emmen h\u00e2\u1e95e-lle\u1e95\u00ee yerz\u00fc\u1e33uk\u00fcm in emseke riz\u1e33ah. bel lecc\u00fb f\u00ee `ut\u00fcvviv ven\u00fcf\u00fbr."}, {"id": 591314, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5263, "text": "efemey yem\u015f\u00ee m\u00fckibben `al\u00e2 vechih\u00ee ehd\u00e2 emmey yem\u015f\u00ee seviyyen `al\u00e2 \u1e63ir\u00e2\u1e6dim m\u00fcste\u1e33\u00eem."}, {"id": 591315, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5264, "text": "\u1e33ul h\u00fcve-lle\u1e95\u00ee en\u015feek\u00fcm vece`ale lek\u00fcm\u00fc-ssem`a vel'eb\u1e63\u00e2ra vel'ef'ideh. \u1e33al\u00eelem m\u00e2 te\u015fk\u00fcr\u00fbn."}, {"id": 591316, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5265, "text": "\u1e33ul h\u00fcve-lle\u1e95\u00ee \u1e95era'ek\u00fcm fi-l'ar\u1e0di veileyhi tu\u1e25\u015fer\u00fbn."}, {"id": 591317, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5266, "text": "veye\u1e33\u00fbl\u00fbne met\u00e2 h\u00e2\u1e95e-lva`d\u00fc in k\u00fcnt\u00fcm \u1e63\u00e2di\u1e33\u00een."}, {"id": 591318, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5267, "text": "\u1e33ul inneme-l`ilm\u00fc `inde-ll\u00e2h. veinnem\u00e2 ene ne\u1e95\u00eerum m\u00fcb\u00een."}, {"id": 591319, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5268, "text": "felemm\u00e2 raevh\u00fc z\u00fclfeten s\u00eeet vuc\u00fbh\u00fc-lle\u1e95\u00eene kefer\u00fb ve\u1e33\u00eele h\u00e2\u1e95e-lle\u1e95\u00ee k\u00fcnt\u00fcm bih\u00ee tedde`\u00fbn."}, {"id": 591320, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5269, "text": "\u1e33ul era'eyt\u00fcm in ehlekeniye-ll\u00e2h\u00fc vemem me`iye ev ra\u1e25imen\u00e2 femey y\u00fcc\u00eeru-lk\u00e2fir\u00eene min `a\u1e95\u00e2bin el\u00eem."}, {"id": 591321, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5270, "text": "\u1e33ul h\u00fcve-rra\u1e25m\u00e2n\u00fc \u00e2menn\u00e2 bih\u00ee ve`aleyhi tevekkeln\u00e2. feseta`lem\u00fbne men h\u00fcve f\u00ee \u1e0dal\u00e2lim m\u00fcb\u00een."}, {"id": 591322, "identifier": "tr.transliteration", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5271, "text": "\u1e33ul era'eyt\u00fcm in a\u1e63be\u1e25a m\u00e2\u00fck\u00fcm gavran femey ye't\u00eek\u00fcm bim\u00e2im me`\u00een."}]