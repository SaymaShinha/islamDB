3, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5479, "text": "ev zid `aleyhi verattili-l\u1e33ur'\u00e2ne tert\u00eel\u00e2."}, {"id": 591527, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5480, "text": "inn\u00e2 sen\u00fcl\u1e33\u00ee `aleyke \u1e33avlen \u015de\u1e33\u00eel\u00e2."}, {"id": 591528, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5481, "text": "inne n\u00e2\u015fiete-lleyli hiye e\u015fedd\u00fc va\u1e6d'ev vea\u1e33vem\u00fc \u1e33\u00eel\u00e2."}, {"id": 591529, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5482, "text": "inne leke fi-nneh\u00e2ri seb\u1e25an \u1e6dav\u00eel\u00e2."}, {"id": 591530, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5483, "text": "ve\u1e95k\u00fcri-sme rabbike vetebettel ileyhi tebt\u00eel\u00e2."}, {"id": 591531, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5484, "text": "rabb\u00fc-lme\u015fri\u1e33i velmagribi l\u00e2 il\u00e2he ill\u00e2 h\u00fcve fette\u1e2bi\u1e95h\u00fc vek\u00eel\u00e2."}, {"id": 591532, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5485, "text": "va\u1e63bir `al\u00e2 m\u00e2 ye\u1e33\u00fbl\u00fbne vehc\u00fcrh\u00fcm hecran cem\u00eel\u00e2."}, {"id": 591533, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5486, "text": "ve\u1e95ern\u00ee velm\u00fcke\u1e95\u1e95ib\u00eene \u00fcli-nna`meti vemehhilh\u00fcm \u1e33al\u00eel\u00e2."}, {"id": 591534, "identifier": "tr.transliteration", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5487, "text": "inne ledeyn\u00e2 enk\u00e2lev vece\u1e25\