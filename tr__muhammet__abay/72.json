[{"id": 591494, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5448, "text": "\u1e33ul \u00fb\u1e25iye ileyye enneh\u00fc-steme`a neferum mine-lcinni fe\u1e33\u00e2l\u00fb inn\u00e2 semi`n\u00e2 \u1e33ur'\u00e2nen `aceb\u00e2."}, {"id": 591495, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5449, "text": "yehd\u00ee ile-rru\u015fdi fe\u00e2menn\u00e2 bih. velen n\u00fc\u015frike birabbin\u00e2 e\u1e25ad\u00e2."}, {"id": 591496, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5450, "text": "veenneh\u00fb te`\u00e2l\u00e2 cedd\u00fc rabbin\u00e2 me-tte\u1e2ba\u1e95e \u1e63\u00e2\u1e25ibetev vel\u00e2 veled\u00e2."}, {"id": 591497, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5451, "text": "veenneh\u00fb k\u00e2ne ye\u1e33\u00fbl\u00fc sef\u00eeh\u00fcn\u00e2 `ale-ll\u00e2hi \u015fe\u1e6da\u1e6d\u00e2."}, {"id": 591498, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5452, "text": "veenn\u00e2 \u017canenn\u00e2 el len te\u1e33\u00fble-l'ins\u00fc velcinn\u00fc `ale-ll\u00e2hi ke\u1e95ib\u00e2."}, {"id": 591499, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5453, "text": "veenneh\u00fb k\u00e2ne ric\u00e2l\u00fcm mine-l'insi ye`\u00fb\u1e95\u00fbne biric\u00e2lim mine-lcinni fez\u00e2d\u00fbh\u00fcm rahe\u1e33\u00e2."}, {"id": 591500, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5454, "text": "veenneh\u00fcm \u017cann\u00fb kem\u00e2 \u017canent\u00fcm el ley yeb`a\u015de-ll\u00e2h\u00fc e\u1e25ad\u00e2."}, {"id": 591501, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5455, "text": "veenn\u00e2 lemesne-ssem\u00e2e fevecedn\u00e2h\u00e2 m\u00fcliet \u1e25arasen \u015fed\u00eedev ve\u015f\u00fch\u00fcb\u00e2."}, {"id": 591502, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5456, "text": "veenn\u00e2 k\u00fcnn\u00e2 na\u1e33`ud\u00fc minh\u00e2 me\u1e33\u00e2`ide lissem`. femey yestemi`i-l'\u00e2ne yecid leh\u00fb \u015fih\u00e2ber ra\u1e63ad\u00e2."}, {"id": 591503, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5457, "text": "veenn\u00e2 l\u00e2 nedr\u00ee e\u015ferrun \u00fcr\u00eede bimen fi-l'ar\u1e0di em er\u00e2de bihim rabb\u00fch\u00fcm ra\u015fed\u00e2."}, {"id": 591504, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5458, "text": "veenn\u00e2 minne-\u1e63\u1e63\u00e2li\u1e25\u00fbne veminn\u00e2 d\u00fbne \u1e95\u00e2lik. k\u00fcnn\u00e2 \u1e6dar\u00e2i\u1e33a \u1e33ided\u00e2."}, {"id": 591505, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5459, "text": "veenn\u00e2 \u017canenn\u00e2 el len n\u00fc`cize-ll\u00e2he fi-l'ar\u1e0di velen n\u00fc`cizeh\u00fb herab\u00e2."}, {"id": 591506, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5460, "text": "veenn\u00e2 lemm\u00e2 semi`ne-lh\u00fcd\u00e2 \u00e2menn\u00e2 bih. femey y\u00fc'mim birabbih\u00ee fel\u00e2 ye\u1e2b\u00e2f\u00fc ba\u1e2bsev vel\u00e2 rahe\u1e33\u00e2."}, {"id": 591507, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00c7eviriyaz\u0131", "transelator_en_name": "Muhammet Abay", "format": "text", "type": "transliteration", "ayah_number": 5461, "text": "veenn\u00e2 minne-lm\u00fcslim\u00fbne veminne-l\u1e33\u00e2si\u1e6d\u00fbn. femen esleme fe\u00fcl\u00e2ike te\u1e25arrav ra\u015fed\u00e2."}, {"id": 591508, "identifier": "tr.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_typ