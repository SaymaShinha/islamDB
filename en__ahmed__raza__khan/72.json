[{"id": 192380, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5448, "text": "Proclaim (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cI have received the divine revelation that some jinns attentively listened to my recitation, so they said, \u2018We have heard a unique Qur\u2019an.\u2019"}, {"id": 192381, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5449, "text": "\u2018That guides to the path of goodness, we have therefore accepted faith in it; and we shall never ascribe anyone as a partner to our Lord.\u2019"}, {"id": 192382, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5450, "text": "\u2018And that our Lord\u2019s Majesty is Supreme \u2013 He has neither chosen a wife nor a child.\u2019"}, {"id": 192383, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5451, "text": "\u2018And that the fool among us used to utter false exaggerations against Allah.\u2019"}, {"id": 192384, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5452, "text": "\u2018Whereas we thought that men and jinns would never fabricate a lie against Allah!\u2019"}, {"id": 192385, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5453, "text": "\u2018And indeed some men among humans used to take the protection of some men among jinns, so it further increased their haughtiness.\u2019"}, {"id": 192386, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5454, "text": "\u2018And that they assumed, like you humans assume, that Allah would not send any Noble Messenger.\u2019"}, {"id": 192387, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5455, "text": "\u2018And we reached the sky, so we found it strongly guarded and filled with comets.\u2019"}, {"id": 192388, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5456, "text": "\u2018And that we sometimes used to sit in some places in the sky, to listen; so whoever now listens finds a fiery comet waiting for him.\u2019"}, {"id": 192389, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5457, "text": "\u2018And we do not know whether harm is intended for those on the earth, or whether their Lord intends goodness for them.\u2019"}, {"id": 192390, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5458, "text": "\u2018And among us some are virtuous and some are the other type; we are split into several branches.\u2019"}, {"id": 192391, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5459, "text": "\u2018And we are certain that we cannot defeat Allah in the earth, nor can we run out of His grasp.\u2019"}, {"id": 192392, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5460, "text": "\u2018And that when we heard the guidance, we accepted faith in it; so whoever accepts faith in his Lord, has no fear \u2013 neither of any loss nor of any injustice.\u2019"}, {"id": 192393, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5461, "text": "\u2018And that some among us are Muslims and some are the unjust; and whoever has accepted Islam \u2013 it is they who have thought rightly.\u2019"}, {"id": 192394, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5462, "text": "\u2018And as for the unjust \u2013 they are the fuel of hell.\u2019\u201d"}, {"id": 192395, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5463, "text": "And proclaim (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cI have received the divine revelation that \u2018Had they remained upright on the straight path, We would have given them abundant water.\u2019"}, {"id": 192396, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5464, "text": "\u2018In order to test them with it; and whoever turns away from the remembrance of his Lord \u2013 He will put him in a punishment that keeps on increasing.\u2019"}, {"id": 192054, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5465, "text": "\u2018And that the mosques are for Allah only \u2013 therefore do not worship anyone along with Allah.\u2019"}, {"id": 192397, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5466, "text": "\u2018And that when Allah\u2019s bondman stood up to worship Him, the jinns had almost crowded upon him.\u2019\u201d"}, {"id": 192398, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5467, "text": "Say (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cI worship only Allah, and I do not ascribe any partner to Him.\u201d"}, {"id": 192399, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5468, "text": "Proclaim (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cI am not the master of any harm or benefit for you.\u201d"}, {"id": 192400, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5469, "text": "Say, \u201cNo one will ever save me from Allah, and other than Him, I will not find any refuge.\u201d"}, {"id": 192401, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5470, "text": "\u201cI only convey the commands from Allah, and His messages; and whoever disobeys the commands of Allah and His Noble Messenger \u2013 then indeed for him is the fire of hell, in which they will remain for ever and ever.\u201d"}, {"id": 192402, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5471, "text": "Till the time when they will see what they are promised \u2013 so they will now come to know whose aide is weak, and who is lesser in number."}, {"id": 192403, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5472, "text": "Say (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cI do not know whether what you are promised is close or if my Lord will postpone it for a while.\u201d"}, {"id": 192379, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5473, "text": "The All Knowing of all the hidden does not give anyone the control over His secrets."}, {"id": 192404, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5474, "text": "Except to His chosen Noble Messengers \u2013 so He appoints guards in front and behind him. (Allah gave the knowledge of the hidden to the Holy Prophet \u2013 peace and blessings be upon him.)"}, {"id": 192405, "identifier": "en.ahmedraza", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5475, "text": "In order to see that they have conveyed the messages of their Lord \u2013 and His knowledge encompasses all whatever they have, and He has kept all things accounted for."}]