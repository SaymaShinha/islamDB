[{"id": 192406, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5476, "text": "O the One Wrapped in piety! (Prophet Mohammed \u2013 peace and blessings be upon him)"}, {"id": 192407, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5477, "text": "Stand up for worship during the night, except for some part of it."}, {"id": 192408, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5478, "text": "For half the night, or reduce some from it."}, {"id": 192409, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5479, "text": "Or increase a little upon it, and recite the Qur\u2019an slowly in stages."}, {"id": 192410, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5480, "text": "Indeed We shall soon ordain a heavy responsibility upon you."}, {"id": 192411, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5481, "text": "Indeed getting up in the night is tougher, and the words flow with strength."}, {"id": 192412, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5482, "text": "Indeed you have a lot of matters during the day."}, {"id": 192413, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5483, "text": "And remember the name of your Lord and, leaving others, devote yourself solely to Him."}, {"id": 192414, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5484, "text": "Lord of the East and Lord of the West \u2013 there is no God except Him, therefore make Him your sole Trustee of affairs."}, {"id": 192415, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5485, "text": "And patiently endure upon what the disbelievers say, and leave them for good."}, {"id": 192416, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5486, "text": "And leave them to Me \u2013 these wealthy deniers \u2013 and give them some respite."}, {"id": 192417, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5487, "text": "Indeed We have heavy fetters and a blazing fire."}, {"id": 192418, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5488, "text": "And food that chokes, and a painful punishment."}, {"id": 192419, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5489, "text": "On a day when the earth and the mountains will tremble, and the mountains turn into dunes of flowing sand."}, {"id": 192420, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5490, "text": "We have indeed sent a Noble Messenger towards you, a present witness over you \u2013 the way We had sent a Noble Messenger towards Firaun."}, {"id": 192421, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5491, "text": "In response Firaun disobeyed the Noble Messenger, so We seized him with a severe seizure."}, {"id": 192422, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5492, "text": "So how will you save yourselves, if you disbelieve, on a day that will turn children old?"}, {"id": 192424, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5493, "text": "The heaven will split asunder with its grief; the promise of Allah will surely occur."}, {"id": 192425, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5494, "text": "This is indeed an advice; so whoever wishes may take the path towards his Lord."}, {"id": 192426, "identifier": "en.ahmedraza", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 509, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5495, "text": "Indeed your Lord knows that you stand up in prayer, sometimes almost two-thirds of the night, and sometimes half the night or sometimes a third of it \u2013 and also a group of those along with you; Allah keeps measure of the night and day; He knows that you, O Muslims, will not be able to measure the night, so He has inclined towards you with mercy \u2013 therefore recite from the Qur\u2019an as much as is easy for you; He knows that soon some of you will fall ill, and some will travel in the land seeking the munificence of Allah, and some will be fighting in Allah\u2019s cause; therefore recite from the Qur\u2019an as much as is easy for you, and establish prayer and pay the obligatory charity, and lend an excellent loan to Allah; and whatever good you send ahead for yourselves, you will find it with Allah, better and having a great reward; and seek forgiveness from Allah; indeed Allah is Oft Forgiving, Most Merciful."}]