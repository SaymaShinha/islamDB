[{"id": 192427, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5496, "text": "O the Cloaked One! (Prophet Mohammed \u2013 peace and blessings be upon him)"}, {"id": 192428, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5497, "text": "Rise up and warn!"}, {"id": 192429, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5498, "text": "And proclaim the Purity of your Lord."}, {"id": 192430, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5499, "text": "And keep your clothes clean."}, {"id": 192431, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5500, "text": "And stay away from idols."}, {"id": 192432, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5501, "text": "And do not favour others in order to receive more."}, {"id": 192433, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5502, "text": "And for the sake of your Lord, patiently endure."}, {"id": 192434, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5503, "text": "So when the Trumpet will be blown."}, {"id": 192435, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5504, "text": "So that is a tough day."}, {"id": 192436, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5505, "text": "Not easy upon the disbelievers."}, {"id": 192437, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5506, "text": "Leave him to Me, the one whom I created single."}, {"id": 192438, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5507, "text": "And gave him vast wealth."}, {"id": 192439, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5508, "text": "And gave him sons present before him."}, {"id": 192440, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5509, "text": "And made several preparations for him."}, {"id": 192441, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5510, "text": "Yet he desires that I should give more."}, {"id": 192442, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5511, "text": "Never! For he is an enemy to Our signs!"}, {"id": 192443, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5512, "text": "Soon I shall mount him on Saood, the hill of fire."}, {"id": 192444, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5513, "text": "Indeed he thought, and inwardly decided."}, {"id": 192445, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5514, "text": "So accursed be he, how evilly did he decide!"}, {"id": 192446, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5515, "text": "Again accursed be he, how evilly did he decide!"}, {"id": 192447, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5516, "text": "He then dared to lift his gaze."}, {"id": 192448, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5517, "text": "Then frowned and grimaced."}, {"id": 192449, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5518, "text": "Then he turned away, and was haughty."}, {"id": 192450, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5519, "text": "And said, \u201cThis is nothing but magic learnt from earlier men.\u201d"}, {"id": 192451, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5520, "text": "\u201cThis is nothing but the speech of a man.\u201d"}, {"id": 192452, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5521, "text": "I will soon fling him into hell."}, {"id": 192453, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5522, "text": "And what have you understood, what hell is!"}, {"id": 192454, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5523, "text": "It neither leaves, nor spares."}, {"id": 192455, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5524, "text": "It strips away the hide of man."}, {"id": 192456, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5525, "text": "Above it are nineteen guards."}, {"id": 192457, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5526, "text": "We have not appointed the guards of hell, except angels; and did not keep this number except to test the disbelievers \u2013 in order that the People given the Book(s) may be convinced, and to increase the faith of the believers \u2013 and so that the People given the Book(s) and the Muslims may not have any doubt \u2013 and so that those in whose hearts is a disease and the disbelievers, may say, \u201cWhat does Allah mean by this amazing example?\u201d This is how Allah sends astray whomever He wills, and guides whomever He wills; and no one knows the armies of Allah except Him; and this is not but an advice to man."}, {"id": 192458, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5527, "text": "Yes, never!* By oath of the moon. (Hell will never spare the disbelievers)."}, {"id": 192459, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5528, "text": "And by oath of the night when it turns back."}, {"id": 192460, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5529, "text": "And by oath of the morning, when it spreads light."}, {"id": 192461, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5530, "text": "Indeed hell is one of the greatest entities."}, {"id": 192462, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5531, "text": "Warn the men."}, {"id": 192463, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5532, "text": "For the one among you who wishes to come forward or stay back."}, {"id": 192464, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5533, "text": "Every soul is mortgaged for its own deeds."}, {"id": 192465, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5534, "text": "Except those on the right side."}, {"id": 192466, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5535, "text": "In Gardens, they seek answers,"}, {"id": 192467, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5536, "text": "- From the guilty."}, {"id": 192468, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5537, "text": "\u201cWhat took you into the hell?\u201d"}, {"id": 192469, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5538, "text": "They said, \u201cWe never used to offer the prayer.\u201d"}, {"id": 192470, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5539, "text": "\u201cNor used to feed the needy.\u201d"}, {"id": 192471, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5540, "text": "\u201cAnd used to dwell on evil matters with those who think evilly.\u201d"}, {"id": 192472, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5541, "text": "\u201cAnd used to deny the Day of Justice.\u201d"}, {"id": 192473, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5542, "text": "\u201cTill death overcame us.\u201d"}, {"id": 192474, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5543, "text": "So the intercession of the intercessors will not benefit them. (The disbelievers will not have any intercessor.)"}, {"id": 192475, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5544, "text": "So what is the matter with them that they turn away from the advice?"}, {"id": 192476, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5545, "text": "As if they were startled donkeys \u2013"}, {"id": 192477, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5546, "text": "Fleeing away from a lion."}, {"id": 192478, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5547, "text": "Rather each one of them desires that he should be given open Books."}, {"id": 192479, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5548, "text": "Never! In fact they do not fear the Hereafter."}, {"id": 192480, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5549, "text": "Yes indeed, this is an advice."}, {"id": 192481, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5550, "text": "So whoever wills may heed advice from it."}, {"id": 192482, "identifier": "en.ahmedraza", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5551, "text": "And what advice will they heed, except if Allah wills? Only He deserves to be feared, and His only is the greatness of forgiving."}]