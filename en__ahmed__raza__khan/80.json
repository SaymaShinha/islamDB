[{"id": 192689, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5759, "text": "He frowned and turned away."}, {"id": 192690, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5760, "text": "Because the blind man had come in his august presence."}, {"id": 192691, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5761, "text": "And what do you know, he may be of the pure!"}, {"id": 192692, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5762, "text": "Or that he may accept advice, so the advice may benefit him."}, {"id": 192693, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5763, "text": "For him who does not care,"}, {"id": 192694, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5764, "text": "So you are after him!"}, {"id": 192695, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5765, "text": "And you have nothing to lose if he does not become pure."}, {"id": 192696, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5766, "text": "And for him who came to you striving,"}, {"id": 192697, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5767, "text": "And whereas he fears,"}, {"id": 192698, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5768, "text": "So you leave him, and are engrossed elsewhere!"}, {"id": 192699, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5769, "text": "Not this way \u2013 this is the advice."}, {"id": 192638, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5770, "text": "So whoever wishes may remember it."}, {"id": 188519, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5771, "text": "On honourable pages."}, {"id": 189153, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5772, "text": "Exalted, pure."}, {"id": 189487, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5773, "text": "Written by the hands of emissaries."}, {"id": 189968, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5774, "text": "Who are noble, virtuous."}, {"id": 190089, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5775, "text": "May man be slain \u2013 how ungrateful he is!"}, {"id": 190113, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5776, "text": "From what did He create him?"}, {"id": 192700, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5777, "text": "From a drop of liquid; He created him and then set several measures for him."}, {"id": 192701, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5778, "text": "Then eased the way for him."}, {"id": 192702, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5779, "text": "Then gave him death, so had him put in the grave."}, {"id": 192703, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5780, "text": "Then, when He willed, He brought him out. (As during the night of Holy Prophet\u2019s ascension, when all the Prophets gathered behind him in the Al Aqsa mosque in Jerusalem. Or when Allah will raise everyone on the Day of Resurrection.)"}, {"id": 192704, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5781, "text": "Not one \u2013 he has not yet completed what he was commanded."}, {"id": 188390, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5782, "text": "So man must look at his food."}, {"id": 188411, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5783, "text": "That We watered it in abundance."}, {"id": 188693, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5784, "text": "Then We split the earth properly."}, {"id": 188765, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5785, "text": "Thereby produced grain in it."}, {"id": 188960, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5786, "text": "And grapes and fodder,"}, {"id": 189070, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5787, "text": "And olives and date palms,"}, {"id": 190096, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5788, "text": "And dense gardens,"}, {"id": 190169, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5789, "text": "And fruits and grass,"}, {"id": 190348, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5790, "text": "In order to benefit you and your cattle."}, {"id": 190484, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5791, "text": "So when the deafening Shout arrives,"}, {"id": 190766, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5792, "text": "On that day man will run away from his brother."}, {"id": 192350, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5793, "text": "And from his mother and father,"}, {"id": 192705, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5794, "text": "And from his wife and sons."}, {"id": 192706, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5795, "text": "On that day, each one has just one issue, which is enough for him."}, {"id": 192707, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5796, "text": "Many a face will be glittering on that day."}, {"id": 192708, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5797, "text": "Laughing, rejoicing."}, {"id": 192709, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5798, "text": "And many a face will be covered with dust on that day."}, {"id": 192710, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5799, "text": "Blackness overcoming them."}, {"id": 192711, "identifier": "en.ahmedraza", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 5800, "text": "It is they, the disbelievers, the sinners."}]