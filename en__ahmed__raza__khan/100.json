[{"id": 193057, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6147, "text": "By oath of those that sprint, breathing heavily. (The horses used in Holy War.)"}, {"id": 193058, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6148, "text": "Striking stones with their hooves, sparking fire."}, {"id": 193059, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6149, "text": "And by oath of those who raid at dawn."}, {"id": 193060, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6150, "text": "So thereupon raising dust."}, {"id": 193061, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6151, "text": "Then penetrate to the centre of the enemy army."}, {"id": 193062, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6152, "text": "Indeed man is very ungrateful towards his Lord."}, {"id": 193063, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6153, "text": "And indeed he himself is a witness to it."}, {"id": 193064, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6154, "text": "And indeed he loves wealth to the extreme."}, {"id": 193065, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6155, "text": "So does he not know? When those in the graves are raised,"}, {"id": 193066, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6156, "text": "And all what lies in the breasts is opened \u2013"}, {"id": 193067, "identifier": "en.ahmedraza", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6157, "text": "On that day their Lord surely knows all about them!"}]