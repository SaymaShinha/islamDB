[{"id": 193127, "identifier": "en.ahmedraza", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6217, "text": "May both the hands of Abu Lahab be destroyed \u2013 and they are destroyed!"}, {"id": 193128, "identifier": "en.ahmedraza", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6218, "text": "His wealth did not benefit him in the least, nor did whatever he earned."}, {"id": 193129, "identifier": "en.ahmedraza", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6219, "text": "He will soon enter the flaming fire."}, {"id": 193130, "identifier": "en.ahmedraza", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0