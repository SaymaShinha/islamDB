[{"id": 191405, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4474, "text": "Ha; Meem. (Alphabets of the Arabic language \u2013 Allah, and to whomever He reveals, know their precise meanings.)"}, {"id": 191406, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4475, "text": "The sending down of the Book is from Allah, the Most Honourable, the Wise."}, {"id": 191407, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4476, "text": "Indeed in the heavens and the earth are signs for believers."}, {"id": 191408, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4477, "text": "And in your creation, and all the creatures He scatters in the earth \u2013 in them are signs for the people who are certain."}, {"id": 191409, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4478, "text": "And in the alternation of night and day, and in the means of sustenance \u2013 water \u2013 which Allah sends down from the sky whereby He gave life to the earth after its death, and in the movement of the winds \u2013 (in all these) are signs for people of intellect."}, {"id": 191410, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4479, "text": "These are the verses of Allah which We recite to you with the truth; so forsaking Allah and His signs, what will they believe in?"}, {"id": 191411, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4480, "text": "Ruin is for every great slanderer, excessive sinner."}, {"id": 191412, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4481, "text": "Who hears the verses of Allah which are recited to him, then remains stubborn, proud, as if he did not hear them; therefore give him the glad tidings of a painful punishment."}, {"id": 191413, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4482, "text": "And when he comes to know about any of the verses, he mocks at it; for them is a disgraceful punishment."}, {"id": 191414, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4483, "text": "Hell is after them; and what they have earned will not benefit them at all, nor those whom they have chosen as supporters besides Allah; and for them is a terrible punishment."}, {"id": 191415, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4484, "text": "This is the (true) guidance; and for those who disbelieve in the verses of their Lord is the severest of painful punishments."}, {"id": 191416, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4485, "text": "It is Allah Who has subjected the sea for you so that ships may sail upon it by His command, and for you to seek His munificence, and so that you may give thanks."}, {"id": 191417, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4486, "text": "And has subjected for you all whatever is in the heavens and in the earth, by His command; indeed in this are signs for people who ponder."}, {"id": 191418, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4487, "text": "Tell the believers to ignore those who do not expect the days of Allah, in order that Allah may give a nation the reward of what they used to earn."}, {"id": 191419, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4488, "text": "Whoever does a good deed, so it is for his own good; and whoever commits evil, does for his own harm; and you will then be returned towards your Lord."}, {"id": 191420, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4489, "text": "And indeed We gave the Descendants of Israel the Book, and the rule, and the Prophethood, and gave good things for sustenance and gave them superiority over all others of their time."}, {"id": 191421, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4490, "text": "And We gave them clear proofs in the command; so they did not differ except after the knowledge had come to them \u2013 due to jealousy among themselves; indeed your Lord will judge between them on the Day of Resurrection concerning the matter in which they differ."}, {"id": 191422, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4491, "text": "We then placed you (O dear prophet Mohammed \u2013 peace and blessings be upon him) upon the clear path of the command, therefore continue following it and do not listen to the desires of the ignorant."}, {"id": 191423, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4492, "text": "Indeed they cannot benefit you at all against Allah; and indeed the unjust are the friends of each other; and Allah is the Friend of the pious."}, {"id": 191424, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4493, "text": "This is an enlightenment for mankind, and a guidance and mercy for the people of faith."}, {"id": 191425, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4494, "text": "Do those who commit evil assume that We will make them equal to those who believe and do good deeds, therefore both becoming equal in life and death? What an evil judgement they impose!"}, {"id": 191426, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4495, "text": "And Allah has created the heavens and the earth with the truth, and so that every soul is repaid for what it has earned, and they will not be wronged."}, {"id": 191427, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4496, "text": "Just look at him who makes his desire as his God, and Allah has sent him astray despite his having knowledge, and set a seal upon his ears and his heart, and a covering upon his eyes; so who will guide him after Allah? So do you not ponder?"}, {"id": 191428, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4497, "text": "And they said, \u201cThere is nothing except our life of this world \u2013 we die and we live \u2013 and nothing destroys us except the passage of time\u201d; and they do not have any knowledge of it; they only make guesses."}, {"id": 191429, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4498, "text": "And when Our clear verses are recited to them, their only argument is, \u201cBring back our forefathers, if you are truthful!\u201d"}, {"id": 191430, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4499, "text": "Proclaim (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cIt is Allah Who gives you life, then will give you death, then will gather you all on the Day of Resurrection in which there is no doubt, but most people do not know.\u201d"}, {"id": 191431, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4500, "text": "And to Allah only belongs the kingship of the heavens and the earth; and when the Last Day is established \u2013 on that day the followers of falsehood will suffer loss."}, {"id": 191432, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4501, "text": "And you will see every group down on its knees; every group will be called towards its book of deeds; \u201cThis day you will be repaid for what you did.\u201d"}, {"id": 191433, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4502, "text": "\u201cThis Book of Ours speaks against you with the truth; indeed We had been recording all what you did.\u201d"}, {"id": 191434, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4503, "text": "So those who believed and did good deeds \u2013 their Lord will admit them into His mercy; this is the clear triumph."}, {"id": 191435, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4504, "text": "And to those who disbelieved, it will be said \u201cWas it not that Our verses were recited to you? In response you were haughty and were a guilty people.\u201d"}, {"id": 191436, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4505, "text": "\u201cAnd when it was said, \u2018Indeed Allah\u2019s promise is true and there is no doubt regarding the Last Day\u201d, you used to say, \u2018We do not know what the Last Day is \u2013 we think it is nothing except an imagination, and we are not convinced.\u2019\u201d"}, {"id": 191437, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4506, "text": "And the evils of their deeds appeared to them, and the punishment they used to mock at encompassed them."}, {"id": 191438, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4507, "text": "And it will be said, \u201cThis day We shall forsake you, the way you had forgotten the confronting of this day of yours \u2013 and your destination is the fire, and you do not have any supporters.\u201d"}, {"id": 191439, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4508, "text": "\u201cThis is because you mocked at the signs of Allah, and the worldly life deceived you\u201d; so this day they are not to be removed from the fire, nor will anyone seek amends from them."}, {"id": 191440, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4509, "text": "So all praise is for Allah only, the Lord of the heavens and the Lord of the earth, the Lord of the Creation!"}, {"id": 191441, "identifier": "en.ahmedraza", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 4510, "text": "And for Him only is the Greatness, in the heavens and in the earth; and He only is the Most Honourable, the Wise."}]