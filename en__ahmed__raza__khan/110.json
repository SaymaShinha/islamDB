[{"id": 193124, "identifier": "en.ahmedraza", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6214, "text": "When the help and victory of Allah come,"}, {"id": 193125, "identifier": "en.ahmedraza", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6215, "text": "And you see people entering the religion of Allah in multitudes,"}, {"id": 193126, "identifier": "en.ahmedraza", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6216, "text": "Then proclaim the Purity of your Lord while praising Him, and seek forgiveness from Him; indeed He is the Most Acceptor of Repentance."}]