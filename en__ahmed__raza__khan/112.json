[{"id": 193132, "identifier": "en.ahmedraza", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6222, "text": "Proclaim (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cHe is Allah, He is One.\u201d"}, {"id": 193133, "identifier": "en.ahmedraza", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6223, "text": "\u201cAllah is the Un-wanting.\u201d (Perfect, does not require anything.)"}, {"id": 193134, "identifier": "en.ahmedraza", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6224, "text": "\u201cHe has no offspring, nor is He born from anything.\u201d"}, {"id": 193135, "identifier": "en.ahmedraza", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6225, "text": "\u201cAnd there is none equal to Him.\u201d"}]