[{"id": 193136, "identifier": "en.ahmedraza", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6226, "text": "Proclaim (O dear Prophet Mohammed \u2013 peace and blessings be upon him), \u201cI take refuge of the One Who creates the Daybreak.\u201d"}, {"id": 193137, "identifier": "en.ahmedraza", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6227, "text": "\u201cFrom the evil of His entire creation.\u201d"}, {"id": 193138, "identifier": "en.ahmedraza", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6228, "text": "\u201cAnd from the evil of the matter that darkens when it sets.\u201d"}, {"id": 193139, "identifier": "en.ahmedraza", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6229, "text": "\u201cAnd from the evil of the witches who blow into knots.\u201d"}, {"id": 193140, "identifier": "en.ahmedraza", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6230, "text": "\u201cAnd from the evil of the envier when he is envious of me.\u201d"}]