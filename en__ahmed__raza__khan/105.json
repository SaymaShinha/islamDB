[{"id": 193099, "identifier": "en.ahmedraza", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6189, "text": "O dear Prophet (Mohammed \u2013 peace and blessings be upon him), did you not see how did your Lord deal with the People of the Elephant?"}, {"id": 193100, "identifier": "en.ahmedraza", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6190, "text": "Did He not put their scheme into ruin?"}, {"id": 193101, "identifier": "en.ahmedraza", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6191, "text": "And send flocks of birds upon them,"}, {"id": 193102, "identifier": "en.ahmedraza", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\