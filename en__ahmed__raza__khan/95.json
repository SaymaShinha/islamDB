[{"id": 193009, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6099, "text": "By oath of the fig and of the olive,"}, {"id": 193010, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6100, "text": "And by oath of Mount Sinai,"}, {"id": 193011, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6101, "text": "And by oath of this secure land,"}, {"id": 193012, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6102, "text": "We have indeed created man in the best shape."}, {"id": 193013, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6103, "text": "We then turned him towards all the lowest of the low states."}, {"id": 193014, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6104, "text": "Except those who accepted faith and did good deeds \u2013 for them is a never ending reward."}, {"id": 193015, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6105, "text": "So after this, what causes you to deny the judgement?"}, {"id": 193016, "identifier": "en.ahmedraza", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Ahmed Raza Khan", "transelator_en_name": "Ahmed Raza Khan", "format": "text", "type": "translation", "ayah_number": 6106, "text": "Is not Allah the Greatest Judge upon all judges?"}]