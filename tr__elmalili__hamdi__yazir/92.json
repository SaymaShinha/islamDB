[{"id": 585843, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6059, "text": "\u00d6rtt\u00fc\u011f\u00fc zaman geceye,"}, {"id": 585844, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6060, "text": "A\u00e7\u0131ld\u0131\u011f\u0131 zaman g\u00fcnd\u00fcze,"}, {"id": 585845, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6061, "text": "Erke\u011fi ve di\u015fiyi yaratana and olsun ki,"}, {"id": 585846, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Ger\u00e7ekten sizin i\u015finiz ba\u015fka ba\u015fkad\u0131r."}, {"id": 585847, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6063, "text": "Bundan b\u00f6yle her kim mal\u0131n\u0131 hay\u0131r i\u00e7in verir ve korunursa,"}, {"id": 585848, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6064, "text": "Ve en g\u00fczel olan\u0131 do\u011frularsa,"}, {"id": 585849, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6065, "text": "Biz onu en kolay yola muvaffak k\u0131laca\u011f\u0131z."}, {"id": 585850, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6066, "text": "Kim de cimrilik eder ve kendini hi\u00e7bir \u015feye ihtiyac\u0131 kalmam\u0131\u015f g\u00f6r\u00fcr."}, {"id": 585851, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6067, "text": "Ve en g\u00fczeli de yalanlarsa,"}, {"id": 585852, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6068, "text": "Onu da en zor yola haz\u0131rlar\u0131z."}, {"id": 585853, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6069, "text": "\u00c7ukura yuvarland\u0131\u011f\u0131 zaman mal\u0131 onu kurtaramayacak."}, {"id": 585854, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6070, "text": "Do\u011fru yolu g\u00f6stermek muhakkak bize aittir."}, {"id": 585855, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6071, "text": "Ku\u015fkusuz ahiret de d\u00fcnya da bizimdir."}, {"id": 585856, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6072, "text": "Ben sizi k\u00f6p\u00fcrd\u00fck\u00e7e k\u00f6p\u00fcren bir ate\u015fe kar\u015f\u0131 uyard\u0131m."}, {"id": 585857, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6073, "text": "Ona ancak en azg\u0131n olan girer."}, {"id": 585858, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6074, "text": "\u00d6yle azg\u0131n ki, yalanlam\u0131\u015f ve s\u0131rt\u0131n\u0131 d\u00f6nm\u00fc\u015ft\u00fcr."}, {"id": 585859, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6075, "text": "En \u00e7ok korunan ise ondan uzakla\u015ft\u0131r\u0131lacakt\u0131r."}, {"id": 585860, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6076, "text": "O ki, Allah yolunda mal\u0131n\u0131 verir, temizlenir."}, {"id": 585861, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6077, "text": "Onun yan\u0131nda, ba\u015fka bir kimse i\u00e7in kar\u015f\u0131l\u0131\u011f\u0131 verilecek hi\u00e7bir nimet yoktur."}, {"id": 585862, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6078, "text": "O ancak y\u00fcce Rabbinin r\u0131zas\u0131n\u0131 aramak i\u00e7in verir."}, {"id": 585863, "identifier": "tr.yazir", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6079, "text": "Elbette yak\u0131nda kendisi de ho\u015fnut olacakt\u0131r."}]