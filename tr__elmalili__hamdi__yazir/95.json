, "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6105, "text": "O halde sana dini ne yalanlat\u0131r?"}, {"id": 585890, "identifier": "tr.yazir", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6106, "text": "Allah, hakimlerin hakimi de\u011fil mi?"}]