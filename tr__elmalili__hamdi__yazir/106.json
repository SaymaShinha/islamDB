[{"id": 585978, "identifier": "tr.yazir", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6194, "text": "Kurey\u015f'in il\u00e2f\u0131 (g\u00fcven ve bar\u0131\u015f andla\u015fmalar\u0131ndan faydalanmalar\u0131n\u0131 sa\u011flamak) i\u00e7in."}, {"id": 585979, "identifier": "tr.yazir", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6195, "text": "K\u0131\u015f ve yaz seferlerinde (faydaland\u0131klar\u0131 andla\u015fmalar\u0131n) kadrini bilmi\u015f olmalar\u0131 i\u00e7in."}, {"id": 585980, "identifier": "tr.yazir", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6196, "text": "Bu Beyt (K\u0