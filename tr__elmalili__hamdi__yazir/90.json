[{"id": 585808, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Andolsun bu beldeye"}, {"id": 585809, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6025, "text": "Ki sen bu beldede oturmaktas\u0131n."}, {"id": 585810, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6026, "text": "Ve and olsun baba ve \u00e7ocu\u011funa."}, {"id": 585811, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Biz insan\u0131 ger\u00e7ekten bir s\u0131k\u0131nt\u0131 i\u00e7inde yaratt\u0131k."}, {"id": 585812, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6028, "text": "\u0130nsan, kendisine kar\u015f\u0131 kimse g\u00fc\u00e7 yetiremez mi san\u0131yor?"}, {"id": 585813, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6029, "text": "Ben, y\u0131\u011f\u0131n y\u0131\u011f\u0131n mal yok ettim diyor."}, {"id": 585814, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6030, "text": "Kendisini bir g\u00f6ren olmad\u0131 m\u0131 san\u0131yor?"}, {"id": 585815, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Biz ona iki g\u00f6z vermedik mi?"}, {"id": 585816, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6032, "text": "Bir dil ve iki dudak?"}, {"id": 585817, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6033, "text": "Ona iki yolu g\u00f6sterdik."}, {"id": 585818, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Fakat o, o sarp yoku\u015fa g\u00f6\u011f\u00fcs veremedi."}, {"id": 585819, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6035, "text": "Bildin mi sen, o sarp yoku\u015f nedir?"}, {"id": 585820, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6036, "text": "K\u00f6le azat etmek,"}, {"id": 585821, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6037, "text": "Veya salg\u0131n bir k\u0131tl\u0131k g\u00fcn\u00fcnde yemek yedirmektir,"}, {"id": 585822, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6038, "text": "Yak\u0131nl\u0131\u011f\u0131 olan bir yetime,"}, {"id": 585823, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6039, "text": "Veya hi\u00e7bir \u015feyi olmayan yoksula."}, {"id": 585824, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6040, "text": "Sonra da iman edip de sabr\u0131 tavsiye eden ve merhamet tavsiye edenlerden olmakt\u0131r."}, {"id": 585825, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6041, "text": "\u0130\u015fte bunlar, amel defterleri sa\u011flar\u0131ndan verilenlerdir."}, {"id": 585826, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6042, "text": "\u00c2yetlerimizi tan\u0131mayanlar ise, onlard\u0131r i\u015fte amel defterleri sollar\u0131ndan verilenler."}, {"id": 585827, "identifier": "tr.yazir", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Onlar\u0131n \u00fczerlerine bir ate\u015f bast\u0131r\u0131l\u0131p kap\u0131lar\u0131 kapanacakt\u0131r."}]