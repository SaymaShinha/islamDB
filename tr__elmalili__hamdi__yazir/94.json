[{"id": 585875, "identifier": "tr.yazir", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6091, "text": "Biz senin i\u00e7in (mutlulu\u011fun) g\u00f6\u011fs\u00fcn\u00fc a\u00e7mad\u0131k m\u0131?"}, {"id": 585876, "identifier": "tr.yazir", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6092, "text": "Senden y\u00fck\u00fcn\u00fc indirmedik mi?"}, {"id": 585877, "identifier": "tr.yazir", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 6093, "text": "O senin s\u0131rt\u0131n\u0131 ezen y\u00fck\u00fc."}, {"id": 585878, "identifier": "tr.yazir", "surah_n