[{"id": 585116, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5324, "text": "(Ger\u00e7ekle\u015fecek) K\u0131yamet!"}, {"id": 585117, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5325, "text": "Nedir, o K\u0131yamet?"}, {"id": 585118, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5326, "text": "Ger\u00e7ekle\u015fenin (K\u0131aymetin) ne oldu\u011funu sen nerden bileceksin?"}, {"id": 585119, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5327, "text": "Sem\u00fbd ve \u00c2d, kap\u0131lar\u0131n\u0131 \u00e7alacak olan o felaketi yalan saym\u0131\u015flard\u0131."}, {"id": 585120, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5328, "text": "Sem\u00fbd kavmi korkun\u00e7 bir sesle yok edildi."}, {"id": 585121, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5329, "text": "\u00c2d kavmi ise g\u00fcr\u00fclt\u00fcl\u00fc ve azg\u0131n bir f\u0131rt\u0131na ile yok edildiler."}, {"id": 585122, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5330, "text": "Allah o f\u0131rt\u0131nay\u0131 \u00fczerlerine yedi gece sekiz g\u00fcnd\u00fcz musallat etmi\u015fti. \u00d6yle ki, o kavmi i\u00e7i bo\u015f hurma k\u00fct\u00fckleri gibi orac\u0131kta yere serilmi\u015f halde g\u00f6r\u00fcrd\u00fcn."}, {"id": 585123, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5331, "text": "Bak \u015fimdi g\u00f6rebilir misin onlardan bir kal\u0131nt\u0131?"}, {"id": 585124, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5332, "text": "Firavun, ondan \u00f6ncekiler ve alt\u0131 \u00fcst\u00fcne getirilen beldeler de hep o hatay\u0131 i\u015fleyegeldiler."}, {"id": 585125, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5333, "text": "Hep Rablerinin el\u00e7ilerine kar\u015f\u0131 geldiler. O da onlar\u0131 pek \u015fiddetli bir \u015fekilde yakalay\u0131verdi."}, {"id": 585126, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5334, "text": "Ku\u015fkusuz, sular kabar\u0131nca sizi gemide biz ta\u015f\u0131d\u0131k."}, {"id": 585127, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5335, "text": "Onu size bir ibret yapal\u0131m ve belleyici kulaklar bellesin diye."}, {"id": 585128, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5336, "text": "S\u00fbr'a bir tek \u00fcfleme \u00fcflendi\u011fi,"}, {"id": 585129, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5337, "text": "Arz ve da\u011flar yerlerinden kald\u0131r\u0131l\u0131p \u015fiddetle birbirine \u00e7arp\u0131larak darmada\u011f\u0131n oldu\u011fu zaman,"}, {"id": 585130, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5338, "text": "\u0130\u015fte o g\u00fcn olacak olur."}, {"id": 585131, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5339, "text": "O g\u00fcn g\u00f6k yar\u0131lm\u0131\u015f, sarkm\u0131\u015ft\u0131r."}, {"id": 585132, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5340, "text": "Melekler de onun etraf\u0131ndad\u0131r, O g\u00fcn Rabbinin Ar\u015f\u0131n\u0131 bunlar\u0131n da \u00fcst\u00fcnde sekiz melek y\u00fcklenir."}, {"id": 585133, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5341, "text": "O g\u00fcn (hesap i\u00e7in Allah'a) arz olunursunuz, \u00f6yle ki gizli bir haliniz kalmaz."}, {"id": 585134, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5342, "text": "Kitab\u0131 sa\u011f\u0131ndan verilen, \"al\u0131n okuyun kitab\u0131m\u0131..\""}, {"id": 585135, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5343, "text": "\"\u00c7\u00fcnk\u00fc ben hesab\u0131ma kavu\u015faca\u011f\u0131m\u0131 sezmi\u015ftim\" der."}, {"id": 585136, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5344, "text": "Art\u0131k o ho\u015fnut bir hayattad\u0131r."}, {"id": 585137, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5345, "text": "Y\u00fcksek bir cennettedir."}, {"id": 585138, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5346, "text": "Ki o cennetin meyveleri sarkm\u0131\u015ft\u0131r."}, {"id": 585139, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5347, "text": "\"Ge\u00e7mi\u015f g\u00fcnlerde yapt\u0131\u011f\u0131n\u0131z i\u015flerden \u00f6t\u00fcr\u00fc afiyetle yeyin, i\u00e7in.\" (denir)."}, {"id": 585140, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5348, "text": "Kitab\u0131 sol taraf\u0131ndan verilen ise der ki: \"Ke\u015fke kitab\u0131m verilmeseydi de,"}, {"id": 585141, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5349, "text": "Hesab\u0131m\u0131n ne oldu\u011funu bilmeseydim,"}, {"id": 585142, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5350, "text": "Ne olurdu o \u00f6l\u00fcm, i\u015f bitirici olsayd\u0131."}, {"id": 585143, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5351, "text": "Mal\u0131m bana hi\u00e7 fayda vermedi."}, {"id": 585144, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5352, "text": "G\u00fcc\u00fcm de benden yok olup gitti.\""}, {"id": 585145, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5353, "text": "(Zebanilere \u015f\u00f6yle denir): \"Onu yakalay\u0131n da ba\u011flay\u0131n.\""}, {"id": 585146, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5354, "text": "\"Sonra cehenneme at\u0131n onu.\""}, {"id": 585147, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5355, "text": "\"Sonra da boyu yetmi\u015f ar\u015f\u0131n zincir i\u00e7erisinde onu oraya sokun.\""}, {"id": 585148, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5356, "text": "\u00c7\u00fcnk\u00fc o, b\u00fcy\u00fck Allah'a inanm\u0131yordu."}, {"id": 585149, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5357, "text": "Yoksula yedirmeye te\u015fvik etmiyordu."}, {"id": 585150, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5358, "text": "Bu sebeple bug\u00fcn burada onun candan bir dostu yoktur."}, {"id": 585151, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5359, "text": "Bir irinden ba\u015fka yiyecek de yok."}, {"id": 585152, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5360, "text": "Onu g\u00fcnahk\u00e2rlardan ba\u015fkas\u0131 yemez."}, {"id": 585153, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5361, "text": "Andolsun g\u00f6rd\u00fcklerinize,"}, {"id": 585154, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5362, "text": "Ve g\u00f6rmediklerinize.."}, {"id": 585155, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5363, "text": "Ku\u015fkusuz Kur'\u00e2n, \u015ferefli bir peygamberin (Allah'tan) getirdi\u011fi s\u00f6zd\u00fcr."}, {"id": 585156, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5364, "text": "O bir \u015fair s\u00f6z\u00fc de\u011fildir, siz \u00e7ok az inan\u0131yorsunuz."}, {"id": 585157, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5365, "text": "Bir k\u00e2hin s\u00f6z\u00fc de de\u011fildir, ne de az d\u00fc\u015f\u00fcn\u00fcyorsunuz!"}, {"id": 585158, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5366, "text": "O, \u00e2lemlerin Rabbi taraf\u0131ndan indirilmedir."}, {"id": 585159, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5367, "text": "O, bize isn\u00e2den baz\u0131 s\u00f6zler uydurmaya kalk\u0131\u015fsayd\u0131,"}, {"id": 585160, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5368, "text": "Elbette biz onu bundan dolay\u0131 kuvvetle yakalard\u0131k."}, {"id": 585161, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5369, "text": "Sonra da onun \u015fah damar\u0131n\u0131 keser atard\u0131k."}, {"id": 585162, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5370, "text": "O vakit sizden hi\u00e7biriniz ona siper de olamazd\u0131n\u0131z."}, {"id": 585163, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5371, "text": "O hi\u00e7 ku\u015fkusuz, takva sahipleri i\u00e7in unutulmayacak bir \u00f6\u011f\u00fctt\u00fcr."}, {"id": 585164, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5372, "text": "Bununla beraber biz biliyoruz ki sizden inanmayanlar var."}, {"id": 585165, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5373, "text": "Ku\u015fkusuz bu Kur'\u00e2n kafirler i\u00e7in bir pi\u015fmanl\u0131k vesilesidir."}, {"id": 585166, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5374, "text": "Ger\u00e7ekten o, \u015f\u00fcphe g\u00f6t\u00fcrmez bir bilgidir."}, {"id": 585167, "identifier": "tr.yazir", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5375, "text": "O halde, haydi tesbih et Rabbinin y\u00fcce ismiyle"}]