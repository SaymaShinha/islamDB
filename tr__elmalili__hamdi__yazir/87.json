[{"id": 585734, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5949, "text": "Rabbinin y\u00fcce ad\u0131n\u0131 tesbih et."}, {"id": 585735, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5950, "text": "Yarat\u0131p d\u00fczene koyan O'dur."}, {"id": 585736, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5951, "text": "Takdir edip hidayeti g\u00f6steren O'dur."}, {"id": 585737, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5952, "text": "Otla\u011f\u0131 \u00e7\u0131karan,"}, {"id": 585738, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5953, "text": "Sonra da onu karams\u0131 bir sel k\u00f6p\u00fc\u011f\u00fc haline getiren O'dur."}, {"id": 585739, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5954, "text": "Bundan b\u00f6yle sana Kur'\u00e2n'\u0131 okutaca\u011f\u0131z da unutmayacaks\u0131n."}, {"id": 585740, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5955, "text": "Yaln\u0131z Allah'\u0131n diledi\u011fi ba\u015fkad\u0131r. \u00c7\u00fcnk\u00fc o a\u00e7\u0131\u011f\u0131 da bilir, gizliyi de."}, {"id": 585741, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5956, "text": "Seni en kolay yola muvaffak k\u0131laca\u011f\u0131z."}, {"id": 585742, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5957, "text": "Onun i\u00e7in \u00f6\u011f\u00fct ver, e\u011fer \u00f6\u011f\u00fct fayda verirse."}, {"id": 585743, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Sayg\u0131s\u0131 olan \u00f6\u011f\u00fct alacakt\u0131r."}, {"id": 585744, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5959, "text": "Pek bedbaht olan da ondan ka\u00e7\u0131nacakt\u0131r."}, {"id": 585745, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5960, "text": "O ki, en b\u00fcy\u00fck ate\u015fe girecektir."}, {"id": 585746, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5961, "text": "Sonra ne \u00f6lecek onda, ne de hayat bulacakt\u0131r."}, {"id": 585747, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5962, "text": "Do\u011frusu felah buldu (g\u00fcnahtan) temizlenen. \u00a0"}, {"id": 585748, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5963, "text": "Rabbinin ad\u0131n\u0131 an\u0131p namaz k\u0131lan."}, {"id": 585749, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5964, "text": "Fakat siz d\u00fcnya hayat\u0131n\u0131 tercih ediyorsunuz."}, {"id": 585750, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5965, "text": "Oysa ahiret daha hay\u0131rl\u0131 ve daha kal\u0131c\u0131d\u0131r."}, {"id": 585751, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5966, "text": "Ku\u015fkusuz bu ilk sahifelerde vard\u0131r,"}, {"id": 585752, "identifier": "tr.yazir", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Elmal\u0131l\u0131 Hamdi Yaz\u0131r", "transelator_en_name": "Elmalili Hamdi Yazir", "format": "text", "type": "translation", "ayah_number": 5967, "text": "\u0130brahim ve Musa'n\u0131n sahifelerinde."}]