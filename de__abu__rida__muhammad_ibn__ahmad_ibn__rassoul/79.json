[{"id": 136490, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5713, "text": "Bei den (Engeln, die die Seelen der Ungl\u00e4ubigen) heftig entrei\u00dfen"}, {"id": 136491, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5714, "text": "und bei denen (, die die Seelen der Gl\u00e4ubigen) leicht emporheben"}, {"id": 136492, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5715, "text": "und bei denen (, die auf Gehei\u00df Allahs zwischen Himmel und Erde) einher schweben"}, {"id": 136493, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5716, "text": "dann bei denen (, die mit den Seelen der Gl\u00e4ubigen ins Paradies) eifrig voraneilen"}, {"id": 136494, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5717, "text": "dann bei denen, die jegliche Angelegenheit (des irdischen Lebens) lenken!"}, {"id": 136495, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5718, "text": "Eines Tages wird die Dr\u00f6hnende dr\u00f6hnen"}, {"id": 136496, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5719, "text": "gefolgt von der Darauffolgenden."}, {"id": 136497, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5720, "text": "Herzen werden an jenem Tage zittern"}, {"id": 136498, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5721, "text": "und ihre Augen werden niedergeschlagen sein."}, {"id": 136499, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5722, "text": "Sie sagen: \"Sollen wir wirklich in unseren fr\u00fcheren Zustand zur\u00fcckgebracht werden?"}, {"id": 136500, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5723, "text": "Wie? Selbst wenn wir verwestes Gebein geworden sind?\""}, {"id": 136501, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5724, "text": "Sie sagen: \"Das w\u00e4re dann eine verlustreiche Wiederkehr.\""}, {"id": 136502, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5725, "text": "Es wird nur ein einziger Schreckenslaut sein"}, {"id": 136503, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5726, "text": "und siehe, sie sind dann auf der Erdoberfl\u00e4che."}, {"id": 136504, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5727, "text": "Ist die Geschichte von Moses zu dir gedrungen?"}, {"id": 136505, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5728, "text": "Damals rief ihn sein Herr im heiligen Wadi Tuwa"}, {"id": 136506, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5729, "text": "\"Geh hin zu Pharao; denn er hat das Ma\u00df \u00fcberschritten."}, {"id": 136507, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5730, "text": "Sprich dann (zu ihm): \"Willst du dich nicht reinigen?"}, {"id": 136508, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5731, "text": "Und ich werde dich zu deinem Herrn f\u00fchren, auf da\u00df du dich f\u00fcrchten m\u00f6gest!\"\""}, {"id": 136509, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5732, "text": "So zeigte er ihm das gro\u00dfe Wunder."}, {"id": 136510, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5733, "text": "Er aber leugnete und blieb ungehorsam."}, {"id": 136511, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5734, "text": "Dann kehrte er den R\u00fccken und lief weg"}, {"id": 136512, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5735, "text": "versammelte alsdann (sein Volk) und rief aus"}, {"id": 136513, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5736, "text": "indem er sagte: \"lch bin euer h\u00f6chster Herr.\""}, {"id": 136514, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5737, "text": "Da erfa\u00dfte ihn Allah zur Strafe f\u00fcr jene und diese Tat."}, {"id": 136515, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5738, "text": "Hierin ist wahrlich eine Lehre f\u00fcr den, der f\u00fcrchtet."}, {"id": 136516, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5739, "text": "Seid ihr denn schwerer zu erschaffen oder der Himmel, den Er gebaut hat?"}, {"id": 136517, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5740, "text": "Er hat seine H\u00f6he gehoben und ihn dann vollkommen gemacht."}, {"id": 136518, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5741, "text": "Und Er machte seine Nacht finster und lie\u00df sein Tageslicht hervorgehen."}, {"id": 136519, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5742, "text": "Und Er breitete hernach die Erde aus."}, {"id": 136520, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5743, "text": "Aus ihr brachte Er ihr Wasser und ihr Weideland hervor."}, {"id": 136521, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5744, "text": "Und Er festigte die Berge"}, {"id": 136522, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5745, "text": "(dies alles) als eine Versorgung f\u00fcr euch und f\u00fcr euer Vieh."}, {"id": 136523, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5746, "text": "Doch wenn das gr\u00f6\u00dfte Unheil kommt"}, {"id": 136524, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5747, "text": "an jenem Tag, da der Mensch sich (all) das ins Ged\u00e4chtnis zur\u00fcckrufen wird, was er erstrebt hat"}, {"id": 136525, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5748, "text": "und die Gahim vor Augen gestellt wird f\u00fcr den, der sieht"}, {"id": 136526, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5749, "text": "Wer aber aufs\u00e4ssig war"}, {"id": 136527, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5750, "text": "und das irdische Leben vorzog"}, {"id": 136528, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5751, "text": "so wird wahrlich die Gahim (seine) Herberge sein."}, {"id": 136529, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5752, "text": "Wer aber das Stehen vor seinem Herrn gef\u00fcrchtet hatte und die eigne Seele von niedrem Gel\u00fcst abhielt"}, {"id": 136530, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5753, "text": "so wird das Paradies sicherlich (seine) Herberge sein."}, {"id": 136531, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5754, "text": "Sie befragen dich wegen der Stunde: \"Wann wird ihr Termin wohl sein?\""}, {"id": 136532, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5755, "text": "Was wei\u00dft du von ihr zu sagen!"}, {"id": 136533, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5756, "text": "Das endg\u00fcltige Wissen darum ist allein deinem Herrn (vorbehalten)"}, {"id": 136534, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Du bist nur ein Warner f\u00fcr den, der sie f\u00fcrchtet."}, {"id": 136535, "identifier": "de.aburida", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5758, "text": "An jenem Tage, an dem sie sie schauen, als h\u00e4tten sie (auf der Erde) nicht l\u00e4nger geweilt als einen Abend oder den Morgen darauf."}]