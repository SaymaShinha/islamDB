[{"id": 135568, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Beim Stern, wenn er heruntersaust!"}, {"id": 135569, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Euer Gef\u00e4hrte ist weder verwirrt, noch befindet er sich im Unrecht"}, {"id": 135570, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4787, "text": "noch spricht er aus Begierde."}, {"id": 135571, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4788, "text": "Vielmehr ist es eine Offenbarung, die (ihm) eingegeben wird."}, {"id": 135572, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4789, "text": "Gelehrt hat ihn einer, der \u00fcber starke Macht verf\u00fcgt"}, {"id": 135573, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4790, "text": "dessen Macht sich auf alles erstreckt; darum stand er aufrecht da"}, {"id": 135574, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4791, "text": "als er am obersten Horizont war."}, {"id": 135575, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Hierauf n\u00e4herte er sich; kam dann nach unten"}, {"id": 135576, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4793, "text": "bis er eine Entfernung von zwei Bogenl\u00e4ngen erreicht hatte oder noch n\u00e4her."}, {"id": 135577, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Und er offenbarte Seinem Diener, was er offenbarte."}, {"id": 135578, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4795, "text": "(Und) dessen Herz hielt es nicht f\u00fcr gelogen, was er sah."}, {"id": 135579, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Wollt ihr da mit ihm \u00fcber das streiten, was er sah?"}, {"id": 135580, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Und er sah ihn bei einer anderen Begegnung"}, {"id": 135581, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4798, "text": "beim Lotusbaum am \u00e4u\u00dfersten Ende"}, {"id": 135582, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4799, "text": "an dem das Paradies der Geborgenheit liegt."}, {"id": 135583, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4800, "text": "Dabei \u00fcberflutete den Lotusbaum, was (ihn) \u00fcberflutete."}, {"id": 135584, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4801, "text": "Da wankte der Blick nicht, noch schweifte er ab."}, {"id": 135585, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Wahrlich, er hatte eines der gr\u00f6\u00dften Zeichen seines Herrn gesehen."}, {"id": 135586, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Was haltet ihr nun von Al-Lat und AI-`Uzza"}, {"id": 135587, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4804, "text": "und Manah, der dritten der anderen?"}, {"id": 132830, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Wie? Sollten euch die Knaben zustehen und Ihm die M\u00e4dchen?"}, {"id": 133198, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Das w\u00e4re wahrhaftig eine unbillige Verteilung."}, {"id": 135588, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Wahrlich, es sind nur die Namen, die ihr euch ausgedacht habt ihr und eure V\u00e4ter -, f\u00fcr die Allah keinerlei Erm\u00e4chtigung hinabgesandt hat. Sie folgen einem blo\u00dfen Wahn und ihren pers\u00f6nlichen Neigungen, obwohl doch die Weisung ihres Herrn zu ihnen kam."}, {"id": 135589, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Kann der Mensch denn haben, was er nur w\u00fcnscht?"}, {"id": 135590, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Aber Allahs ist das Diesseits und das Jenseits."}, {"id": 135591, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4810, "text": "Und so mancher Engel ist in den Himmeln, dessen F\u00fcrsprache nichts n\u00fctzen wird, es sei denn, nachdem Allah dem die Erlaubnis (dazu) gegeben hat, dem Er will und der Ihm beliebt."}, {"id": 135592, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Solche, die nicht an das Jenseits glauben, benennen die Engel mit weiblichen Namen."}, {"id": 135593, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Jedoch sie besitzen kein Wissen hiervon. Sie gehen nur Vermutungen nach; und Vermutungen ersetzen nicht im geringsten die Wahrheit."}, {"id": 135594, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Darum wende dich von dem ab, der Unserer Ermahnung den R\u00fccken kehrt und nichts als das Leben in dieser Welt begehrt."}, {"id": 135595, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Das ist die Summe ihres Wissens. Wahrlich, dein Herr kennt denjenigen recht wohl, der von Seinem Wege abirrt, und Er kennt auch jenen wohl, der den Weg befolgt."}, {"id": 135596, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Und Allahs ist, was in den Himmeln und was auf Erden ist, auf da\u00df Er denen, die B\u00f6ses tun, ihren Lohn f\u00fcr das gebe, was sie gewirkt haben; und auf da\u00df Er die, die Gutes tun, mit dem Allerbesten belohne."}, {"id": 135597, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Jene, die die gro\u00dfen S\u00fcnden und Sch\u00e4ndlichkeiten meiden - mit Ausnahme der Lappalien - wahrlich, dein Herr ist von weitumfassender Vergebung. Er kennt euch sehr wohl; als Er euch aus der Erde hervorbrachte, und als ihr Embryos in den Leibern eurer M\u00fctter waret. Darum erkl\u00e4rt euch nicht selber als rein. Er kennt diejenigen am besten, die (Ihn) f\u00fcrchten."}, {"id": 135598, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Siehst du den, der sich abkehrt"}, {"id": 135599, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4818, "text": "und wenig gibt und geizt?"}, {"id": 135600, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Hat er wohl die Kenntnis des Verborgenen, so da\u00df er es sehen k\u00f6nnte?"}, {"id": 135601, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Oder ist ihm nicht erz\u00e4hlt worden, was in den Schriftbl\u00e4ttern Moses' steht"}, {"id": 135602, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4821, "text": "und Abrahams, der (die Gebote) erf\u00fcllte?"}, {"id": 135603, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4822, "text": "(Geschrieben steht,) da\u00df keine lasttragende (Seele) die Last einer anderen tragen soll"}, {"id": 135604, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4823, "text": "und da\u00df dem Menschen nichts anderes zuteil wird als das, wonach er strebt"}, {"id": 135605, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4824, "text": "und da\u00df sein Streben bald sichtbar wird."}, {"id": 135606, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4825, "text": "Dann wird er daf\u00fcr mit reichlichem Lohn belohnt werden."}, {"id": 135607, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4826, "text": "Und (es steht geschrieben,) da\u00df es bei deinem Herrn enden wird"}, {"id": 135608, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4827, "text": "und da\u00df Er es ist, Der zum Lachen und Weinen bringt"}, {"id": 135609, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4828, "text": "und da\u00df Er es ist, Der sterben l\u00e4\u00dft und lebendig macht"}, {"id": 135610, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4829, "text": "und da\u00df Er die Paare (als) m\u00e4nnliche und weibliche (Wesen) erschaffen hat"}, {"id": 135611, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4830, "text": "aus einem Samentropfen, der ausgesto\u00dfen wird"}, {"id": 135612, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4831, "text": "und da\u00df Ihm die zweite Sch\u00f6pfung obliegt"}, {"id": 135613, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4832, "text": "und da\u00df Er allein reich und arm macht"}, {"id": 135614, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4833, "text": "und da\u00df Er der Herr des Sirius ist"}, {"id": 135615, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4834, "text": "und da\u00df Er die einstigen `Ad vernichtete"}, {"id": 134029, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4835, "text": "und die Tamud, und keinen verschonte"}, {"id": 135616, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4836, "text": "und (da\u00df Er) vordem das Volk Noahs (vernichtete) - wahrlich, sie waren h\u00f6chst ungerecht und widerspenstig."}, {"id": 135617, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4837, "text": "Und Er lie\u00df die verderbten St\u00e4dte einst\u00fcrzen"}, {"id": 135618, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4838, "text": "so da\u00df sie bedeckte, was (sie) bedeckte."}, {"id": 135619, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4839, "text": "Welche Wohltaten deines Herrn willst du denn bestreiten?"}, {"id": 135620, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Dies ist ein Warner wie die fr\u00fcheren Warner."}, {"id": 135621, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Die Stunde naht."}, {"id": 135622, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4842, "text": "Keiner au\u00dfer Allah kann sie abwenden."}, {"id": 135623, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Wundert ihr euch \u00fcber diese Verk\u00fcndigung?"}, {"id": 135624, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4844, "text": "Und ihr lacht; aber Weinen tut ihr nicht?"}, {"id": 135625, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4845, "text": "Und wollt ihr achtlos (hinsichtlich dieser Verk\u00fcndigung) bleiben?"}, {"id": 135626, "identifier": "de.aburida", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 4846, "text": "So fallt denn vor Allah anbetend nieder und dient (Ihm)."}]