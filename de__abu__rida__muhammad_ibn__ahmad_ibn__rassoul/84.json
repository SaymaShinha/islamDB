[{"id": 136663, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5885, "text": "Wenn der Himmel zerbricht"}, {"id": 136664, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5886, "text": "und seinem Herrn gehorcht und sich Ihm gef\u00fcgig zeigt"}, {"id": 136665, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5887, "text": "und wenn die Erde ausgebreitet wird"}, {"id": 136666, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5888, "text": "und herauswirft, was sie verbirgt, und sich (von allem) freimacht"}, {"id": 136667, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5889, "text": "und ihrem Herrn gehorcht und sich Ihm gef\u00fcgig zeigt."}, {"id": 136668, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5890, "text": "Du Mensch! Du strebst mit aller M\u00fche deinem Herrn zu; und du sollstIhm begegnen."}, {"id": 136669, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5891, "text": "Was nun den anbelangt, dem sein Buch in seine Rechte gegeben wird"}, {"id": 136670, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5892, "text": "der wird einer leichten Rechenschaft unterzogen sein"}, {"id": 136671, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5893, "text": "und wird fr\u00f6hlich zu seinen Angeh\u00f6rigen zur\u00fcckkehren."}, {"id": 136672, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5894, "text": "Was aber den anbelangt, dem sein Buch hinter seinem R\u00fccken gegeben wird"}, {"id": 136673, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5895, "text": "der wird sich bald Vernichtung herbeiw\u00fcnschen"}, {"id": 136674, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5896, "text": "und wird im H\u00f6llenfeuer brennen"}, {"id": 136675, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5897, "text": "er war gewi\u00df gl\u00fccklich unter seinen Angeh\u00f6rigen."}, {"id": 136676, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5898, "text": "Siehe, er dachte, da\u00df er nie davon abkommen (und zu Allah zur\u00fcckkehren) w\u00fcrde."}, {"id": 136677, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5899, "text": "Wahrlich, sein Herr hat ihn wohl durchschaut."}, {"id": 136678, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5900, "text": "Doch! Ich schw\u00f6re bei der Abendd\u00e4mmerung"}, {"id": 136679, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5901, "text": "und bei der Nacht und dem, was sie verh\u00fcllt"}, {"id": 136680, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5902, "text": "und bei dem Mond, wenn er voll wird"}, {"id": 136681, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5903, "text": "da\u00df ihr sicherlich von einem Zustand (der Not) in den anderen versetzt werdet."}, {"id": 136682, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5904, "text": "Was also ist ihnen, da\u00df sie nicht glauben"}, {"id": 136683, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": true, "sajda_id": 14, "sajda_obligatory": false, "sajda_recommended": true, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5905, "text": "und wenn ihnen der Quran verlesen wird, sich nicht in Anbetung niederwerfen?"}, {"id": 136684, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5906, "text": "Im Gegenteil, die da ungl\u00e4ubig sind, erkl\u00e4ren (die Botschaft Allahs) f\u00fcr eine L\u00fcge."}, {"id": 136685, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5907, "text": "Und Allah wei\u00df am besten, was sie verbergen."}, {"id": 136686, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5908, "text": "Darum verk\u00fcnde ihnen eine schmerzliche Strafe."}, {"id": 136687, "identifier": "de.aburida", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5909, "text": "Nicht so denjenigen, die glauben und gute Werke tun - ihnen wird unendlicher Lohn zuteil sein."}]