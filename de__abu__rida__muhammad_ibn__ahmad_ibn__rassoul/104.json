[{"id": 136956, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6180, "text": "Wehe jedem Stichler, Verleumder"}, {"id": 136957, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6181, "text": "der ein Verm\u00f6gen zusammentr\u00e4gt und es gez\u00e4hlt zur\u00fccklegt!"}, {"id": 136958, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6182, "text": "Er meint, da\u00df sein Verm\u00f6gen ihn unsterblich mache."}, {"id": 136959, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Aber nein! Er wird wahrlich in Al-Hutama geschleudert werden."}, {"id": 136960, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6184, "text": "Doch was l\u00e4\u00dft dich wissen, was Al-Hutama ist?"}, {"id": 136961, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6185, "text": "(Es ist) Allahs angez\u00fcndetes Feuer"}, {"id": 136962, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6186, "text": "das bis zu dem (innersten der) Herzen vordringt."}, {"id": 136963, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6187, "text": "Es schl\u00e4gt \u00fcber ihnen zusammen"}, {"id": 136964, "identifier": "de.aburida", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 6188, "text": "in langgestreckten S\u00e4ulen."}]