[{"id": 136330, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5552, "text": "Ich schw\u00f6re beim Tag der Auferstehung"}, {"id": 136331, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5553, "text": "und Ich schw\u00f6re bei jeder reum\u00fctigen Seele."}, {"id": 136332, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5554, "text": "Meint der Mensch etwa, da\u00df Wir seine Gebeine nicht sammeln werden?"}, {"id": 136333, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Aber ja, Wir sind imstande, seine Finger gleichm\u00e4\u00dfig zu formen."}, {"id": 136334, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5556, "text": "Doch der Mensch w\u00fcnscht sich, S\u00fcndhaftigkeit vorauszuschicken."}, {"id": 136335, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5557, "text": "Er fragt: \"Wann wird der Tag der Auferstehung sein?\""}, {"id": 136336, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5558, "text": "Dann, wenn das Auge geblendet ist"}, {"id": 136337, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5559, "text": "und der Mond sich verfinstert"}, {"id": 136338, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5560, "text": "und die Sonne und der Mond miteinander vereinigt werden."}, {"id": 136339, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5561, "text": "An jenem Tage wird der Mensch sagen: \"Wohin (k\u00f6nnte ich) nun fliehen?\""}, {"id": 136340, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5562, "text": "Nein! Es gibt keine Zuflucht!"}, {"id": 136341, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5563, "text": "(Nur) bei deinem Herrn wird an jenem Tage die Endstation sein."}, {"id": 136342, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5564, "text": "Verk\u00fcndet wird dem Menschen an jenem Tage, was er vorausgeschickt und was er zur\u00fcckgelassen hat."}, {"id": 136343, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5565, "text": "Nein, der Mensch ist Zeuge gegen sich selber"}, {"id": 136344, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5566, "text": "auch wenn er seine Entschuldigungen vorbringt."}, {"id": 136345, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5567, "text": "Bewege deine Zunge nicht mit ihm (dem Quran), um dich damit zu \u00fcbereilen."}, {"id": 136346, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5568, "text": "Uns obliegt seine Sammlung und seine Verlesung."}, {"id": 136347, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5569, "text": "Darum folge seiner Verlesung, wenn Wir ihn verlesen lassen"}, {"id": 136348, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5570, "text": "dann obliegt Uns, seine Bedeutung darzulegen."}, {"id": 136349, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5571, "text": "Nein, ihr aber liebt das Weltliche"}, {"id": 136350, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5572, "text": "und vernachl\u00e4ssigt das Jenseits."}, {"id": 136351, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5573, "text": "An jenem Tage wird es strahlende Gesichter geben"}, {"id": 136352, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5574, "text": "die zu ihrem Herrn schauen."}, {"id": 136353, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5575, "text": "Und manche Gesichter werden an jenem Tage gramvoll sein"}, {"id": 136354, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5576, "text": "denn sie ahnen, da\u00df ihnen bald darauf ein schreckliches Ungl\u00fcck widerfahren soll."}, {"id": 136355, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5577, "text": "Ja! Wenn (die Seele eines Sterbenden) bis zum Schl\u00fcsselbein emporsteigt"}, {"id": 136356, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5578, "text": "und gesprochen wird: \"Wer kann die Zauberformel sprechen(, um sie zu retten)?\""}, {"id": 136357, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5579, "text": "und er (der Mensch) w\u00e4hnt, da\u00df (die Stunde des) Abschieds gekommen ist"}, {"id": 136358, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5580, "text": "und (da\u00df) sich Bein mit Bein (im Todeskampf) verf\u00e4ngt"}, {"id": 136359, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5581, "text": "dann wird an jenem Tage das Treiben zu deinem Herrn sein"}, {"id": 136360, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5582, "text": "denn er spendete nicht und betete nicht"}, {"id": 136361, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5583, "text": "sondern er leugnete und wandte sich (von Ihm) ab."}, {"id": 136362, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5584, "text": "Dann ging er mit stolzem Gang zu den Seinen."}, {"id": 136363, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5585, "text": "\"Wehe dir denn! Wehe!"}, {"id": 136364, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5586, "text": "Und abermals wehe dir! Und nochmals wehe!\""}, {"id": 136365, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5587, "text": "Meint der Mensch etwa, er w\u00fcrde sich selber \u00fcberlassen sein?"}, {"id": 136366, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5588, "text": "War er nicht ein Ergu\u00df verspritzten Spermas?"}, {"id": 136367, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Dann wurde er ein Blutklumpen; dann bildete und vervollkommnete Er (ihn)"}, {"id": 136368, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5590, "text": "Alsdann schuf Er aus ihm ein Paar, den Mann und die Frau."}, {"id": 136369, "identifier": "de.aburida", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Abu Rida", "transelator_en_name": "Abu Rida Muhammad ibn Ahmad ibn Rassoul", "format": "text", "type": "translation", "ayah_number": 5591, "text": "Ist Er denn nicht imstande, die Toten ins Leben zu rufen?"}]