rah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Simple", "transelator_en_name": "Simple", "format": "text", "type": "quran", "ayah_number": 5438, "text": "\u0648\u064e\u0627\u0644\u0644\u0651\u064e\u0647\u064f \u062c\u064e\u0639\u064e\u0644\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0627\u0644\u0652\u0623\u064e\u0631\u0652\u0636\u064e \u0628\u0650\u0633\u064e\u0627\u0637\u064b\u0627"}, {"id": 42744, "identifier": "quran-simple", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Simple", "transelator_en_name": "Simple", "format": "text", "type": "quran", "ayah_number": 5439, "text": "\u0644\u0650\u062a\u064e\u0633\u0652\u0644\u064f\u0643\u064f\u0648\u0627 \u0645\u0650\u0646\u0652\u0647\u064e\u0627 \u0633\u064f\u0628\u064f\u0644\u064b\u0627 \u0641\u0650\u062c\u064e\u0627\u062c\u064b\u0627"}, {"id": 42746, "identifier": "quran-simple", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 505, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Simple", "transelator_en_name": "Simple", "format": "text", "type": "quran", "ayah_number": 5440, "text": "\u0642\u064e\u0627\u0644\u064e \u0646\u064f\u0648\u062d\u064c \u0631\u064e\u0628\u0651\u06