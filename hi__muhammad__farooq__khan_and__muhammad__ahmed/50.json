[{"id": 347951, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4631, "text": "\u0915\u093c\u093e\u092b\u093c\u0970; \u0917\u0935\u093e\u0939 \u0939\u0948 \u0915\u093c\u0941\u0930\u0906\u0928 \u092e\u091c\u0940\u0926! -"}, {"id": 347962, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4632, "text": "\u092c\u0932\u094d\u0915\u093f \u0909\u0928\u094d\u0939\u0947\u0902 \u0924\u094b \u0907\u0938 \u092c\u093e\u0924 \u092a\u0930 \u0906\u0936\u094d\u091a\u0930\u094d\u092f \u0939\u0941\u0906 \u0915\u093f \u0909\u0928\u0915\u0947 \u092a\u093e\u0938 \u0909\u0928\u094d\u0939\u0940\u0902 \u092e\u0947\u0902 \u0938\u0947 \u090f\u0915 \u0938\u093e\u0935\u0927\u093e\u0928 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u093e \u0906 \u0917\u092f\u093e\u0964 \u092b\u093f\u0930 \u0907\u0928\u0915\u093e\u0930 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u0947 \u0915\u0939\u0928\u0947 \u0932\u0917\u0947, \"\u092f\u0939 \u0924\u094b \u0906\u0936\u094d\u091a\u0930\u094d\u092f \u0915\u0940 \u092c\u093e\u0924 \u0939\u0948"}, {"id": 347972, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4633, "text": "\"\u0915\u094d\u092f\u093e \u091c\u092c \u0939\u092e \u092e\u0930 \u091c\u093e\u090f\u0901\u0917\u0947 \u0914\u0930 \u092e\u093f\u091f\u094d\u091f\u0940 \u0939\u094b \u091c\u093e\u090f\u0901\u0917\u0947 (\u0924\u094b \u092b\u093f\u0930 \u0939\u092e \u091c\u0940\u0935\u093f \u0939\u094b\u0915\u0930 \u092a\u0932\u091f\u0947\u0902\u0917\u0947)? \u092f\u0939 \u092a\u0932\u091f\u0928\u093e \u0924\u094b \u092c\u0939\u0941\u0924 \u0926\u0942\u0930 \u0915\u0940 \u092c\u093e\u0924 \u0939\u0948!\""}, {"id": 347983, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4634, "text": "\u0939\u092e \u091c\u093e\u0928\u0924\u0947 \u0939\u0948 \u0915\u093f \u0927\u0930\u0924\u0940 \u0909\u0928\u092e\u0947\u0902 \u091c\u094b \u0915\u0941\u091b \u0915\u092e\u0940 \u0915\u0930\u0924\u0940 \u0939\u0948 \u0914\u0930 \u0939\u092e\u093e\u0930\u0947 \u092a\u093e\u0938 \u0938\u0941\u0930\u0915\u094d\u0937\u093f\u0924 \u0930\u0916\u0928\u0947\u0935\u093e\u0932\u0940 \u090f\u0915 \u0915\u093f\u0924\u093e\u092c \u092d\u0940 \u0939\u0948"}, {"id": 347984, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4635, "text": "\u092c\u0932\u094d\u0915\u093f \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u0938\u0924\u094d\u092f \u0915\u094b \u091d\u0941\u0920\u0932\u093e \u0926\u093f\u092f\u093e \u091c\u092c \u0935\u0939 \u0909\u0928\u0915\u0947 \u092a\u093e\u0938 \u0906\u092f\u093e\u0964 \u0905\u0924\u0903 \u0935\u0947 \u090f\u0915 \u0909\u0932\u091d\u0928 \u092d\u0940 \u092c\u093e\u0924 \u092e\u0947\u0902 \u092a\u0921\u093c\u0947 \u0939\u0941\u090f \u0939\u0948"}, {"id": 347952, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4636, "text": "\u0905\u091a\u094d\u091b\u093e \u0924\u094b \u0915\u094d\u092f\u093e \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u0905\u092a\u0928\u0947 \u090a\u092a\u0930 \u0906\u0915\u093e\u0936 \u0915\u094b \u0928\u0939\u0940\u0902 \u0926\u0947\u0916\u093e, \u0939\u092e\u0928\u0947 \u0909\u0938\u0947 \u0915\u0948\u0938\u093e \u092c\u0928\u093e\u092f\u093e \u0914\u0930 \u0909\u0938\u0947 \u0938\u091c\u093e\u092f\u093e\u0964 \u0914\u0930 \u0909\u0938\u092e\u0947\u0902 \u0915\u094b\u0908 \u0926\u0930\u093e\u0930 \u0928\u0939\u0940\u0902"}, {"id": 347994, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4637, "text": "\u0914\u0930 \u0927\u0930\u0924\u0940 \u0915\u094b \u0939\u092e\u0928\u0947 \u092b\u0948\u0932\u093e\u092f\u093e \u0914\u0930 \u0909\u0938\u092e\u0947 \u0905\u091f\u0932 \u092a\u0939\u093e\u0921\u093c \u0921\u093e\u0932 \u0926\u093f\u090f\u0964 \u0914\u0930 \u0939\u092e\u0928\u0947 \u0909\u0938\u092e\u0947\u0902 \u0939\u0930 \u092a\u094d\u0930\u0915\u093e\u0930 \u0915\u0940 \u0938\u0941\u0928\u094d\u0926\u0930 \u091a\u0940\u091c\u093c\u0947 \u0909\u0917\u093e\u0908,"}, {"id": 347995, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4638, "text": "\u0906\u0901\u0916\u0947\u0902 \u0916\u094b\u0932\u0928\u0947 \u0914\u0930 \u092f\u093e\u0926 \u0926\u093f\u0932\u093e\u0928\u0947 \u0915\u0947 \u0909\u0926\u094d\u0926\u0947\u0936\u094d\u092f \u0938\u0947, \u0939\u0930 \u092c\u0928\u094d\u0926\u0947 \u0915\u0947 \u0932\u093f\u090f \u091c\u094b \u0930\u0941\u091c\u0942 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u093e \u0939\u094b"}, {"id": 347996, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4639, "text": "\u0914\u0930 \u0939\u092e\u0928\u0947 \u0906\u0915\u093e\u0936 \u0938\u0947 \u092c\u0930\u0915\u0924\u0935\u093e\u0932\u093e \u092a\u093e\u0928\u0940 \u0909\u0924\u093e\u0930\u093e, \u092b\u093f\u0930 \u0909\u0938\u0938\u0947 \u092c\u093e\u0917\u093c \u0914\u0930 \u092b\u093c\u0938\u0932 \u0915\u0947 \u0905\u0928\u093e\u091c\u0964"}, {"id": 347997, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4640, "text": "\u0914\u0930 \u090a\u0901\u091a\u0947-\u090a\u0901\u091a\u0947 \u0916\u091c\u0942\u0930 \u0915\u0947 \u0935\u0943\u0915\u094d\u0937 \u0909\u0917\u093e\u090f \u091c\u093f\u0928\u0915\u0947 \u0917\u0941\u091a\u094d\u091b\u0947 \u0924\u0939 \u092a\u0930 \u0924\u0939 \u0939\u094b\u0924\u0947 \u0939\u0948,"}, {"id": 347998, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4641, "text": "\u092c\u0928\u094d\u0926\u094b\u0902 \u0915\u0940 \u0930\u094b\u091c\u0940 \u0915\u0947 \u0932\u093f\u090f\u0964 \u0914\u0930 \u0939\u092e\u0928\u0947 \u0909\u0938 (\u092a\u093e\u0928\u0940) \u0915\u0947 \u0926\u094d\u0935\u093e\u0930\u093e \u0928\u093f\u0930\u094d\u091c\u0940\u0935 \u0927\u0930\u0924\u0940 \u0915\u094b \u091c\u0940\u0935\u0928 \u092a\u094d\u0930\u0926\u093e\u0928 \u0915\u093f\u092f\u093e\u0964 \u0907\u0938\u0940 \u092a\u094d\u0930\u0915\u093e\u0930 \u0928\u093f\u0915\u0932\u0928\u093e \u092d\u0940 \u0939\u0948\u0902"}, {"id": 347999, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4642, "text": "\u0909\u0928\u0938\u0947 \u092a\u0939\u0932\u0947 \u0928\u0942\u0939 \u0915\u0940 \u0915\u093c\u094c\u092e, '\u0905\u0930\u094d-\u0930\u0938' \u0935\u093e\u0932\u0947, \u0938\u092e\u0942\u0926,"}, {"id": 348000, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4643, "text": "\u0906\u0926, \u092b\u093c\u093f\u0930\u0914\u0928 , \u0932\u0942\u0924 \u0915\u0947 \u092d\u093e\u0908,"}, {"id": 348001, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4644, "text": "'\u0905\u0932-\u0910\u0915\u093e' \u0935\u093e\u0932\u0947 \u0914\u0930 \u0924\u0941\u092c\u094d\u092c\u093e \u0915\u0947 \u0932\u094b\u0917 \u092d\u0940 \u091d\u0941\u0920\u0932\u093e \u091a\u0941\u0915\u0947 \u0939\u0948\u0964 \u092a\u094d\u0930\u0924\u094d\u092f\u0947\u0915 \u0928\u0947 \u0930\u0938\u0942\u0932\u094b\u0902 \u0915\u094b \u091d\u0941\u0920\u0932\u093e\u092f\u093e\u0964 \u0905\u0928\u094d\u0924\u0924\u0903 \u092e\u0947\u0930\u0940 \u0927\u092e\u0915\u0940 \u0938\u0924\u094d\u092f\u093e\u092a\u093f\u0924 \u0939\u094b\u0915\u0930 \u0930\u0939\u0940"}, {"id": 348002, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4645, "text": "\u0915\u094d\u092f\u093e \u0939\u092e \u092a\u0939\u0932\u0940 \u092c\u093e\u0930 \u092a\u0948\u0926\u093e \u0915\u0930\u0928\u0947 \u0938\u0947 \u0905\u0938\u092e\u0930\u094d\u0925 \u0930\u0939\u0947? \u0928\u0939\u0940\u0902, \u092c\u0932\u094d\u0915\u093f \u0935\u0947 \u090f\u0915 \u0928\u0908 \u0938\u0943\u0937\u094d\u091f\u093f \u0915\u0947 \u0935\u093f\u0937\u092f \u092e\u0947\u0902 \u0938\u0928\u094d\u0926\u0947\u0939 \u092e\u0947\u0902 \u092a\u0921\u093c\u0947 \u0939\u0948"}, {"id": 348003, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4646, "text": "\u0939\u092e\u0928\u0947 \u092e\u0928\u0941\u0937\u094d\u092f \u0915\u094b \u092a\u0948\u0926\u093e \u0915\u093f\u092f\u093e \u0939\u0948 \u0914\u0930 \u0939\u092e \u091c\u093e\u0928\u0924\u0947 \u0939\u0948 \u091c\u094b \u092c\u093e\u0924\u0947\u0902 \u0909\u0938\u0915\u0947 \u091c\u0940 \u092e\u0947\u0902 \u0906\u0924\u0940 \u0939\u0948\u0964 \u0914\u0930 \u0939\u092e \u0909\u0938\u0938\u0947 \u0909\u0938\u0915\u0940 \u0917\u0930\u0926\u0928 \u0915\u0940 \u0930\u0917 \u0938\u0947 \u092d\u0940 \u0905\u0927\u093f\u0915 \u0928\u093f\u0915\u091f \u0939\u0948"}, {"id": 348004, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4647, "text": "\u091c\u092c \u0926\u094b \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u0947 (\u092b\u093c\u0930\u093f\u0936\u0924\u0947) \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0915\u0930 \u0930\u0939\u0947 \u0939\u094b\u0924\u0947 \u0939\u0948, \u0926\u093e\u090f\u0901 \u0938\u0947 \u0914\u0930 \u092c\u093e\u090f\u0901 \u0938\u0947 \u0935\u0947 \u0932\u0917\u0947 \u092c\u0948\u0920\u0947 \u0939\u094b\u0924\u0947 \u0939\u0948"}, {"id": 348005, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4648, "text": "\u0915\u094b\u0908 \u092c\u093e\u0924 \u0909\u0938\u0928\u0947 \u0915\u0939\u0940 \u0928\u0939\u0940\u0902 \u0915\u093f \u0909\u0938\u0915\u0947 \u092a\u093e\u0938 \u090f\u0915 \u0928\u093f\u0930\u0940\u0915\u094d\u0937\u0915 \u0924\u0948\u092f\u093e\u0930 \u0930\u0939\u0924\u093e \u0939\u0948"}, {"id": 348006, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4649, "text": "\u0914\u0930 \u092e\u094c\u0924 \u0915\u0940 \u092c\u0947\u0939\u094b\u0936\u0940 \u0932\u0947 \u0906\u0908 \u0905\u0935\u093f\u0936\u094d\u0935 \u0928\u0940\u092f \u091a\u0940\u091c\u093c! \u092f\u0939\u0940 \u0935\u0939 \u091a\u0940\u091c\u093c \u0939\u0948 \u091c\u093f\u0938\u0938\u0947 \u0924\u0942 \u0915\u0924\u0930\u093e\u0924\u093e \u0925\u093e"}, {"id": 348007, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4650, "text": "\u0914\u0930 \u0928\u0930\u0938\u093f\u0902\u0918\u093e \u092b\u0942\u0901\u0915 \u0926\u093f\u092f\u093e \u0917\u092f\u093e\u0964 \u092f\u0939\u0940 \u0939\u0948 \u0935\u0939 \u0926\u093f\u0928 \u091c\u093f\u0938\u0915\u0940 \u0927\u092e\u0915\u0940 \u0926\u0940 \u0917\u0908 \u0925\u0940"}, {"id": 348008, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4651, "text": "\u0939\u0930 \u0935\u094d\u092f\u0915\u094d\u0924\u093f \u0907\u0938 \u0926\u0936\u093e \u092e\u0947\u0902 \u0906 \u0917\u092f\u093e \u0915\u093f \u0909\u0938\u0915\u0947 \u0938\u093e\u0925 \u090f\u0915 \u0932\u093e\u0928\u0947\u0935\u093e\u0932\u093e \u0939\u0948 \u0914\u0930 \u090f\u0915 \u0917\u0935\u093e\u0939\u0940 \u0926\u0947\u0928\u0947\u0935\u093e\u0932\u093e"}, {"id": 348009, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4652, "text": "\u0924\u0942 \u0907\u0938 \u091a\u0940\u091c\u093c \u0915\u0940 \u0913\u0930 \u0938\u0947 \u0917\u093c\u092b\u093c\u0932\u0924 \u092e\u0947\u0902 \u0925\u093e\u0964 \u0905\u092c \u0939\u092e\u0928\u0947 \u0924\u0941\u091d\u0938\u0947 \u0924\u0947\u0930\u093e \u092a\u0930\u0926\u093e \u0939\u091f\u093e \u0926\u093f\u092f\u093e, \u0924\u094b \u0906\u091c \u0924\u0947\u0930\u0940 \u0928\u093f\u0917\u093e\u0939 \u092c\u0921\u093c\u0940 \u0924\u0947\u091c\u093c \u0939\u0948"}, {"id": 348010, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4653, "text": "\u0909\u0938\u0915\u0947 \u0938\u093e\u0925\u0940 \u0928\u0947 \u0915\u0939\u093e, \"\u092f\u0939 \u0939\u0948 (\u0924\u0947\u0930\u0940 \u0938\u091c\u093e)! \u092e\u0947\u0930\u0947 \u092a\u093e\u0938 \u0915\u0941\u091b (\u0938\u0939\u093e\u092f\u0924\u093e \u0915\u0947 \u0932\u093f\u090f) \u092e\u094c\u091c\u0942\u0926 \u0928\u0939\u0940\u0902\u0964\""}, {"id": 348011, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4654, "text": "\"\u0921\u093e\u0932 \u0926\u094b, \u0921\u093e\u0932 \u0926\u094b, \u091c\u0939\u0928\u094d\u0928\u092e \u092e\u0947\u0902! \u0939\u0930 \u0905\u0915\u0943\u0924\u091c\u094d\u091e \u0926\u094d\u0935\u0947\u0937 \u0930\u0916\u0928\u0947 \u0935\u093e\u0932\u0947,"}, {"id": 348012, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4655, "text": "\u092d\u0932\u093e\u0908 \u0938\u0947 \u0930\u094b\u0915\u0928\u0947\u0935\u093e\u0932\u0947, \u0938\u0940\u092e\u093e \u0915\u093e \u0905\u0924\u093f\u0915\u094d\u0930\u092e\u0923 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u0947, \u0938\u0928\u094d\u0926\u0947\u0939\u0917\u094d\u0930\u0938\u094d\u0924 \u0915\u094b"}, {"id": 347917, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4656, "text": "\u091c\u093f\u0938\u0928\u0947 \u0905\u0932\u094d\u0932\u093e\u0939 \u0915\u0947 \u0938\u093e\u0925 \u0915\u093f\u0938\u0940 \u0926\u0942\u0938\u0930\u0947 \u0915\u094b \u092a\u0942\u091c\u094d\u092f-\u092a\u094d\u0930\u092d\u0941 \u0920\u0939\u0930\u093e\u092f\u093e\u0964 \u0924\u094b \u0921\u093e\u0932 \u0926\u094b \u0909\u0938\u0947 \u0915\u0920\u094b\u0930 \u092f\u093e\u0924\u0928\u093e \u092e\u0947\u0902\u0964\""}, {"id": 348013, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4657, "text": "\u0909\u0938\u0915\u093e \u0938\u093e\u0925\u0940 \u092c\u094b\u0932\u093e, \"\u0910 \u0939\u092e\u093e\u0930\u0947 \u0930\u092c! \u092e\u0948\u0902\u0928\u0947 \u0909\u0938\u0947 \u0938\u0930\u0915\u0936 \u0928\u0939\u0940\u0902 \u092c\u0928\u093e\u092f\u093e, \u092c\u0932\u094d\u0915\u093f \u0935\u0939 \u0938\u094d\u0935\u092f\u0902 \u0939\u0940 \u092a\u0930\u0932\u0947 \u0926\u0930\u091c\u0947 \u0915\u0940 \u0917\u0941\u092e\u0930\u093e\u0939\u0940 \u092e\u0947\u0902 \u0925\u093e\u0964\""}, {"id": 348014, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4658, "text": "\u0915\u0939\u093e, \"\u092e\u0947\u0930\u0947 \u0938\u093e\u092e\u0928\u0947 \u092e\u0924 \u091d\u0917\u0921\u093c\u094b\u0964 \u092e\u0948\u0902 \u0924\u094b \u0924\u0941\u092e\u094d\u0939\u0947\u0902 \u092a\u0939\u0932\u0947 \u0939\u0940 \u0905\u092a\u0928\u0940 \u0927\u092e\u0915\u0940 \u0938\u0947 \u0938\u093e\u0935\u0927\u093e\u0928 \u0915\u0930 \u091a\u0941\u0915\u093e \u0925\u093e\u0964 -"}, {"id": 348015, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4659, "text": "\"\u092e\u0947\u0930\u0947 \u092f\u0939\u093e\u0901 \u092c\u093e\u0924 \u092c\u0926\u0932\u093e \u0928\u0939\u0940\u0902 \u0915\u0930\u0924\u0940 \u0914\u0930 \u0928 \u092e\u0948\u0902 \u0905\u092a\u0928\u0947 \u092c\u0928\u094d\u0926\u094b\u0902 \u092a\u0930 \u0924\u0928\u093f\u0915 \u092d\u0940 \u0905\u0924\u094d\u092f\u093e\u091a\u093e\u0930 \u0915\u0930\u0924\u093e \u0939\u0942\u0901\u0964\""}, {"id": 347974, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4660, "text": "\u091c\u093f\u0938 \u0926\u093f\u0928 \u0939\u092e \u091c\u0939\u0928\u094d\u0928\u092e \u0938\u0947 \u0915\u0939\u0947\u0902\u0917\u0947, \"\u0915\u094d\u092f\u093e \u0924\u0942 \u092d\u0930 \u0917\u0908?\" \u0914\u0930 \u0935\u0939 \u0915\u0939\u0947\u0917\u0940, \"\u0915\u094d\u092f\u093e \u0905\u092d\u0940 \u0914\u0930 \u092d\u0940 \u0915\u0941\u091b \u0939\u0948?\""}, {"id": 348017, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4661, "text": "\u0914\u0930 \u091c\u0928\u094d\u0928\u0924 \u0921\u0930 \u0930\u0916\u0928\u0947\u0935\u093e\u0932\u094b\u0902 \u0915\u0947 \u0932\u093f\u090f \u0928\u093f\u0915\u091f \u0915\u0930 \u0926\u0940 \u0917\u0908, \u0915\u0941\u091b \u092d\u0940 \u0926\u0942\u0930 \u0928 \u0930\u0939\u0940"}, {"id": 348018, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4662, "text": "\"\u092f\u0939 \u0939\u0948 \u0935\u0939 \u091a\u0940\u091c\u093c \u091c\u093f\u0938\u0915\u093e \u0924\u0941\u092e\u0938\u0947 \u0935\u093e\u0926\u093e \u0915\u093f\u092f\u093e \u091c\u093e\u0924\u093e \u0925\u093e \u0939\u0930 \u0930\u0941\u091c\u0942 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u0947, \u092c\u0921\u093c\u0940 \u0928\u093f\u0917\u0930\u093e\u0928\u0940 \u0930\u0916\u0928\u0947\u0935\u093e\u0932\u0947 \u0915\u0947 \u0932\u093f\u090f; -"}, {"id": 348019, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4663, "text": "\"\u091c\u094b \u0930\u0939\u092e\u093e\u0928 \u0938\u0947 \u0921\u0930\u093e \u092a\u0930\u094b\u0915\u094d\u0937 \u092e\u0947\u0902 \u0914\u0930 \u0906\u092f\u093e \u0930\u0941\u091c\u0942 \u0930\u0939\u0928\u0947\u0935\u093e\u0932\u093e \u0939\u0943\u0926\u092f \u0932\u0947\u0915\u0930 -"}, {"id": 348020, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4664, "text": "\"\u092a\u094d\u0930\u0935\u0947\u0936 \u0915\u0930\u094b \u0909\u0938 (\u091c\u0928\u094d\u0928\u0924) \u092e\u0947\u0902 \u0938\u0932\u093e\u092e\u0924\u0940 \u0915\u0947 \u0938\u093e\u0925\" \u0935\u0939 \u0936\u093e\u0936\u094d\u0935\u0924 \u0926\u093f\u0935\u0938 \u0939\u0948"}, {"id": 348021, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4665, "text": "\u0909\u0928\u0915\u0947 \u0932\u093f\u090f \u0909\u0938\u092e\u0947\u0902 \u0935\u0939 \u0938\u092c \u0915\u0941\u091b \u0939\u0948 \u091c\u094b \u0935\u0947 \u091a\u093e\u0939\u0947 \u0914\u0930 \u0939\u092e\u093e\u0930\u0947 \u092a\u093e\u0938 \u0909\u0938\u0938\u0947 \u0905\u0927\u093f\u0915 \u092d\u0940 \u0939\u0948"}, {"id": 348022, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4666, "text": "\u0909\u0928\u0938\u0947 \u092a\u0939\u0932\u0947 \u0939\u092e \u0915\u093f\u0924\u0928\u0940 \u0939\u0940 \u0928\u0938\u094d\u0932\u094b\u0902 \u0915\u094b \u0935\u093f\u0928\u0937\u094d\u091f \u0915\u0930 \u091a\u0941\u0915\u0947 \u0939\u0948\u0964 \u0935\u0947 \u0932\u094b\u0917 \u0936\u0915\u094d\u0924\u093f \u092e\u0947\u0902 \u0909\u0928\u0938\u0947 \u0915\u0939\u0940\u0902 \u092c\u0922\u093c-\u091a\u0922\u093c\u0915\u0930 \u0925\u0947\u0964 (\u092a\u0928\u093e\u0939 \u0915\u0940 \u0924\u0932\u093e\u0936 \u092e\u0947\u0902) \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u0928\u0917\u0930\u094b\u0902 \u0915\u094b \u091b\u093e\u0928 \u092e\u093e\u0930\u093e, \u0915\u094b\u0908 \u0939\u0948 \u092d\u093e\u0917\u0928\u0947 \u0915\u094b \u0920\u093f\u0915\u093e\u0928\u093e?"}, {"id": 348023, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4667, "text": "\u0928\u093f\u0936\u094d\u091a\u092f \u0939\u0940 \u0907\u0938\u092e\u0947\u0902 \u0909\u0938 \u0935\u094d\u092f\u0915\u094d\u0924\u093f \u0915\u0947 \u0932\u093f\u090f \u0936\u093f\u0915\u094d\u0937\u093e-\u0938\u093e\u092e\u0917\u094d\u0930\u0940 \u0939\u0948 \u091c\u093f\u0938\u0915\u0947 \u092a\u093e\u0938 \u0926\u093f\u0932 \u0939\u094b \u092f\u093e \u0935\u0939 (\u0926\u093f\u0932 \u0938\u0947) \u0939\u093e\u091c\u093f\u0930 \u0930\u0939\u0915\u0930 \u0915\u093e\u0928 \u0932\u0917\u093e\u090f"}, {"id": 348024, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4668, "text": "\u0939\u092e\u0928\u0947 \u0906\u0915\u093e\u0936\u094b\u0902 \u0914\u0930 \u0927\u0930\u0924\u0940 \u0915\u094b \u0914\u0930 \u091c\u094b \u0915\u0941\u091b \u0909\u0928\u0915\u0947 \u092c\u0940\u091a \u0939\u0948 \u091b\u0903 \u0926\u093f\u0928\u094b\u0902 \u092e\u0947\u0902 \u092a\u0948\u0926\u093e \u0915\u0930 \u0926\u093f\u092f\u093e \u0914\u0930 \u0939\u092e\u0947\u0902 \u0915\u094b\u0908 \u0925\u0915\u093e\u0928 \u0928 \u091b\u0942 \u0938\u0915\u0940"}, {"id": 348025, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4669, "text": "\u0905\u0924\u0903 \u091c\u094b \u0915\u0941\u091b \u0935\u0947 \u0915\u0939\u0924\u0947 \u0939\u0948 \u0909\u0938\u092a\u0930 \u0927\u0948\u0930\u094d\u092f \u0938\u0947 \u0915\u093e\u092e \u0932\u094b \u0914\u0930 \u0905\u092a\u0928\u0947 \u0930\u092c \u0915\u0940 \u092a\u094d\u0930\u0936\u0902\u0938\u093e \u0915\u0940 \u0924\u0938\u092c\u0940\u0939 \u0915\u0930\u094b; \u0938\u0942\u0930\u094d\u092f\u094b\u0926\u092f \u0938\u0947 \u092a\u0942\u0930\u094d\u0935 \u0914\u0930 \u093