r": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4674, "text": "\u091c\u093f\u0938 \u0926\u093f\u0928 \u0927\u0930\u0924\u0940 \u0909\u0928\u092a\u0930 \u0938\u0947 \u092b\u091f \u091c\u093e\u090f\u0917\u0940 \u0914\u0930 \u0935\u0947 \u0924\u0947\u091c\u0940 \u0938\u0947 \u0928\u093f\u0915\u0932 \u092a\u0921\u093c\u0947\u0902\u0917\u0947\u0964 \u092f\u0939 \u0907\u0915\u091f\u094d\u0920\u093e \u0915\u0930\u0928\u093e \u0939\u092e\u093e\u0930\u0947 \u0932\u093f\u090f \u0905\u0924\u094d\u092f\u0928\u094d\u0924 \u0938\u0930\u0932 \u0939\u0948"}, {"id": 348031, "identifier": "hi.farooq", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "hi", "name": "\u095e\u093e\u0930\u0942\u0958 \u0959\u093e\u0928 & \u0905\u0939\u092e\u0926", "transelator_en_name": "Muhammad Farooq Khan and Muhammad Ahmed", "format": "text", "type": "translation", "ayah_number": 4675, "text": "\u0939\u092e \u091c\u093e\u0928\u0924\u0947 \u0939\u0948 \u091c\u094b \u0915\u0941\u091b \u0935\u0947 \u0915\u0939\u0924\u0947 \u0939\u0948, \u0924\u0941\u092e \u0909\u0928\u092a\u0930 \u0915\u094b\u0908 \u091c\u093c\u092c\u0930\u0926\u0938\u094d\u0924\u0940 \u0915\u0930\u0928\u0947\u0935\u093e\u0932\u0947