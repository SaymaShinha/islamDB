[{"id": 236803, "identifier": "en.hilali", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6217, "text": "Perish the two hands of Abu Lahab (an uncle of the Prophet), and perish he!"}, {"id": 236804, "identifier": "en.hilali", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6218, "text": "His wealth and his children (etc.) will not benefit him!"}, {"id": 236805, "identifier": "en.hilali", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6219, "text": "He will be burnt in a Fire of blazing flames!"}, {"id": 236806, "identifier": "en.hilali", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6220, "text": "And his wife too, who carries wood (thorns of Sadan which she used to put on the way of the Prophet (Peace be upon him), or use to slander him)."}, {"id": 236807, "identifier": "en.hilali", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6221, "text": "In her neck is a twisted rope of Masad (palm fibre)."}]