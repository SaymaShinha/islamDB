[{"id": 230733, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 1, "text": "In the Name of Allah, the Most Beneficent, the Most Merciful."}, {"id": 230734, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 2, "text": "All the praises and thanks be to Allah, the Lord of the 'Alamin (mankind, jinns and all that exists)."}, {"id": 230735, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 3, "text": "The Most Beneficent, the Most Merciful."}, {"id": 230736, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 4, "text": "The Only Owner (and the Only Ruling Judge) of the Day of Recompense (i.e. the Day of Resurrection)"}, {"id": 230737, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5, "text": "You (Alone) we worship, and you (Alone) we ask for help (for each and everything)."}, {"id": 230738, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6, "text": "Guide us to the Straight Way"}, {"id": 230739, "identifier": "en.hilali", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 7, "text": "The Way of those on whom You have bestowed Your Grace, not (the way) of those who earned Your Anger, nor of those who went astray."}]