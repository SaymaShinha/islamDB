[{"id": 236438, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5849, "text": "Woe to Al-Mutaffifin [those who give less in measure and weight (decrease the rights of others)],"}, {"id": 236439, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5850, "text": "Those who, when they have to receive by measure from men, demand full measure,"}, {"id": 236440, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5851, "text": "And when they have to give by measure or weight to men, give less than due."}, {"id": 236441, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Think they not that they will be resurrected (for reckoning),"}, {"id": 236442, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5853, "text": "On a Great Day,"}, {"id": 236443, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5854, "text": "The Day when (all) mankind will stand before the Lord of the 'Alamin (mankind, jinns and all that exists)?"}, {"id": 236444, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5855, "text": "Nay! Truly, the Record (writing of the deeds) of the Fujjar (disbelievers, sinners, evil-doers and wicked) is (preserved) in Sijjin."}, {"id": 236445, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5856, "text": "And what will make you know what Sijjin is?"}, {"id": 236446, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5857, "text": "A Register inscribed."}, {"id": 236447, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5858, "text": "Woe, that Day, to those who deny [(Allah, His Angels, His Books, His Messengers, the Day of Resurrection, and Al-Qadar (Divine Preordainments)]."}, {"id": 236448, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5859, "text": "Those who deny the Day of Recompense."}, {"id": 236449, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5860, "text": "And none can deny it except every transgressor beyond bounds, (in disbelief, oppression and disobedience of Allah, the sinner!)"}, {"id": 236450, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5861, "text": "When Our Verses (of the Quran) are recited to him he says: \"Tales of the ancients!\""}, {"id": 236451, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5862, "text": "Nay! But on their hearts is the Ran (covering of sins and evil deeds) which they used to earn."}, {"id": 236452, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5863, "text": "Nay! Surely, they (evil-doers) will be veiled from seeing their Lord that Day."}, {"id": 236453, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5864, "text": "Then, verily they will indeed enter and taste the burning flame of Hell."}, {"id": 236454, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5865, "text": "Then, it will be said to them: \"This is what you used to deny!\""}, {"id": 236455, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5866, "text": "Nay! Verily, the Record (writing of the deeds) of Al-Abrar (the pious who fear Allah and avoid evil), is (preserved) in 'Illiyyun."}, {"id": 236456, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5867, "text": "And what will make you know what 'Illiyyun is?"}, {"id": 236457, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5868, "text": "A Register inscribed."}, {"id": 236458, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5869, "text": "To which bear witness those nearest (to Allah, i.e. the angels)."}, {"id": 236459, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5870, "text": "Verily, Al-Abrar (the pious who fear Allah and avoid evil) will be in delight (Paradise)."}, {"id": 236460, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5871, "text": "On thrones, looking (at all things)."}, {"id": 236461, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5872, "text": "You will recognise in their faces the brightness of delight."}, {"id": 236462, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5873, "text": "They will be given to drink pure sealed wine."}, {"id": 236463, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5874, "text": "The last thereof (that wine) will be the smell of musk, and for this let (all) those strive who want to strive (i.e. hasten earnestly to the obedience of Allah)."}, {"id": 236464, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5875, "text": "It (that wine) will be mixed with Tasnim."}, {"id": 236465, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5876, "text": "A spring whereof drink those nearest to Allah."}, {"id": 236466, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5877, "text": "Verily! (During the worldly life) those who committed crimes used to laugh at those who believed."}, {"id": 236467, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5878, "text": "And whenever they passed by them, used to wink one to another (in mockery);"}, {"id": 236468, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5879, "text": "And when they returned to their own people, they would return jesting;"}, {"id": 236469, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5880, "text": "And when they saw them, they said: \"Verily! These have indeed gone astray!\""}, {"id": 236470, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5881, "text": "But they (disbelievers, sinners) had not been sent as watchers over them (the believers)."}, {"id": 236471, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5882, "text": "But this Day (the Day of Resurrection) those who believe will laugh at the disbelievers"}, {"id": 236472, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5883, "text": "On (high) thrones, looking (at all things)."}, {"id": 236473, "identifier": "en.hilali", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5884, "text": "Are not the disbelievers paid (fully) for what they used to do?"}]