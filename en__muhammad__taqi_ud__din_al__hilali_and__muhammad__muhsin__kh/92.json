[{"id": 236646, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6059, "text": "By the night as it envelops;"}, {"id": 236647, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6060, "text": "And by the day as it appears in brightness;"}, {"id": 236648, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6061, "text": "And by Him Who created male and female;"}, {"id": 236649, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Certainly, your efforts and deeds are diverse (different in aims and purposes);"}, {"id": 236650, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6063, "text": "As for him who gives (in charity) and keeps his duty to Allah and fears Him,"}, {"id": 236651, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6064, "text": "And believes in Al-Husna."}, {"id": 236652, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6065, "text": "We will make smooth for him the path of ease (goodness)."}, {"id": 236653, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6066, "text": "But he who is greedy miser and thinks himself self-sufficient."}, {"id": 236654, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6067, "text": "And gives the lie to Al-Husna (see Verse No: 6 footnote);"}, {"id": 236655, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6068, "text": "We will make smooth for him the path for evil;"}, {"id": 236656, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6069, "text": "And what will his wealth benefit him when he goes down (in destruction)."}, {"id": 236657, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6070, "text": "Truly! Ours it is (to give) guidance,"}, {"id": 236658, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6071, "text": "And truly, unto Us (belong) the last (Hereafter) and the first (this world)."}, {"id": 236659, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6072, "text": "Therefore I have warned you of a Fire blazing fiercely (Hell);"}, {"id": 236660, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6073, "text": "None shall enter it save the most wretched,"}, {"id": 236661, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6074, "text": "Who denies and turns away."}, {"id": 236662, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6075, "text": "And Al-Muttaqun (the pious and righteous - see V. 2:2) will be far removed from it (Hell)."}, {"id": 236663, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6076, "text": "He who spends his wealth for increase in self-purification,"}, {"id": 236664, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6077, "text": "And have in his mind no favour from anyone for which a reward is expected in return,"}, {"id": 236665, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6078, "text": "Except only the desire to seek the Countenance of his Lord, the Most High;"}, {"id": 236666, "identifier": "en.hilali", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6079, "text": "He surely will be pleased (when he will enter Paradise)."}]