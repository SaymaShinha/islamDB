[{"id": 236729, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6169, "text": "The mutual rivalry for piling up of worldly things diverts you,"}, {"id": 236757, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6170, "text": "Until you visit the graves (i.e. till you die)."}, {"id": 236758, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6171, "text": "Nay! You shall come to know!"}, {"id": 236759, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6172, "text": "Again, Nay! You shall come to know!"}, {"id": 236760, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6173, "text": "Nay! If you knew with a sure knowledge (the end result of piling up, you would not have occupied yourselves in worldly things)"}, {"id": 236761, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6174, "text": "Verily, You shall see the blazing Fire (Hell)!"}, {"id": 236762, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6175, "text": "And again, you shall see it with certainty of sight!"}, {"id": 236763, "identifier": "en.hilali", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6176, "text": "Then, on that Day, you shall be asked about the delight (you indulged in, in this world)!"}]