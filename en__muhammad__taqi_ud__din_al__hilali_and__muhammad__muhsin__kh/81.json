[{"id": 236390, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5801, "text": "When the sun Kuwwirat (wound round and lost its light and is overthrown)."}, {"id": 236391, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5802, "text": "And when the stars shall fall;"}, {"id": 236392, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5803, "text": "And when the mountains shall made to pass away;"}, {"id": 236393, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5804, "text": "And when the pregnant she-camels shall be neglected;"}, {"id": 236394, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5805, "text": "And when the wild beasts shall be gathered together;"}, {"id": 236395, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5806, "text": "And when the seas shall become as blazing Fire or shall overflow;"}, {"id": 236396, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5807, "text": "And when the souls shall be joined with their bodies;"}, {"id": 236397, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5808, "text": "And when the female (infant) buried alive (as the pagan Arabs used to do) shall be questioned."}, {"id": 236398, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5809, "text": "For what sin she was killed?"}, {"id": 236399, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5810, "text": "And when the written pages of deeds (good and bad) of every person shall be laid open;"}, {"id": 236400, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5811, "text": "And when the heaven shall be stripped off and taken away from its place;"}, {"id": 236401, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5812, "text": "And when Hell-fire shall be kindled to fierce ablaze."}, {"id": 236402, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5813, "text": "And when Paradise shall be brought near,"}, {"id": 236403, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5814, "text": "(Then) every person will know what he has brought (of good and evil)."}, {"id": 236404, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5815, "text": "So verily, I swear by the planets that recede (i.e. disappear during the day and appear during the night)."}, {"id": 236405, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5816, "text": "And by the planets that move swiftly and hide themselves,"}, {"id": 236406, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5817, "text": "And by the night as it departs;"}, {"id": 236407, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5818, "text": "And by the dawn as it brightens;"}, {"id": 236408, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5819, "text": "Verily, this is the Word (this Quran brought by) a most honourable messenger [Jibrael (Gabriel), from Allah to the Prophet Muhammad (Peace be upon him)]."}, {"id": 236409, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5820, "text": "Owner of power, and high rank with (Allah) the Lord of the Throne,"}, {"id": 236410, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5821, "text": "Obeyed (by the angels), trustworthy there (in the heavens)."}, {"id": 236411, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5822, "text": "And (O people) your companion (Muhammad (Peace be upon him)) is not a madman;"}, {"id": 236412, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5823, "text": "And indeed he (Muhammad (Peace be upon him)) saw him [Jibrael (Gabriel)] in the clear horizon (towards the east)."}, {"id": 236413, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5824, "text": "And he (Muhammad (Peace be upon him)) withholds not a knowledge of the unseen."}, {"id": 236414, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5825, "text": "And it (the Quran) is not the word of the outcast Shaitan (Satan)."}, {"id": 236415, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5826, "text": "Then where are you going?"}, {"id": 236416, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5827, "text": "Verily, this (the Quran) is no less than a Reminder to (all) the 'Alamin (mankind and jinns)."}, {"id": 236417, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5828, "text": "To whomsoever among you who wills to walk straight,"}, {"id": 236418, "identifier": "en.hilali", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5829, "text": "And you will not, unless (it be) that Allah wills, the Lord of the 'Alamin (mankind, jinns and all that exists)."}]