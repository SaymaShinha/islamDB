[{"id": 236794, "identifier": "en.hilali", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6208, "text": "Say (O Muhammad (Peace be upon him) to these Mushrikun and Kafirun): \"O Al-Kafirun (disbelievers in Allah, in His Oneness, in His Angels, in His Books, in His Messengers, in the Day of Resurrection, and in Al-Qadar, etc.)!"}, {"id": 236795, "identifier": "en.hilali", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6209, "text": "\"I worship not that which you worship,"}, {"id": 236796, "identifier": "en.hilali", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6210, "text": "\"Nor will you worship that which I worship."}, {"id": 236797, "identifier": "en.hilali", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6211, "text": "\"And I shall not worship that which you are worshipping."}, {"id": 236798, "identifier": "en.hilali", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6212, "text": "\"Nor will you worship that which I worship."}, {"id": 236799, "identifier": "en.hilali", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6213, "text": "\"To you be your religion, and to me my religion (Islamic Monotheism).\""}]