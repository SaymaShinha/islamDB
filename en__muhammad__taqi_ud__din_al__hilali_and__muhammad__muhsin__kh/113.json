[{"id": 236812, "identifier": "en.hilali", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6226, "text": "Say: \"I seek refuge with (Allah) the Lord of the daybreak,"}, {"id": 236813, "identifier": "en.hilali", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6227, "text": "\"From the evil of what He has created;"}, {"id": 236814, "identifier": "en.hilali", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6228, "text": "\"And from the evil of the darkening (night) as it comes with its darkness; (or the moon as it sets or goes away)."}, {"id": 236815, "identifier": "en.hilali", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6229, "text": "\"And from the evil of the witchcrafts when they blow in the knots,"}, {"id": 236816, "identifier": "en.hilali", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6230, "text": "\"And from the evil of the envier when he envies.\""}]