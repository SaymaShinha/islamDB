[{"id": 236726, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6139, "text": "When the earth is shaken with its (final) earthquake."}, {"id": 236727, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6140, "text": "And when the earth throws out its burdens,"}, {"id": 236728, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6141, "text": "And man will say: \"What is the matter with it?\""}, {"id": 236730, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6142, "text": "That Day it will declare its information (about all what happened over it of good or evil)."}, {"id": 236731, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6143, "text": "Because your Lord has inspired it."}, {"id": 236732, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6144, "text": "That Day mankind will proceed in scattered groups that they may be shown their deeds."}, {"id": 236733, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6145, "text": "So whosoever does good equal to the weight of an atom (or a small ant), shall see it."}, {"id": 236734, "identifier": "en.hilali", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 6146, "text": "And whosoever does evil equal to the weight of an atom (or a small ant), shall see it."}]