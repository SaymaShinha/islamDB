[{"id": 236521, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5932, "text": "By the heaven, and At-Tariq (the night-comer, i.e. the bright star);"}, {"id": 236522, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5933, "text": "And what will make you to know what At-Tariq (night-comer) is?"}, {"id": 236523, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5934, "text": "(It is) the star of piercing brightness;"}, {"id": 236524, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5935, "text": "There is no human being but has a protector over him (or her) (i.e. angels incharge of each human being guarding him, writing his good and bad deeds, etc.)"}, {"id": 236525, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5936, "text": "So let man see from what he is created!"}, {"id": 236526, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5937, "text": "He is created from a water gushing forth."}, {"id": 236527, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5938, "text": "Proceeding from between the back-bone and the ribs,"}, {"id": 236528, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5939, "text": "Verily, (Allah) is Able to bring him back (to life)!"}, {"id": 236529, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5940, "text": "The Day when all the secrets (deeds, prayers, fasting, etc.) will be examined (as to their truth)."}, {"id": 236530, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5941, "text": "Then will (man) have no power, nor any helper."}, {"id": 236531, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5942, "text": "By the sky (having rain clouds) which gives rain, again and again."}, {"id": 236532, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5943, "text": "And the earth which splits (with the growth of trees and plants),"}, {"id": 236533, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5944, "text": "Verily! This (the Quran) is the Word that separates (the truth from falsehood, and commands strict legal laws for mankind to cut the roots of evil)."}, {"id": 236534, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5945, "text": "And it is not a thing for amusement."}, {"id": 236535, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Verily, they are but plotting a plot (against you O Muhammad (Peace be upon him))."}, {"id": 236536, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5947, "text": "And I (too) am planning a plan."}, {"id": 236537, "identifier": "en.hilali", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Hilali & Khan", "transelator_en_name": "Muhammad Taqi-ud-Din al-Hilali and Muhammad Muhsin Khan", "format": "text", "type": "translation", "ayah_number": 5948, "text": "So give a respite to the disbelievers. Deal you gently with them for a while."}]