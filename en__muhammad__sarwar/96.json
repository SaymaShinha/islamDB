[{"id": 230413, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6107, "text": "(Muhammad), read in the name of your Lord who created (all things)."}, {"id": 230414, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6108, "text": "He created man from a clot of blood."}, {"id": 230415, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6109, "text": "Recite! Your Lord is the most Honorable One,"}, {"id": 230416, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6110, "text": "who, by the pen, taught the human being:"}, {"id": 230417, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6111, "text": "He taught the human being what he did not know."}, {"id": 230418, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Despite this, the human being still tends to rebel"}, {"id": 230419, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6113, "text": "because he thinks that he is independent."}, {"id": 230420, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6114, "text": "However, (all things) will return to your Lord."}, {"id": 230421, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6115, "text": "Have you seen the one who prohibits"}, {"id": 230422, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6116, "text": "a servant of Ours from prayer?"}, {"id": 230423, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6117, "text": "What will happen if the praying person is rightly guided"}, {"id": 230424, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6118, "text": "or if he commands others to maintain piety!?"}, {"id": 230425, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6119, "text": "What will happen if the prohibiting rejects the Truth and turns away from it!?"}, {"id": 230426, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6120, "text": "Does he not realize that God sees him?"}, {"id": 230427, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6121, "text": "Let him know that if he does not desist, We shall certainly drag him by his forelocks,"}, {"id": 230428, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6122, "text": "his lying sinful forelock."}, {"id": 230429, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6123, "text": "Let him call on his associates for help"}, {"id": 230430, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6124, "text": "and We too will call the stern and angry keepers of hell."}, {"id": 230431, "identifier": "en.sarwar", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6125, "text": "(Muhammad), never yield to him! Prostrate yourself and try to come closer to God."}]