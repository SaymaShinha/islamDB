[{"id": 229019, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4676, "text": "By the winds which carry dust particles,"}, {"id": 229020, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4677, "text": "by the clouds which are heavily loaded with water,"}, {"id": 229021, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4678, "text": "by the ships which smoothly sail on the oceans,"}, {"id": 229022, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4679, "text": "by the angels which distribute the affairs,"}, {"id": 229023, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4680, "text": "that what you are promised is certainly true"}, {"id": 229024, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4681, "text": "and the Day of Judgment will inevitably take place."}, {"id": 229025, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4682, "text": "By the beautiful heavens,"}, {"id": 229026, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4683, "text": "your ideas are confused."}, {"id": 229027, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Let whoever wishes, turn away from Our Quran."}, {"id": 229028, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Death to those whose opinions are merely baseless conjectures."}, {"id": 229029, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4686, "text": "and who wander in the abyss of confusion."}, {"id": 229030, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4687, "text": "They ask, \"When it will be the Day of Judgment?\""}, {"id": 229031, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4688, "text": "On the Day of Judgment they will be punished by the fire"}, {"id": 229032, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4689, "text": "and will be told, \"Suffer the torment which you wanted to experience immediately\"."}, {"id": 229033, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4690, "text": "The pious ones will live amidst gardens and springs,"}, {"id": 229034, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4691, "text": "receiving their reward from their Lord. They had been righteous people before the Day of Judgment."}, {"id": 229035, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4692, "text": "They slept very little during the night"}, {"id": 229036, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4693, "text": "and asked for forgiveness in the early morning."}, {"id": 229037, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4694, "text": "They assigned a share of their property for the needy and the destitute."}, {"id": 229038, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4695, "text": "In the earth there is evidence (of the Truth) for those who have strong faith."}, {"id": 229039, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4696, "text": "There is also evidence of the Truth within your own selves. Will you then not see?"}, {"id": 229040, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4697, "text": "In the heavens there is your sustenance and that which you were promised (Paradise)."}, {"id": 229041, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4698, "text": "This, by the Lord of the heavens and the earth is as certain as your ability to speak."}, {"id": 229042, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Have you heard the story of the honorable guests of Abraham?"}, {"id": 229043, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4700, "text": "When they came to him saying, \"Peace be with you,\" he replied to their greeting in the same manner and said to himself, \"These are a strange people\"."}, {"id": 229044, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4701, "text": "He went quietly to his wife and returned to his guests with a fat, roasted calf."}, {"id": 229045, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4702, "text": "He placed it before them. Then he said, \"Why are you not eating?\""}, {"id": 229046, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4703, "text": "He began to feel afraid. They said, \"Do not be afraid,\" and then gave him the glad news of the birth of a knowledgeable son."}, {"id": 229047, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4704, "text": "His wife came forward, crying and beating her face, saying, \"I am an old barren woman!\""}, {"id": 229048, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4705, "text": "They said, \"This is true but your Lord has said, (that you will have a son); He is All-wise and All-knowing\"."}, {"id": 229049, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4706, "text": "Abraham asked, \"Messengers, what is your task?\""}, {"id": 229050, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4707, "text": "They replied, \"We have been sent to a sinful people"}, {"id": 229051, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4708, "text": "to bring down upon them showers of marked lumps of clayy."}, {"id": 229052, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4709, "text": "They are transgressors in the presence of your Lord\"."}, {"id": 229053, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4710, "text": "We saved the believers among them,"}, {"id": 229054, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4711, "text": "but We found only one Muslim house."}, {"id": 229055, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4712, "text": "We left therein evidence for those who fear the painful torment."}, {"id": 229056, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4713, "text": "There is also evidence (of the Truth) in the story of Moses when We sent him to the Pharaoh with clear authority."}, {"id": 229057, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4714, "text": "The Pharaoh and his forces turned away from him, saying, \"He is either a magician or an insane person\"."}, {"id": 229058, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4715, "text": "We seized him and his army and threw them into the sea. He himself was to be blamed."}, {"id": 229059, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4716, "text": "There is also evidence of the Truth in the story of Ad whom We struck with a violent wind"}, {"id": 229060, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4717, "text": "which turned everything it approached into dust."}, {"id": 229061, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4718, "text": "There is also evidence (of the Truth) in the story of the Thamud, who were told to enjoy themselves for an appointed time."}, {"id": 229062, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4719, "text": "They transgressed against the command of their Lord. So a blast of sound struck them and they were unable to do anything but stare."}, {"id": 229063, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4720, "text": "They were unable to stand up, nor were they helped."}, {"id": 229064, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4721, "text": "The people of Noah who lived before them were also evil doing people."}, {"id": 229065, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4722, "text": "We have made the heavens with Our own hands and We expanded it."}, {"id": 229066, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4723, "text": "We have spread out the earth and how brilliantly it is spread!"}, {"id": 229067, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4724, "text": "We have created everything in pairs so that perhaps you may take heed."}, {"id": 229068, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4725, "text": "(Muhammad), tell them,\"Seek refuge in God. I have been sent from Him to plainly warn you."}, {"id": 229069, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Do not choose other gods besides Him. I have been sent from Him to plainly warn you\"."}, {"id": 229070, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4727, "text": "In the same way no Messenger came to those who lived before them without his people calling him a magician or an insane person."}, {"id": 229071, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Have they inherited such dealings with the Prophets from their predecessors or are they a rebellious people?"}, {"id": 229072, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4729, "text": "(Muhammad), leave them alone and you will not be blamed."}, {"id": 229073, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Keep on reminding them. This benefits the believers."}, {"id": 229074, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4731, "text": "We have created jinn and human beings only that they might worship Me."}, {"id": 229075, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4732, "text": "I do not expect to receive any sustenance from them or that they should feed Me."}, {"id": 229076, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4733, "text": "It is God Who is the Sustainer and the Lord of invincible strength."}, {"id": 229077, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4734, "text": "The unjust will bear a burden like that of their unjust predecessors. Let them not make Me bring immediate punishment upon them."}, {"id": 229078, "identifier": "en.sarwar", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Woe to the disbelievers when the day with which they have been threatened comes!"}]