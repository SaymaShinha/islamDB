[{"id": 230159, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5849, "text": "Woe to those who are fraudulent in (weighing and measuring),"}, {"id": 230160, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5850, "text": "those who demand a full measure from others"}, {"id": 230161, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5851, "text": "but when they measure or weigh, give less."}, {"id": 230162, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Do they not realize that they will be resurrected"}, {"id": 230163, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5853, "text": "on a great day"}, {"id": 230164, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5854, "text": "when mankind will stand before the Lord of the Universe?"}, {"id": 230165, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5855, "text": "Woe to them! Let them know that the records of the sinner's deeds are in Sijin."}, {"id": 230166, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5856, "text": "Would that you knew what Sijin is!?"}, {"id": 230167, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5857, "text": "It is a comprehensively written Book (of records)."}, {"id": 230168, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5858, "text": "Woe, on that day, to those who have rejected God's revelations"}, {"id": 230169, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5859, "text": "and those who have rejected the Day of Judgment."}, {"id": 230170, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5860, "text": "No one rejects it except the sinful transgressors"}, {"id": 230171, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5861, "text": "who, when listening to Our revelations, say, \"These are only ancient legends\"."}, {"id": 230172, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5862, "text": "They will never have faith. In fact, their hearts are stained from their deeds."}, {"id": 230173, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5863, "text": "On the Day of Judgment, they will certainly be barred from the mercy of their Lord."}, {"id": 230174, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5864, "text": "They will suffer the heat of fire"}, {"id": 230175, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5865, "text": "and who will be told, \"This is what you had called a lie\"."}, {"id": 230176, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5866, "text": "However, the records of the deeds of the virtuous ones will certainly be in Illiyin."}, {"id": 230177, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5867, "text": "Would that you knew what Illiyin is!"}, {"id": 230178, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5868, "text": "It is a comprehensively written Book (of records)."}, {"id": 230179, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5869, "text": "The ones nearest to God will bring it to public."}, {"id": 230180, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5870, "text": "The virtuous will live in bliss,"}, {"id": 230181, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5871, "text": "reclining on couches, reviewing (the bounties given to them)."}, {"id": 230182, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5872, "text": "You can trace on their faces the joy of their bliss."}, {"id": 230183, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5873, "text": "They will be given pure wine out of sealed containers"}, {"id": 230184, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5874, "text": "which have the fragrance of musk. This is the kind of place for which one should really aspire."}, {"id": 230185, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5875, "text": "With the wine is a drink from Tasnim,"}, {"id": 230186, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5876, "text": "a spring, the nearest ones to God will drink from it."}, {"id": 230187, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5877, "text": "The sinners had been laughing at the believers."}, {"id": 230188, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5878, "text": "When passing by them, they would wink at one another"}, {"id": 230189, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5879, "text": "and, on returning to their people, boast about what they had done."}, {"id": 230190, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5880, "text": "On seeing the believers, they would say, \"These people have gone astray\"."}, {"id": 230191, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5881, "text": "No one has appointed them to watch over the believers."}, {"id": 230192, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5882, "text": "On the Day of Judgment, the believers will laugh at the disbelievers"}, {"id": 230193, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5883, "text": "while reclining on couches and reviewing (the bounties given to them)."}, {"id": 230194, "identifier": "en.sarwar", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5884, "text": "Will not the disbelievers then be duly recompensed for their laughing at the believers?"}]