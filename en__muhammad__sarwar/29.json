[{"id": 227688, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3341, "text": "Alif. Lam. Mim."}, {"id": 227689, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3342, "text": "Do people think they will not be tested because they say, \"We have faith?\""}, {"id": 227690, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3343, "text": "We had certainly tried those who lived before them to make sure who were truthful in their faith and who were liars."}, {"id": 227691, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3344, "text": "Do the evil-doers think they can escape Us? How terrible is their judgment?"}, {"id": 227692, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3345, "text": "Let those who have the desire to be in the presence of God on the Day of Judgment know that their day will certainly be coming. God is All-Hearing and All-Knowing."}, {"id": 227693, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3346, "text": "Whoever strives hard should know that it is for his own good. God is independent of the whole world."}, {"id": 227694, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3347, "text": "We shall expiate the sins of the righteously striving believers and shall reward them better than their deeds."}, {"id": 227695, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3348, "text": "We have advised the human being, \"Be kind to your parents. Do not obey them if they force you to consider equal to Me things which you do not know are such.\" You will all return (to Me) and I shall show all that you have done."}, {"id": 227696, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3349, "text": "We shall admit the righteously striving believers into the company of the pious ones."}, {"id": 227697, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3350, "text": "Some people say, \"We have faith in God.\" But when they face some hardship for His cause, they begin to consider the persecution that they have experienced from people as a torment from God. When your Lord grants you a victory, they say, \"We were with you.\" Does God not know best what is in the hearts of every creature?"}, {"id": 227698, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3351, "text": "God certainly knows all about the believers and the hypocrites."}, {"id": 227699, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3352, "text": "The unbelievers say to the believers. \"Follow our way. We shall take the responsibility for your sins.\" They cannot take responsibility for any of the sins. They are only liars."}, {"id": 227700, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3353, "text": "Besides the other burdens that they will have to carry, they will certainly be loaded with the burden of their own sins. They will be questioned on the Day of Judgment about what they had falsely invented."}, {"id": 227701, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3354, "text": "We sent Noah to his people and he lived with them for nine hundred and fifty years, then the flood engulfed them for their injustice."}, {"id": 227702, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3355, "text": "We saved Noah and the people in the Ark and made (their case) a miracle for the world."}, {"id": 227703, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3356, "text": "Abraham told his people, \"Worship God and have fear of Him. It is better for you if only you knew it."}, {"id": 227704, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3357, "text": "You worship idols besides God and you create falsehood. Whatever you worship besides Him cannot provide you with anything for your sustenance. Seek your sustenance from the bounties of God. Worship Him. Give Him thanks. To Him you will all return.\""}, {"id": 227705, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3358, "text": "If you, (pagans), call our (revelations) lies, certainly many generations living before you have also done the same thing. The duty of a Messenger is only to preach clearly."}, {"id": 227706, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3359, "text": "Have they not seen how God begins the creation and then turns it back? This is not difficult at all for God."}, {"id": 227707, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3360, "text": "(Muhammad), say to them, \"Travel through the land and see how He has begun the creation and how He will invent the next life. God has power over all things."}, {"id": 227708, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3361, "text": "He punishes or grants mercy to whomever He wants and to Him you will all return."}, {"id": 227709, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3362, "text": "You cannot challenge God in the heavens or in the earth. No one besides God is your guardian or helper."}, {"id": 227710, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3363, "text": "Those who have rejected God's revelations have no hope in receiving His mercy. They will face a painful torment."}, {"id": 227711, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3364, "text": "(Abraham's) people had no answer except suggesting, \"Kill him or burn him.\" But God saved him from the fire. In this there is evidence (of truth) for the believing people."}, {"id": 227712, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3365, "text": "Abraham said, \"You believe in idols besides God only out of worldly love, but on the Day of Judgment you will reject and condemn each other. Your dwelling will be fire and no one will help you.\""}, {"id": 227713, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3366, "text": "Only Lot believed in (Abraham) and said, \"I seek refuge in my Lord, for He is Majestic and All-wise\"."}, {"id": 227714, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3367, "text": "We granted Isaac and Jacob to Abraham and We bestowed upon his offspring, prophethood and the Book. We gave him his reward in this world and in the next life, he will be among the pious ones."}, {"id": 227715, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3368, "text": "Lot told his people, \"You are certainly committing the kind of indecency which no one in the world has committed before\"."}, {"id": 227716, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3369, "text": "Do you engage in carnal relations with men, rob the travellers, and commit evil in your gatherings? His people had no answer but to say, \"Bring upon us the torment of God if you are truthful\"."}, {"id": 227717, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3370, "text": "He prayed, \"Lord help me against the evil-doing people.\""}, {"id": 227718, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3371, "text": "When Our angelic Messengers brought glad news to Abraham, they told him, \"We are about to destroy the people of this town for their injustice\"."}, {"id": 227719, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3372, "text": "Abraham said, \"Lot is there in that town!\" They said, \"We know everyone there. We shall certainly save him and his family except his wife who will remain behind.\""}, {"id": 227720, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3373, "text": "When Our angelic Messengers came to Lot, he was grieved and depressed to see them. They told him, \"Do not be afraid or grieved. We will rescue you and your family except your wife who will remain behind."}, {"id": 227721, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3374, "text": "We will bring torment from the sky on this town because of the evil-deeds of its inhabitants\"."}, {"id": 227722, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3375, "text": "We left manifest evidence (of the truth) there for the people of understanding."}, {"id": 227723, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3376, "text": "We sent to the people of Midian their brother Shu`ayb. He told them, \"Worship God. Have hope in the life to come. Do not spread evil in the land\"."}, {"id": 227724, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3377, "text": "They rejected him so We jolted them with a violent earthquake and they were left motionless in their houses."}, {"id": 227725, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3378, "text": "How the people of Ad and Thamud were destroyed is evident to you from their homes. Satan made their deeds seem attractive to them and prevented them from the right path, even though they had visions."}, {"id": 227726, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3379, "text": "Korah, the Pharaoh, and Haman were also destroyed. Moses had brought them illustrious miracles, but they were puffed-up with pride in the land and they could not defeat Us."}, {"id": 227727, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3380, "text": "We punished all of these people because of their sin. Some of them were struck by a violent sand-storm, some by a blast of sound, others were swallowed up by the earth, and some were drowned (in the sea). God did not do injustice to them, but they had wronged themselves."}, {"id": 227728, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3381, "text": "The belief of considering other things as one's guardians besides God is as feeble as a spider's web. The spider's web is the frailest of all dwellings, if only they knew it."}, {"id": 227729, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3382, "text": "God knows whatever they worship besides Him; He is the Majestic and All-wise."}, {"id": 227730, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3383, "text": "These are parables which We tell to human being, but only the learned ones understand them."}, {"id": 227731, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3384, "text": "God has created the heavens and the earth for a genuine purpose. In this there is evidence (of the truth) for the believers."}, {"id": 227732, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 346, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3385, "text": "(Muhammad), recite to them what has been revealed to you in the Book and be steadfast in prayer; prayer keeps one away from indecency and evil. It is the greatest act of worshipping God. God knows what you do."}, {"id": 227733, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3386, "text": "Do not argue with the People of the Book except only by the best manner, except the unjust among them. Tell them, \"We believe in what is revealed to us and to you. Our Lord and your Lord is one. We have submitted ourselves to His will\"."}, {"id": 227734, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3387, "text": "We have revealed the Book to you. Some of the People of the Book and some of the pagans also believe in it. No one rejects Our revelations except the infidels."}, {"id": 227735, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3388, "text": "You were not able to read or write before the Quran was revealed to you; otherwise, the followers of falsehood would have tried to confuse the matter."}, {"id": 227736, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3389, "text": "In fact, the Quran consists of illustrious verses that exist in the hearts of those who have knowledge. No one rejects Our revelations except the unjust ones."}, {"id": 227737, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3390, "text": "They say, \"Why a miracle is not sent to him from his Lord.\" Say, \"Miracles are in the hands of God. I am simply a warner.\""}, {"id": 227738, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3391, "text": "Is it not enough for them that We have revealed the Book to you to be recited to them. It is a mercy and a reminder for the believers."}, {"id": 227739, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3392, "text": "Say, \"God is sufficient as a witness between me and you. He knows all that is in the heavens and the earth.\" Those who have faith in falsehood and disbelieve in God are certainly lost."}, {"id": 227740, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3393, "text": "They demand you to bring upon them torment immediately. Had not the time been fixed, the torment would certainly have approached them. It would have come to them suddenly and they would not have even realized how it came."}, {"id": 227741, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3394, "text": "They demand you to bring upon them the torment immediately. Hell will certainly engulf the unbelievers."}, {"id": 227742, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3395, "text": "They will be told on the Day of Judgment, when the torment will surround them from all sides, \"Suffer the consequences of your deeds.\""}, {"id": 227743, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3396, "text": "My believing servants, My land is vast. Worship Me alone."}, {"id": 227744, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3397, "text": "Every soul will experience the agony of death and to Me you will all return."}, {"id": 227745, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3398, "text": "We shall give mansions in Paradise wherein streams flow to the righteously striving believers and therein they will live forever. How blessed is the reward of the hard working people"}, {"id": 227746, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3399, "text": "who have exercised patience and who have had trust in their Lord."}, {"id": 227747, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3400, "text": "There are many living creatures which do not carry their sustenance, but God provides them and you with sustenance. He is All-hearing and All-knowing."}, {"id": 227748, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3401, "text": "If you ask them, \"Who has created the heavens and the earth and has subdued the sun and moon?\" They will say, \"God has done it.\" So why are they wandering about!?"}, {"id": 227749, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3402, "text": "God increases the sustenance of whichever of His servants He wants and He determines their share. God has the knowledge of all things."}, {"id": 227750, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3403, "text": "If you ask them, \"Who has sent down water from the sky to revive the dead earth?\" They will say, \"God has done it.\" Say, \"It is only God who deserves all praise, but many of them do not understand.\""}, {"id": 227751, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3404, "text": "The worldly life is not more than a childish game. It is the life hereafter which will be the real life, if only they knew it."}, {"id": 227752, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3405, "text": "When they sail in a boat, they sincerely pray to God with pure faith. But when We bring them safely to land, they start considering things equal to God."}, {"id": 227753, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3406, "text": "Let them be ungrateful to what We have granted them. Let them enjoy themselves, but they will soon know (the consequences of their deeds)."}, {"id": 227754, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3407, "text": "Have the (pagans) not seen that We have made the holy precinct a safe place while all the people around are suffering terror. Do they believe in falsehood and disbelieve in God's bounties?"}, {"id": 227755, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3408, "text": "Who is more unjust than one who invents falsehood against God or rejects the Truth after it has come to him? Is not hell the dwelling for the disbelievers."}, {"id": 227756, "identifier": "en.sarwar", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3409, "text": "We shall certainly guide those who strive for Our cause to Our path. God is certainly with the righteous ones."}]