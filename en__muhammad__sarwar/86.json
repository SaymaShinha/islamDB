[{"id": 230242, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5932, "text": "By the heavens and al-tariq."}, {"id": 230243, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5933, "text": "Do you know what al-tariq is?"}, {"id": 230244, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5934, "text": "(It is a nightly radiant star)."}, {"id": 230245, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5935, "text": "There is no soul which is not guarded (by the two angels who record all of its deeds)."}, {"id": 230246, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5936, "text": "Let the human being reflect that from what he has been created."}, {"id": 230247, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5937, "text": "He has been created from an ejected drop of fluid"}, {"id": 230248, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5938, "text": "which comes out of the loins and ribs."}, {"id": 230249, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5939, "text": "God has all power to resurrect him."}, {"id": 230250, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5940, "text": "On the day when all secrets will be made public,"}, {"id": 230251, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5941, "text": "he will have no power, nor anyone to help him."}, {"id": 230252, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5942, "text": "By the rotating heavens"}, {"id": 230253, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5943, "text": "and the replenishing earth,"}, {"id": 230254, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5944, "text": "the Quran is the final word,"}, {"id": 230255, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5945, "text": "and it is certainly not a jest."}, {"id": 230256, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5946, "text": "They (disbelievers) plot every evil plan,"}, {"id": 230257, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5947, "text": "but I too plan against them."}, {"id": 230258, "identifier": "en.sarwar", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5948, "text": "Give respite to the disbelievers and leave them alone for a while."}]