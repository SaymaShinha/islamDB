[{"id": 227848, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3504, "text": "Alif. Lam. Mim."}, {"id": 227849, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3505, "text": "There is no doubt that this Book is revealed by the Lord of the Universe."}, {"id": 227850, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3506, "text": "Do they say that he, (Muhammad), has invented it? No, it is the truth from your Lord so that you will warn the people who have not received a warning before you. Perhaps they will seek guidance."}, {"id": 227851, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3507, "text": "God is the one who created the heavens and the earth and all that is between them in six days, then He established His dominion over the Throne. No one besides Him is your guardian or intercessor. Will you then not take heed?"}, {"id": 227852, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3508, "text": "He sends the regulation of the affair from the heavens to the earth, then on the day which is equal to one thousand years of yours, it will ascend to Him."}, {"id": 227853, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3509, "text": "He knows the unseen and the seen. He is Majestic and All-merciful."}, {"id": 227854, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3510, "text": "It is He Who created everything in the best manner and began the creation of the human being from clay."}, {"id": 227855, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3511, "text": "He made His offspring come into existence from an extract of insignificant fluid,"}, {"id": 227856, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3512, "text": "then He gave it proper shape and blew His spirit in it. He made ears, eyes and hearts for you, but you give Him very little thanks."}, {"id": 227857, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3513, "text": "They have said, \"How can we be brought to life again after we have been lost in the earth?\" In fact, they have no faith in the Day of Judgment"}, {"id": 227858, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3514, "text": "(Muhammad), say, \"The angel of death, who is appointed over everyone of you, will cause you to die and to your Lord you will all return.\""}, {"id": 227859, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3515, "text": "Would that you could see (on the Day of Judgment) that criminals, with their heads hanging down before their Lord, saying, \"Our Lord, we have seen and heard. Send us back to act righteously. Now we have strong faith\"."}, {"id": 227860, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3516, "text": "Had We wanted, We could have given guidance to every soul, but My decree, that hell will be filled-up with jinn and people, has already been executed."}, {"id": 227861, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3517, "text": "They will be told, \"Suffer on this Day of Judgment. For your having ignored it, We have ignored you. Suffer everlasting torment for your evil deeds.\""}, {"id": 227862, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": true, "sajda_id": 10, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3518, "text": "The only people who believe in Our revelations are those who, when reminded about them, bow down in prostration and glorify their Lord with His praise without pride."}, {"id": 227863, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3519, "text": "Their sides give-up rest in beds in order to pray before their Lord in fear and hope. They spend for the cause of God out of what we have given them."}, {"id": 227840, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3520, "text": "No soul knows what delight awaits them as the reward for their deeds."}, {"id": 227865, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3521, "text": "Is a believer equal to an evil-doer? They are not equal at all."}, {"id": 227866, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3522, "text": "The righteously striving believers will have Paradise for their dwelling as the reward of their good deeds."}, {"id": 227867, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3523, "text": "However, the dwelling of the sinful ones will be hell fire. Each time they try to come out, they will be turned back with this remark, \"Suffer the torment of the fire which you had called a lie\"."}, {"id": 227868, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3524, "text": "We shall certainly make them suffer worldly torment before suffering the great torment so that perhaps they may return to Us."}, {"id": 227869, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3525, "text": "Who are more unjust than those who are reminded of the revelation of their Lord, but have ignored them? We will take revenge on the criminal."}, {"id": 227870, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3526, "text": "We gave the Book to Moses - do not have any doubt about the Day of Judgment - and made it a guide for the children of Israel."}, {"id": 227871, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3527, "text": "We appointed some of the Israelites as leaders for their exercising patience to guide the others to Our commands. They had firm belief in Our revelations."}, {"id": 227872, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3528, "text": "Your Lord will issue His decree about their (believers and disbeliever's) differences on the Day of Judgment."}, {"id": 227873, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3529, "text": "Was it not a lesson for them, (the unbelievers), that We destroyed the many generations living before them among whose ruined dwellings they are now walking. In this there is many evidence (of the truth). Will they not then listen?"}, {"id": 227874, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3530, "text": "Have they not seen that We drive the water to the barren land and cause crops to grow which they and their cattle consume? Why, then, they will not see?"}, {"id": 227875, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3531, "text": "They say, \"If what you say is true, when will the final triumph come?\""}, {"id": 227876, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3532, "text": "Say, \"On the day of the final triumph, the faith of the disbelievers will be of no avail to them, nor will they be given any respite\"."}, {"id": 227877, "identifier": "en.sarwar", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 3533, "text": "Turn away from them and wait. They are, also, waiting."}]