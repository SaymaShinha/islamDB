[{"id": 230445, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6139, "text": "When the earth is shaken by a terrible quake"}, {"id": 230446, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6140, "text": "and it throws out its burden,"}, {"id": 230447, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6141, "text": "the human being will say (in horror), \"What is happening to it?\""}, {"id": 230448, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6142, "text": "On that day the earth will declare all (the activities of the human being) which have taken place on it,"}, {"id": 230449, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6143, "text": "having been inspired by your Lord."}, {"id": 230450, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6144, "text": "On that day, people will come out of their graves in different groups to see (the results of) their own deeds."}, {"id": 230451, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6145, "text": "Whoever has done an atom's weight of good,"}, {"id": 230452, "identifier": "en.sarwar", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6146, "text": "will see it and whoever has done an atom's weight of evil, will also see it."}]