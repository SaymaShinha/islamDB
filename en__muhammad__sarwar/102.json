[{"id": 230474, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6169, "text": "The desire to have more of the worldly gains have pre-occupied you so much (that you have neglected remembring God),"}, {"id": 230475, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6170, "text": "until you come to the graves."}, {"id": 230476, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6171, "text": "You shall know."}, {"id": 230477, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6172, "text": "You shall cetainly know (about the consequences of your deeds)."}, {"id": 230478, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6173, "text": "You will certainly have the knowledge of your deeds beyond all doubt."}, {"id": 230479, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6174, "text": "You will be shown hell"}, {"id": 230480, "identifier": "en.sarwar", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "p