[{"id": 230405, "identifier": "en.sarwar", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6099, "text": "By the fig, by the olive,"}, {"id": 230406, "identifier": "en.sarwar", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6100, "text": "by Mount Sinai"}, {"id": 230407, "identifier": "en.sarwar", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 6101, "text": "and by this inviolable city, Mecca."}, {"id": 230408, "identifier": "en.sarwar", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_qua