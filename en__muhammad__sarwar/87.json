[{"id": 230259, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5949, "text": "(Muhammad), glorify the Name of your lord, the Most High,"}, {"id": 230260, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5950, "text": "Who has created (all things) proportionately,"}, {"id": 230261, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5951, "text": "decreed their destinies, and provided them with guidance."}, {"id": 230262, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5952, "text": "It is He who has caused the grass to grow,"}, {"id": 230263, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5953, "text": "then caused it to wither away."}, {"id": 230264, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5954, "text": "We shall teach you (the Quran) and you will not forget it"}, {"id": 230265, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5955, "text": "unless God wills it to be otherwise. He knows all that is made public and all that remains hidden."}, {"id": 230266, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5956, "text": "We shall make all your tasks easy."}, {"id": 230267, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5957, "text": "Therefore, keep on preaching as long as it is of benefit."}, {"id": 230268, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Those who have fear of God will benefit"}, {"id": 230269, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5959, "text": "but the reprobates will turn away"}, {"id": 230270, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5960, "text": "and suffer the heat of the great fire"}, {"id": 230271, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5961, "text": "wherein they will neither live nor die."}, {"id": 230272, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5962, "text": "Lasting happiness will be for those who purify themselves,"}, {"id": 230273, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5963, "text": "remember the name of the Lord, and pray to Him."}, {"id": 230274, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5964, "text": "However, (the disbelievers) prefer the worldly life"}, {"id": 230275, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5965, "text": "even though the life hereafter will be better and will last forever."}, {"id": 230276, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5966, "text": "This is what is written in the ancient heavenly Books,"}, {"id": 230277, "identifier": "en.sarwar", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Sarwar", "transelator_en_name": "Muhammad Sarwar", "format": "text", "type": "translation", "ayah_number": 5967, "text": "the Scriptures of Abraham and Moses."}]