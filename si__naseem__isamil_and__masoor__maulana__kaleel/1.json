[{"id": 486409, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 1, "text": "\ufeff\u0d85\u0dc3\u0dd3\u0db8\u0dd2\u0dad \u0daf\u0dba\u0dcf\u0dc0 \u0dc4\u0dcf \u0d85\u0dc3\u0db8\u0dc3\u0db8 \u0d85\u0db1\u0dd4\u0d9a\u0db8\u0dca\u0db4\u0dcf\u0dc0\u0dd9\u0db1\u0dca \u0dba\u0dd4\u0dad\u0dd4 \u0d85\u0dbd\u0dca\u0dbd\u0dcf\u0dc4\u0dca\u0d9c\u0dda \u0db1\u0dcf\u0db8\u0dba\u0dd9\u0db1\u0dca"}, {"id": 486410, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 2, "text": "\u0dc3\u0dd2\u0dba\u0dbd\u0dd6\u0db8 \u0db4\u0dca\u200d\u0dbb\u0dc1\u0d82\u0dc3\u0dcf \u0d85\u0dbd\u0dca\u0dbd\u0dcf\u0dc4\u0dca\u0da7\u0dba (\u0d94\u0dc4\u0dd4) \u0dc3\u0d9a\u0dbd \u0dbd\u0ddd\u0d9a\u0dba\u0db8 \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab\u0dba \u0d9a\u0dbb, \u0dc3\u0d82\u0dc0\u0dbb\u0dca\u0db0\u0db1\u0dba \u0d9a\u0dbb, \u0dc3\u0d82\u0dbb\u0d9a\u0dca\u0dc2\u0dab\u0dba \u0d9a\u0dbb, \u0db4\u0dbb\u0dd2\u0db4\u0ddd\u0dc2\u0dab\u0dba \u0d9a\u0dbb, \u0db4\u0dbb\u0dd2\u0db4\u0dcf\u0dbd\u0db1\u0dba \u0d9a\u0dbb\u0db1\u0dca\u0db1\u0dcf\u0dc0\u0dd6\u0daf"}, {"id": 486411, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 3, "text": "\u0d85\u0db4\u0dbb\u0dd2\u0db8\u0dd2\u0dad \u0d85\u0db1\u0dd4\u0d9a\u0db8\u0dca\u0db4\u0dcf\u0dc0\u0daf, \u0d85\u0dc3\u0db8\u0dc3\u0db8 \u0daf\u0dba\u0dcf\u0dc0\u0daf"}, {"id": 486412, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 4, "text": "\u0dc0\u0dd2\u0db1\u0dd2\u0dc1\u0dca\u0da0\u0dba \u0daf\u0dd2\u0db1 \u0d85\u0db0\u0dd2\u0db4\u0dad\u0dd2(\u0daf \u0dc0\u0db1\u0dca\u0db1\u0dda\u0dba)"}, {"id": 486413, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 5, "text": "(\u0d85\u0db1\u0dd4\u0d9a\u0db8\u0dca\u0db4\u0dcf\u0dc0\u0daf \u0daf\u0dba\u0dcf\u0dc0\u0daf \u0db4\u0dd2\u0dbb\u0dd2 \u0d85\u0db4 \u0daf\u0dd9\u0dc0\u0dd2\u0dba\u0db1\u0dda) \u0d94\u0db6\u0dc0\u0db8 \u0d85\u0db4 \u0db1\u0db8\u0daf\u0dd2\u0db1\u0dca\u0db1\u0dd9\u0db8\u0dd4. \u0d94\u0db6\u0d9c\u0dd9\u0db1\u0dca\u0db8 \u0d85\u0db4 \u0db4\u0dd2\u0dc4\u0dd2\u0da7 \u0db4\u0dad\u0db1\u0dca\u0db1\u0dd9\u0db8\u0dd4."}, {"id": 486414, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 6, "text": "\u0d94\u0db6 \u0d85\u0db4\u0da7 \u0d8d\u0da2\u0dd4 \u0db8\u0dcf\u0dbb\u0dca\u0d9c\u0dba \u0db4\u0dd9\u0db1\u0dca\u0dc0\u0dcf \u0daf\u0dd9\u0db1\u0dd4 \u0db8\u0dd0\u0db1\u0dc0"}, {"id": 486415, "identifier": "si.naseemismail", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 7, "text": "\u0d94\u0db6\u0d9c\u0dda \u0d85\u0db1\u0dd4\u0d9a\u0db8\u0dca\u0db4\u0dcf\u0dc0 \u0dc4\u0dcf \u0daf\u0dba\u0dcf\u0dc0 \u0d9a\u0dcf \u0dc4\u0da7 \u0db4\u0dc4\u0dc5 \u0d9a\u0dc5\u0dda\u0daf, \u0d94\u0dc0\u0dd4\u0db1\u0dca\u0d9c\u0dda \u0db8\u0dcf\u0dbb\u0dca\u0d9c\u0dba\u0dd9\u0dc4\u0dd2 (\u0dba\u0dcf\u0db8\u0da7 \u0dc3\u0dbd\u0dc3\u0dca\u0dc0\u0db1\u0dd4 \u0db8\u0dd0\u0db1\u0dc0) (\u0d91\u0dba \u0d94\u0db6\u0d9c\u0dda) \u0d9a\u0ddd\u0db4\u0dba\u0da7 \u0db4\u0dcf\u0dad\u0dca\u200d\u0dbb \u0dc0\u0dd6 \u0db8\u0dcf\u0dbb\u0dca\u0d9c\u0dba\u0dd9\u0dc4\u0dd2\u0daf \u0db1\u0ddc\u0dc0, \u0d85\u0dba\u0dc4\u0db8\u0d9c \u0dc0\u0dd6\u0dc0\u0db1\u0dca\u0d9c\u0dda \u0db8\u0dcf\u0dbb\u0dca\u0d9c\u0dba\u0dd9\u0dc4\u0dd2\u0daf \u0db1\u0ddc\u0dc0."}]