[{"id": 492456, "identifier": "si.naseemismail", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 6126, "text": "\u0db1\u0dd2\u0dba\u0dad \u0dc0\u0dc1\u0dba\u0dd9\u0db1\u0dca\u0db8 \u0d85\u0db4\u0dd2 \u0db8\u0dd9\u0dba (\u0db8\u0dd9\u0db8 \u0d9a\u0dd4\u0dbb\u0dca\u0d86\u0db1\u0dba \u0d89\u0dad\u0dcf\u0db8\u0dad\u0dca \u0d9c\u0dde\u0dbb\u0dc0\u0dba\u0dd9\u0db1\u0dca \u0db4\u0dd2\u0dbb\u0dd2) \u0dbd\u0dd9\u0dba\u0dd2\u0dbd\u0dad\u0dd4\u0dbd\u0dca \u0d9a\u0daf\u0dca\u0dbb\u0dca \u0db1\u0db8\u0dd0\u0dad\u0dd2 \u0d91\u0d9a\u0dca \u0dbb\u0dcf\u0dad\u0dca\u200d\u0dbb\u0dd2\u0dba\u0d9a\u0daf\u0dd3 (\u0db4\u0dc5\u0db8\u0dd4\u0dc0\u0dd9\u0db1\u0dca\u0db8) \u0db4\u0dc4\u0dc5 \u0d9a\u0dbb \u0dad\u0dd0\u0db6\u0dd4\u0dc0\u0dd9\u0db8\u0dd4."}, {"id": 492457, "identifier": "si.naseemismail", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "si", "name": "Naseem Ismail", "transelator_en_name": "Naseem Isamil and Masoor Maulana, Kaleel", "format": "text", "type": "translation", "ayah_number": 6127, "text": "(\u0db1\u0db6\u0dd2\u0dba\u0dda!) \u0d91\u0db8 \u0d9c\u0dde\u0dbb\u0dc0\u0dba\u0dd9\u0db1\u0dca \u0db4\u0dd2\u0dbb\u0dd2 \u0dbb\u0dcf\u0dad\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dda (\u0db8\u0dc4\u0dd2\u0d