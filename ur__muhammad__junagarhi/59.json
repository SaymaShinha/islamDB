ormat": "text", "type": "translation", "ayah_number": 5147, "text": "\u0627\u06af\u0631 \u06c1\u0645 \u0627\u0633 \u0642\u0631\u0622\u0646 \u06a9\u0648 \u06a9\u0633\u06cc \u067e\u06c1\u0627\u0691 \u067e\u0631 \u0627\u062a\u0627\u0631\u062a\u06d2 \u062a\u0648 \u062a\u0648 \u062f\u06cc\u06a9\u06be\u062a\u0627 \u06a9\u06c1 \u062e\u0648\u0641 \u0627\u0644\u0670\u06c1\u06cc \u0633\u06d2 \u0648\u0647 \u067e\u0633\u062a \u06c1\u0648\u06a9\u0631 \u0679\u06a9\u0691\u06d2 \u0679\u06a9\u0691\u06d2 \u06c1\u0648 \u062c\u0627\u062a\u0627 \u06c1\u0645 \u0627\u0646 \u0645\u062b\u0627\u0644\u0648\u06ba \u06a9\u0648 \u0644\u0648\u06af\u0648\u06ba \u06a9\u06d2 \u0633\u0627\u0645\u0646\u06d2 \u0628\u06cc\u0627\u0646 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u062a\u0627\u06a9\u06c1 \u0648\u0647 \u063a\u0648\u0631 \u0648\u0641\u06a9\u0631 \u06a9\u0631\u06cc\u06ba"}, {"id": 672419, "identifier": "ur.junagarhi", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0645\u062d\u0645\u062f \u062c\u0648\u0646\u0627\u06af\u0691\u06be\u06cc", "transelator_en_name": "Muhammad Junagarhi", "format": "text", "type": "translation", "ayah_number": 5148, "text": "\u0648\u06c1\u06cc \u0627\u0644\u0644\u06c1 \u06c1\u06d2 \u062c\u0633 \u06a9\u06d2 \u0633\u0648\u0627 \u06a9\u0648\u0626\u06cc \u0645\u0639\u0628\u0648\u062f \u0646\u06c1\u06cc\u06ba\u060c \u0686\u06be\u067e\u06d2 \u06a9\u06be\u0644\u06d2 \u06a9\u0627 \u062c\u0627\u0646\u0646\u06d2 \u0648\u0627\ufefb \u0645\u06c1\u0631\u0628\u0627\u0646 \u0627\u0648\u0631 \u0631\u062d\u0645 \u06a9\u0631\u0646\u06d2 \u0648\u0627\ufefb"}, {"id": 672420, "identifier": "ur.junagarhi", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0