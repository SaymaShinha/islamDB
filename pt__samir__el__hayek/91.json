[{"id": 429884, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6044, "text": "Pelo sol e pelo seu esplendor (matinal),"}, {"id": 429885, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6045, "text": "Pela lua, que o segue,"}, {"id": 429886, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6046, "text": "Pelo dia, que o revela,"}, {"id": 429887, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6047, "text": "Pela noite, que o encobre."}, {"id": 429888, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6048, "text": "Pelo firmamento e por Quem o construiu,"}, {"id": 429889, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6049, "text": "Pela terra e por Quem a dilatou,"}, {"id": 429890, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6050, "text": "Pela alma e por Quem aperfei\u00e7oou,"}, {"id": 429891, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6051, "text": "E lhe imprimiu o discernimento entre o que \u00e9 certo e o que \u00e9 errado,"}, {"id": 429892, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6052, "text": "Que ser\u00e1 venturoso quem a purificar (a alma),"}, {"id": 429893, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6053, "text": "E desventurado quem a corromper."}, {"id": 429894, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6054, "text": "A tribo de Tamud, por suas transgress\u00f5es, desmentiu o seu mensageiro."}, {"id": 429895, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6055, "text": "E o mais perverso deles se incumbiu (de matar a camela)."}, {"id": 429896, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6056, "text": "Por\u00e9m, o mensageiro de Deus lhes disse: \u00c9 a camela de Deus! N\u00e3o a priveis da sua bebida!"}, {"id": 429897, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6057, "text": "Por\u00e9m, desmentiram-no e a esquartejaram, pelo que o seu Senhor os exterminou, pelos seus pecados, a todos por igual."}, {"id": 429898, "identifier": "pt.elhayek", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6058, "text": "E Ele n\u00e3o teme as conseq\u00fc\u00eancias."}]