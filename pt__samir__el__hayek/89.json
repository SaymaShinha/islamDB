[{"id": 429834, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 5994, "text": "Pela aurora,"}, {"id": 429835, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 5995, "text": "E pelas dez noites,"}, {"id": 429836, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 5996, "text": "E pelo par e pelo \u00edmpar,"}, {"id": 429837, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 5997, "text": "E pela noite, quando se retira (que sereis castigados)!"}, {"id": 429838, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 5998, "text": "Porventura, n\u00e3o h\u00e1 nisso um juramento adequado, para o sensato?"}, {"id": 429839, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 5999, "text": "N\u00e3o reparaste em como o teu Senhor procedeu, em rela\u00e7\u00e3o \u00e0 (tribo de) Ad,"}, {"id": 429840, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6000, "text": "Aos (habitantes de) Iram, (cidade) de pilares elevados,"}, {"id": 429841, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6001, "text": "Cujo similar n\u00e3o foi criado em toda a terra?"}, {"id": 429842, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6002, "text": "E no povo de Samud, que perfurou rochas no vale?"}, {"id": 429843, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6003, "text": "E no Fara\u00f3, o senhor das estacas,"}, {"id": 429844, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6004, "text": "Os quais transgrediram, na terra,"}, {"id": 429845, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6005, "text": "E multiplicaram, nela, a corrup\u00e7\u00e3o,"}, {"id": 429846, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6006, "text": "Pelo que o teu Senhor lhes infligiu variados castigos?"}, {"id": 429847, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6007, "text": "Atenta para o fato de que o teu Senhor est\u00e1 sempre alerta."}, {"id": 429848, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6008, "text": "Quanto ao homem, quando seu Senhor o experimenta, honrando-o e agraciando-o, diz (empertigado): Meu Senhor me honra!"}, {"id": 429849, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6009, "text": "Por\u00e9m, quando o prova, restringindo a Sua gra\u00e7a, diz: Meu Senhor me afronta!"}, {"id": 429850, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6010, "text": "Qual! V\u00f3s n\u00e3o honrais o \u00f3rf\u00e3o,"}, {"id": 429851, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6011, "text": "Nem nos estimulais a alimentar o necessitado;"}, {"id": 429852, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6012, "text": "E consumis avidamente as heran\u00e7as,"}, {"id": 429853, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6013, "text": "E cobi\u00e7ais insaciavelmente os bens terrenos!"}, {"id": 429854, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6014, "text": "Qual! Quando a terra for triturada fortemente,"}, {"id": 429855, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6015, "text": "E aparecer o teu Senhor, com os Seus anjos em desfile,"}, {"id": 429856, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6016, "text": "E o inferno, nesse dia, for destacado, ent\u00e3o o homem recordar\u00e1; por\u00e9m de que lhe servir\u00e1 a recorda\u00e7\u00e3o!"}, {"id": 429857, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6017, "text": "Dir\u00e1: Oxal\u00e1 tivesse diligenciado (na pr\u00e1tica do bem), durante a minha vida!"}, {"id": 429858, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6018, "text": "Por\u00e9m, nesse dia, ningu\u00e9m castigar\u00e1 como Ele (o far\u00e1),"}, {"id": 429859, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6019, "text": "Nem ningu\u00e9m acorrentar\u00e1, como Ele (o far\u00e1);"}, {"id": 429860, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6020, "text": "E tu, \u00f3 alma em paz,"}, {"id": 429861, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6021, "text": "Retorna ao teu Senhor, satisfeita (com Ele) e Ele satisfeito (contigo)!"}, {"id": 429862, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6022, "text": "Entre no n\u00famero dos Meus servos!"}, {"id": 429863, "identifier": "pt.elhayek", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6023, "text": "E entra no Meu jardim!"}]