[{"id": 428529, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Pelos ventos disseminadores e impetuosos,"}, {"id": 428530, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4677, "text": "Que carregam pesos enormes,"}, {"id": 428531, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4678, "text": "Que fluem com modera\u00e7\u00e3o e suavidade,"}, {"id": 428532, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4679, "text": "E que s\u00e3o distribuidores, segundo a ordem (divina),"}, {"id": 428533, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Que o que vos \u00e9 prometido \u00e9 ver\u00eddico,"}, {"id": 428534, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4681, "text": "E que o Ju\u00edzo \u00e9 infal\u00edvel!"}, {"id": 428535, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4682, "text": "Pelo c\u00e9u, pleno de sendas,"}, {"id": 428536, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Que seguis palavras discordantes,"}, {"id": 428537, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4684, "text": "As quais vos desencaminhar\u00e3o."}, {"id": 428538, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Que pere\u00e7am os inventores de mentiras!"}, {"id": 428539, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4686, "text": "Que est\u00e3o descuidados, submersos na confus\u00e3o!"}, {"id": 428540, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Perguntaram: Quando chegar\u00e1 o Dia do Ju\u00edzo?"}, {"id": 428541, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4688, "text": "(Ser\u00e1) o dia em que ser\u00e3o testados no fogo!"}, {"id": 428542, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4689, "text": "(Ser-lhes-\u00e1 dito): Provai o vosso teste! Eis aqui o que pretendestes apressar!"}, {"id": 428543, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Em verdade, os tementes habitar\u00e3o entre jardins e mananciais,"}, {"id": 428544, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4691, "text": "Desfrutando de tudo com que o seu Senhor os agraciar, porque foram benfeitores."}, {"id": 428545, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4692, "text": "Porque possu\u00edram o h\u00e1bito de pouco dormir \u00e0 noite."}, {"id": 428546, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4693, "text": "E, ao amanhecer, imploravam o perd\u00e3o de suas faltas."}, {"id": 428548, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4694, "text": "E h\u00e1 em seus bens uma parte para o mendigo e o desafortunado"}, {"id": 428549, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4695, "text": "E na terra, h\u00e1 sinais para os que est\u00e3o seguros na f\u00e9."}, {"id": 428550, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4696, "text": "E tamb\u00e9m (os h\u00e1) em v\u00f3s mesmos. N\u00e3o vedes, acaso?"}, {"id": 428551, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4697, "text": "E no c\u00e9u est\u00e1 o vosso sustento, bem como tudo quanto vos tem sido prometido."}, {"id": 428552, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4698, "text": "Pelo Senhor dos c\u00e9us e da terra, que isto \u00e9 t\u00e3o verdadeiro como \u00e9 certo que falais!"}, {"id": 428553, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Tens ouvido (\u00f3 Mensageiro) a hist\u00f3ria dos honor\u00e1veis h\u00f3spedes de Abra\u00e3o?"}, {"id": 428554, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Quando se apresentaram a ele e disseram: Paz!, respondeu-lhes: Paz! (E pensou): \"\u00c9 gente desconhecida\"."}, {"id": 428555, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4701, "text": "E voltou rapidamente para os seus, e trouxe (na volta) um bezerro cevado."}, {"id": 428556, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4702, "text": "Que lhes ofereceu... Disse (ante a hesita\u00e7\u00e3o deles): N\u00e3o comeis?"}, {"id": 428557, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4703, "text": "Ent\u00e3o sentiu medo deles. Disseram-lhe: N\u00e3o temas! E anunciaram-lhe (o nascimento de) uma crian\u00e7a, que seria s\u00e1bia."}, {"id": 428558, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4704, "text": "E sua mulher irrompeu, (rindo) em voz alta; e, batendo na pr\u00f3pria face, disse: Eu, uma anci\u00e3 est\u00e9ril!"}, {"id": 428559, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Disseram-lhe: Assim prescreveu teu Senhor, porque Ele \u00e9 o Prudente, o Sapient\u00edssimo."}, {"id": 428560, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4706, "text": "Perguntou Abra\u00e3o: Qual \u00e9, ent\u00e3o, a vossa incumb\u00eancia, \u00f3 mensageiro?"}, {"id": 428561, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Responderam-lhe: Em verdade, fomos enviados a um povo de pecadores,"}, {"id": 428562, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4708, "text": "Para que lan\u00e7\u00e1ssemos sobre eles pedras de argila,"}, {"id": 428563, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4709, "text": "Destinados, da parte do teu Senhor, aos transgressores."}, {"id": 428564, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4710, "text": "E evacuamos os fi\u00e9is que nela (Sodoma) havia."}, {"id": 428565, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4711, "text": "Por\u00e9m, encontramos nela uma s\u00f3 casa de mu\u00e7ulmanos."}, {"id": 428566, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4712, "text": "E deixamos l\u00e1 um sinal, para aqueles que temem o doloroso castigo."}, {"id": 428567, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4713, "text": "E em Mois\u00e9s (tamb\u00e9m, havia um sinal), quando o enviamos ao Fara\u00f3, com uma autoridade evidente."}, {"id": 428568, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Por\u00e9m, (o Fara\u00f3) o recha\u00e7ou, com os seus chefes, dizendo: \u00c9 um mago ou um energ\u00fameno!"}, {"id": 428569, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Por\u00e9m, apanhamo-lo, juntamente com as suas hostes, e os precipitamos no mar, porque eram r\u00e9probos."}, {"id": 428570, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4716, "text": "E (na hist\u00f3ria do povo de) Ad h\u00e1 um exemplo; desencadeamos contra eles um vento assolador,"}, {"id": 428571, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4717, "text": "Que n\u00e3o passava sobre aquilo a que ia de encontro, sem o reduzir a cinzas."}, {"id": 428572, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4718, "text": "E (no povo de) Tamud tendes um exemplo, ao lhes ser dito: Desfrutai transitoriamente!"}, {"id": 428573, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Por\u00e9m, desacataram insolentemente a ordem de seu Senhor, e a centelha os fulminou, enquanto observavam."}, {"id": 428574, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4720, "text": "E n\u00e3o puderam manter-se de p\u00e9, nem socorrer-se mutuamente."}, {"id": 428575, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4721, "text": "E anteriormente a eles houve o povo de No\u00e9; em verdade, era um povo depravado."}, {"id": 428576, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4722, "text": "E constru\u00edmos o firmamento com poder e per\u00edcia, e N\u00f3s o estamos expandindo."}, {"id": 428577, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4723, "text": "E dilatamos a terra; e que excelente Dilatador tendes em N\u00f3s!"}, {"id": 428578, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4724, "text": "E criamos um casal de cada esp\u00e9cie, para que mediteis."}, {"id": 428579, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4725, "text": "Apressai-vos, pois, para Deus, porque sou, da Sua parte, um elucidativo admoestador para v\u00f3s."}, {"id": 428580, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4726, "text": "E n\u00e3o coloqueis outra divindade junto a Deus, porque sou da Sua parte, um elucidativo admoestador para v\u00f3s."}, {"id": 428581, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4727, "text": "Mesmo assim, n\u00e3o se apresentou mensageiro algum \u00e0quelas que vos precederam, sem que dissessem: \u00c9 um mago ou umenerg\u00fameno!"}, {"id": 428582, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Acaso, t\u00ea-la-\u00e3o eles transmitido (a express\u00e3o), de um para o outro? Qual! S\u00e3o um povo de transgressores."}, {"id": 428583, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4729, "text": "Afasta-te, pois, deles, porque n\u00e3o ser\u00e1s reprovado."}, {"id": 428584, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4730, "text": "E admoesta-os, porque a admoesta\u00e7\u00e3o ser\u00e1 proveitosa para os fi\u00e9is."}, {"id": 428585, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4731, "text": "N\u00e3o criei os g\u00eanios e os humanos, sen\u00e3o para Me adorarem."}, {"id": 428586, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4732, "text": "N\u00e3o lhes pe\u00e7o sustento algum, nem quero que Me alimentam."}, {"id": 428587, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Sabei que Deus \u00e9 o Sustentador por excel\u00eancia, Potente, Inquebrantabil\u00edssimo."}, {"id": 428588, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Em verdade, os in\u00edquos auferir\u00e3o a mesma sorte que os seus antepassados. Assim, que n\u00e3o Me constranjam a apressar (ocastigo)!"}, {"id": 428589, "identifier": "pt.elhayek", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Ai, pois, dos incr\u00e9dulos no dia que lhes tem sido prometido!"}]