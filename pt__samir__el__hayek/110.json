[{"id": 430054, "identifier": "pt.elhayek", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6214, "text": "Quando te chegar o socorro de Deus e o triunfo,"}, {"id": 430055, "identifier": "pt.elhayek", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6215, "text": "E vires entrar a gente, em massa, na religi\u00e3o de Deus,"}, {"id": 430056, "identifier": "pt.elhayek", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6216, "text": "Celebra, ent\u00e3o, os louvores do teu Senhor, e implora o Seu perd\u00e3o, porque Ele \u00e9 Remiss\u00f3rio."}]