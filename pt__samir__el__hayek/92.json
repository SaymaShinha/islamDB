[{"id": 429899, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6059, "text": "Pela noite, quando cobre (a luz),"}, {"id": 429900, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6060, "text": "Pelo dia, quando resplandece,"}, {"id": 429901, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6061, "text": "Por Quem criou o masculino e o feminino,"}, {"id": 429902, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Que os vossos esfor\u00e7os s\u00e3o diferentes (quanto \u00e0s metas a atingir)."}, {"id": 429903, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6063, "text": "Por\u00e9m, \u00e0quele que d\u00e1 (em caridade e \u00e9 temente a Deus,"}, {"id": 429904, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6064, "text": "E cr\u00ea no melhor,"}, {"id": 429905, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6065, "text": "Facilitaremos o caminho do conforto."}, {"id": 429906, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6066, "text": "Por\u00e9m, \u00e0quele que mesquinhar e se considerar suficiente,"}, {"id": 429907, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6067, "text": "E negar o melhor,"}, {"id": 429908, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6068, "text": "Facilitaremos o caminho da adversidade."}, {"id": 429909, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6069, "text": "E de nada lhe valer\u00e3o os seus bens, quando ele cair no abismo."}, {"id": 429910, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6070, "text": "Sabei que a N\u00f3s incumbe a orienta\u00e7\u00e3o,"}, {"id": 429911, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6071, "text": "Assim como tamb\u00e9m s\u00e3o Nossos o fim e o come\u00e7o."}, {"id": 429912, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6072, "text": "Por isso, vos tenho admoestado com o fogo voraz,"}, {"id": 429913, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6073, "text": "Em que n\u00e3o entrar\u00e1 sen\u00e3o o mais desventurado,"}, {"id": 429914, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6074, "text": "Que desmentir (a verdade) e desdenhar."}, {"id": 429915, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6075, "text": "Contudo, livrar-se-\u00e1 dele o mais temente a Deus,"}, {"id": 429916, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6076, "text": "Que aplica os seus bens, com o fito de purific\u00e1-los,"}, {"id": 429917, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6077, "text": "E n\u00e3o faz favores a ningu\u00e9m com o fito de ser recompensado,"}, {"id": 429918, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6078, "text": "Sen\u00e3o com o intuito de ver o Rosto do seu Senhor, o Alt\u00edssimo;"}, {"id": 429919, "identifier": "pt.elhayek", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6079, "text": "A logo alcan\u00e7ar\u00e1 (completa) satisfa\u00e7\u00e3o."}]