[{"id": 428640, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Pela estrela, quando cai,"}, {"id": 428547, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Que vosso camarada jamais se extravia, nem erra,"}, {"id": 428609, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4787, "text": "Nem fala por capricho."}, {"id": 428641, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4788, "text": "Isso n\u00e3o \u00e9 sen\u00e3o a inspira\u00e7\u00e3o que lhe foi revelada,"}, {"id": 428642, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4789, "text": "Que lhe transmitiu o fort\u00edssimo,"}, {"id": 428643, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4790, "text": "O sensato, o qual lhe apareceu (em sua majestosa forma)."}, {"id": 428644, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4791, "text": "Quando estava na parte mais alta do horizonte."}, {"id": 428645, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Ent\u00e3o, aproximou-se dele estreitamente,"}, {"id": 428646, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4793, "text": "At\u00e9 a uma dist\u00e2ncia de dois arcos (de atirar setas), ou menos ainda."}, {"id": 428647, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4794, "text": "E revelou ao Seu servo o que Ele havia revelado."}, {"id": 428648, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4795, "text": "O cora\u00e7\u00e3o (do Mensageiro) n\u00e3o mentiu, acerca do que viu."}, {"id": 428649, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Disputareis, acaso, sobre o que ele viu?"}, {"id": 428650, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Realmente o viu, numa Segunda descida,"}, {"id": 428651, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4798, "text": "Junto ao limite da \u00e1rvore de l\u00f3tus."}, {"id": 428652, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4799, "text": "Junto \u00e0 qual est\u00e1 o jardim da morada (eterna)."}, {"id": 428653, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4800, "text": "Quando aquela coisa envolvente cobriu a \u00e1rvore de l\u00f3tus,"}, {"id": 428654, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4801, "text": "N\u00e3o desviou o olhar, nem transgrediu."}, {"id": 428655, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Em verdade, presenciou os maiores sinais do seu Senhor."}, {"id": 428656, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Considerai Al-L\u00e1t e Al-Uzza."}, {"id": 428657, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4804, "text": "E a outra, a terceira (deusa), Manata."}, {"id": 428658, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Porventura, pertence-vos o sexo masculino e a Ele o feminino?"}, {"id": 428659, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Tal, ent\u00e3o, seria uma partilha injusta."}, {"id": 428660, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Tais (divindades) n\u00e3o s\u00e3o mais do que nomes, com que as denominastes, v\u00f3s e vossos antepassados, acerca do que Deusn\u00e3o vos conferiu autoridade alguma. N\u00e3o seguem sen\u00e3o as sua pr\u00f3prias conjecturas e as lux\u00farias das suas almas, n\u00e3oobstante ter-lhes chegado a orienta\u00e7\u00e3o do seu Senhor!"}, {"id": 428661, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Porventura, obter\u00e1 o homem tudo quanto ambiciona?"}, {"id": 428662, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Sabei que s\u00f3 a Deus pertence a outra vida e a presente."}, {"id": 428663, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4810, "text": "E quantos anjos h\u00e1 nos c\u00e9us, cujas intercess\u00f5es de nada valer\u00e3o, salvo a daqueles que a Deus aprouver e comprazer!"}, {"id": 428664, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Sabei que aqueles que n\u00e3o cr\u00eaem na outra vida denominam os anjos com nomes femininos,"}, {"id": 428665, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Embora care\u00e7am de todo o conhecimento a esse respeito. N\u00e3o fazem sen\u00e3o seguir conjecturas, sendo que a conjecturajamais prevaleceu, em nada, sobre a verdade."}, {"id": 428666, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Afasta-te pois, de quem desdenha a Nossa Mensagem, e n\u00e3o ambiciona sen\u00e3o a vida terrena."}, {"id": 428667, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Tal \u00e9 o alcance do seu conhecimento. Em verdade, teu Senhor \u00e9 o mais conhecedor de quem se desvia da Sua senda, assim como \u00e9 o mais conhecedor de quem se encaminha."}, {"id": 428668, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4815, "text": "A Deus pertence tudo quanto existe nos c\u00e9us e na terra, para castigar os mal\u00e9volos, segundo o que tenham cometido, erecompensar os benfeitores com o melhor."}, {"id": 428669, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Estes s\u00e3o os que se abst\u00eam dos pecados graves e das obscenidades, conquanto cometam faltas leves. Que saibam que oteu Senhor \u00e9 Amplo na indulg\u00eancia; Ele vos conhece melhor do que ningu\u00e9m, uma vez que foi Ele Que vos criou na terra, emque \u00e9reis embri\u00f5es nas entranhas de vossas m\u00e3es. N\u00e3o atribuais pois, pureza a v\u00f3s mesmo, porque Ele bem conhece ostementes."}, {"id": 428670, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Que opinas, pois, de quem desdenha,"}, {"id": 428671, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4818, "text": "Que pouco d\u00e1, e, depois, endurece (o cora\u00e7\u00e3o)?"}, {"id": 428672, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Porventura, est\u00e1 de posse do incognosc\u00edvel e prognostica (o futuro)?"}, {"id": 428673, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Qual, n\u00e3o foi inteirado de tudo quanto cont\u00eam os livros de Mois\u00e9s,"}, {"id": 428674, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4821, "text": "E os de Abra\u00e3o, que cumpriu (as suas obriga\u00e7\u00f5es),"}, {"id": 428675, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4822, "text": "De que nenhum pecador arcar\u00e1 com culpa alheia?"}, {"id": 428676, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4823, "text": "De que o homem n\u00e3o obt\u00e9m sen\u00e3o o fruto do seu proceder?"}, {"id": 428677, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4824, "text": "De que o seu proceder ser\u00e1 examinado?"}, {"id": 428678, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4825, "text": "Depois, ser-lhe-\u00e1 retribu\u00eddo, com a mais eq\u00fcitativa recompensa?"}, {"id": 428679, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4826, "text": "E que pertence ao teu Senhor o limite."}, {"id": 428680, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4827, "text": "E que Ele faz rir e chorar."}, {"id": 428681, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4828, "text": "E que Ele d\u00e1 a vida e a morte."}, {"id": 428682, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4829, "text": "E que Ele criou (tudo) em pares: o masculino e o feminino,"}, {"id": 428683, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4830, "text": "De uma gosta de esperma, quando alojada (em seu lugar)."}, {"id": 428684, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4831, "text": "E que a Ele compete a Segunda cria\u00e7\u00e3o."}, {"id": 428685, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4832, "text": "E que Ele enriquece e d\u00e1 satisfa\u00e7\u00e3o."}, {"id": 428686, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4833, "text": "E que Ele \u00e9 o Senhor do (astro) S\u00edrio."}, {"id": 428687, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4834, "text": "E que Ele exterminou o primitivo povo de Ad."}, {"id": 428688, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4835, "text": "E o povo de Tamud, sem deixar (membro) algum?"}, {"id": 428689, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4836, "text": "E, antes, o povo de No\u00e9, porque era ainda mais in\u00edquo e transgressor?"}, {"id": 428690, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4837, "text": "E destruiu as cidades nefastas (Sodoma e Gomorra)?"}, {"id": 428691, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4838, "text": "E as cobriu com um v\u00e9u envolvente?"}, {"id": 428692, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4839, "text": "De qual das merc\u00eas do teu Senhor duvidas, pois, (\u00f3 humano)?"}, {"id": 428693, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Eis aqui uma admoesta\u00e7\u00e3o dos primeiros admoestadores."}, {"id": 428694, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Aproxima-se a Hora iminente!"}, {"id": 427411, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4842, "text": "Ningu\u00e9m, al\u00e9m de Deus, poder\u00e1 revel\u00e1-la."}, {"id": 427983, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Por que vos assombrais, ent\u00e3o, com esta Mensagem?"}, {"id": 428254, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4844, "text": "E rides ao inv\u00e9s de chorardes,"}, {"id": 428344, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4845, "text": "Em vossos lazeres?"}, {"id": 428695, "identifier": "pt.elhayek", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 4846, "text": "Prostrai-vos, outrossim, perante Deus, e adorai-O."}]