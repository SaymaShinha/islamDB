[{"id": 429966, "identifier": "pt.elhayek", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6126, "text": "Sabei que o revelamos (o Alcor\u00e3o), na Noite do Decreto."}, {"id": 429967, "identifier": "pt.elhayek", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6127, "text": "E o que te far\u00e1 entender o que \u00e9 a Noite do Decreto?"}, {"id": 429968, "identifier": "pt.elhayek", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6128, "text": "A Noite do Decreto \u00e9 melhor do que mil meses."}, {"id": 429969, "identifier": "pt.elhayek", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6129, "text": "Nela descem os anjos e o Esp\u00edrito (Anjo Gabriel), com a anu\u00eancia do seu Senhor, para executar todas as Suas ordens."}, {"id": 429970, "identifier": "pt.elhayek", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "pt", "name": "El-Hayek", "transelator_en_name": "Samir El-Hayek", "format": "text", "type": "translation", "ayah_number": 6130, "text": "(Ela) \u00e9 paz, at\u00e9 ao romper da aurora!"}]