ommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5478, "text": "\u0646\u0635\u0641 [\u0647\u0645\u0647 \u0633\u0627\u0639\u0627\u062a] \u0634\u0628 [\u0631\u0627 \u0628\u0647 \u0639\u0628\u0627\u062f\u062a \u0627\u062e\u062a\u0635\u0627\u0635 \u062f\u0647] \u06cc\u0627 \u0627\u0646\u062f\u06a9\u06cc \u0627\u0632\u0646\u0635\u0641 \u0628\u06a9\u0627\u0647\u060c"}, {"id": 285992, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5479, "text": "\u06cc\u0627 [\u0645\u0642\u062f\u0627\u0631\u06cc] \u0628\u0631 \u0646\u0635\u0641 \u0628\u06cc\u0641\u0632\u0627\u060c \u0648 \u0642\u0631\u0622\u0646 \u0631\u0627 \u0634\u0645\u0631\u062f\u0647 \u0648 \u0631\u0648\u0634\u0646 \u0648 \u0628\u0627 \u062a\u0623\u0645\u0644 \u0648 \u062f\u0642\u062a \u0628\u062e\u0648\u0627\u0646."}, {"id": 285993, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5480, "text": "\u0628\u0647 \u06cc\u0642\u06cc\u0646 \u0645\u0627 \u0628\u0647 \u0632\u0648\u062f\u06cc \u06af\u0641\u062a\u0627\u0631\u06cc \u0633\u0646\u06af\u06cc\u0646 [\u0686\u0648\u0646 \u0622\u06cc\u0627\u062a \u0642\u0631\u0622\u0646] \u0628\u0647 \u062a\u0648 \u0627\u0644\u0642\u0627 \u062e\u0648\u0627\u0647\u06cc\u0645\u06a9\u0631\u062f."}, {"id": 285994, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5481, "text": "\u0628\u06cc \u062a\u0631\u062f\u06cc\u062f \u0639\u0628\u0627\u062f\u062a [\u06cc \u06a9\u0647 \u062f\u0631 \u062f\u0644] \u0634\u0628 [\u0627\u0646\u062c\u0627\u0645 \u0645\u06cc \u06af\u06cc\u0631\u062f] \u0645\u062d\u06a9\u0645 \u062a\u0631 \u0648 \u067e\u0627\u06cc\u062f\u0627\u0631\u062a\u0631 \u0648 \u06af\u0641\u062a\u0627\u0631 \u062f\u0631 \u0622\u0646 \u0627\u0633\u062a\u0648\u0627\u0631\u062a\u0631 \u0648 \u062f\u0631\u0633\u062a \u062a\u0631 \u0627\u0633\u062a."}, {"id": 285995, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5482, "text": "\u062a\u0648 \u0631\u0627 \u062f\u0631 \u0631\u0648\u0632 [\u0628\u0631\u0627\u06cc \u0645\u0634\u0627\u063a\u0644 \u0641\u0631\u0627\u0648\u0627\u0646 \u0645\u0639\u0646\u0648\u06cc \u0648 \u0647\u062f\u0627\u06cc\u062a \u0645\u0631\u062f\u0645 \u0648 \u062d\u0644 \u0645\u0634\u06a9\u0644\u0627\u062a \u0646\u06cc\u0627\u0632\u0645\u0646\u062f\u0627\u0646\u060c] \u0631\u0641\u062a \u0648 \u0622\u0645\u062f\u06cc \u0637\u0648\u0644\u0627\u0646\u06cc \u0627\u0633\u062a [\u067e\u0633 \u0633\u0627\u0639\u0627\u062a \u0634\u0628 \u0628\u0631\u0627\u06cc \u0639\u0628\u0627\u062f\u062a \u0641\u0631\u0635\u062a\u06cc \u0628\u0647\u062a\u0631 \u0627\u0633\u062a.]"}, {"id": 285996, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5483, "text": "\u0646\u0627\u0645 \u067e\u0631\u0648\u0631\u062f\u06af\u0627\u0631\u062a \u0631\u0627 [\u0628\u0647 \u0632\u0628\u0627\u0646 \u062d\u0627\u0644 \u0648 \u0642\u0627\u0644] \u06cc\u0627\u062f \u06a9\u0646 [\u0648 \u0627\u0632 \u063a\u06cc\u0631 \u0627\u0648 \u0642\u0637\u0639 \u0627\u0645\u06cc\u062f \u0646\u0645\u0627] \u0648 \u0641\u0642\u0637 \u062f\u0644 \u0628\u0631 \u0627\u0648 \u0628\u0646\u062f."}, {"id": 285997, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5484, "text": "[\u0647\u0645\u0627\u0646 \u06a9\u0647] \u067e\u0631\u0648\u0631\u062f\u06af\u0627\u0631 \u0645\u0634\u0631\u0642 \u0648 \u0645\u063a\u0631\u0628 [\u0627\u0633\u062a]\u060c \u0647\u06cc\u0686 \u0645\u0639\u0628\u0648\u062f\u06cc \u062c\u0632 \u0627\u0648 \u0646\u06cc\u0633\u062a\u060c \u067e\u0633 \u0627\u0648 \u0631\u0627 \u0648\u06a9\u06cc\u0644 \u0648 \u06a9\u0627\u0631\u0633\u0627\u0632 \u062e\u0648\u062f \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u060c"}, {"id": 285998, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u0627\u0646\u0635\u0627\u0631\u06cc\u0627\u0646", "transelator_en_name": "Hussain Ansarian", "format": "text", "type": "translation", "ayah_number": 5485, "text": "\u0648 \u0628\u0631 \u06af\u0641\u062a\u0627\u0631 [\u0648 \u0622\u0632\u0627\u0631] \u0645\u0634\u0631\u06a9\u0627\u0646 \u0634\u06a9\u06cc\u0628\u0627 \u0628\u0627\u0634 \u0648 \u0627\u0632 \u0622\u0646\u0627\u0646 \u0628\u0647 \u0634\u06cc\u0648\u0647 \u0627\u06cc \u067e\u0633\u0646\u062f\u06cc\u062f\u0647 \u062f\u0648\u0631\u06cc \u06a9\u0646\u060c"}, {"id": 285999, "identifier": "fa.ansarian", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzza