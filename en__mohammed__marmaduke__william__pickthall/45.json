[{"id": 216345, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4474, "text": "Ha. Mim."}, {"id": 216346, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4475, "text": "The revelation of the Scripture is from Allah, the Mighty, the Wise."}, {"id": 216347, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4476, "text": "Lo! in the heavens and the earth are portents for believers."}, {"id": 216349, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4477, "text": "And in your creation, and all the beasts that He scattereth in the earth, are portents for a folk whose faith is sure."}, {"id": 216350, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4478, "text": "And the difference of night and day and the provision that Allah sendeth down from the sky and thereby quickeneth the earth after her death, and the ordering of the winds, are portents for a people who have sense."}, {"id": 216351, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4479, "text": "These are the portents of Allah which We recite unto thee (Muhammad) with truth. Then in what fact, after Allah and His portents, will they believe?"}, {"id": 216352, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4480, "text": "Woe unto each sinful liar,"}, {"id": 216353, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4481, "text": "Who heareth the revelations of Allah recited unto him, and then continueth in pride as though he heard them not. Give him tidings of a painful doom."}, {"id": 216354, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4482, "text": "And when he knoweth aught of Our revelations he maketh it a jest. For such there is a shameful doom."}, {"id": 216355, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4483, "text": "Beyond them there is hell, and that which they have earned will naught avail them, nor those whom they have chosen for protecting friends beside Allah. Theirs will be an awful doom."}, {"id": 216356, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4484, "text": "This is guidance. And those who disbelieve the revelations of their Lord, for them there is a painful doom of wrath."}, {"id": 216357, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4485, "text": "Allah it is Who hath made the sea of service unto you that the ships may run thereon by His command, and that ye may seek of His bounty, and that haply ye may be thankful;"}, {"id": 216358, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4486, "text": "And hath made of service unto you whatsoever is in the heavens and whatsoever is in the earth; it is all from Him. Lo! herein verily are portents for a people who reflect."}, {"id": 216360, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4487, "text": "Tell those who believe to forgive those who hope not for the days of Allah; in order that He may requite folk what they used to earn."}, {"id": 216361, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4488, "text": "Whoso doeth right, it is for his soul, and whoso doeth wrong, it is against it. And afterward unto your Lord ye will be brought back."}, {"id": 216362, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4489, "text": "And verily we gave the Children of Israel the Scripture and the Command and the Prophethood, and provided them with good things and favoured them above (all) peoples;"}, {"id": 216363, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4490, "text": "And gave them plain commandments. And they differed not until after the knowledge came unto them, through rivalry among themselves. Lo! thy Lord will judge between them on the Day of Resurrection concerning that wherein they used to differ."}, {"id": 216364, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4491, "text": "And now have We set thee (O Muhammad) on a clear road of (Our) commandment; so follow it, and follow not the whims of those who know not."}, {"id": 216365, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4492, "text": "Lo! they can avail thee naught against Allah. And lo! as for the wrong-doers, some of them are friends of others; and Allah is the Friend of those who ward off (evil)."}, {"id": 216366, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4493, "text": "This is clear indication for mankind, and a guidance and a mercy for a folk whose faith is sure."}, {"id": 216367, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4494, "text": "Or do those who commit ill-deeds suppose that We shall make them as those who believe and do good works, the same in life and death? Bad is their judgment!"}, {"id": 216368, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4495, "text": "And Allah hath created the heavens and the earth with truth, and that every soul may be repaid what it hath earned. And they will not be wronged."}, {"id": 216369, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4496, "text": "Hast thou seen him who maketh his desire his god, and Allah sendeth him astray purposely, and sealeth up his hearing and his heart, and setteth on his sight a covering? Then who will lead him after Allah (hath condemned him)? Will ye not then heed?"}, {"id": 216370, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4497, "text": "And they say: There is naught but our life of the world; we die and we live, and naught destroyeth us save time; when they have no knowledge whatsoever of (all) that; they do but guess."}, {"id": 216371, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4498, "text": "And when Our clear revelations are recited unto them their only argument is that they say: Bring (back) our fathers. then, if ye are truthful."}, {"id": 216372, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4499, "text": "Say (unto them, O Muhammad): Allah giveth life to you, then causeth you to die, then gathereth you unto the Day of Resurrection whereof there is no doubt. But most of mankind know not."}, {"id": 216373, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4500, "text": "And unto Allah belongeth the Sovereignty of the heavens and the earth; and on the day when the Hour riseth, on that day those who follow falsehood will be lost."}, {"id": 216374, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4501, "text": "And thou wilt see each nation crouching, each nation summoned to its record. (And it will be said unto them): This day ye are requited what ye used to do."}, {"id": 216375, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4502, "text": "This Our Book pronounceth against you with truth. Lo! We have caused (all) that ye did to be recorded."}, {"id": 216376, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4503, "text": "Then, as for those who believed and did good works, their Lord will bring them in unto His mercy. That is the evident triumph."}, {"id": 216377, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4504, "text": "And as for those who disbelieved (it will be said unto them): Were not Our revelations recited unto you? But ye were scornful and became a guilty folk."}, {"id": 216378, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4505, "text": "And when it was said: Lo! Allah's promise is the truth, and there is no doubt of the Hour's coming, ye said: We know not what the Hour is. We deem it naught but a conjecture, and we are by no means convinced."}, {"id": 216379, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4506, "text": "And the evils of what they did will appear unto them, and that which they used to deride will befall them."}, {"id": 216380, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4507, "text": "And it will be said: This day We forget you, even as ye forgot the meeting of this your day; and your habitation is the Fire, and there is none to help you."}, {"id": 216382, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4508, "text": "This, forasmuch as ye made the revelations of Allah a jest, and the life of the world beguiled you. Therefor this day they come not forth from thence, nor can they make amends."}, {"id": 216383, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4509, "text": "Then praise be to Allah, Lord of the heavens and Lord of the earth, the Lord of the Worlds."}, {"id": 216384, "identifier": "en.pickthall", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4510, "text": "And unto Him (alone) belongeth Majesty in the heavens and the earth, and He is the Mighty, the Wise."}]