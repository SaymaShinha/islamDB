[{"id": 217479, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5623, "text": "By the emissary winds, (sent) one after another"}, {"id": 217480, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5624, "text": "By the raging hurricanes,"}, {"id": 217481, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5625, "text": "By those which cause earth's vegetation to revive;"}, {"id": 217482, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5626, "text": "By those who winnow with a winnowing,"}, {"id": 217483, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5627, "text": "By those who bring down the Reminder,"}, {"id": 217484, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5628, "text": "To excuse or to warn,"}, {"id": 217485, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5629, "text": "Surely that which ye are promised will befall."}, {"id": 217486, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5630, "text": "So when the stars are put out,"}, {"id": 217487, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5631, "text": "And when the sky is riven asunder,"}, {"id": 217488, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5632, "text": "And when the mountains are blown away,"}, {"id": 217489, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5633, "text": "And when the messengers are brought unto their time appointed -"}, {"id": 217490, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5634, "text": "For what day is the time appointed?"}, {"id": 217491, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5635, "text": "For the Day of Decision."}, {"id": 217492, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5636, "text": "And what will convey unto thee what the Day of Decision is! -"}, {"id": 217493, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5637, "text": "Woe unto the repudiators on that day!"}, {"id": 217494, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5638, "text": "Destroyed We not the former folk,"}, {"id": 217495, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5639, "text": "Then caused the latter folk to follow after?"}, {"id": 217496, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5640, "text": "Thus deal We ever with the guilty."}, {"id": 217498, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5641, "text": "Woe unto the repudiators on that day!"}, {"id": 217499, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5642, "text": "Did We not create you from a base fluid"}, {"id": 217500, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5643, "text": "Which We laid up in a safe abode"}, {"id": 217501, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5644, "text": "For a known term?"}, {"id": 217502, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5645, "text": "Thus We arranged. How excellent is Our arranging!"}, {"id": 217503, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5646, "text": "Woe unto the repudiators on that day!"}, {"id": 217504, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5647, "text": "Have We not made the earth a receptacle"}, {"id": 217505, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5648, "text": "Both for the living and the dead,"}, {"id": 217506, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5649, "text": "And placed therein high mountains and given you to drink sweet water therein?"}, {"id": 217507, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5650, "text": "Woe unto the repudiators on that day!"}, {"id": 217508, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5651, "text": "(It will be said unto them:) Depart unto that (doom) which ye used to deny;"}, {"id": 217509, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5652, "text": "Depart unto the shadow falling threefold,"}, {"id": 217510, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5653, "text": "(Which yet is) no relief nor shelter from the flame."}, {"id": 217511, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5654, "text": "Lo! it throweth up sparks like the castles,"}, {"id": 217512, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5655, "text": "(Or) as it might be camels of bright yellow hue."}, {"id": 217513, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5656, "text": "Woe unto the repudiators on that day!"}, {"id": 217514, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5657, "text": "This is a day wherein they speak not,"}, {"id": 217515, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5658, "text": "Nor are they suffered to put forth excuses."}, {"id": 217516, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5659, "text": "Woe unto the repudiators on that day!"}, {"id": 217517, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5660, "text": "This is the Day of Decision, We have brought you and the men of old together."}, {"id": 217518, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5661, "text": "If now ye have any wit, outwit Me."}, {"id": 217519, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5662, "text": "Woe unto the repudiators on that day!"}, {"id": 217520, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5663, "text": "Lo! those who kept their duty are amid shade and fountains,"}, {"id": 217521, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5664, "text": "And fruits such as they desire."}, {"id": 217522, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5665, "text": "(Unto them it is said:) Eat, drink and welcome, O ye blessed, in return for what ye did."}, {"id": 217523, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5666, "text": "Thus do We reward the good."}, {"id": 217524, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5667, "text": "Woe unto the repudiators on that day!"}, {"id": 217525, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5668, "text": "Eat and take your ease (on earth) a little. Lo! ye are guilty."}, {"id": 217526, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5669, "text": "Woe unto the repudiators on that day!"}, {"id": 217527, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5670, "text": "When it is said unto them: Bow down, they bow not down!"}, {"id": 217528, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5671, "text": "Woe unto the repudiators on that day!"}, {"id": 217529, "identifier": "en.pickthall", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5672, "text": "In what statement, after this, will they believe?"}]