[{"id": 217034, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5178, "text": "All that is in the heavens and all that is in the earth glorifieth Allah, the Sovereign Lord, the Holy One, the Mighty, the Wise."}, {"id": 217035, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5179, "text": "He it is Who hath sent among the unlettered ones a messenger of their own, to recite unto them His revelations and to make them grow, and to teach them the Scripture and wisdom, though heretofore they were indeed in error manifest,"}, {"id": 217036, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5180, "text": "Along with others of them who have not yet joined them. He is the Mighty, the Wise."}, {"id": 217037, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5181, "text": "That is the bounty of Allah; which He giveth unto whom He will. Allah is of Infinite Bounty."}, {"id": 217038, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5182, "text": "The likeness of those who are entrusted with the Law of Moses, yet apply it not, is as the likeness of the ass carrying books. Wretched is the likeness of folk who deny the revelations of Allah. And Allah guideth not wrongdoing folk."}, {"id": 217039, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5183, "text": "Say (O Muhammad): O ye who are Jews! If ye claim that ye are favoured of Allah apart from (all) mankind, then long for death if ye are truthful."}, {"id": 217041, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5184, "text": "But they will never long for it because of all that their own hands have sent before, and Allah is Aware of evil-doers."}, {"id": 217042, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5185, "text": "Say (unto them, O Muhammad): Lo! the death from which ye shrink will surely meet you, and afterward ye will be returned unto the Knower of the Invisible and the Visible, and He will tell you what ye used to do."}, {"id": 217043, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 487, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5186, "text": "O ye who believe! When the call is heard for the prayer of the day of congregation, haste unto remembrance of Allah and leave your trading. That is better for you if ye did but know."}, {"id": 217044, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 487, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5187, "text": "And when the prayer is ended, then disperse in the land and seek of Allah's bounty, and remember Allah much, that ye may be successful."}, {"id": 217045, "identifier": "en.pickthall", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 487, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5188, "text": "But when they spy some merchandise or pastime they break away to it and leave thee standing. Say: That which Allah hath is better than pastime and than merchandise, and Allah is the Best of providers."}]