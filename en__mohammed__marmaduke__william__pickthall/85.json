[{"id": 217766, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5910, "text": "By the heaven, holding mansions of the stars,"}, {"id": 217767, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5911, "text": "And by the Promised Day."}, {"id": 217768, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5912, "text": "And by the witness and that whereunto he beareth testimony,"}, {"id": 217769, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5913, "text": "(Self-)destroyed were the owners of the ditch"}, {"id": 217770, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Of the fuel-fed fire,"}, {"id": 217771, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5915, "text": "When they sat by it,"}, {"id": 217772, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5916, "text": "And were themselves the witnesses of what they did to the believers."}, {"id": 217773, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5917, "text": "They had naught against them save that they believed in Allah, the Mighty, the Owner of Praise,"}, {"id": 217774, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Him unto Whom belongeth the Sovereignty of the heavens and the earth; and Allah is of all things the Witness."}, {"id": 217775, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Lo! they who persecute believing men and believing women and repent not, theirs verily will be the doom of hell, and theirs the doom of burning."}, {"id": 217776, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5920, "text": "Lo! those who believe and do good works, theirs will be Gardens underneath which rivers flow. That is the Great Success."}, {"id": 217777, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Lo! the punishment of thy Lord is stern."}, {"id": 217778, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Lo! He it is Who produceth, then reproduceth,"}, {"id": 217779, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5923, "text": "And He is the Forgiving, the Loving,"}, {"id": 217780, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5924, "text": "Lord of the Throne of Glory,"}, {"id": 217781, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Doer of what He will."}, {"id": 217782, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Hath there come unto thee the story of the hosts"}, {"id": 217783, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5927, "text": "Of Pharaoh and (the tribe of) Thamud?"}, {"id": 217784, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Nay, but those who disbelieve live in denial"}, {"id": 217785, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5929, "text": "And Allah, all unseen, surroundeth them."}, {"id": 217786, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5930, "text": "Nay, but it is a glorious Qur'an."}, {"id": 217787, "identifier": "en.pickthall", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5931, "text": "On a guarded tablet."}]