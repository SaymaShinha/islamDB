[{"id": 217302, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5448, "text": "Say (O Muhammad): It is revealed unto me that a company of the Jinn gave ear, and they said: Lo! we have heard a marvellous Qur'an,"}, {"id": 217303, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5449, "text": "Which guideth unto righteousness, so we believe in it and we ascribe no partner unto our Lord."}, {"id": 217304, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5450, "text": "And (we believe) that He - exalted be the glory of our Lord! - hath taken neither wife nor son,"}, {"id": 217305, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5451, "text": "And that the foolish one among us used to speak concerning Allah an atrocious lie."}, {"id": 217306, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5452, "text": "And lo! we had supposed that humankind and jinn would not speak a lie concerning Allah -"}, {"id": 217307, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5453, "text": "And indeed (O Muhammad) individuals of humankind used to invoke the protection of individuals of the jinn, so that they increased them in revolt against Allah);"}, {"id": 217308, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5454, "text": "And indeed they supposed, even as ye suppose, that Allah would not raise anyone (from the dead) -"}, {"id": 217309, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5455, "text": "And (the Jinn who had listened to the Qur'an said): We had sought the heaven but had found it filled with strong warders and meteors."}, {"id": 217310, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5456, "text": "And we used to sit on places (high) therein to listen. But he who listeneth now findeth a flame in wait for him;"}, {"id": 217311, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5457, "text": "And we know not whether harm is boded unto all who are in the earth, or whether their Lord intendeth guidance for them."}, {"id": 217312, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5458, "text": "And among us there are righteous folk and among us there are far from that. We are sects having different rules."}, {"id": 217313, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5459, "text": "And we know that we cannot escape from Allah in the earth, nor can we escape by flight."}, {"id": 217314, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5460, "text": "And when we heard the guidance, we believed therein, and whoso believeth in his Lord, he feareth neither loss nor oppression."}, {"id": 217315, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5461, "text": "And there are among us some who have surrendered (to Allah) and there are among us some who are unjust. And whoso hath surrendered to Allah, such have taken the right path purposefully."}, {"id": 217316, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5462, "text": "And as for those who are unjust, they are firewood for hell."}, {"id": 217317, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5463, "text": "If they (the idolaters) tread the right path, We shall give them to drink of water in abundance"}, {"id": 217318, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5464, "text": "That We may test them thereby, and whoso turneth away from the remembrance of his Lord; He will thrust him into ever-growing torment."}, {"id": 217319, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5465, "text": "And the places of worship are only for Allah, so pray not unto anyone along with Allah."}, {"id": 217320, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5466, "text": "And when the slave of Allah stood up in prayer to Him, they crowded on him, almost stifling."}, {"id": 217321, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5467, "text": "Say (unto them, O Muhammad): I pray unto Allah only, and ascribe unto Him no partner."}, {"id": 217322, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5468, "text": "Say: Lo! I control not hurt nor benefit for you."}, {"id": 217323, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5469, "text": "Say: Lo! none can protect me from Allah, nor can I find any refuge beside Him"}, {"id": 217324, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5470, "text": "(Mine is) but conveyance (of the Truth) from Allah, and His messages; and whoso disobeyeth Allah and His messenger, lo! his is fire of hell, wherein such dwell for ever."}, {"id": 217325, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5471, "text": "Till (the day) when they shall behold that which they are promised (they may doubt); but then they will know (for certain) who is weaker in allies and less in multitude."}, {"id": 217326, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5472, "text": "Say (O Muhammad, unto the disbelievers): I know not whether that which ye are promised is nigh, or if my Lord hath set a distant term for it."}, {"id": 217327, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5473, "text": "(He is) the Knower of the Unseen, and He revealeth unto none His secret,"}, {"id": 217328, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5474, "text": "Save unto every messenger whom He hath chosen, and then He maketh a guard to go before him and a guard behind him"}, {"id": 217329, "identifier": "en.pickthall", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5475, "text": "That He may know that they have indeed conveyed the messages of their Lord. He surroundeth all their doings, and He keepeth count of all things."}]