[{"id": 216510, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4631, "text": "Qaf. By the Glorious Qur'an,"}, {"id": 216511, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4632, "text": "Nay, but they marvel that a warner of their own hath come unto them; and the disbelievers say: This is a strange thing:"}, {"id": 216512, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4633, "text": "When we are dead and have become dust (shall we be brought back again)? That would be a far return!"}, {"id": 216513, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4634, "text": "We know that which the earth taketh of them, and with Us is a recording Book."}, {"id": 216514, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4635, "text": "Nay, but they have denied the truth when it came unto them, therefor they are now in troubled case."}, {"id": 216515, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4636, "text": "Have they not then observed the sky above them, how We have constructed it and beautified it, and how there are no rifts therein?"}, {"id": 216516, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4637, "text": "And the earth have We spread out, and have flung firm hills therein, and have caused of every lovely kind to grow thereon,"}, {"id": 214635, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4638, "text": "A vision and a reminder for every penitent slave."}, {"id": 216517, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4639, "text": "And We send down from the sky blessed water whereby We give growth unto gardens and the grain of crops,"}, {"id": 216518, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4640, "text": "And lofty date-palms with ranged clusters,"}, {"id": 216519, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4641, "text": "Provision (made) for men; and therewith We quicken a dead land. Even so will be the resurrection of the dead."}, {"id": 216520, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4642, "text": "The folk of Noah denied (the truth) before them, and (so did) the dwellers at Ar-Rass and (the tribe of) Thamud,"}, {"id": 216521, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4643, "text": "And (the tribe of) A'ad, and Pharaoh, and the brethren of Lot,"}, {"id": 216522, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4644, "text": "And the dwellers in the wood, and the folk of Tubb'a: every one denied their messengers, therefor My threat took effect."}, {"id": 216523, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4645, "text": "Were We then worn out by the first creation? Yet they are in doubt about a new creation."}, {"id": 216524, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4646, "text": "We verily created man and We know what his soul whispereth to him, and We are nearer to him than his jugular vein."}, {"id": 216525, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4647, "text": "When the two Receivers receive (him), seated on the right hand and on the left,"}, {"id": 216526, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4648, "text": "He uttereth no word but there is with him an observer ready."}, {"id": 216527, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4649, "text": "And the agony of death cometh in truth. (And it is said unto him): This is that which thou wast wont to shun."}, {"id": 216528, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4650, "text": "And the trumpet is blown. This is the threatened Day."}, {"id": 216529, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4651, "text": "And every soul cometh, along with it a driver and a witness."}, {"id": 216530, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4652, "text": "(And unto the evil-doer it is said): Thou wast in heedlessness of this. Now We have removed from thee thy covering, and piercing is thy sight this day."}, {"id": 216467, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4653, "text": "And (unto the evil-doer) his comrade saith: This is that which I have ready (as testimony)."}, {"id": 216487, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4654, "text": "(And it is said): Do ye twain hurl to hell each rebel ingrate,"}, {"id": 216505, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4655, "text": "Hinderer of good, transgressor, doubter,"}, {"id": 216531, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4656, "text": "Who setteth up another god along with Allah. Do ye twain hurl him to the dreadful doom."}, {"id": 216532, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4657, "text": "His comrade saith: Our Lord! I did not cause him to rebel, but he was (himself) far gone in error."}, {"id": 216533, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4658, "text": "He saith: Contend not in My presence, when I had already proffered unto you the warning."}, {"id": 216534, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 453, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4659, "text": "The sentence that cometh from Me cannot be changed, and I am in no wise a tyrant unto the slaves."}, {"id": 216535, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4660, "text": "On the day when We say unto hell: Art thou filled? and it saith: Can there be more to come?"}, {"id": 216536, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4661, "text": "And the Garden is brought nigh for those who kept from evil, no longer distant."}, {"id": 216537, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4662, "text": "(And it is said): This is that which ye were promised. (It is) for every penitent and heedful one,"}, {"id": 216538, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4663, "text": "Who feareth the Beneficent in secret and cometh with a contrite heart."}, {"id": 216539, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4664, "text": "Enter it in peace. This is the day of immortality."}, {"id": 216540, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 519, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4665, "text": "There they have all that they desire, and there is more with Us."}, {"id": 216541, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4666, "text": "And how many a generation We destroyed before them, who were mightier than these in prowess so that they overran the lands! Had they any place of refuge (when the judgment came)?"}, {"id": 216542, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4667, "text": "Lo! therein verily is a reminder for him who hath a heart, or giveth ear with full intelligence."}, {"id": 216543, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4668, "text": "And verily We created the heavens and the earth, and all that is between them, in six Days, and naught of weariness touched Us."}, {"id": 216544, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4669, "text": "Therefor (O Muhammad) bear with what they say, and hymn the praise of thy Lord before the rising and before the setting of the sun;"}, {"id": 216545, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4670, "text": "And in the night-time hymn His praise, and after the (prescribed) prostrations."}, {"id": 216546, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4671, "text": "And listen on the day when the crier crieth from a near place,"}, {"id": 216547, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4672, "text": "The day when they will hear the (Awful) Cry in truth. That is the day of coming forth (from the graves)."}, {"id": 216548, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4673, "text": "Lo! We it is Who quicken and give death, and unto Us is the journeying."}, {"id": 216549, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4674, "text": "On the day when the earth splitteth asunder from them, hastening forth (they come). That is a gathering easy for Us (to make)."}, {"id": 216550, "identifier": "en.pickthall", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4675, "text": "We are Best Aware of what they say, and thou (O Muhammad) art in no wise a compeller over them. But warn by the Qur'an him who feareth My threat."}]