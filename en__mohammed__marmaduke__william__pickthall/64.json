[{"id": 217058, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5200, "text": "All that is in the heavens and all that is in the earth glorifieth Allah; unto Him belongeth Sovereignty and unto Him belongeth praise, and He is Able to do all things."}, {"id": 217059, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5201, "text": "He it is Who created you, but one of you is a disbeliever and one of you is a believer, and Allah is Seer of what ye do."}, {"id": 217060, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5202, "text": "He created the heavens and the earth with truth, and He shaped you and made good your shapes, and unto Him is the journeying."}, {"id": 217061, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5203, "text": "He knoweth all that is in the heavens and the earth, and He knoweth what ye conceal and what ye publish. And Allah is Aware of what is in the breasts (of men)."}, {"id": 217062, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5204, "text": "Hath not the story reached you of those who disbelieved of old and so did taste the ill-effects of their conduct, and theirs will be a painful doom."}, {"id": 217063, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5205, "text": "That was because their messengers (from Allah) kept coming unto them with clear proofs (of Allah's Sovereignty), but they said: Shall mere mortals guide us? So they disbelieved and turned away, and Allah was independent (of them). Allah is Absolute, Owner of Praise."}, {"id": 217064, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5206, "text": "Those who disbelieve assert that they will not be raised again. Say (unto them, O Muhammad): Yea, verily, by my Lord! ye will be raised again and then ye will be informed of what ye did; and that is easy for Allah."}, {"id": 217065, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5207, "text": "So believe in Allah and His messenger and the light which We have revealed. And Allah is Informed of what ye do."}, {"id": 217066, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5208, "text": "The day when He shall gather you unto the Day of Assembling, that will be a day of mutual disillusion. And whoso believeth in Allah and doeth right, He will remit from him his evil deeds and will bring him unto Gardens underneath which rivers flow, therein to abide for ever. That is the supreme triumph."}, {"id": 217067, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5209, "text": "But those who disbelieve and deny Our revelations, such are owners of the Fire; they will abide therein - a hapless journey's end!"}, {"id": 217068, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5210, "text": "No calamity befalleth save by Allah's leave. And whosoever believeth in Allah, He guideth his heart. And Allah is Knower of all things."}, {"id": 217069, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5211, "text": "Obey Allah and obey His messenger; but if ye turn away, then the duty of Our messenger is only to convey (the message) plainly."}, {"id": 217070, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5212, "text": "Allah! There is no Allah save Him. In Allah, therefore, let believers put their trust."}, {"id": 217071, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5213, "text": "O ye who believe! Lo! among your wives and your children there are enemies for you, therefor beware of them. And if ye efface and overlook and forgive, then lo! Allah is Forgiving, Merciful."}, {"id": 217072, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5214, "text": "Your wealth and your children are only a temptation, whereas Allah! with Him is an immense reward."}, {"id": 217073, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5215, "text": "So keep your duty to Allah as best ye can, and listen, and obey, and spend; that is better for your souls. And whoso is saved from his own greed, such are the successful."}, {"id": 217074, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5216, "text": "If ye lend unto Allah a goodly loan, He will double it for you and will forgive you, for Allah is Responsive, Clement,"}, {"id": 217075, "identifier": "en.pickthall", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5217, "text": "Knower of the Invisible and the Visible, the Mighty, the Wise."}]