[{"id": 217705, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5849, "text": "Woe unto the defrauders:"}, {"id": 217706, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5850, "text": "Those who when they take the measure from mankind demand it full,"}, {"id": 217707, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5851, "text": "But if they measure unto them or weight for them, they cause them loss."}, {"id": 217708, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Do such (men) not consider that they will be raised again"}, {"id": 217709, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5853, "text": "Unto an Awful Day,"}, {"id": 217710, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5854, "text": "The day when (all) mankind stand before the Lord of the Worlds?"}, {"id": 217711, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5855, "text": "Nay, but the record of the vile is in Sijjin -"}, {"id": 217712, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5856, "text": "Ah! what will convey unto thee what Sijjin is! -"}, {"id": 217713, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5857, "text": "A written record."}, {"id": 217714, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5858, "text": "Woe unto the repudiators on that day!"}, {"id": 217715, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5859, "text": "Those who deny the Day of Judgment"}, {"id": 217716, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5860, "text": "Which none denieth save each criminal transgressor,"}, {"id": 217717, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5861, "text": "Who, when thou readest unto him Our revelations, saith: (Mere) fables of the men of old."}, {"id": 217718, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5862, "text": "Nay, but that which they have earned is rust upon their hearts."}, {"id": 217719, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5863, "text": "Nay, but surely on that day they will be covered from (the mercy of) their Lord."}, {"id": 217720, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5864, "text": "Then lo! they verily will burn in hell,"}, {"id": 217721, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5865, "text": "And it will be said (unto them): This is that which ye used to deny."}, {"id": 217722, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5866, "text": "Nay, but the record of the righteous is in 'Illiyin -"}, {"id": 217723, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5867, "text": "Ah, what will convey unto thee what 'Illiyin is! -"}, {"id": 217724, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5868, "text": "A written record,"}, {"id": 217725, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5869, "text": "Attested by those who are brought near (unto their Lord)."}, {"id": 217726, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5870, "text": "Lo! the righteous verily are in delight,"}, {"id": 217727, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5871, "text": "On couches, gazing,"}, {"id": 217728, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5872, "text": "Thou wilt know in their faces the radiance of delight."}, {"id": 217729, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5873, "text": "They are given to drink of a pure wine, sealed,"}, {"id": 217730, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5874, "text": "Whose seal is musk - for this let (all) those strive who strive for bliss -"}, {"id": 217731, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5875, "text": "And mixed with water of Tasnim,"}, {"id": 217732, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5876, "text": "A spring whence those brought near (to Allah) drink."}, {"id": 217733, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5877, "text": "Lo! the guilty used to laugh at those who believed,"}, {"id": 217734, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5878, "text": "And wink one to another when they passed them;"}, {"id": 217735, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5879, "text": "And when they returned to their own folk, they returned jesting;"}, {"id": 217736, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5880, "text": "And when they saw them they said: Lo! these have gone astray."}, {"id": 217737, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5881, "text": "Yet they were not sent as guardians over them."}, {"id": 217738, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5882, "text": "This day it is those who believe who have the laugh of disbelievers,"}, {"id": 217739, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5883, "text": "On high couches, gazing."}, {"id": 217740, "identifier": "en.pickthall", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5884, "text": "Are not the disbelievers paid for what they used to do?"}]