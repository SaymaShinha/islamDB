, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 6167, "text": "Ah, what will convey unto thee what she is! -"}, {"id": 217619, "identifier": "en.pickthall", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 6168, "text": "Raging Fire."}]