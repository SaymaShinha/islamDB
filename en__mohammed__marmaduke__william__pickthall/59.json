[{"id": 216985, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5127, "text": "All that is in the heavens and all that is in the earth glorifieth Allah, and He is the Mighty, the Wise."}, {"id": 216986, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5128, "text": "He it is Who hath caused those of the People of the Scripture who disbelieved to go forth from their homes unto the first exile. Ye deemed not that they would go forth, while they deemed that their strongholds would protect them from Allah. But Allah reached them from a place whereof they recked not, and cast terror in their hearts so that they ruined their houses with their own hands and the hands of the believers. So learn a lesson, O ye who have eyes!"}, {"id": 216987, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5129, "text": "And if Allah had not decreed migration for them, He verily would have punished them in the world, and theirs in the Hereafter is the punishment of the Fire."}, {"id": 216988, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5130, "text": "That is because they were opposed to Allah and His messenger; and whoso is opposed to Allah, (for him) verily Allah is stern in reprisal."}, {"id": 216989, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5131, "text": "Whatsoever palm-trees ye cut down or left standing on their roots, it was by Allah's leave, in order that He might confound the evil-livers."}, {"id": 216990, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5132, "text": "And that which Allah gave as spoil unto His messenger from them, ye urged not any horse or riding-camel for the sake thereof, but Allah giveth His messenger lordship over whom He will. Allah is Able to do all things."}, {"id": 216991, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5133, "text": "That which Allah giveth as spoil unto His messenger from the people of the townships, it is for Allah and His messenger and for the near of kin and the orphans and the needy and the wayfarer, that it become not a commodity between the rich among you. And whatsoever the messenger giveth you, take it. And whatsoever he forbiddeth, abstain (from it). And keep your duty to Allah. Lo! Allah is stern in reprisal."}, {"id": 216992, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5134, "text": "And (it is) for the poor fugitives who have been driven out from their homes and their belongings, who seek bounty from Allah and help Allah and His messenger. They are the loyal."}, {"id": 216993, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5135, "text": "Those who entered the city and the faith before them love those who flee unto them for refuge, and find in their breasts no need for that which hath been given them, but prefer (the fugitives) above themselves though poverty become their lot. And whoso is saved from his own avarice - such are they who are successful."}, {"id": 216994, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5136, "text": "And those who came (into the faith) after them say: Our Lord! Forgive us and our brethren who were before us in the faith, and place not in our hearts any rancour toward those who believe. Our Lord! Thou art Full of Pity, Merciful."}, {"id": 216995, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5137, "text": "Hast thou not observed those who are hypocrites, (how) they tell their brethren who disbelieve among the People of the Scripture: If ye are driven out, we surely will go out with you, and we will never obey anyone against you, and if ye are attacked we verily will help you. And Allah beareth witness that they verily are liars."}, {"id": 216996, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5138, "text": "(For) indeed if they are driven out they go not out with them, and indeed if they are attacked they help them not, and indeed if they had helped them they would have turned and fled, and then they would not have been victorious."}, {"id": 216997, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5139, "text": "Ye are more awful as a fear in their bosoms than Allah. That is because they are a folk who understand not."}, {"id": 216998, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5140, "text": "They will not fight against you in a body save in fortified villages or from behind walls. Their adversity among themselves is very great. Ye think of them as a whole whereas their hearts are divers. That is because they are a folk who have no sense."}, {"id": 216999, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5141, "text": "On the likeness of those (who suffered) a short time before them, they taste the ill-effects of their own conduct, and theirs is painful punishment."}, {"id": 217000, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5142, "text": "(And the hypocrites are) on the likeness of the devil when he telleth man to disbelieve, then, when he disbelieveth saith: Lo! I am quit of thee. Lo! I fear Allah, the Lord of the Worlds."}, {"id": 217001, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5143, "text": "And the consequence for both will be that they are in the Fire, therein abiding. Such is the reward of evil-doers."}, {"id": 216964, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5144, "text": "O ye who believe! Observe your duty to Allah. And let every soul look to that which it sendeth on before for the morrow. And observe your duty to Allah. Lo! Allah is Informed of what ye do."}, {"id": 216983, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5145, "text": "And be not ye as those who forgot Allah, therefor He caused them to forget their souls. Such are the evil-doers."}, {"id": 216946, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5146, "text": "Not equal are the owners of the Fire and the owners of the Garden. The owners of the Garden, they are the victorious."}, {"id": 217002, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5147, "text": "If We had caused this Qur'an to descend upon a mountain, thou (O Muhammad) verily hadst seen it humbled, rent asunder by the fear of Allah. Such similitudes coin We for mankind that haply they may reflect."}, {"id": 217003, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5148, "text": "He is Allah, than Whom there is no other Allah, the Knower of the Invisible and the Visible. He is the Beneficent, Merciful."}, {"id": 217004, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5149, "text": "He is Allah, than Whom there is no other Allah, the Sovereign Lord, the Holy One, Peace, the Keeper of Faith, the Guardian, the Majestic, the Compeller, the Superb. Glorified be Allah from all that they ascribe as partner (unto Him)."}, {"id": 217005, "identifier": "en.pickthall", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 5150, "text": "He is Allah, the Creator, the Shaper out of naught, the Fashioner. His are the most beautiful names. All that is in the heavens and the earth glorifieth Him, and He is the Mighty, the Wise."}]