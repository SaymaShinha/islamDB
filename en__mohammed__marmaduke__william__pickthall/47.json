[{"id": 216421, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4546, "text": "Those who disbelieve and turn (men) from the way of Allah, He rendereth their actions vain."}, {"id": 216422, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4547, "text": "And those who believe and do good works and believe in that which is revealed unto Muhammad - and it is the truth from their Lord - He riddeth them of their ill-deeds and improveth their state."}, {"id": 216423, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4548, "text": "That is because those who disbelieve follow falsehood and because those who believe follow the truth from their Lord. Thus Allah coineth their similitudes for mankind."}, {"id": 216424, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4549, "text": "Now when ye meet in battle those who disbelieve, then it is smiting of the necks until, when ye have routed them, then making fast of bonds; and afterward either grace or ransom till the war lay down its burdens. That (is the ordinance). And if Allah willed He could have punished them (without you) but (thus it is ordained) that He may try some of you by means of others. And those who are slain in the way of Allah, He rendereth not their actions vain."}, {"id": 216425, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4550, "text": "He will guide them and improve their state,"}, {"id": 216426, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4551, "text": "And bring them in unto the Garden which He hath made known to them."}, {"id": 216427, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4552, "text": "O ye who believe! If ye help Allah, He will help you and will make your foothold firm."}, {"id": 216428, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4553, "text": "And those who disbelieve, perdition is for them, and He will make their actions vain."}, {"id": 216429, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4554, "text": "That is because they are averse to that which Allah hath revealed, therefor maketh He their actions fruitless."}, {"id": 216430, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4555, "text": "Have they not travelled in the land to see the nature of the consequence for those who were before them? Allah wiped them out. And for the disbelievers there will be the like thereof."}, {"id": 216431, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4556, "text": "That is because Allah is patron of those who believe, and because the disbelievers have no patron."}, {"id": 216432, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4557, "text": "Lo! Allah will cause those who believe and do good works to enter Gardens underneath which rivers flow; while those who disbelieve take their comfort in this life and eat even as the cattle eat, and the Fire is their habitation."}, {"id": 216433, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4558, "text": "And how many a township stronger than thy township (O Muhammad) which hath cast thee out, have We destroyed, and they had no helper!"}, {"id": 216434, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4559, "text": "Is he who relieth on a clear proof from his Lord like those for whom the evil that they do is beautified while they follow their own lusts?"}, {"id": 216435, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4560, "text": "A similitude of the Garden which those who keep their duty (to Allah) are promised: Therein are rivers of water unpolluted, and rivers of milk whereof the flavour changeth not, and rivers of wine delicious to the drinkers, and rivers of clear-run honey; therein for them is every kind of fruit, with pardon from their Lord. (Are those who enjoy all this) like those who are immortal in the Fire and are given boiling water to drink so that it teareth their bowels?"}, {"id": 216436, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4561, "text": "Among them are some who give ear unto thee (Muhammad) till, when they go forth from thy presence they say unto those who have been given knowledge: What was that he said just now? Those are they whose hearts Allah hath sealed, and they follow their own lusts."}, {"id": 216437, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4562, "text": "While as for those who walk aright, He addeth to their guidance, and giveth them their protection (against evil)."}, {"id": 216438, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4563, "text": "Await they aught save the Hour, that it should come upon them unawares? And the beginnings thereof have already come. But how, when it hath come upon them, can they take their warning?"}, {"id": 216439, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4564, "text": "So know (O Muhammad) that there is no Allah save Allah, and ask forgiveness for thy sin and for believing men and believing women. Allah knoweth (both) your place of turmoil and your place of rest."}, {"id": 216440, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4565, "text": "And those who believe say: If only a surah were revealed! But when a decisive surah is revealed and war is mentioned therein, thou seest those in whose hearts is a disease looking at thee with the look of men fainting unto death. Therefor woe unto them!"}, {"id": 216441, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4566, "text": "Obedience and a civil word. Then, when the matter is determined, if they are loyal to Allah it will be well for them."}, {"id": 216442, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4567, "text": "Would ye then, if ye were given the command, work corruption in the land and sever your ties of kinship?"}, {"id": 216443, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4568, "text": "Such are they whom Allah curseth so that He deafeneth them and maketh blind their eyes."}, {"id": 216444, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4569, "text": "Will they then not meditate on the Qur'an, or are there locks on the hearts?"}, {"id": 216445, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4570, "text": "Lo! those who turn back after the guidance hath been manifested unto them, Satan hath seduced them, and He giveth them the rein."}, {"id": 216446, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4571, "text": "That is because they say unto those who hate what Allah hath revealed: We will obey you in some matters; and Allah knoweth their secret talk."}, {"id": 216447, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4572, "text": "Then how (will it be with them) when the angels gather them, smiting their faces and their backs!"}, {"id": 216448, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4573, "text": "That will be because they followed that which angereth Allah, and hated that which pleaseth Him. Therefor He hath made their actions vain."}, {"id": 216449, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4574, "text": "Or do those in whose hearts is a disease deem that Allah will not bring to light their (secret) hates?"}, {"id": 216450, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4575, "text": "And if We would, We could show them unto thee (Muhammad) so that thou shouldst know them surely by their marks. And thou shalt know them by the burden of their talk. And Allah knoweth your deeds."}, {"id": 216451, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4576, "text": "And verily We shall try you till We know those of you who strive hard (for the cause of Allah) and the steadfast, and till We test your record."}, {"id": 216452, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4577, "text": "Lo! those who disbelieve and turn from the way of Allah and oppose the messenger after the guidance hath been manifested unto them, they hurt Allah not a jot, and He will make their actions fruitless."}, {"id": 216453, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4578, "text": "O ye who believe! Obey Allah and obey the messenger, and render not your actions vain."}, {"id": 216454, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4579, "text": "Lo! those who disbelieve and turn from the way of Allah and then die disbelievers, Allah surely will not pardon them."}, {"id": 216455, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4580, "text": "So do not falter and cry out for peace when ye (will be) the uppermost, and Allah is with you, and He will not grudge (the reward of) your actions."}, {"id": 216456, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4581, "text": "The life of the world is but a sport and a pastime. And if ye believe and ward off (evil). He will give you your wages, and will not ask of you your wordly wealth."}, {"id": 216457, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4582, "text": "If He should ask it of you and importune you, ye would hoard it, and He would bring to light your (secret) hates."}, {"id": 216458, "identifier": "en.pickthall", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Pickthall", "transelator_en_name": "Mohammed Marmaduke William Pickthall", "format": "text", "type": "translation", "ayah_number": 4583, "text": "Lo! ye are those who are called to spend in the way of Allah, yet among you there are some who hoard. And as for him who hoardeth, he hoardeth only from his soul. And Allah is the Rich, and ye are the poor. And if ye turn away He will exchange you for some other folk, and they will not be the likes of you."}]