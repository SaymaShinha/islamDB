i", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 5428, "text": "\u062b\u064f\u0645\u0651\u064e \u0625\u0650\u0646\u0651\u0650\u0649\u0653 \u0623\u064e\u0639\u0652\u0644\u064e\u0646\u062a\u064f \u0644\u064e\u0647\u064f\u0645\u0652 \u0648\u064e\u0623\u064e\u0633\u0652\u0631\u064e\u0631\u0652\u062a\u064f \u0644\u064e\u0647\u064f\u0645\u0652 \u0625\u0650\u0633\u0652\u0631\u064e\u0627\u0631\u064b\u06ed\u0627"}, {"id": 5284, "identifier": "quran-uthmani", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 5429, "text": "\u0641\u064e\u0642\u064f\u0644\u0652\u062a\u064f \u0671\u0633\u0652\u062a\u064e\u063a\u0652\u0641\u0650\u0631\u064f\u0648\u0627\u06df \u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u0652 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u063a\u064e\u0641\u0651\u064e\u0627\u0631\u064b\u06ed\u0627"}, {"id": 5285, "identifier": "quran-uthmani", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_oblig