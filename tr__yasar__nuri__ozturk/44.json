[{"id": 609164, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4415, "text": "H\u00e2, M\u00eem."}, {"id": 609165, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4416, "text": "O ayan-beyan g\u00f6steren Kitap'a yemin olsun ki,"}, {"id": 609166, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4417, "text": "Biz onu kutlu/bereketli bir gecede indirdik. Hi\u00e7 ku\u015fkusuz, biz uyar\u0131c\u0131lar\u0131z."}, {"id": 609167, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4418, "text": "Hikmetlerle dolu her i\u015f ve olu\u015f o gecede ay\u0131rt edilir,"}, {"id": 609113, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4419, "text": "Kat\u0131m\u0131zdan bir emir olarak. Hi\u00e7 ku\u015fkusuz biz, resuller g\u00f6ndeririz,"}, {"id": 609168, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4420, "text": "Senin Rabbinden bir rahmet olarak. Hi\u00e7 ku\u015fkusuz O, gere\u011fince duyan, gere\u011fince bilendir."}, {"id": 609169, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4421, "text": "G\u00f6klerin, yerin ve bunlar aras\u0131ndakilerin Rabbidir O, e\u011fer g\u00f6r\u00fcrcesine biliyor iseniz."}, {"id": 609170, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4422, "text": "Tanr\u0131 yoktur O'ndan ba\u015fka! Diriltir ve \u00f6ld\u00fcr\u00fcr. Sizin de Rabbinizdir O, \u00f6nceki atalar\u0131n\u0131z\u0131n da Rabbidir,"}, {"id": 609171, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4423, "text": "\u0130\u015f, onlar\u0131n sand\u0131\u011f\u0131 gibi de\u011fil! Bir ku\u015fku i\u00e7inde oynay\u0131p oyalanmaktad\u0131rlar."}, {"id": 609172, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4424, "text": "Art\u0131k sen g\u00f6\u011f\u00fcn a\u00e7\u0131k\u00e7a izlenen bir duman getirece\u011fi g\u00fcn\u00fc g\u00f6zle."}, {"id": 609173, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4425, "text": "\u0130nsanlar\u0131 ku\u015fat\u0131p sarar. \u0130nletici bir azapt\u0131r bu."}, {"id": 609174, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4426, "text": "\"Ey Rabbimiz, kald\u0131r bizden bu azab\u0131. Biz ger\u00e7ekten m\u00fcminleriz.\""}, {"id": 609175, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4427, "text": "Nerede onlarda \u00f6\u011f\u00fct almak? Yemin olsun, delillerle a\u00e7\u0131klayan bir resul gelmi\u015fti onlara."}, {"id": 609176, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4428, "text": "Ama ondan y\u00fcz \u00e7evirdiler ve \u015f\u00f6yle dediler: \"E\u011fitilmi\u015f bir mecnun!\""}, {"id": 609177, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4429, "text": "Biz azab\u0131 biraz kald\u0131r\u0131r\u0131z; siz eski halinize tekrar d\u00f6nersiniz."}, {"id": 609178, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4430, "text": "G\u00fcn gelir, en b\u00fcy\u00fck vuru\u015fla vururuz biz. \u015eu bir ger\u00e7ek ki, intikam da al\u0131r\u0131z biz!"}, {"id": 609179, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4431, "text": "Kudretimize yemin olsun ki, onlardan \u00f6nce Firavun'un kavmini de ince bir imtihana \u00e7ektik de, asil ve onurlu bir resul geldi onlara."}, {"id": 609180, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4432, "text": "\u015e\u00f6yle sesleniyordu: \"Ey Allah'\u0131n kullar\u0131, bana gelin! \u00c7\u00fcnk\u00fc ben sizin i\u00e7in g\u00fcvenilir bir resul\u00fcm.\""}, {"id": 609181, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4433, "text": "\"Allah'a kar\u015f\u0131 ululuk taslamay\u0131n! Ben size apa\u00e7\u0131k bir kan\u0131t getirmekteyim.\""}, {"id": 609182, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4434, "text": "\"Ben, beni ta\u015flam\u0131nzdan Rabbim ve Rabbinize s\u0131\u011f\u0131nd\u0131m.\""}, {"id": 609183, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4435, "text": "\"Bana inanmad\u0131n\u0131zsa bari benden uzak durun!\""}, {"id": 609184, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4436, "text": "Sonra Rabbine, \"Bunlar su\u00e7 i\u015fleyen bir topluluktur.\" diye yakard\u0131."}, {"id": 609185, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4437, "text": "Bunun \u00fczerine, Allah buyurdu: \"O halde kullar\u0131m\u0131 geceleyin yola \u00e7\u0131kar, \u00e7\u00fcnk\u00fc takip edileceksiniz.\""}, {"id": 609186, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4438, "text": "\"Denizi a\u00e7\u0131k b\u0131rak, \u00e7\u00fcnk\u00fc onlar, bo\u011fulmaya mahk\u00fbm edilmi\u015f bir ordudur.\""}, {"id": 609187, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4439, "text": "Geriye nice bah\u00e7eler, nice p\u0131narlar b\u0131rakt\u0131lar."}, {"id": 609188, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4440, "text": "Nice ekinler, nice se\u00e7kin makamlar."}, {"id": 609189, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4441, "text": "\u0130\u00e7inde zevk s\u00fcrd\u00fckleri nice nimetler."}, {"id": 609190, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4442, "text": "\u0130\u015fte b\u00f6yle! Onlara ba\u015fka bir toplumu miras\u00e7\u0131 k\u0131ld\u0131k."}, {"id": 609191, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4443, "text": "G\u00f6k de a\u011flamad\u0131 onlar i\u00e7in yer de. Y\u00fczlerine bak\u0131lmad\u0131 bile!"}, {"id": 609192, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4444, "text": "Yemin olsun, \u0130srailo\u011fullar\u0131n\u0131, rezil edici bir azaptan kurtard\u0131k."}, {"id": 609193, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4445, "text": "Firavun'dan kurtard\u0131k. Firavun, haddi a\u015fanlar\u0131n b\u00fcy\u00fckl\u00fck taslayanlar\u0131ndan biriydi."}, {"id": 609194, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4446, "text": "Yemin olsun, biz onlar\u0131 bir ilim sayesinde \u00e2lemlere \u00fcst\u00fcn k\u0131lm\u0131\u015ft\u0131k."}, {"id": 609195, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4447, "text": "Onlara, i\u00e7inde a\u00e7\u0131k bir imtihan bulunan ayetler vermi\u015ftik."}, {"id": 609196, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4448, "text": "\u015eimdi, \u015funlar tutmu\u015f diyorlar ki:"}, {"id": 609197, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4449, "text": "\"\u0130lk \u00f6l\u00fcm\u00fcm\u00fczden ba\u015fkas\u0131 yok! Biz diriltilecek filan de\u011filiz!\""}, {"id": 609198, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4450, "text": "\"E\u011fer do\u011fru s\u00f6zl\u00fclerseniz, atalar\u0131m\u0131z\u0131 geri getirin!\""}, {"id": 609199, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4451, "text": "Onlar m\u0131 hay\u0131rl\u0131 yoksa T\u00fcbba' halk\u0131yla onlardan \u00f6nce gelenler mi? Onlar\u0131 hel\u00e2k ettik; \u00e7\u00fcnk\u00fc onlar, su\u00e7 i\u015flemi\u015f insanlard\u0131."}, {"id": 609200, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4452, "text": "Biz g\u00f6kleri, yeri ve bunlar aras\u0131ndakileri e\u011flenmek i\u00e7in yaratmad\u0131k."}, {"id": 609201, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4453, "text": "\u0130kisini de, sadece ger\u00e7e\u011fi g\u00f6stermek \u00fczere yaratt\u0131k. Ama onlar\u0131n \u00e7oklar\u0131 bilmiyorlar."}, {"id": 609202, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4454, "text": "Hi\u00e7 ku\u015fkusuz, ayr\u0131m g\u00fcn\u00fc, hepsinin bulu\u015fma zaman\u0131d\u0131r/bulu\u015fma yeridir."}, {"id": 609203, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4455, "text": "Bir g\u00fcnd\u00fcr ki o, dostun dosta yarar\u0131 olmaz. Onlara yard\u0131m da edilmez."}, {"id": 609204, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4456, "text": "Allah'\u0131n rahmet etti\u011fi kimse m\u00fcstesna. Allah Az\u00eez'dir, Rah\u00eem'dir."}, {"id": 609205, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4457, "text": "\u015eu bir ger\u00e7ek ki zakkum a\u011fac\u0131,"}, {"id": 609206, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4458, "text": "Su\u00e7lular\u0131n yeme\u011fidir."}, {"id": 609207, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4459, "text": "Erimi\u015f maden misali, kar\u0131nlarda kaynar."}, {"id": 609208, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4460, "text": "S\u0131cak suyun kaynamas\u0131 gibi..."}, {"id": 609209, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4461, "text": "\"Tutun onu, cehennemin tam ortas\u0131na g\u00f6t\u00fcr\u00fcn!\""}, {"id": 609210, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4462, "text": "\"Sonra ba\u015f\u0131n\u0131n \u00fcst\u00fcne, kaynar su azab\u0131ndan d\u00f6k\u00fcn!\""}, {"id": 609211, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4463, "text": "\"Tat bakal\u0131m! Hani sen onurluydun, se\u00e7kindin.\""}, {"id": 609212, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4464, "text": "\"\u0130\u015fte budur o ku\u015fkulan\u0131p durdu\u011funuz \u015fey.\""}, {"id": 609213, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4465, "text": "Korunup sak\u0131nanlar, g\u00fcvenli bir makamdad\u0131r;"}, {"id": 609214, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4466, "text": "Bah\u00e7elerde, p\u0131nar ba\u015flar\u0131nda."}, {"id": 609215, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4467, "text": "\u0130nce ipekten, parlak atlastan giymi\u015f olarak, kar\u015f\u0131l\u0131kl\u0131 oturmaktad\u0131rlar."}, {"id": 609216, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4468, "text": "\u0130\u015fte b\u00f6yle! Onlar\u0131 iri g\u00f6zl\u00fc hurilerle de e\u015fle\u015ftirmi\u015fizdir."}, {"id": 609217, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4469, "text": "Orada, g\u00fcvenli bir bi\u00e7imde her t\u00fcrl\u00fc meyveyi isterler."}, {"id": 609218, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4470, "text": "Orada, ilk \u00f6l\u00fcm d\u0131\u015f\u0131nda \u00f6l\u00fcm tatmazlar. Allah onlar\u0131 cehennem azab\u0131ndan korumu\u015ftur."}, {"id": 609219, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4471, "text": "Rabbinden bir l\u00fctuf olarak b\u00f6yledir. \u0130\u015fte budur o b\u00fcy\u00fck ba\u015far\u0131."}, {"id": 609220, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4472, "text": "Biz o Kur'an'\u0131 senin dilinle/senin diline kolayla\u015ft\u0131rd\u0131k ki, d\u00fc\u015f\u00fcn\u00fcp \u00f6\u011f\u00fct alabilsinler."}, {"id": 609221, "identifier": "tr.ozturk", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 4473, "text": "Art\u0131k, beklemeye ge\u00e7! \u00c7\u00fcnk\u00fc onlar da beklemekteler."}]