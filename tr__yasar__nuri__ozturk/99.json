[{"id": 610867, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6139, "text": "Yerk\u00fcre, o sars\u0131nt\u0131yla sars\u0131ld\u0131\u011f\u0131 zaman,"}, {"id": 610868, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6140, "text": "Ve toprak, a\u011f\u0131rl\u0131klar\u0131n\u0131 \u00e7\u0131kard\u0131\u011f\u0131 zaman,"}, {"id": 610869, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6141, "text": "Ve insan: \"Ne oluyor buna?\" dedi\u011fi zaman,"}, {"id": 610870, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6142, "text": "\u0130\u015fte o g\u00fcn yerk\u00fcre, t\u00fcm haberlerini s\u00f6yler/anlat\u0131r."}, {"id": 610871, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6143, "text": "\u00c7\u00fcnk\u00fc Rabbin ona vahyetmi\u015ftir."}, {"id": 610872, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6144, "text": "O g\u00fcn insanlar, yap\u0131p ettikleri kendilerine g\u00f6sterilsin diye k\u00fcmeler halinde ortaya f\u0131rlayacaklard\u0131r."}, {"id": 610873, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6145, "text": "Art\u0131k, kim bir zerre miktar\u0131 hay\u0131r \u00fcretmi\u015fse onu g\u00f6r\u00fcr."}, {"id": 610874, "identifier": "tr.ozturk", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6146, "text": "Ve kim bir zerre miktar\u0131 \u015fer \u00fcretmi\u015fse onu g\u00f6r\u00fcr."}]