[{"id": 610290, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5552, "text": "Hay\u0131r, \u00f6yle de\u011fil! K\u0131yamet g\u00fcn\u00fcne yemin ederim ki,"}, {"id": 610291, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5553, "text": "\u00d6yle de\u011fil! Kendisini \u0131srarla k\u0131nayan benli\u011fe de yemin ederim."}, {"id": 610292, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5554, "text": "\u0130nsan, kendisinin kemiklerini asla bir araya toplamayaca\u011f\u0131m\u0131z\u0131 m\u0131 san\u0131yor?"}, {"id": 610293, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Hay\u0131r, sand\u0131\u011f\u0131 gibi de\u011fil! Biz onun parmak u\u00e7lar\u0131n\u0131 da tam bir bi\u00e7imde d\u00fczenlemeye g\u00fcc\u00fc yetenleriz."}, {"id": 610294, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5556, "text": "Fakat insan kendi \u00f6n\u00fcnde rezillik sergilemeyi ister."}, {"id": 610295, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5557, "text": "\"K\u0131yamet g\u00fcn\u00fc nerede/ne zaman?\" diye sorar."}, {"id": 610296, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5558, "text": "G\u00f6z \u015fim\u015fek \u00e7akt\u0131\u011f\u0131nda,"}, {"id": 610297, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5559, "text": "Ay tutuldu\u011funda,"}, {"id": 610298, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5560, "text": "Ve G\u00fcne\u015f'le Ay biraraya getirildi\u011finde,"}, {"id": 610299, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5561, "text": "Der ki insan o g\u00fcn: \"Ka\u00e7\u0131lacak yer nerede?\""}, {"id": 610300, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5562, "text": "Hay\u0131r, yok s\u0131\u011f\u0131nacak yer!"}, {"id": 610301, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5563, "text": "Var\u0131l\u0131p durulacak yer Rabbinin huzurudur o g\u00fcn."}, {"id": 610302, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5564, "text": "Haber verilir insana o g\u00fcn \u00f6nden g\u00f6nderdi\u011fi de arkaya b\u0131rakt\u0131\u011f\u0131 da."}, {"id": 610303, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5565, "text": "Ger\u00e7ek \u015fu ki insan, \u00f6z benli\u011fi \u00fczerine y\u00f6nelmi\u015f keskin ve derin bir bak\u0131\u015ft\u0131r;"}, {"id": 610304, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5566, "text": "D\u00f6kse de ortaya t\u00fcm mazeretlerini."}, {"id": 610305, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5567, "text": "Onu aceleye getiresin diye dilini onunla hareketlendirme!"}, {"id": 610306, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5568, "text": "Onu toplamak ve okumak bize d\u00fc\u015fer."}, {"id": 610307, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5569, "text": "O halde, biz onu okudu\u011fumuzda, sen onun okunu\u015funu izle."}, {"id": 610308, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5570, "text": "Sonra onu a\u00e7\u0131klamak da bizim i\u015fimiz olacakt\u0131r."}, {"id": 610309, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5571, "text": "Hay\u0131r, hay\u0131r! Siz hemencecik geleni seversiniz."}, {"id": 610310, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5572, "text": "Ve sonradan gelece\u011fi terk edersiniz."}, {"id": 610311, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5573, "text": "Y\u00fczler vard\u0131r o g\u00fcn par\u0131lt\u0131l\u0131,"}, {"id": 610312, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5574, "text": "Rabbine do\u011fru bakan."}, {"id": 610313, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5575, "text": "Ve y\u00fczler vard\u0131r o g\u00fcn, as\u0131k/buruk,"}, {"id": 610314, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5576, "text": "Kendisine, bel k\u0131racak bir hesap y\u00f6nelece\u011fini sezinler."}, {"id": 610315, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5577, "text": "\u0130\u015f, onlar\u0131n sand\u0131\u011f\u0131 gibi de\u011fil! Can, k\u00f6pr\u00fcc\u00fcklere dayand\u0131\u011f\u0131nda,"}, {"id": 610316, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5578, "text": "\"Kim var okuyup \u00fcfleyecek?\" denilir!"}, {"id": 604902, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5579, "text": "Sezinlemi\u015ftir ki odur ayr\u0131l\u0131k."}, {"id": 604905, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5580, "text": "Dola\u015fm\u0131\u015ft\u0131r el-ayak/kol-bacak."}, {"id": 604906, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5581, "text": "Rabbine do\u011frudur o g\u00fcn sevkiyat."}, {"id": 610317, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5582, "text": "Ne tasdik etti ne sadaka verdi ne namaz k\u0131ld\u0131/dua etti."}, {"id": 610318, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5583, "text": "Tam aksine, yalanlad\u0131, gerisin geri d\u00f6nd\u00fc."}, {"id": 610319, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5584, "text": "Sonra da \u00e7al\u0131m sata sata ailesine gitti."}, {"id": 610320, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5585, "text": "\u00c7ok uygundur sana bu bela, \u00e7ok uygun!"}, {"id": 610321, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5586, "text": "Evet, \u00e7ok uygundur sana bu bela, \u00e7ok uygun!"}, {"id": 610322, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5587, "text": "\u0130nsan, ba\u015f\u0131bo\u015f b\u0131rak\u0131laca\u011f\u0131n\u0131 m\u0131 san\u0131yor?"}, {"id": 610323, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5588, "text": "O, d\u00f6k\u00fclen meniden bir sperm de\u011fil miydi?"}, {"id": 610324, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Sonra o, bir \u00e7i\u011fnem et oldu da Allah onu yaratt\u0131, ard\u0131ndan d\u00fczg\u00fcn bir \u015fekle ula\u015ft\u0131rd\u0131."}, {"id": 610325, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5590, "text": "Nihayet ondan iki \u00e7ifti, erke\u011fi ve di\u015fiyi v\u00fccuda getirdi."}, {"id": 610326, "identifier": "tr.ozturk", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5591, "text": "Peki bunu yapan, \u00f6l\u00fcy\u00fc diriltmeye g\u00fc\u00e7 yetiremez mi?"}]