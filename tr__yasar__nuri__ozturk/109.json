[{"id": 610936, "identifier": "tr.ozturk", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6208, "text": "De ki: \"Ey nank\u00f6r k\u00e2firler!"}, {"id": 610937, "identifier": "tr.ozturk", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6209, "text": "Kulluk etmem sizin kulluk etti\u011finize."}, {"id": 610938, "identifier": "tr.ozturk", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6210, "text": "Siz de ibadet etmezsiniz benim ibadet etti\u011fime."}, {"id": 610939, "identifier": "tr.ozturk", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6211, "text": "Kul de\u011filim sizin tapt\u0131\u011f\u0131n\u0131za,"}, {"id": 610940, "identifier": "tr.ozturk", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6212, "text": "Ve ibadet edenler de\u011filsiniz benim ibadet etti\u011fime."}, {"id": 610941, "identifier": "tr.ozturk", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6213, "text": "Sizin dininiz size, benim dinim bana!\""}]