[{"id": 610875, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6147, "text": "Yemin olsun soluyu\u015flar\u0131yla ses \u00e7\u0131kararak ko\u015fanlara/nefes nefese sald\u0131ranlara,"}, {"id": 610876, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6148, "text": "\u00c7ak\u0131p \u00e7ak\u0131p ate\u015f \u00e7\u0131karanlara,"}, {"id": 610877, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6149, "text": "Sabahleyin ak\u0131n edenlere/bask\u0131n yap\u0131p toprak fethedenlere,"}, {"id": 610878, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6150, "text": "Derken, onunla toz duman \u00e7\u0131karanlara,"}, {"id": 610879, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6151, "text": "Derken, onunla bir toplulu\u011fun ortas\u0131na dalanlara ki,"}, {"id": 610880, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6152, "text": "\u0130nsan, Rabbine kar\u015f\u0131 ger\u00e7ekten \u00e7ok nank\u00f6rd\u00fcr!"}, {"id": 610881, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6153, "text": "Ve kendisi de buna iyiden iyiye tan\u0131kt\u0131r."}, {"id": 610882, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6154, "text": "O, mal ve servet arzusu y\u00fcz\u00fcnden alabildi\u011fine kat\u0131d\u0131r."}, {"id": 610883, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6155, "text": "Bilmez mi ki o, kabirler i\u00e7indekiler d\u0131\u015far\u0131 f\u0131rlat\u0131ld\u0131\u011f\u0131nda,"}, {"id": 610884, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6156, "text": "G\u00f6\u011f\u00fcslerin i\u00e7indekiler derlenip topland\u0131\u011f\u0131nda,"}, {"id": 610885, "identifier": "tr.ozturk", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6157, "text": "Hi\u00e7 ku\u015fkusuz, o g\u00fcn, Rableri onlardan iyice haberdar olacakt\u0131r."}]