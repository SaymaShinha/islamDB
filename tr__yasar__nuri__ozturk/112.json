[{"id": 610950, "identifier": "tr.ozturk", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6222, "text": "De ki: O, Allah't\u0131r; Ahad'd\u0131r, tektir!"}, {"id": 610951, "identifier": "tr.ozturk", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6223, "text": "Allah't\u0131r; Samed'dir/t\u00fcm ihtiya\u00e7lar\u0131n, niyetlerin, \u00f6vg\u00fclerin, yakar\u0131\u015flar\u0131n y\u00f6neldi\u011fi tek kuvvettir!"}, {"id": 610952, "identifier": "tr.ozturk", "surah_number": 112, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u062e\u0644\u0627\u0635", "surah_en_name": "Al-Ikhlaas", "surah_en_name_translation": "Sincerity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 554, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6224, "text": "Ne do\u011furmu\u015ftur O, ne do\u011furulmu\u015ftur!"}, {"id": 610953, "i