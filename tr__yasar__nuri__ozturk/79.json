[{"id": 610448, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5713, "text": "Yemin olsun, \u00e7ekip koparanlara/yay \u00e7ekenlere/kuyudan su \u00e7ekenlere/ba\u011fs\u0131z-bek\u00e7isiz ko\u015fan atlara/ayr\u0131l\u0131k y\u00fcz\u00fcnden hasret \u00e7ekenlere/dald\u0131r\u0131p dald\u0131r\u0131p \u00e7\u0131karanlara,"}, {"id": 610449, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5714, "text": "Yemin olsun, rahat\u00e7a, incitmeden \u00e7ekenlere/d\u00fc\u011f\u00fcm\u00fc h\u00fcnerle \u00e7\u00f6zenlere/bir yerden bir yere gidenlere/co\u015fkuyla i\u00e7 \u00e7ekenlere,"}, {"id": 610450, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5715, "text": "Yemin olsun, bo\u015flukta yahut suda y\u00fcz\u00fcp gidenlere,"}, {"id": 610451, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5716, "text": "Derken \u00f6ne ge\u00e7ip yar\u0131\u015f\u0131 kazananlara,"}, {"id": 610452, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5717, "text": "Bir i\u015f ve olu\u015fu \u00e7ekip \u00e7evirenlere,"}, {"id": 610453, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5718, "text": "Ki o g\u00fcn \u015fiddetle sarsacak olan saracakt\u0131r."}, {"id": 610454, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5719, "text": "Onu, ard\u0131s\u0131ra gelen izleyecektir."}, {"id": 610455, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5720, "text": "Baz\u0131 kalpler o g\u00fcn kayg\u0131dan titreyecektir."}, {"id": 610456, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5721, "text": "Onlar\u0131n g\u00f6zleri yerlere e\u011filecektir."}, {"id": 610457, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5722, "text": "\"Biz ger\u00e7ekten bu \u00e7ukurda eski halimize d\u00f6nd\u00fcr\u00fclecek miyiz?\" diyorlar."}, {"id": 610458, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5723, "text": "\"Un-ufak kemikler haline geldikten sonra, \u00f6yle mi!\""}, {"id": 610459, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5724, "text": "\"H\u00fcsran dolu bir d\u00f6n\u00fc\u015ft\u00fcr bu \u00f6yleyse!\" diye konu\u015ftular."}, {"id": 610460, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5725, "text": "Oysaki o, sert bir komut sesinden ibarettir."}, {"id": 610461, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5726, "text": "Bir anda hepsi uyan\u0131p ortaya geliverir."}, {"id": 610462, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5727, "text": "Ula\u015ft\u0131 m\u0131 sana M\u00fbsa'n\u0131n haberi?"}, {"id": 610463, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5728, "text": "Hani, Rabbi ona, kutsal vadide, Tuva'da seslenmi\u015fti:"}, {"id": 610464, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5729, "text": "\"Firavun'a git! \u0130yice azd\u0131 o.\""}, {"id": 610465, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5730, "text": "\"De ki ona: 'Ar\u0131n\u0131p temizlenmeye ne dersin?\""}, {"id": 610466, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5731, "text": "\"Seni Rabbine k\u0131lavuzlayay\u0131m da g\u00f6n\u00fclden \u00fcrperesin!\""}, {"id": 610467, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5732, "text": "Derken, ona o en b\u00fcy\u00fck mucizeyi g\u00f6sterdi."}, {"id": 610468, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5733, "text": "Ama o yalanlad\u0131, isyan etti."}, {"id": 610469, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5734, "text": "Sonra, s\u0131rt\u0131n\u0131 d\u00f6nd\u00fc; ko\u015fuyordu."}, {"id": 610470, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5735, "text": "Derken, bir araya toplay\u0131p ba\u011f\u0131rd\u0131."}, {"id": 610471, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5736, "text": "Dedi ki: \"Ben sizin en y\u00fcce rabbinizim.\""}, {"id": 610472, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5737, "text": "Bunun \u00fczerine Allah, onu sonraya ve \u00f6nceye ibret olmak \u00fczere bir ceza ile \u00e7arpt\u0131."}, {"id": 610473, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5738, "text": "Ku\u015fkusuz, bunda, i\u00e7ine \u00fcrperti d\u00fc\u015fen i\u00e7in tam bir ibret vard\u0131r."}, {"id": 610474, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5739, "text": "Siz mi daha zorsunuz yarat\u0131l\u0131\u015f\u00e7a, g\u00f6k m\u00fc?"}, {"id": 610475, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5740, "text": "Onu O yap\u0131p kurdu. Onun boyunu y\u00fckseltti; ard\u0131ndan ona ahenk ve d\u00fczen verdi."}, {"id": 610476, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5741, "text": "Gecesini karartt\u0131, ku\u015flu\u011funu ortaya \u00e7\u0131kard\u0131."}, {"id": 610477, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5742, "text": "Bundan sonra da yeri yay\u0131p deve ku\u015fu yumurtas\u0131 bi\u00e7iminde yuvarlatt\u0131."}, {"id": 610478, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5743, "text": "Ondan suyunu, otla\u011f\u0131n\u0131 \u00e7\u0131kard\u0131."}, {"id": 610479, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5744, "text": "Da\u011flar\u0131, demir atm\u0131\u015f gibi oturttu;"}, {"id": 610480, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5745, "text": "Sizin i\u00e7in ve hayvanlar\u0131n\u0131z i\u00e7in bir ge\u00e7im arac\u0131 olarak."}, {"id": 610481, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5746, "text": "O g\u00fc\u00e7 yetmez b\u00fcy\u00fck felaket geldi\u011finde,"}, {"id": 610482, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5747, "text": "O g\u00fcn insan, u\u011frunda gayret sarfetti\u011fi \u015feyi hat\u0131rlar."}, {"id": 610483, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5748, "text": "G\u00f6ren ki\u015fi i\u00e7in cehennem apa\u00e7\u0131k ortaya \u00e7\u0131kar\u0131lm\u0131\u015ft\u0131r."}, {"id": 610484, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5749, "text": "Art\u0131k azm\u0131\u015f olan,"}, {"id": 610485, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5750, "text": "Ve i\u011freti hayat\u0131 ye\u011flemi\u015f olan i\u00e7in,"}, {"id": 610486, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5751, "text": "Cehennem, bar\u0131na\u011f\u0131n ta kendisidir."}, {"id": 610487, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5752, "text": "Rabbinin y\u00fcceli\u011finden korkup nefsini bo\u015f heveslerden yasaklam\u0131\u015f olan i\u00e7inse,"}, {"id": 610488, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5753, "text": "Cennet, bar\u0131na\u011f\u0131n ta kendisidir."}, {"id": 610489, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5754, "text": "O saatten soruyorlar sana, \"gelip demir atmas\u0131 ne zaman?\" diye."}, {"id": 610490, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5755, "text": "Nerede sende, onu hat\u0131rlatacak \u015fey!"}, {"id": 610491, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5756, "text": "Ona ili\u015fkin bilginin sonu Rabbine var\u0131r."}, {"id": 610492, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Sen sadece, ondan korkanlar\u0131 uyarans\u0131n."}, {"id": 610493, "identifier": "tr.ozturk", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 5758, "text": "Onu g\u00f6rd\u00fckleri g\u00fcn onlar, d\u00fcnyada sanki bir ak\u015fam veya onun ku\u015fluk vaktinden ba\u015fka kalmam\u0131\u015fa d\u00f6nerler."}]