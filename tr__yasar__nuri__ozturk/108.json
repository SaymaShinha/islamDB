[{"id": 610933, "identifier": "tr.ozturk", "surah_number": 108, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0648\u062b\u0631", "surah_en_name": "Al-Kawthar", "surah_en_name_translation": "Abundance", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 550, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6205, "text": "Hi\u00e7 ku\u015fkusuz, biz verdik sana Kevser'i/iyilik, bereket, mutluluk, g\u00fczellik, soy ve ayd\u0131nl\u0131\u011f\u0131n t\u00fckenmezini."}, {"id": 610934, "identifier": "tr.ozturk", "surah_number": 108, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0648\u062b\u0631", "surah_en_name": "Al-Kawthar", "surah_en_name_translation": "Abundance", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 550, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6206, "text": "O halde, sen de Rabbin i\u00e7in namaz k\u0131l/dua et ve g\u00f6\u011fs\u00fcn\u00fc gererek dimdik dur/sa\u011f elini sol elinin \u00fczerine koyup k\u0131yam et/namaz\u0131 vakti girer girmez k\u0131l/kavray\u0131\u015f\u0131n\u0131 bilgi ile derinle\u015ftir/eti yenecek hayvan kes!"}, {"id": 610935, "identifier": "tr.ozturk", "surah_number": 108, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0648\u062b\u0631", "surah_en_name": "Al-Kawthar", "surah_en_name_translation": "Abundance", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 550, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6207, "text": "Ku\u015fkun olmas\u0131n ki, ebter/soyu kesik, seni k\u00f6t\u00fcleyenin ta kendisidir!"}]