[{"id": 610926, "identifier": "tr.ozturk", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6198, "text": "G\u00f6rd\u00fcn m\u00fc o, dini yalan sayan\u0131?"}, {"id": 610927, "identifier": "tr.ozturk", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6199, "text": "\u0130\u015fte odur yetimi itip kakan;"}, {"id": 610928, "identifier": "tr.ozturk", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6200, "text": "Yoksulu doyurmay\u0131 \u00f6zendirmez o."}, {"id": 610929, "identifier": "tr.ozturk", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u