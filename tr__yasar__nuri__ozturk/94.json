[{"id": 610822, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6091, "text": "A\u00e7\u0131p geni\u015fletmedik mi senin g\u00f6\u011fs\u00fcn\u00fc!"}, {"id": 610823, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6092, "text": "\u0130ndirmedik mi \u00fczerinden a\u011f\u0131r y\u00fck\u00fcn\u00fc!"}, {"id": 610824, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6093, "text": "Ki o, belini \u00e7at\u0131rdatm\u0131\u015ft\u0131 senin."}, {"id": 610825, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6094, "text": "Ve y\u00fcceltmedik mi senin \u015fan\u0131n\u0131!"}, {"id": 610826, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6095, "text": "Demek ki, zorlu\u011fun yan\u0131nda bir kolayl\u0131k mutlaka var!"}, {"id": 610827, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6096, "text": "Zorlu\u011fun yan\u0131nda bir kolayl\u0131k muhakkak var!"}, {"id": 610828, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6097, "text": "O halde, bir i\u015f ve olu\u015ftan bo\u015fal\u0131r bo\u015falmaz yeni bir i\u015fe koyulup yorul!"}, {"id": 610829, "identifier": "tr.ozturk", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6098, "text": "Ve yaln\u0131z Rabbine y\u00f6nelip do\u011frul!"}]