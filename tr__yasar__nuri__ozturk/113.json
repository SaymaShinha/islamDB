[{"id": 610954, "identifier": "tr.ozturk", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6226, "text": "De ki: \"Yar\u0131lan karanl\u0131ktan \u00e7\u0131kan sabah\u0131n Rabbine/yar\u0131l\u0131\u015flardan f\u0131\u015fk\u0131ran olu\u015fun Rabbine s\u0131\u011f\u0131n\u0131r\u0131m!"}, {"id": 610955, "identifier": "tr.ozturk", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6227, "text": "Yaratt\u0131klar\u0131n\u0131n \u015ferrinden,"}, {"id": 610956, "identifier": "tr.ozturk", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "\u00d6zt\u00fcrk", "transelator_en_name": "Yasar Nuri Ozturk", "format": "text", "type": "translation", "ayah_number": 6228, "text": "\u00c7\u00f6kt\u00fc\u011f\u00fc zaman karanl\u0131\u011f\u0131n/gelip \u00e7att\u0131\u011f\