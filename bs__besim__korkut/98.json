[{"id": 111903, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6131, "text": "Nisu se nevjernici izme\u0111u sljedbenika Knjige i mnogobo\u0161ci odvojili, sve dok im nije do\u0161ao dokaz jasni;"}, {"id": 111904, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6132, "text": "od Allaha Poslanik, koji \u010dita listove \u010diste,"}, {"id": 111905, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6133, "text": "u kojima su propisi ispravni."}, {"id": 111906, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6134, "text": "A podvojili su se oni kojima je data Knjiga ba\u0161 onda kada im je do\u0161ao dokaz jasni,"}, {"id": 111907, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6135, "text": "a nare\u0111eno im je da se samo Allahu klanjaju, da Mu iskreno, kao pravovjerni, vjeru ispovijedaju, i da molitvu obavljaju, i da milostinju udjeljuju; a to je \u2013 ispravna vjera."}, {"id": 111908, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6136, "text": "Oni koji ne vjeruju izme\u0111u sljedbenika Knjige i mnogobo\u0161ci bi\u0107e, sigurno, u vatri d\u017eehennemskoj, u njoj \u0107e vje\u010dno ostati; oni su najgora stvorenja."}, {"id": 111909, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6137, "text": "A oni koji vjeruju i \u010dine dobra djela \u2013 oni su, zbilja, najbolja stvorenja,"}, {"id": 111910, "identifier": "bs.korkut", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6138, "text": "njih nagrada u Gospodara njihova \u010deka; edenski vrtovi kroz koje \u0107e rijeke te\u0107i, u kojima \u0107e vje\u010dno i zauvijek boraviti; Allah \u0107e biti njima zadovoljan, a i oni \u0107e biti Njime zadovoljni. To \u0107e biti za onoga koji se bude bojao Gospodara svoga."}]