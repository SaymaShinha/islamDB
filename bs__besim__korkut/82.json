[{"id": 111602, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5830, "text": "Kada se nebo rascijepi,"}, {"id": 111603, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5831, "text": "i kada zvijezde popadaju,"}, {"id": 111604, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5832, "text": "i kada se mora jedna u druga uliju,"}, {"id": 111605, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5833, "text": "i kada se grobovi ispreturaju,"}, {"id": 111606, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5834, "text": "svako \u0107e saznati \u0161ta je pripremio, a \u0161ta je propustio."}, {"id": 111607, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5835, "text": "O \u010dovje\u010de, za\u0161to da te obmanjuje to \u0161to je Gospodar tvoj plemenit,"}, {"id": 111608, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5836, "text": "koji te je stvorio \u2013 pa u\u010dinio da si skladan i da si uspravan \u2013"}, {"id": 111609, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5837, "text": "i kakav je htio lik ti dao?"}, {"id": 111610, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5838, "text": "A ne valja tako! Vi u Sudnji dan ne vjerujete,"}, {"id": 111611, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5839, "text": "a nad vama bdiju \u010duvari,"}, {"id": 111612, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5840, "text": "kod Nas cijenjeni pisari,"}, {"id": 111613, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5841, "text": "koji znaju ono \u0161to radite."}, {"id": 111614, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5842, "text": "\u010cestiti \u0107e, sigurno, u D\u017eennet,"}, {"id": 111615, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5843, "text": "a grje\u0161nici, sigurno, u D\u017eehennem,"}, {"id": 111616, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5844, "text": "na Sudnjem danu u njemu \u0107e gorjeti"}, {"id": 111617, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5845, "text": "i vi\u0161e iz njega ne\u0107e izvedeni biti."}, {"id": 111618, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5846, "text": "A zna\u0161 li ti \u0161ta je Sudnji dan,"}, {"id": 111619, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5847, "text": "i jo\u0161 jednom: zna\u0161 li ti \u0161ta je Sudnji dan?"}, {"id": 111620, "identifier": "bs.korkut", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 5848, "text": "Dan kada niko nikome ne\u0107e mo\u0107i nimalo pomo\u0107i, toga Dana \u0107e vlast jedino Allah imati."}]