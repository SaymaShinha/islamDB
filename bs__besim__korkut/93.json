[{"id": 111853, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6080, "text": "Tako mi jutra"}, {"id": 111854, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6081, "text": "i no\u0107i kada se uti\u0161a \u2013"}, {"id": 111855, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Gospodar tvoj nije te ni napustio ni omrznuo!"}, {"id": 111856, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6083, "text": "Onaj svijet je, zaista, bolji za tebe od ovoga svijeta,"}, {"id": 111857, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6084, "text": "a Gospodar tvoj \u0107e tebi, sigurno, dati, pa \u0107e\u0161 zadovoljan biti!"}, {"id": 111858, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Zar nisi siro\u010de bio, pa ti je On uto\u010di\u0161te pru\u017eio,"}, {"id": 111859, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6086, "text": "i za pravu vjeru nisi znao, pa te je na Pravi put uputio,"}, {"id": 111860, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6087, "text": "i siromah si bio, pa te je imu\u0107nim u\u010dinio?"}, {"id": 111861, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6088, "text": "Zato siro\u010de ne ucvili,"}, {"id": 111862, "identifier": "bs.korkut", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6089, "text": "a na prosjaka ne podvikni,"}, 