[{"id": 109234, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3410, "text": "Elif L\u0101m M\u012bm."}, {"id": 109235, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3411, "text": "Bizantinci su pobije\u0111eni"}, {"id": 109236, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3412, "text": "u susjednoj zemlji, ali oni \u0107e, poslije poraza svoga, sigurno pobijediti"}, {"id": 109237, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3413, "text": "za nekoliko godina \u2013 i prije, i poslije, Allahova je odluka \u2013 i tada \u0107e se vjernici radovati"}, {"id": 109238, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3414, "text": "Allahovoj pomo\u0107i \u2013 On poma\u017ee kome ho\u0107e, On je Silni i Samilosni \u2013"}, {"id": 109239, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3415, "text": "obe\u0107anje je Allahovo, a Allah \u0107e obe\u0107anje Svoje ispuniti, ali ve\u0107ina ljudi ne zna;"}, {"id": 109240, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3416, "text": "oni znaju samo spolja\u0161nju stranu \u017eivota na ovome svijetu, a prema onome svijetu su ravnodu\u0161ni."}, {"id": 109241, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3417, "text": "A za\u0161to ne razmisle sami o sebi? Allah je stvorio nebesa i Zemlju i ono \u0161to je izme\u0111u njih \u2013 sa ciljem i do roka odre\u0111enog. A mnogi ljudi ne vjeruju da \u0107e pred Gospodara svoga doista iza\u0107i."}, {"id": 109242, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3418, "text": "Za\u0161to ne putuju po svijetu pa da vide kako su zavr\u0161ili oni prije njih? Oni su bili od njih ja\u010di, i zemlju su orali i obra\u0111ivali je vi\u0161e nego \u0161to je obra\u0111uju ovi, i poslanici su im donosili jasne dokaze; Allah im nije u\u010dinio nepravdu, sami su sebi nepravdu nanijeli."}, {"id": 109243, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3419, "text": "Oni koji su zlo \u010dinili zavr\u0161i\u0107e naju\u017easnijom patnjom, zato \u0161to su Allahove rije\u010di poricali i \u0161to su ih ruglu izvrgavali."}, {"id": 109244, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3420, "text": "Allah iz ni\u010dega stvara, On \u0107e to ponovo u\u010diniti i na kraju Njemu \u0107ete se vratiti."}, {"id": 109245, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3421, "text": "Na dan kad nastupi \u010cas \u2013 mnogobo\u0161ci \u0107e svaku nadu izgubiti:"}, {"id": 109246, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3422, "text": "bo\u017eanstva njihova ne\u0107e im biti zagovornici, a zbog bo\u017eanstava svojih bili su nevjernici."}, {"id": 109247, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3423, "text": "Na dan kad nastupi \u010cas o\u017eivljenja \u2013 ljudi \u0107e se razdvojiti:"}, {"id": 109248, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3424, "text": "oni koji su vjerovali i dobra djela \u010dinili \u2013 u d\u017eennetskom perivoju \u0107e se radovati,"}, {"id": 109249, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3425, "text": "a oni koji nisu vjerovali i koji su ajete Na\u0161e i susret na Sudnjem danu poricali \u2013 u trajnoj \u0107e muci biti."}, {"id": 109250, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3426, "text": "Pa, hvaljen neka je Allah kad god omrknete i kad god osvanete \u2013"}, {"id": 109251, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3427, "text": "Njemu neka je pohvala i na nebesima i na Zemlji \u2013 i predve\u010der i u podne! \u2013"}, {"id": 109252, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3428, "text": "On iz ne\u017eivog stvara \u017eivo i \u017eivo pretvara u ne\u017eivo. On o\u017eivljava zemlju nakon mrtvila njezina \u2013 isto tako \u0107ete i vi biti o\u017eivljeni."}, {"id": 109253, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3429, "text": "Jedan od dokaza Njegovih je to \u0161to vas od zemlje stvara, i odjednom vas, ljudi, svuda ima razasutih;"}, {"id": 109254, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3430, "text": "i jedan od dokaza Njegovih je to \u0161to za vas, od vrste va\u0161e, stvara \u017eene da se uz njih smirite, i \u0161to izme\u0111u vas uspostavlja ljubav i samilost; to su, zaista, pouke za ljude koji razmi\u0161ljaju;"}, {"id": 109255, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3431, "text": "i jedan od dokaza Njegovih je stvaranje nebesa i Zemlje, i raznovrsnost jezika va\u0161ih i boja va\u0161ih; to su, zaista, pouke za one koji znaju;"}, {"id": 109256, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3432, "text": "i jedan od dokaza Njegovih je san va\u0161 no\u0107u i po danu, i nastojanje va\u0161e da steknete ne\u0161to iz obilja Njegova; to su, zaista, pouke za ljude koji \u010duju;"}, {"id": 109257, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3433, "text": "i jedan od dokaza Njegovih je to \u0161to vam pokazuje munju, da se pobojite i ponadate, i to \u0161to spu\u0161ta s neba ki\u0161u i o\u017eivljava njome zemlju poslije mrtvila njezina; to su, zaista, pouke za ljude koji razumiju."}, {"id": 109258, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3434, "text": "I jedan od dokaza Njegovih je i to \u0161to nebo i Zemlja postoje voljom Njegovom. Zatim to \u0161to \u0107ete, \u010dim vas On samo jednom iz zemlje pozove, brzo ustati."}, {"id": 109259, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3435, "text": "Njemu pripada sve \u0161to je na nebesima i na Zemlji, sve je Njemu poslu\u0161no."}, {"id": 109260, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3436, "text": "On je Taj koji iz ni\u010dega stvara i On \u0107e to ponovo u\u010diniti, to je Njemu lahko; On je uzvi\u0161en i na nebesima i na Zemlji; On je silan i mudar."}, {"id": 109261, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3437, "text": "On vam kao primjer navodi vas same: da li su oni koji su u posjedu va\u0161em izjedna\u010deni s vama u onome \u0161to vam Mi dajemo, pa ste u tome isti, i da li ih se bojite kao \u0161to se vi jedni drugih bojite? \u2013 Eto, tako Mi, podrobno, izla\u017eemo dokaze Na\u0161e ljudima koji razmi\u0161ljaju."}, {"id": 109262, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3438, "text": "Ali, nevjernici lahkomisleno slijede strasti svoje \u2013 a ko \u0107e na Pravi put uputiti onoga koga je Allah u zabludi ostavio? \u2013 njima ne\u0107e mo\u0107i niko u pomo\u0107 prite\u0107i."}, {"id": 109263, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3439, "text": "Ti upravi lice svoje vjeri, kao pravi vjernik, vjeri, djelu Allahovu, prema kojoj je On ljude na\u010dinio \u2013 ne treba se mijenjati Allahova vjera, jer to je prava vjera, ali ve\u0107ina ljudi to ne zna \u2013"}, {"id": 109264, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3440, "text": "obra\u0107aju\u0107i Mu se predano! Bojte se Njega i obavljajte molitvu, i ne budite od onih koji Mu druge ravnim smatraju,"}, {"id": 109265, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3441, "text": "od onih koji su vjeru svoju razbili i u stranke se podijelili; svaka stranka zadovoljna onim \u0161to ispovijeda."}, {"id": 109266, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3442, "text": "A kad ljude nevolja sna\u0111e, oni se Gospodaru svome pokajni\u010dki obra\u0107aju, a poslije, kad im On d\u00e2 da okuse milost Njegovu, odjednom neki od njih Gospodaru svome druge ravnim smatraju"}, {"id": 109267, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3443, "text": "da bi bili nezahvalni na onome \u0161to im Mi dajemo. \"Pa u\u017eivajte, a sazna\u0107ete!\""}, {"id": 109140, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3444, "text": "Zar smo im Mi poslali kakav dokaz koji govori u prilog onih koje Njemu ravnim smatraju?"}, {"id": 109268, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3445, "text": "Kad dopustimo ljudima da se blagodatima nasla\u0111uju, oni im se obraduju, a kad ih pogodi nevolja, zbog onoga \u0161to su ruke njihove \u010dinile, odjednom o\u010dajavaju."}, {"id": 109269, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3446, "text": "Zar oni ne znaju da Allah u obilju daje hranu onome kome On ho\u0107e, i da uskra\u0107uje? To su, uistinu, dokazi za ljude koji vjeruju."}, {"id": 109270, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3447, "text": "Zato podaj bli\u017enjemu pravo njegovo, i siromahu i putniku! To je bolje za one koji nastoje da se Allahu umile; ti \u0107e posti\u0107i \u0161ta \u017eele."}, {"id": 109271, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3448, "text": "A novac koji dajete da se uve\u0107a novcem drugih ljudi ne\u0107e se kod Allaha uve\u0107ati, a za milostinju koju udijelite da biste se Allahu umilili \u2013 takvi \u0107e dobra djela svoja umnogostru\u010diti."}, {"id": 109272, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3449, "text": "Allah vas stvara, i opskrbljuje; On \u0107e vam \u017eivot oduzeti i na kraju vas o\u017eiviti. Postoji li ijedno bo\u017eanstvo va\u0161e koje bilo \u0161ta od toga \u010dini? Hvaljen neka je On i vrlo visoko iznad onih koje Njemu smatraju ravnim!"}, {"id": 109273, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3450, "text": "Zbog onoga \u0161to ljudi rade, pojavio se mete\u017e i na kopnu i na moru, da im On d\u00e2 da iskuse kaznu zbog onoga \u0161to rade, ne bi li se popravili."}, {"id": 109274, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3451, "text": "Reci: \"Putujte po svijetu pa pogledajte kako su on\u012b prije zavr\u0161ili; ve\u0107inom su oni mnogobo\u0161ci bili.\""}, {"id": 109275, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3452, "text": "Zato ti upravi lice svoje prema pravoj vjeri, prije nego \u0161to Allahovom voljom nastupi Dan koji niko ne\u0107e mo\u0107i odbiti. Toga dana On \u0107e ih razdvojiti:"}, {"id": 109276, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3453, "text": "oni koji nisu vjerovali \u2013 na svoju \u0161tetu nisu vjerovali, a oni koji su dobra djela \u010dinili \u2013 sebi su D\u017eennet pripremili,"}, {"id": 109277, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3454, "text": "da On iz obilja Svoga nagradi one koji su vjerovali i dobra djela \u010dinili; a On, zaista, ne voli nevjernike."}, {"id": 109278, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3455, "text": "Jedan od dokaza Njegovih je to \u0161to On \u0161alje vjetrove kao nosioce radosnih vijesti da vam d\u00e2 da milost Njegovu osjetite i da la\u0111e voljom Njegovom plove i da iz obilja Njegova stje\u010dete, i da biste zahvalni bili."}, {"id": 109279, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3456, "text": "I prije tebe smo poslanike narodima njihovim slali i oni su im prave dokaze donosili, pa smo one koji su grije\u0161ili ka\u017enjavali \u2013 a du\u017enost Nam je bila vjernike pomo\u0107i."}, {"id": 109280, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3457, "text": "Allah je Taj koji vjetrove \u0161alje, pa oni oblake tjeraju i On ih po nebu, kako On ho\u0107e, rasprostire i na komade dijeli, pa ti vidi\u0161 ki\u0161u kako iz njih pada, i kad je On na robove Svoje na koje \u017eeli prolije, oni se odjednom rado\u0161\u0107u ispune,"}, {"id": 109281, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3458, "text": "iako su bili o\u010dajni prije nego \u0161to se spustila na njih."}, {"id": 109282, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3459, "text": "Zato pogledaj tragove Allahove milosti \u2013 kako On o\u017eivi zemlju nakon mrtvila njezina! On \u0107e, uistinu, i mrtve o\u017eiviti, On sve mo\u017ee."}, {"id": 109283, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3460, "text": "A da po\u0161aljemo vjetar i da oni vide da je sve po\u017eutjelo, oni bi, i poslije toga, blagodati poricali."}, {"id": 109284, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3461, "text": "Ti ne mo\u017ee\u0161 mrtve dozvati, ni gluhe, kad se od tebe okrenu, dovikati,"}, {"id": 109286, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3462, "text": "niti mo\u017ee\u0161 slijepe od zablude njihove odvratiti; mo\u017ee\u0161 jedino dozvati one koji u ajete Na\u0161e vjeruju, jedino oni \u0107e se odazvati."}, {"id": 109287, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3463, "text": "Allah je Taj koji vas nejakim stvara, i onda vam, poslije nejakosti, snagu daje, a poslije snage iznemoglost i sijede vlasi; On stvara \u0161to ho\u0107e; On sve zna i sve mo\u017ee."}, {"id": 109288, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3464, "text": "A na Dan kad nastupi \u010cas o\u017eivljenja, zlo\u010dinci \u0107e se zaklinjati da su u grobovima samo jedan \u010das ostali; a i prije su istinu izbjegavali."}, {"id": 109289, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3465, "text": "A re\u0107i \u0107e oni kojima je dato znanje i vjerovanje: \"Vi ste ostali, prema Allahovoj odredbi, sve do Dana o\u017eivljenja \u2013 a ovo je Dan o\u017eivljenja, samo \u0161to vi niste znali.\""}, {"id": 109290, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3466, "text": "Toga Dana ne\u0107e ni\u010dem poslu\u017eiti pravdanja onima koji su se prema sebi ogrije\u0161ili i ne\u0107e se od njih tra\u017eiti da se Allahu umile."}, {"id": 109291, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3467, "text": "U ovom Kur'anu Mi navodimo ljudima svakovrsne primjere. A kad bi im ti ne znam kakvo \u010dudo donio, opet bi rekli oni koji ne\u0107e da vjeruju: \"Vi samo iznosite la\u017ei!\""}, {"id": 109292, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3468, "text": "Eto tako Allah pe\u010dati srca onih koji ne\u0107e da znaju."}, {"id": 109293, "identifier": "bs.korkut", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3469, "text": "A ti budi strpljiv! Allahovo obe\u0107anje je, zaista, istina i neka te nikako ne obmanu oni koji \u010dvrsto ne vjeruju."}]