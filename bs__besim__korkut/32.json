[{"id": 109325, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3504, "text": "Elif L\u0101m M\u012bm."}, {"id": 109326, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3505, "text": "Knjigu objavljuje, u to nema sumnje, Gospodar svjetova,"}, {"id": 109327, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3506, "text": "a oni govore: \"On je izmi\u0161lja!\" Ne, ona je istina od Gospodara tvoga da opominje\u0161 narod kojemu prije tebe nije do\u0161ao niko da ga opominje, da bi i\u0161ao Pravim putem."}, {"id": 109328, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3507, "text": "Allah je nebesa i Zemlju i ono \u0161to je izme\u0111u njih u \u0161est vremenskih razdoblja stvorio, a onda svemirom zagospodario; vi, osim Njega, ni za\u0161titnika ni posrednika nemate, pa za\u0161to se ne urazumite?"}, {"id": 109329, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3508, "text": "On upravlja svima, od neba do Zemlje, a onda se sve to Njemu vra\u0107a u danu koji, prema va\u0161em ra\u010dunanju vremena, hiljadu godina traje."}, {"id": 109330, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3509, "text": "To je Onaj koji zna i nevidljivi i vidljivi svijet, Silni i Milostivi,"}, {"id": 109331, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3510, "text": "koji sve savr\u0161eno stvara, koji je prvog \u010dovjeka stvorio od ilova\u010de \u2013"}, {"id": 109332, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3511, "text": "a potomstvo njegovo stvara od kapi hude teku\u0107ine,"}, {"id": 109333, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3512, "text": "zatim mu savr\u0161eno udove uobli\u010di i \u017eivot mu udahne \u2013 i On vam i sluh i vid i pameti daje \u2013 a kako vi malo zahvaljujete!"}, {"id": 109334, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3513, "text": "Oni govore: \"Zar \u0107emo, kad nestanemo pod zemljom, ponovo stvoreni biti?\" Oni ne vjeruju da \u0107e pred Gospodara svoga izi\u0107i."}, {"id": 109335, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3514, "text": "Reci: \"Melek smrti, koji vam je za to odre\u0111en, du\u0161e \u0107e vam uzeti, a poslije \u0107ete se Gospodaru svome vratiti.\""}, {"id": 109337, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3515, "text": "A da ti je vidjeti grje\u0161nike kako \u0107e, oborenih glava pred Gospodarom svojim, re\u0107i: \"Gospodaru na\u0161, vidjeli smo i \u010duli smo, pa nas povrati da dobra djela \u010dinimo, mi, doista, \u010dvrsto vjerujemo!\""}, {"id": 109338, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3516, "text": "A kad bismo htjeli, svakog \u010dovjeka bismo na Pravi put uputili, ali Ja sam ve\u0107 istinu rekao: \"Napuni\u0107u, zaista, D\u017eehennem d\u017einima i ljudima zajedno!\""}, {"id": 109339, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3517, "text": "Pa trpite zato \u0161to ste zaboravljali da \u0107ete ovaj dan do\u017eivjeti \u2013 i Mi \u0107emo vas zaboraviti \u2013 i vje\u010dnu patnju trpite zbog onoga \u0161to ste radili."}, {"id": 109340, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": true, "sajda_id": 10, "sajda_obligatory": true, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3518, "text": "U Na\u0161e rije\u010di vjeruju samo oni koji, kad se njima opomenu, licem na tle padaju, i koji Gospodara svoga veli\u010daju i hvale i koji se ne ohole."}, {"id": 109341, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3519, "text": "Bokovi njihovi se postelja li\u0161avaju i oni se Gospodaru svome iz straha i \u017eelje klanjaju, a dio onog \u0161to im Mi dajemo udjeljuju."}, {"id": 109342, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3520, "text": "I niko ne zna kakve ih, kao nagrada za ono \u0161to su \u010dinili, skrivene radosti \u010dekaju."}, {"id": 109343, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3521, "text": "Zar da vjerniku bude isto kao grje\u0161niku? Ne, njima ne\u0107e biti isto:"}, {"id": 109344, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3522, "text": "one koji su vjerovali i dobra djela \u010dinili \u010dekaju d\u017eennetske ba\u0161\u010de u kojima \u0107e boraviti, kao nagrada za ono \u0161to su radili;"}, {"id": 109346, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3523, "text": "a one koji nisu vjerovali \u010deka vatra u kojoj \u0107e prebivati; kad god poku\u0161aju iz nje izi\u0107i, bi\u0107e u nju vra\u0107eni i bi\u0107e im re\u010deno: \"Trpite kaznu u vatri koju ste poricali.\""}, {"id": 109347, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3524, "text": "I Mi \u0107emo u\u010diniti da bla\u017eu kaznu iskuse, prije one najve\u0107e, ne bi li se pokajali."}, {"id": 109348, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3525, "text": "A ima li nepravednijeg od onoga koji, opomenut rije\u010dima Gospodara svoga, njima le\u0111a okrene? Mi \u0107emo, zaista, kazniti zlikovce!"}, {"id": 109349, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3526, "text": "Musau smo Mi Knjigu dali \u2013 ne sumnjaj nimalo u to da je on nije primio \u2013 i putokazom je sinovima Israilovim u\u010dinili."}, {"id": 109350, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3527, "text": "Izme\u0111u njih smo Mi vo\u0111e odre\u0111ivali i oni su, odazivaju\u0107i se zapovijedi Na\u0161oj, na Pravi put upu\u0107ivali, jer su strpljivi bili i u dokaze Na\u0161e \u010dvrsto vjerovali."}, {"id": 109351, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3528, "text": "Gospodar tvoj \u0107e me\u0111u njima na Sudnjem danu presuditi u onome oko \u010dega su se razilazili."}, {"id": 109352, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3529, "text": "Zar ovima nije jasno koliko smo Mi prije njih naroda uni\u0161tili, po \u010dijim oni nastambama hodaju? To su, zaista, dokazi, pa za\u0161to ne\u0107e da \u010duju?"}, {"id": 109353, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3530, "text": "Kako oni ne vide da Mi gonimo ki\u0161u u ogoljelu zemlju, i \u010dinimo da, uz pomo\u0107 njenu, ni\u010de rastinje kojim se hrani stoka njihova, a i oni sami \u2013 pa za\u0161to ne\u0107e da vide?!"}, {"id": 109354, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3531, "text": "I oni govore: \"Kad \u0107e ve\u0107 jednom ta pobjeda, ako istinu govorite?\""}, {"id": 109355, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3532, "text": "Reci: \"Na Sudnjem danu, kada nevjernicima ne\u0107e nikako koristiti to \u0161to \u0107e tada vjerovati i kad im se nimalo vremena ne\u0107e dati.\""}, {"id": 109356, "identifier": "bs.korkut", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 3533, "text": "Zato se ti okreni od njih i \u010dekaj, i oni doista \u010dekaju!"}]