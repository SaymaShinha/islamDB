ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6047, "text": "i no\u0107i kada ga zakloni,"}, {"id": 111821, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6048, "text": "i neba i Onoga koji ga sazda,"}, {"id": 111822, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6049, "text": "i Zemlje i Onoga koji je ravnom u\u010dini,"}, {"id": 111823, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6050, "text": "i du\u0161e i Onoga koji je stvori"}, {"id": 111824, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6051, "text": "pa joj put dobra i put zla shvatljivim u\u010dini,"}, {"id": 111825, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6052, "text": "uspje\u0107e samo onaj ko je o\u010disti,"}, {"id": 111826, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6053, "text": "a bi\u0107e izgubljen onaj ko je na stranputicu odvodi!"}, {"id": 111827, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6054, "text": "Semud je zbog obijesti svoje poricao:"}, {"id": 111828, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6055, "text": "kad se jedan nesretnik izme\u0111u njih podigao,"}, {"id": 111829, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6056, "text": "Allahov poslanik im je doviknuo: \"Brinite se o Allahovoj kamili i vremenu kad treba da pije!\","}, {"id": 111830, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6057, "text": "ali mu oni nisu povjerovali, ve\u0107 su je zaklali \u2013 i Gospodar njihov ih je zbog grijeha njihovih uni\u0161tio i do posljednjeg istrijebio,"}, {"id": 111831, "identifier": "bs.korkut", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 6058, "text": "i ne strahuju\u0107i zbog toga od odgovornosti."}]