[{"id": 110557, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4736, "text": "Tako Mi Gore,"}, {"id": 110558, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4737, "text": "i Knjige u retke napisane,"}, {"id": 110559, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4738, "text": "na ko\u017ei razvijenoj,"}, {"id": 110560, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4739, "text": "i Hrama poklonika punog,"}, {"id": 110561, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4740, "text": "i svoda uzdignutog,"}, {"id": 110562, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4741, "text": "i mora napunjenog \u2013"}, {"id": 110563, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4742, "text": "kazna Gospodara tvoga sigurno \u0107e se dogoditi,"}, {"id": 110564, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4743, "text": "niko je ne\u0107e mo\u0107i otkloniti"}, {"id": 110565, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4744, "text": "na Dan kada se nebo silno uzburka,"}, {"id": 110566, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4745, "text": "a planine s mjesta pomaknu!"}, {"id": 110567, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4746, "text": "Te\u0161ko na taj Dan onima koji su poricali,"}, {"id": 110568, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4747, "text": "koji su se, ogrezli u la\u017e, zabavljali! \u2013"}, {"id": 110569, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4748, "text": "na Dan kada \u0107e grubo u vatru d\u017eehennemsku biti gurnuti:"}, {"id": 110570, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4749, "text": "\"Ovo je vatra koju ste poricali."}, {"id": 110571, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4750, "text": "pa, je li ovo \u010darolija, ili vi ne vidite?"}, {"id": 110572, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4751, "text": "Pr\u017eite se u njoj, isto vam je trpjeli ili ne trpjeli \u2013 to vam je kazna za ono \u0161to ste radili.\""}, {"id": 110573, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4752, "text": "A \u010destiti \u0107e biti u d\u017eennetskim ba\u0161\u010dama i bla\u017eenstvu"}, {"id": 110574, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4753, "text": "i u onom \u0161to im je Gospodar njihov dao \u0107e u\u017eivati \u2013 njih \u0107e Gospodar njihov patnje u ognju sa\u010duvati."}, {"id": 110575, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4754, "text": "\"Jedite i pijte i neka vam je prijatno, to je za ono \u0161to ste radili!\""}, {"id": 110576, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4755, "text": "Bi\u0107e naslonjeni na divanima poredanim, a vjen\u010da\u0107emo ih hurijama d\u017eennetskim."}, {"id": 110577, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4756, "text": "Onima koji su vjerovali i za kojima su se djeca njihova u vjerovanju povela priklju\u010di\u0107emo djecu njihovu, a djela njihova ne\u0107emo nimalo umanjiti \u2013 svaki \u010dovjek je odgovoran za ono \u0161to s\u00e2m \u010dini \u2013"}, {"id": 110578, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4757, "text": "i jo\u0161 \u0107emo ih darovati vo\u0107em i mesom kakvo budu \u017eeljeli;"}, {"id": 110579, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4758, "text": "jedni drugima \u0107e, u njemu, pune \u010da\u0161e dodavati \u2013 zbog njih ne\u0107e biti praznih besjeda i pobuda na grijeh \u2013"}, {"id": 110580, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4759, "text": "a slu\u017ei\u0107e ih posluga njihova nalik na biser skriveni,"}, {"id": 110581, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4760, "text": "i obra\u0107a\u0107e se jedni drugima i jedni druge \u0107e pitati:"}, {"id": 110582, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4761, "text": "\"Prije smo me\u0111u svojima strahovali\" \u2013 govori\u0107e \u2013"}, {"id": 110583, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4762, "text": "\"pa nam je Allah milost darovao i od patnje u ognju nas sa\u010duvao;"}, {"id": 110584, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4763, "text": "mi smo Mu se prije klanjali, On je, doista, Dobro\u010dinitelj i Milostivi.\""}, {"id": 110585, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4764, "text": "Zato ti opominji, jer ti, milo\u0161\u0107u Gospodara svoga, nisi ni prorok ni lud."}, {"id": 110586, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Zar oni da govore: \"On je pjesnik, sa\u010deka\u0107emo da vidimo \u0161ta \u0107e mu su\u0111eno biti.\""}, {"id": 110587, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4766, "text": "\"Pa \u010dekajte\" \u2013 reci ti \u2013 \"i ja \u0107u zajedno sa vama \u010dekati!\""}, {"id": 110588, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Da li im to dolazi od pameti njihove ili su oni inad\u017eije tvrdoglave?"}, {"id": 110589, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Zar oni da govore: \"Izmi\u0161lja ga!\" \u2013 Ne, nego oni ne\u0107e da vjeruju;"}, {"id": 110590, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4769, "text": "zato neka oni sastave govor sli\u010dan Kur'anu, ako istinu govore!"}, {"id": 110591, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4770, "text": "Zar su oni bez Stvoritelja stvoreni ili su oni sami sebe stvorili?!"}, {"id": 110592, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Zar su oni nebesa i Zemlju stvorili?! Ne, nego oni ne\u0107e da vjeruju."}, {"id": 110593, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Zar je u njih blago Gospodara tvoga, ili, zar oni vladaju?!"}, {"id": 110594, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Zar oni imaju ljestve, pa na njima prislu\u0161kuju? Neka onaj me\u0111u njima koji tvrdi da je ne\u0161to \u010duo donese potvrdu o\u010ditu."}, {"id": 110595, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Zar da su za Njega \u2013 k\u0107eri, a za vas da su \u2013 sinovi?!"}, {"id": 110596, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Zar ti tra\u017ei\u0161 od njih nagradu, pa su nametom optere\u0107eni?!"}, {"id": 110597, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Zar je u njih iskonska knjiga, pa oni prepisuju?!"}, {"id": 110598, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Zar oni zamke da postavljaju?! Ta u zamku \u0107e se uhvatiti ba\u0161 oni koji ne vjeruju!"}, {"id": 110599, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Zar oni drugog boga osim Allaha da imaju?! Hvaljen neka je Allah, On je iznad onih koje Mu ravnim smatraju!"}, {"id": 110600, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4779, "text": "I kad bi vidjeli da komad neba pada, rekli bi: \"Oblaci nagomilani!\""}, {"id": 110601, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4780, "text": "Zato ih pusti dok se ne suo\u010de sa Danom u kome \u0107e pomrijeti,"}, {"id": 110602, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4781, "text": "Danom kada im lukavstva njihova nimalo ne\u0107e koristiti i kada im niko ne\u0107e pomo\u0107i."}, {"id": 110603, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4782, "text": "A za sve nasilnike i druga \u0107e kazna prije one biti, ali ve\u0107ina njih ne zna."}, {"id": 110604, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4783, "text": "A ti strpljivo \u010dekaj presudu Gospodara svoga, Mi tebe i vidimo i \u0161titimo; i veli\u010daj i hvali Gospodara svoga kad ustaje\u0161,"}, {"id": 110605, "identifier": "bs.korkut", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "bs", "name": "Korkut", "transelator_en_name": "Besim Korkut", "format": "text", "type": "translation", "ayah_number": 4784, "text": "i no\u0107u Ga veli\u010daj i kad se zvijezde gube."}]