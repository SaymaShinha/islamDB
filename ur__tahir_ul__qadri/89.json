9, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6004, "text": "(\u06cc\u06c1) \u0648\u06c1 \u0644\u0648\u06af (\u062a\u06be\u06d2) \u062c\u0646\u06c1\u0648\u06ba \u0646\u06d2 (\u0627\u067e\u0646\u06d2 \u0627\u067e\u0646\u06d2) \u0645\u0644\u06a9\u0648\u06ba \u0645\u06cc\u06ba \u0633\u0631\u06a9\u0634\u06cc \u06a9\u06cc \u062a\u06be\u06cc\u060c"}, {"id": 685603, "identifier": "ur.qadri", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6005, "text": "\u067e\u06be\u0631 \u0627\u0646 \u0645\u06cc\u06ba \u0628\u0691\u06cc \u0641\u0633\u0627\u062f \u0627\u0646\u06af\u06cc\u0632\u06cc \u06a9\u06cc \u062a\u06be\u06cc\u060c"}, {"id": 685604, "identifier": "ur.qadri", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "lang