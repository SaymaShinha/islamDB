\u06a9\u06c1 \u0645\u06cc\u06ba \u062a\u0645\u06c1\u0627\u0631\u06d2 \u0644\u0626\u06d2 \u0646\u06c1 \u062a\u0648 \u0646\u0642\u0635\u0627\u0646 (\u06cc\u0639\u0646\u06cc \u06a9\u0641\u0631) \u06a9\u0627 \u0645\u0627\u0644\u06a9 \u06c1\u0648\u06ba \u0627\u0648\u0631 \u0646\u06c1 \u0628\u06be\u0644\u0627\u0626\u06cc (\u06cc\u0639\u0646\u06cc \u0627\u06cc\u0645\u0627\u0646) \u06a9\u0627 (\u06af\u0648\u06cc\u0627 \u062d\u0642\u06cc\u0642\u06cc \u0645\u0627\u0644\u06a9 \u0627\u0644\u0644\u06c1 \u06c1\u06d2 \u0645\u06cc\u06ba \u062a\u0648 \u0630\u0631\u06cc\u0639\u06c1 \u0627\u0648\u0631 \u0648\u0633\u06cc\u0644\u06c1 \u06c1\u0648\u06ba)\u060c"}, {"id": 685070, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5469, "text": "\u0622\u067e \u0641\u0631\u0645\u0627 \u062f\u06cc\u06ba \u06a9\u06c1 \u0646\u06c1 \u0645\u062c\u06be\u06d2 \u06c1\u0631\u06af\u0632 \u06a9\u0648\u0626\u06cc \u0627\u0644\u0644\u06c1 \u06a9\u06d2 (\u0627\u064e\u0645\u0631 \u06a9\u06d2 \u062e\u0644\u0627\u0641) \u0639\u0630\u0627\u0628 \u0633\u06d2 \u067e\u0646\u0627\u06c1 \u062f\u06d2 \u0633\u06a9\u062a\u0627 \u06c1\u06d2 \u0627\u0648\u0631 \u0646\u06c1 \u06c1\u06cc \u0645\u06cc\u06ba \u0642\u0637\u0639\u0627\u064b \u0627\u064f\u0633 \u06a9\u06d2 \u0633\u0648\u0627 \u06a9\u0648\u0626\u06cc \u062c\u0627\u0626\u06d2 \u067e\u0646\u0627\u06c1 \u067e\u0627\u062a\u0627 \u06c1\u0648\u06ba\u060c"}, {"id": 685071, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5470, "text": "\u0645\u06af\u0631 \u0627\u0644\u0644\u06c1 \u06a9\u06cc \u062c\u0627\u0646\u0628 \u0633\u06d2 \u0627\u064e\u062d\u06a9\u0627\u0645\u0627\u062a \u0627\u0648\u0631 \u0627\u064f\u0633 \u06a9\u06d2 \u067e\u06cc\u063a\u0627\u0645\u0627\u062a \u06a9\u0627 \u067e\u06c1\u0646\u0686\u0627\u0646\u0627 (\u0645\u06cc\u0631\u06cc \u0630\u0650\u0645\u0651\u06c1 \u062f\u0627\u0631\u06cc \u06c1\u06d2)\u060c \u0627\u0648\u0631 \u062c\u0648 \u06a9\u0648\u0626\u06cc \u0627\u0644\u0644\u06c1 \u0627\u0648\u0631 \u0627\u064f\u0633 \u06a9\u06d2 \u0631\u0633\u0648\u0644 (\u0635\u0644\u06cc \u0627\u0644\u0644\u06c1 \u0639\u0644\u06cc\u06c1 \u0648\u0622\u0644\u06c1 \u0648\u0633\u0644\u0645) \u06a9\u06cc \u0646\u0627\u0641\u0631\u0645\u0627\u0646\u06cc \u06a9\u0631\u06d2 \u062a\u0648 \u0628\u06cc\u0634\u06a9 \u0627\u064f\u0633 \u06a9\u06d2 \u0644\u0626\u06d2 \u062f\u0648\u0632\u062e \u06a9\u06cc \u0622\u06af \u06c1\u06d2 \u062c\u0633 \u0645\u06cc\u06ba \u0648\u06c1 \u06c1\u0645\u06cc\u0634\u06c1 \u0631\u06c1\u06cc\u06ba \u06af\u06d2\u060c"}, {"id": 685072, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5471, "text": "\u06cc\u06c1\u0627\u06ba \u062a\u06a9 \u06a9\u06c1 \u062c\u0628 \u06cc\u06c1 \u0644\u0648\u06af \u0648\u06c1 (\u0639\u0630\u0627\u0628) \u062f\u06cc\u06a9\u06be \u0644\u06cc\u06ba \u06af\u06d2 \u062c\u0633 \u06a9\u0627 \u0627\u0646 \u0633\u06d2 \u0648\u0639\u062f\u06c1 \u06a9\u06cc\u0627 \u062c\u0627 \u0631\u06c1\u0627 \u06c1\u06d2 \u062a\u0648 (\u0627\u064f\u0633 \u0648\u0642\u062a) \u0627\u0646\u06c1\u06cc\u06ba \u0645\u0639\u0644\u0648\u0645 \u06c1\u0648\u06af\u0627 \u06a9\u06c1 \u06a9\u0648\u0646 \u0645\u062f\u062f\u06af\u0627\u0631 \u06a9\u06d2 \u0627\u0639\u062a\u0628\u0627\u0631 \u0633\u06d2 \u06a9\u0645\u0632\u0648\u0631 \u062a\u0631 \u0627\u0648\u0631 \u0639\u062f\u062f \u06a9\u06d2 \u0627\u0650\u0639\u062a\u0628\u0627\u0631 \u0633\u06d2 \u06a9\u0645 \u062a\u0631 \u06c1\u06d2\u060c"}, {"id": 685073, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5472, "text": "\u0622\u067e \u0641\u0631\u0645\u0627 \u062f\u06cc\u06ba: \u0645\u06cc\u06ba \u0646\u06c1\u06cc\u06ba \u062c\u0627\u0646\u062a\u0627 \u06a9\u06c1 \u062c\u0633 (\u0631\u0648\u0632\u0650 \u0642\u06cc\u0627\u0645\u062a) \u06a9\u0627 \u062a\u0645 \u0633\u06d2 \u0648\u0639\u062f\u06c1 \u06a9\u06cc\u0627 \u062c\u0627 \u0631\u06c1\u0627 \u06c1\u06d2 \u0648\u06c1 \u0642\u0631\u06cc\u0628 \u06c1\u06d2 \u06cc\u0627 \u0627\u0633 \u06a9\u06d2 \u0644\u0626\u06d2 \u0645\u06cc\u0631\u06d2 \u0631\u0628 \u0646\u06d2 \u0637\u0648\u06cc\u0644 \u0645\u062f\u062a \u0645\u0642\u0631\u0631 \u0641\u0631\u0645\u0627 \u062f\u06cc \u06c1\u06d2\u060c"}, {"id": 685074, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5473, "text": "(\u0648\u06c1) \u063a\u06cc\u0628 \u06a9\u0627 \u062c\u0627\u0646\u0646\u06d2 \u0648\u0627\u0644\u0627 \u06c1\u06d2\u060c \u067e\u0633 \u0648\u06c1 \u0627\u067e\u0646\u06d2 \u063a\u06cc\u0628 \u067e\u0631 \u06a9\u0633\u06cc (\u0639\u0627\u0645 \u0634\u062e\u0635) \u06a9\u0648 \u0645\u0637\u0644\u0639 \u0646\u06c1\u06cc\u06ba \u0641\u0631\u0645\u0627\u062a\u0627\u060c"}, {"id": 685075, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5474, "text": "\u0633\u0648\u0627\u0626\u06d2 \u0627\u067e\u0646\u06d2 \u067e\u0633\u0646\u062f\u06cc\u062f\u06c1 \u0631\u0633\u0648\u0644\u0648\u06ba \u06a9\u06d2 (\u0627\u064f\u0646\u06c1\u06cc \u06a9\u0648 \u0645\u0637\u0644\u0639 \u0639\u0644\u06cc \u0627\u0644\u063a\u06cc\u0628 \u06a9\u0631\u062a\u0627 \u06c1\u06d2 \u06a9\u06cc\u0648\u0646\u06a9\u06c1 \u06cc\u06c1 \u062e\u0627\u0635\u06c1\u0654 \u0646\u0628\u0648\u062a \u0627\u0648\u0631 \u0645\u0639\u062c\u0632\u06c1\u0654 \u0631\u0633\u0627\u0644\u062a \u06c1\u06d2)\u060c \u062a\u0648 \u0628\u06d2 \u0634\u06a9 \u0648\u06c1 \u0627\u0633 (\u0631\u0633\u0648\u0644 \u0635\u0644\u06cc \u0627\u0644\u0644\u06c1 \u0639\u0644\u06cc\u06c1 \u0648\u0622\u0644\u06c1 \u0648\u0633\u0644\u0645) \u06a9\u06d2 \u0622\u06af\u06d2 \u0627\u0648\u0631 \u067e\u06cc\u0686\u06be\u06d2 (\u0639\u0644\u0645\u0650 \u063a\u06cc\u0628 \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a \u06a9\u06d2 \u0644\u0626\u06d2) \u0646\u06af\u06c1\u0628\u0627\u0646 \u0645\u0642\u0631\u0631 \u0641\u0631\u0645\u0627 \u062f\u06cc\u062a\u0627 \u06c1\u06d2\u060c"}, {"id": 685076, "identifier": "ur.qadri", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5475, "text": "\u062a\u0627\u06a9\u06c1 \u0627\u0644\u0644\u06c1 (\u0627\u0633 \u0628\u0627\u062a \u06a9\u0648) \u0638\u0627\u06c1\u0631 \u0641\u0631\u0645\u0627 \u062f\u06d2 \u06a9\u06c1 \u0628\u06d2 \u0634\u06a9 \u0627\u0646 (\u0631\u0633\u0648\u0644\u0648\u06ba) \u0646\u06d2 \u0627\u067e\u0646\u06d2 \u0631\u0628 \u06a9\u06d2 \u067e\u06cc\u063a\u0627\u0645\u0627\u062a \u067e\u06c1\u0646\u0686\u0627 \u062f\u0626\u06cc\u06d2\u060c \u0627\u0648\u0631 (\u0627\u064e\u062d\u06a9\u0627\u0645\u0627\u062a\u0650 \u0627\u0650\u0644\u0670\u06c1\u06cc\u06c1 \u0627\u0648\u0631 \u0639\u0644\u0648\u0645\u0650 \u063a\u06cc\u0628\u06cc\u06c1 \u0645\u06cc\u06ba \u0633\u06d2) \u062c\u0648 \u06a9\u0686\u06be \u0627\u0646 \u06a9\u06d2 \u067e\u0627\u0633 \u06c1\u06d2 \u0627\u0644\u0644\u06c1 \u0646\u06d2 (\u067e\u06c1\u0644\u06d2 \u06c1\u06cc \u0633\u06d2) \u0627\u064f\u0633 \u06a9\u0627 \u0627\u0650\u062d\u0627\u0637\u06c1 \u0641\u0631\u0645\u0627 \u0631\u06a9\u06be\u0627 \u06c1\u06d2\u060c \u0627\u0648\u0631 \u0627\u064f\u0633 \u0646\u06d2 \u06c1\u0631 \u0686\u06cc\u0632 \u06a9\u06cc \u06af\u0646\u062a\u06cc \u0634\u0645\u0627\u0631 \u06a9\u0631 \u0631\u06a9\u06be\u06cc \u06c1\u06d2\u060c"}]