n", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5595, "text": "\u0628\u06cc\u0634\u06a9 \u06c1\u0645 \u0646\u06d2 \u06a9\u0627\u0641\u0631\u0648\u06ba \u06a9\u06d2 \u0644\u0626\u06d2 (\u067e\u0627\u0624\u06ba \u06a9\u06cc) \u0632\u0646\u062c\u06cc\u0631\u06cc\u06ba \u0627\u0648\u0631 (\u06af\u0631\u062f\u0646 \u06a9\u06d2) \u0637\u0648\u0642 \u0627\u0648\u0631 (\u062f\u0648\u0632\u062e \u06a9\u06cc) \u062f\u06c1\u06a9\u062a\u06cc \u0622\u06af \u062a\u06cc\u0627\u0631 \u06a9\u0631 \u0631\u06a9\u06be\u06cc \u06c1\u06d2\u060c"}, {"id": 685198, "identifier": "ur.qadri", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 5596, "text": "\u0628\u06cc\u0634\u06a9 \u0645\u062e\u0644\u0635 \u0627\u0650\u0637\u0627\u0639\u062a \u06af\u0632\u0627\u0631 (\u0634\u0631\u0627\u0628\u0650 \u0637\u06c1\u0648\u0631 \u06a9\u06d2) \u0627\u06cc\u0633\u06d2 \u062c\u0627\u0645 \u067e\u0626\u06cc\u06ba \u06af\u06d2 \u062c\u0633 \u0645\u06cc\u06ba (\u062e\u0648\u0634\u0628\u0648\u060c \u0631\u0646\u06af\u062a \u0627\u0648\u0631 \u0644\u0630\u062a \u0628\u0691\u06be\u0627\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2) \u06a9\u0627\u0641\u0648\u0631 \u06a9\u06cc \u0622\u0645\u06cc\u0632\u0634 \u06c1\