[{"id": 685777, "identifier": "ur.qadri", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6180, "text": "\u06c1\u0631 \u0627\u0633 \u0634\u062e\u0635 \u06a9\u06d2 \u0644\u0626\u06d2 \u06c1\u0644\u0627\u06a9\u062a \u06c1\u06d2 \u062c\u0648 (\u0631\u0648\u0628\u0631\u0648) \u0637\u0639\u0646\u06c1 \u0632\u0646\u06cc \u06a9\u0631\u0646\u06d2 \u0648\u0627\u0644\u0627 \u06c1\u06d2 (\u0627\u0648\u0631 \u067e\u0633\u0650 \u067e\u0634\u062a) \u0639\u06cc\u0628 \u062c\u0648\u0626\u06cc \u06a9\u0631\u0646\u06d2 \u0648\u0627\u0644\u0627 \u06c1\u06d2\u060c"}, {"id": 685778, "identifier": "ur.qadri", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6181, "text": "(\u062e\u0631\u0627\u0628\u06cc \u0648 \u062a\u0628\u0627\u06c1\u06cc \u06c1\u06d2 \u0627\u0633 \u0634\u062e\u0635 \u06a9\u06d2 \u0644\u0626\u06d2) \u062c\u0633 \u0646\u06d2 \u0645\u0627\u0644 \u062c\u0645\u0639 \u06a9\u06cc\u0627 \u0627\u0648\u0631 \u0627\u0633\u06d2 \u06af\u0646 \u06af\u0646 \u06a9\u0631 \u0631\u06a9\u