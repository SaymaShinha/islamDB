[{"id": 685774, "identifier": "ur.qadri", "surah_number": 103, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0635\u0631", "surah_en_name": "Al-Asr", "surah_en_name_translation": "The Declining Day, Epoch", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 545, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6177, "text": "\u0632\u0645\u0627\u0646\u06c1 \u06a9\u06cc \u0642\u064e\u0633\u0645 (\u062c\u0633 \u06a9\u06cc \u06af\u0631\u062f\u0634 \u0627\u0646\u0633\u0627\u0646\u06cc \u062d\u0627\u0644\u0627\u062a \u067e\u0631 \u06af\u0648\u0627\u06c1 \u06c1\u06d2)\u06d4 (\u06cc\u0627- \u0646\u0645\u0627\u0632\u0650 \u0639\u0635\u0631 \u06a9\u06cc \u0642\u064e\u0633\u0645 (\u06a9\u06c1 \u0648\u06c1 \u0633\u0628 \u0646\u0645\u0627\u0632\u0648\u06ba \u06a9\u0627 \u0648\u0633\u0637 \u06c1\u06d2)\u06d4 (\u06cc\u0627- \u0648\u0642\u062a\u0650 \u0639\u0635\u0631 \u06a9\u06cc \u0642\u064e\u0633\u0645 (\u062c\u0628 \u062f\u0646 \u0628\u06be\u0631 \u0686\u0645\u06a9\u0646\u06d2 \u0648\u0627\u0644\u0627 \u0633\u0648\u0631\u062c \u062e\u0648\u062f \u0688\u0648\u0628\u0646\u06d2 \u06a9\u0627 \u0645\u0646\u0638\u0631 \u067e\u06cc\u0634 \u06a9\u0631\u062a\u0627 \u06c1\u06d2)\u06d4 (\u06cc\u0627- \u0632\u0645\u0627\u0646\u06c1\u0654 \u0628\u0639\u062b\u062a\u0650 \u0645\u0635\u0637\u0641\u06cc\u0670 (\u0635\u0644\u06cc \u0627\u0644\u0644\u06c1 \u0639\u0644\u06cc\u06c1 \u0648\u0622\u0644\u06c1 \u0648\u0633\u0644\u0645) \u06a9\u06cc \u0642\u064e\u0633\u0645 (\u062c\u0648 \u0633\u0627\u0631\u06d2 \u0632\u0645\u0627\u0646\u0648\u06ba \u06a9\u0627 \u0645\u0627\u062d\u0635\u0644 \u0627\u0648\u0631 \u0645\u0642\u0635\u0648\u062f \u06c1\u06d2)\u060c"}, {"id": 685775, "identifier": "ur.qadri", "surah_number": 103, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0635\u0631", "surah_en_name": "Al-Asr", "surah_en_name_translation": "The Declining Day, Epoch", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 545, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6178, "text": "\u0628\u06cc\u0634\u06a9 \u0627\u0646\u0633\u0627\u0646 \u062e\u0633\u0627\u0631\u06d2 \u0645\u06cc\u06ba \u06c1\u06d2 (\u06a9\u06c1 \u0648\u06c1 \u0639\u0645\u0631\u0650 \u0639\u0632\u06cc\u0632 \u06af\u0646\u0648\u0627 \u0631\u06c1\u0627 \u06c1\u06d2)\u060c"}, {"id": 685776, "identifier": "ur.qadri", "surah_number": 103, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0635\u0631", "surah_en_name": "Al-Asr", "surah_en_name_translation": "The Declining Day, Epoch", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 545, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6179, "text": "\u0633\u0648\u0627\u0626\u06d2 \u0627\u0646 \u0644\u0648\u06af\u0648\u06ba \u06a9\u06d2 \u062c\u0648 \u0627\u06cc\u0645\u0627\u0646 \u0644\u06d2 \u0622\u0626\u06d2 \u0627\u0648\u0631 \u0646\u06cc\u06a9 \u0639\u0645\u0644 \u06a9\u0631\u062a\u06d2 \u0631\u06c1\u06d2 \u0627\u0648\u0631 (\u0645\u0639\u0627\u0634\u0631\u06d2 \u0645\u06cc\u06ba) \u0627\u06cc\u06a9 \u062f\u0648\u0633\u0631\u06d2 \u06a9\u0648 \u062d\u0642 \u06a9\u06cc \u062a\u0644\u0642\u06cc\u0646 \u06a9\u0631\u062a\u06d2 \u0631\u06c1\u06d2 \u0627\u0648\u0631 (\u062a\u0628\u0644\u06cc\u063a\u0650 \u062d\u0642 \u06a9\u06d2 \u0646\u062a\u06cc\u062c\u06d2 \u0645\u06cc\u06ba \u067e\u06cc\u0634 \u0622\u0645\u062f\u06c1 \u0645\u0635\u0627\u0626\u0628 \u0648 \u0622\u0644\u0627\u0645 \u0645\u06cc\u06ba) \u0628\u0627\u06c1\u0645 \u0635\u0628\u0631 \u06a9\u06cc \u062a\u0627\u06a9\u06cc\u062f \u06a9\u0631\u062a\u06d2 \u0631\u06c1\u06d2\u060c"}]