[{"id": 685814, "identifier": "ur.qadri", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6217, "text": "\u0627\u0628\u0648\u0644\u06c1\u0628 \u06a9\u06d2 \u062f\u0648\u0646\u0648\u06ba \u06c1\u0627\u062a\u06be \u0679\u0648\u0679 \u062c\u0627\u0626\u06cc\u06ba \u0627\u0648\u0631 \u0648\u06c1 \u062a\u0628\u0627\u06c1 \u06c1\u0648 \u062c\u0627\u0626\u06d2 (\u0627\u0633 \u0646\u06d2 \u06c1\u0645\u0627\u0631\u06d2 \u062d\u0628\u06cc\u0628 \u067e\u0631 \u06c1\u0627\u062a\u06be \u0627\u0679\u06be\u0627\u0646\u06d2 \u06a9\u06cc \u06a9\u0648\u0634\u0634 \u06a9\u06cc \u06c1\u06d2)\u060c"}, {"id": 685815, "identifier": "ur.qadri", "surah_number": 111, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0633\u062f", "surah_en_name": "Al-Masad", "surah_en_name_translation": "The Palm Fibre", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 553, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0637\u0627\u06c1\u0631 \u0627\u0644\u0642\u0627\u062f\u0631\u06cc", "transelator_en_name": "Tahir ul Qadri", "format": "text", "type": "translation", "ayah_number": 6218, "text": "\u0627\u0633\u06d2 \u0627\u0633 \u06a9\u06d2 (\u0645\u0648\u0631\u0648\u062b\u06cc) \u0645\u0627\u0644 \u0646\u06d2 \u06a9\u0686\u06be \u0641\u0627\u0626\u062f\u06c1 \u0646\u06c1 \u067e\u06c1\u0646\u0686\u0627\u06cc\u0627 \u0627\u0648\u0631 \u0646\u06c1 \u06c1\u06cc \u0627\u0633 \u06a9\u06cc \u06a