", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 5796, "text": "O g\u00fcn bir tak\u0131m y\u00fczler par\u0131l par\u0131l,"}, {"id": 571624, "identifier": "tr.vakfi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 5797, "text": "G\u00fcler ve sevinir."}, {"id": 571856, "identifier": "tr.vakfi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 5798, "text": "Yine o g\u00fcn birtak\u0131m y\u00fczleri de keder b\u00fcr\u00fcm\u00fc\u015f,"}, {"id": 572457, "identifier": "tr.vakfi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, 