[{"id": 571992, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Tozdurup savuranlara,"}, {"id": 571993, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4677, "text": "Y\u00fck\u00fcn\u00fc y\u00fcklenenlere,"}, {"id": 571994, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4678, "text": "Kolayca s\u00fcz\u00fclenlere,"}, {"id": 571995, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4679, "text": "\u0130\u015fleri ay\u0131ranlara andolsun ki,"}, {"id": 571996, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Size vadedilen, kesinlikle do\u011frudur."}, {"id": 571997, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4681, "text": "Ve ceza mutlaka vuku bulacakt\u0131r."}, {"id": 571998, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4682, "text": "\u0130\u00e7inde y\u00f6r\u00fcngeleri olan g\u00f6\u011fe andolsun ki,"}, {"id": 571999, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Siz \u00e7eli\u015fkili s\u00f6zler s\u00f6yl\u00fcyorsunuz."}, {"id": 572000, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Ondan (Kur'an'dan veya imandan) d\u00f6nen d\u00f6nd\u00fcr\u00fcl\u00fcr (engellenmez)."}, {"id": 572001, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Kahrolsun o koyu yalanc\u0131lar!"}, {"id": 572002, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4686, "text": "Onlar koyu bir cehalet i\u00e7erisinde kalm\u0131\u015f gafillerdir."}, {"id": 572003, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Ceza g\u00fcn\u00fcn\u00fcn ne zaman oldu\u011funu sorarlar."}, {"id": 572004, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4688, "text": "O g\u00fcn onlar ate\u015fe sokulacaklard\u0131r."}, {"id": 572005, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4689, "text": "Azab\u0131n\u0131z\u0131 tad\u0131n! Acele gelmesini bekledi\u011finiz \u015fey budur i\u015fte! (denir.)"}, {"id": 572006, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4690, "text": "\u015e\u00fcphesiz ki Allah'a isyandan sak\u0131nanlar, cennetlerde ve p\u0131nar ba\u015flar\u0131nda bulunacaklar."}, {"id": 572007, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4691, "text": "Rablerinin kendilerine verdi\u011fini alarak. Ku\u015fkusuz onlar, bundan \u00f6nce d\u00fcnyada g\u00fczel davrananlard\u0131."}, {"id": 572008, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4692, "text": "Geceleri pek az uyurlard\u0131."}, {"id": 572009, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4693, "text": "Seher vakitlerinde de isti\u011ffar ederlerdi."}, {"id": 572010, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Mallar\u0131nda, muhta\u00e7 ve yoksullar i\u00e7in bir hak vard\u0131."}, {"id": 572011, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4695, "text": "Kesin olarak inananlar i\u00e7in yery\u00fcz\u00fcnde ayetler vard\u0131r."}, {"id": 572012, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4696, "text": "Kendi nefislerinizde de \u00f6yle. G\u00f6rm\u00fcyor musunuz?"}, {"id": 572013, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4697, "text": "Semada da r\u0131zk\u0131n\u0131z ve size vadedilen ba\u015fka \u015feyler vard\u0131r."}, {"id": 572014, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4698, "text": "G\u00f6\u011f\u00fcn ve yerin Rabbine andolsun ki bu vaad, sizin konu\u015fman\u0131z gibi kesin ve ger\u00e7ektir."}, {"id": 572015, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4699, "text": "\u0130brahim'in a\u011f\u0131rlanan misafirlerinin haberi sana geldi mi? (Bunlar meleklerdi.)"}, {"id": 572016, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Onlar \u0130brahim'in yan\u0131na girmi\u015fler, selam vermi\u015flerdi. \u0130brahim de selam\u0131 alm\u0131\u015f, i\u00e7inden, \"Bunlar, yabanc\u0131lar\" demi\u015fti."}, {"id": 572017, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4701, "text": "Hemen ailesinin yan\u0131na giderek semiz bir dana (kebab\u0131n\u0131) getirmi\u015f,"}, {"id": 572018, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4702, "text": "Onlar\u0131n \u00f6n\u00fcne koyup \"Yemez misiniz?\" demi\u015fti."}, {"id": 572019, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4703, "text": "Derken onlardan korkmaya ba\u015flad\u0131. \"Korkma\" dediler ve ona bilgin bir o\u011flan \u00e7ocu\u011fu m\u00fcjdelediler."}, {"id": 572020, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Kar\u0131s\u0131 \u00e7\u0131\u011fl\u0131k atarak geldi. Elini y\u00fcz\u00fcne \u00e7arparak: \"Ben k\u0131s\u0131r bir kocakar\u0131y\u0131m!\" dedi."}, {"id": 572022, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Onlar: \"Bu b\u00f6yledir. Rabbin s\u00f6ylemi\u015ftir. O, hikmet sahibidir, bilendir\" dediler."}, {"id": 572023, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4706, "text": "(\u0130brahim:) O halde i\u015finiz nedir, ey el\u00e7iler? dedi."}, {"id": 572024, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4707, "text": "\"Biz, dediler, su\u00e7lu bir kavme g\u00f6nderildik.\""}, {"id": 572025, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4708, "text": "\"\u00dczerlerine \u00e7amurdan ta\u015f ya\u011fd\u0131rmaya (geldik).\""}, {"id": 572026, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4709, "text": "(Bu ta\u015flar,) a\u015f\u0131r\u0131 gidenler i\u00e7in Rabbinin kat\u0131nda i\u015faretlenmi\u015f (ta\u015flard\u0131r)."}, {"id": 572027, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Bunun \u00fczerine orada bulunan m\u00fcminleri \u00e7\u0131kard\u0131k."}, {"id": 572028, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4711, "text": "Zaten orada m\u00fcsl\u00fcmanlardan, bir ev halk\u0131ndan ba\u015fka kimse bulmad\u0131k."}, {"id": 572029, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Ac\u0131 azaptan korkanlar i\u00e7in orada bir i\u015faret b\u0131rakt\u0131k."}, {"id": 572030, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4713, "text": "Musa'da da (ibretler vard\u0131r). Onu apa\u00e7\u0131k bir delil ile Firavun'a g\u00f6ndermi\u015ftik."}, {"id": 572031, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Firavun ordusuyla birlikte y\u00fcz \u00e7evirmi\u015f: \"O, bir b\u00fcy\u00fcc\u00fcd\u00fcr veya bir delidir\" demi\u015fti."}, {"id": 572032, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Nihayet onu da ordular\u0131n\u0131 da yakalay\u0131p denize att\u0131k, bu s\u0131rada kendini k\u0131nay\u0131p duruyordu."}, {"id": 572033, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4716, "text": "Ad kavminde de (ibretler vard\u0131r). Onlara kas\u0131p kavuran r\u00fczgar\u0131 g\u00f6ndermi\u015ftik."}, {"id": 572034, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4717, "text": "\u00dczerinden ge\u00e7ti\u011fi \u015feyi canl\u0131 b\u0131rakm\u0131yor, onu k\u00fcl edip savuruyordu."}, {"id": 572035, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4718, "text": "Semud kavminde de (ibretler vard\u0131r). Onlara: Bir s\u00fcreye kadar faydalan\u0131n, denmi\u015fti."}, {"id": 572036, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Rablerinin emrine kar\u015f\u0131 geldiler. Bu y\u00fczden, bak\u0131p dururlarken onlar\u0131 y\u0131ld\u0131r\u0131m \u00e7arp\u0131verdi."}, {"id": 572037, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4720, "text": "Aya\u011fa kalkacak g\u00fc\u00e7leri kalmam\u0131\u015f, yard\u0131m edenleri de olmam\u0131\u015ft\u0131."}, {"id": 572038, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4721, "text": "Bunlardan \u00f6nce de Nuh kavmini helak etmi\u015ftik. \u00c7\u00fcnk\u00fc onlar yoldan \u00e7\u0131km\u0131\u015f bir toplum idiler."}, {"id": 572039, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4722, "text": "G\u00f6\u011f\u00fc kendi ellerimizle biz kurduk ve biz (onu) elbette geni\u015fleticiyiz."}, {"id": 572040, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Yeri de d\u00f6\u015fedik. (Bak) ne g\u00fczel d\u00f6\u015feyiciyiz!"}, {"id": 572041, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Her \u015feyden de \u00e7ift \u00e7ift yaratt\u0131k ki, d\u00fc\u015f\u00fcn\u00fcp \u00f6\u011f\u00fct alas\u0131n\u0131z."}, {"id": 572042, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4725, "text": "O halde Allah'a ko\u015fun. \u00c7\u00fcnk\u00fc ben, size O'nun kat\u0131ndan (gelmi\u015f) a\u00e7\u0131k bir uyar\u0131c\u0131y\u0131m."}, {"id": 572043, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Allah ile beraber ba\u015fka bir tanr\u0131 edinmeyin. Zira ben size O'nun taraf\u0131ndan (gelmi\u015f) a\u00e7\u0131k bir uyar\u0131c\u0131y\u0131m."}, {"id": 572044, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4727, "text": "\u0130\u015fte b\u00f6ylece, onlardan \u00f6ncekilere her hangi bir peygamber geldi\u011finde hemen: O, bir b\u00fcy\u00fcc\u00fcd\u00fcr veya delidir, dediler."}, {"id": 572045, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Bunu (nesilden nesile) birbirlerine vasiyet mi ettiler? Do\u011frusu onlar azg\u0131n bir topluluktur."}, {"id": 572046, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4729, "text": "Art\u0131k onlara ald\u0131rma. (Davete uymamalar\u0131ndan dolay\u0131) sen k\u0131nanacak de\u011filsin."}, {"id": 572047, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Sen yine de \u00f6\u011f\u00fct ver. \u00c7\u00fcnk\u00fc \u00f6\u011f\u00fct m\u00fcminlere fayda verir."}, {"id": 572048, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Ben cinleri ve insanlar\u0131, ancak bana kulluk etsinler diye yaratt\u0131m."}, {"id": 572049, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4732, "text": "Ben onlardan r\u0131z\u0131k istemiyorum. Beni doyurmalar\u0131n\u0131 da istemiyorum."}, {"id": 572051, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4733, "text": "\u015e\u00fcphesiz r\u0131z\u0131k veren, g\u00fc\u00e7 ve kuvvet sahibi olan ancak Allah't\u0131r."}, {"id": 572052, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Muhakkak ki bu zulmedenlerin de, ge\u00e7mi\u015flerinin pay\u0131 gibi (azaptan) bir paylar\u0131 vard\u0131r! O halde acele etmesinler!"}, {"id": 572053, "identifier": "tr.vakfi", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Ba\u015flar\u0131na gelecek (ac\u0131) g\u00fcnlerinden dolay\u0131 vay o kafirlerin haline!"}]