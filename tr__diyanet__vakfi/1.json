[{"id": 567481, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 1, "text": "Rahman (ve) rahim (olan) Allah'\u0131n ad\u0131yla."}, {"id": 567482, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 2, "text": "Hamd (\u00f6vme ve \u00f6v\u00fclme), alemlerin Rabbi Allah'a mahsustur."}, {"id": 567483, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 3, "text": "O, rahmand\u0131r ve rahimdir."}, {"id": 567484, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4, "text": "Ceza g\u00fcn\u00fcn\u00fcn malikidir."}, {"id": 567485, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 5, "text": "(Rabbimiz!) Ancak sana kulluk ederiz ve yaln\u0131z senden medet umar\u0131z."}, {"id": 567480, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6, "text": "Bize do\u011fru yolu g\u00f6ster."}, {"id": 573690, "identifier": "tr.vakfi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 7, "text": "Kendilerine l\u00fctuf ve ikramda bulundu\u011fun kimselerin yolunu; gazaba u\u011fram\u0131\u015flar\u0131n ve sapm\u0131\u015flar\u0131n yolunu de\u011fil!"}]