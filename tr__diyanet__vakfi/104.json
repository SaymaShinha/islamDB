[{"id": 573466, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6180, "text": "Arkadan \u00e7eki\u015ftirmeyi, y\u00fcze kar\u015f\u0131 e\u011flenmeyi adet edinen herkesin vay haline!"}, {"id": 573467, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6181, "text": "O ki, toplam\u0131\u015f ve onu say\u0131p durmu\u015ftur."}, {"id": 573468, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6182, "text": "(O), mal\u0131n\u0131n kendisini ebedi k\u0131laca\u011f\u0131n\u0131 zanneder."}, {"id": 573469, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Hay\u0131r! Andolsun ki o, Hutame'ye at\u0131lacakt\u0131r."}, {"id": 573470, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6184, "text": "Hutame'nin ne oldu\u011funu bilir misin?"}, {"id": 573471, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6185, "text": "Allah'\u0131n, tutu\u015fturulmu\u015f ate\u015fidir."}, {"id": 573472, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6186, "text": "(Yand\u0131k\u00e7a) t\u0131rman\u0131p kalplerin ta \u00fcst\u00fcne \u00e7\u0131kar."}, {"id": 573473, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6187, "text": "O, onlar\u0131n \u00fczerine kapat\u0131l\u0131p kilitlenecektir."}, {"id": 573474, "identifier": "tr.vakfi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 6188, "text": "(Bu ate\u015fin i\u00e7inde) uzat\u0131lm\u0131\u015f s\u00fctunlara ba\u011flanm\u0131\u015flar."}]