[{"id": 572100, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Batt\u0131\u011f\u0131 zaman y\u0131ld\u0131za andolsun ki;"}, {"id": 572101, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Arkada\u015f\u0131n\u0131z (Muhammed) sapmad\u0131 ve bat\u0131la inanmad\u0131."}, {"id": 572102, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4787, "text": "O, arzusuna g\u00f6re de konu\u015fmaz."}, {"id": 572103, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4788, "text": "O (bildirdikleri) vahyedilenden ba\u015fkas\u0131 de\u011fildir."}, {"id": 572104, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4789, "text": "\u00c7\u00fcnk\u00fc onu g\u00fc\u00e7l\u00fc kuvvetli biri (Cebrail) \u00f6\u011fretti."}, {"id": 572105, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4790, "text": "Ve \u00fcst\u00fcn yarat\u0131l\u0131\u015fl\u0131(melek), do\u011fruldu:"}, {"id": 572106, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4791, "text": "Kendisi en y\u00fcksek ufukta iken."}, {"id": 572107, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Sonra (Muhammed'e) yakla\u015ft\u0131, (yere do\u011fru)sarkt\u0131."}, {"id": 572108, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4793, "text": "O kadar ki (birle\u015ftirilmi\u015f) iki yay aras\u0131 kadar, hatta daha da yak\u0131n oldu."}, {"id": 572109, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Bunun \u00fczerine Allah, kuluna vahyini bildirdi."}, {"id": 572110, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4795, "text": "(G\u00f6zleriyle) g\u00f6rd\u00fc\u011f\u00fcn\u00fc kalbi yalanlamad\u0131."}, {"id": 572111, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Onun g\u00f6rd\u00fckleri hakk\u0131nda \u015fimdi kendisi ile tart\u0131\u015facak m\u0131s\u0131n\u0131z?"}, {"id": 571872, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Andolsun onu, \u00f6nceden bir defa daha g\u00f6rm\u00fc\u015ft\u00fc,"}, {"id": 571896, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4798, "text": "Sidret\u00fc'l-M\u00fcnteha'n\u0131n yan\u0131nda."}, {"id": 572021, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4799, "text": "Cennet\u00fc'l-Me'va da onun yan\u0131ndad\u0131r."}, {"id": 572050, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4800, "text": "Sidre'yi kaplayan kaplam\u0131\u015ft\u0131."}, {"id": 567478, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4801, "text": "G\u00f6z\u00fc kaymad\u0131 ve s\u0131n\u0131r\u0131 a\u015fmad\u0131."}, {"id": 571804, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Andolsun o, Rabbinin en b\u00fcy\u00fck ayetlerinden bir k\u0131sm\u0131n\u0131 g\u00f6rd\u00fc."}, {"id": 567609, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4803, "text": "G\u00f6rd\u00fcn\u00fcz m\u00fc o Lat ve Uzza'y\u0131?"}, {"id": 567666, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4804, "text": "Ve \u00fc\u00e7\u00fcnc\u00fcleri olan \u00f6tekini, Menat'\u0131."}, {"id": 567685, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Demek erkek size, di\u015fi O'na \u00f6yle mi?"}, {"id": 568989, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4806, "text": "O zaman bu, insafs\u0131zca bir taksim!"}, {"id": 572112, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Bunlar (putlar), sizin ve atalar\u0131n\u0131z\u0131n takt\u0131\u011f\u0131 isimlerden ba\u015fka bir \u015fey de\u011fildir. Allah onlar hakk\u0131nda hi\u00e7bir delil indirmemi\u015ftir. Onlar ancak zanna ve nefislerinin arzusuna uyuyorlar. Halbuki kendilerine Rableri taraf\u0131ndan yol g\u00f6sterici gelmi\u015ftir."}, {"id": 572113, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Yoksa insan, her arzu etti\u011fi \u015feye sahip mi olacakt\u0131r?"}, {"id": 572114, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Ahiret de d\u00fcnya da Allah'\u0131nd\u0131r."}, {"id": 572115, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4810, "text": "G\u00f6klerde nice melek var ki onlar\u0131n \u015fefaatleri, diledi\u011fi ve ho\u015fnut oldu\u011fu kimse i\u00e7in Allah'\u0131n izin vermesi d\u0131\u015f\u0131nda, bir i\u015fe yaramaz."}, {"id": 572116, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Ahirete inanmayanlar, meleklere di\u015filerin adlar\u0131n\u0131 tak\u0131yorlar."}, {"id": 572117, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Halbuki onlar\u0131n bu hususta hi\u00e7 bilgileri yoktur. Sadece zanna uyuyorlar. Zan ise hi\u00e7 \u015f\u00fcphesiz hakikat bak\u0131m\u0131ndan bir \u015fey ifade etmez."}, {"id": 572118, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Onun i\u00e7in sen bizi anmaktan y\u00fcz \u00e7eviren ve d\u00fcnya hayat\u0131ndan ba\u015fka bir \u015fey istemeyen kimselere y\u00fcz verme."}, {"id": 572119, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4814, "text": "\u0130\u015fte onlar\u0131n eri\u015febilecekleri bilgi budur. \u015e\u00fcphesiz ki senin Rabbin, evet O, yolundan sapan\u0131 daha iyi bilir; O, hidayette olan\u0131 da \u00e7ok iyi bilir."}, {"id": 572120, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4815, "text": "G\u00f6klerde ve yerde bulunanlar hep Allah'\u0131nd\u0131r. Bu, Allah'\u0131n, k\u00f6t\u00fcl\u00fck edenleri yapt\u0131klar\u0131yla cezaland\u0131rmas\u0131, g\u00fczel davrananlar\u0131 da daha g\u00fczeliyle m\u00fckafatland\u0131rmas\u0131 i\u00e7indir."}, {"id": 572122, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Ufak tefek kusurlar\u0131 d\u0131\u015f\u0131nda, b\u00fcy\u00fck g\u00fcnahlardan ve edepsizliklerden ka\u00e7\u0131nanlara gelince, bil ki Rabbin, aff\u0131 bol oland\u0131r. O, sizi daha topraktan yaratt\u0131\u011f\u0131 zaman ve siz annelerinizin kar\u0131nlar\u0131nda bulundu\u011funuz s\u0131rada (bile), sizi en iyi bilendir. Bunun i\u00e7in kendinizi temize \u00e7\u0131karmay\u0131n. \u00c7\u00fcnk\u00fc O, k\u00f6t\u00fcl\u00fckten sak\u0131nan\u0131 daha iyi bilir."}, {"id": 572123, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4817, "text": "G\u00f6rd\u00fcn m\u00fc arkas\u0131n\u0131 d\u00f6neni?"}, {"id": 572124, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4818, "text": "Az\u0131c\u0131k verip sonra vermemekte direneni?"}, {"id": 572125, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Acaba gayb\u0131n bilgisi kendi yan\u0131ndad\u0131r da o g\u00f6r\u00fcyor mu?"}, {"id": 572126, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Yoksa kendisine haber verilmedi mi? Musa'n\u0131n sahifelerinde bulunan,"}, {"id": 572127, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4821, "text": "Ve ahdine vefa g\u00f6steren \u0130brahim'in( sahifelerinde bulunan \u015fu ger\u00e7ekler):"}, {"id": 572128, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4822, "text": "Ger\u00e7ekten hi\u00e7bir g\u00fcnahkar, ba\u015fkas\u0131n\u0131n g\u00fcnah y\u00fck\u00fcn\u00fc y\u00fcklenemez."}, {"id": 572129, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4823, "text": "Bilsin ki insan i\u00e7in kendi \u00e7al\u0131\u015fmas\u0131ndan ba\u015fka bir \u015fey yoktur."}, {"id": 572130, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4824, "text": "Ve \u00e7al\u0131\u015fmas\u0131 da ileride g\u00f6r\u00fclecektir."}, {"id": 572131, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4825, "text": "Sonra ona kar\u015f\u0131l\u0131\u011f\u0131 tastamam verilecektir."}, {"id": 572132, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4826, "text": "Ve \u015f\u00fcphesiz en son var\u0131\u015f Rabbinedir."}, {"id": 572133, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4827, "text": "Do\u011frusu g\u00fcld\u00fcren de a\u011flatan da O'dur."}, {"id": 572134, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4828, "text": "\u00d6ld\u00fcren de dirilten de O'dur."}, {"id": 572135, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4829, "text": "\u015euras\u0131 muhakkak ki erkek ve di\u015fiden ibaret olan iki \u00e7ifti O yaratt\u0131."}, {"id": 572136, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4830, "text": "(Rahime) at\u0131ld\u0131\u011f\u0131 zaman nutfeden."}, {"id": 572137, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4831, "text": "\u015e\u00fcphesiz tekrar diriltmek de O'na aittir."}, {"id": 572138, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4832, "text": "Zengin eden de yoksul k\u0131lan da O'dur."}, {"id": 572139, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4833, "text": "Do\u011frusu \u015ei'ra y\u0131ld\u0131z\u0131n\u0131n Rabbi de O'dur."}, {"id": 572140, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4834, "text": "Ve \u015f\u00fcphesiz ki \u00f6nceki \u00c2d kavmini O helak etti."}, {"id": 572141, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4835, "text": "Semud'u da (O helak etti) ve geriye hi\u00e7bir \u015fey b\u0131rakmad\u0131."}, {"id": 572142, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4836, "text": "Daha \u00f6nce de \u00e7ok zalim ve pek azg\u0131n, olan Nuh kavmini (helak etmi\u015fti)."}, {"id": 569261, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4837, "text": "Alt\u00fcst olan \u015fehirleri de o b\u00f6yle yapt\u0131."}, {"id": 569989, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4838, "text": "Onlar\u0131n ba\u015f\u0131na getirece\u011fini getirdi!"}, {"id": 572143, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4839, "text": "\u015eimdi Rabbinin nimetlerinin hangisinde \u015f\u00fcpheye d\u00fc\u015fersin."}, {"id": 572144, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4840, "text": "\u0130\u015fte bu ilk uyar\u0131c\u0131lardan bir uyar\u0131c\u0131d\u0131r."}, {"id": 572145, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Yakla\u015fan yakla\u015ft\u0131."}, {"id": 572146, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4842, "text": "Onu (vaktini) Allah'tan ba\u015fka a\u00e7\u0131\u011fa \u00e7\u0131karacak yoktur."}, {"id": 572147, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4843, "text": "\u015eimdi siz bu s\u00f6ze (Kur'an'a) m\u0131 \u015fa\u015f\u0131yorsunuz?"}, {"id": 572148, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4844, "text": "G\u00fcl\u00fcyorsunuz da a\u011flam\u0131yorsunuz!"}, {"id": 572149, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4845, "text": "Ve siz gaflet i\u00e7inde oyalanmaktas\u0131n\u0131z!"}, {"id": 572150, "identifier": "tr.vakfi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "tr", "name": "Diyanet Vakf\u0131", "transelator_en_name": "Diyanet Vakfi", "format": "text", "type": "translation", "ayah_number": 4846, "text": "Haydi Allah'a secde edip O'na kulluk edin!"}]