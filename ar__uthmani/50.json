quran", "ayah_number": 4672, "text": "\u064a\u064e\u0648\u0652\u0645\u064e \u064a\u064e\u0633\u0652\u0645\u064e\u0639\u064f\u0648\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u064a\u0652\u062d\u064e\u0629\u064e \u0628\u0650\u0671\u0644\u0652\u062d\u064e\u0642\u0651\u0650 \u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u064a\u064e\u0648\u0652\u0645\u064f \u0671\u0644\u0652\u062e\u064f\u0631\u064f\u0648\u062c\u0650"}, {"id": 4539, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4673, "text": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0646\u064e\u062d\u0652\u0646\u064f \u0646\u064f\u062d\u0652\u0649\u0650\u06e6 \u0648\u064e\u0646\u064f\u0645\u0650\u064a\u062a\u064f \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u0652\u0646\u064e\u0627 \u0671\u0644\u0652\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, {"id": 4540, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4674, "text": "\u064a\u064e\u0648\u0652\u0645\u064e \u062a\u064e\u0634\u064e\u0642\u0651\u064e\u0642\u064f \u0671\u0644\u0652\u0623\u064e\u0631\u0652\u0636\u064f \u0639\u064e\u0646\u0652\u0647\u064f\u0645\u0652 \u0633\u0650\u0631\u064e\u0627\u0639\u064b\u06ed\u0627