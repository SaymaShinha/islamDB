[{"id": 4500, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4631, "text": "\u0628\u0650\u0633\u0652\u0645\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0631\u0651\u064e\u062d\u0652\u0645\u064e\u0670\u0646\u0650 \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0650 \u0642\u0653 \u06da \u0648\u064e\u0671\u0644\u0652\u0642\u064f\u0631\u0652\u0621\u064e\u0627\u0646\u0650 \u0671\u0644\u0652\u0645\u064e\u062c\u0650\u064a\u062f\u0650"}, {"id": 4501, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4632, "text": "\u0628\u064e\u0644\u0652 \u0639\u064e\u062c\u0650\u0628\u064f\u0648\u0653\u0627\u06df \u0623\u064e\u0646 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0646\u0630\u0650\u0631\u064c\u06ed \u0645\u0651\u0650\u0646\u0652\u0647\u064f\u0645\u0652 \u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0652\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0634\u064e\u0649\u0652\u0621\u064c \u0639\u064e\u062c\u0650\u064a\u0628\u064c"}, {"id": 4471, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4633, "text": "\u0623\u064e\u0621\u0650\u0630\u064e\u0627 \u0645\u0650\u062a\u0652\u0646\u064e\u0627 \u0648\u064e\u0643\u064f\u0646\u0651\u064e\u0627 \u062a\u064f\u0631\u064e\u0627\u0628\u064b\u06ed\u0627 \u06d6 \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0631\u064e\u062c\u0652\u0639\u064c\u06e2 \u0628\u064e\u0639\u0650\u064a\u062f\u064c\u06ed"}, {"id": 4502, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4634, "text": "\u0642\u064e\u062f\u0652 \u0639\u064e\u0644\u0650\u0645\u0652\u0646\u064e\u0627 \u0645\u064e\u0627 \u062a\u064e\u0646\u0642\u064f\u0635\u064f \u0671\u0644\u0652\u0623\u064e\u0631\u0652\u0636\u064f \u0645\u0650\u0646\u0652\u0647\u064f\u0645\u0652 \u06d6 \u0648\u064e\u0639\u0650\u0646\u062f\u064e\u0646\u064e\u0627 \u0643\u0650\u062a\u064e\u0670\u0628\u064c \u062d\u064e\u0641\u0650\u064a\u0638\u064c\u06e2"}, {"id": 4503, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4635, "text": "\u0628\u064e\u0644\u0652 \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u06df \u0628\u0650\u0671\u0644\u0652\u062d\u064e\u0642\u0651\u0650 \u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u0652 \u0641\u064e\u0647\u064f\u0645\u0652 \u0641\u0650\u0649\u0653 \u0623\u064e\u0645\u0652\u0631\u064d\u06e2 \u0645\u0651\u064e\u0631\u0650\u064a\u062c\u064d"}, {"id": 4504, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4636, "text": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u0652 \u064a\u064e\u0646\u0638\u064f\u0631\u064f\u0648\u0653\u0627\u06df \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0641\u064e\u0648\u0652\u0642\u064e\u0647\u064f\u0645\u0652 \u0643\u064e\u064a\u0652\u0641\u064e \u0628\u064e\u0646\u064e\u064a\u0652\u0646\u064e\u0670\u0647\u064e\u0627 \u0648\u064e\u0632\u064e\u064a\u0651\u064e\u0646\u0651\u064e\u0670\u0647\u064e\u0627 \u0648\u064e\u0645\u064e\u0627 \u0644\u064e\u0647\u064e\u0627 \u0645\u0650\u0646 \u0641\u064f\u0631\u064f\u0648\u062c\u064d\u06e2"}, {"id": 4505, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_qua