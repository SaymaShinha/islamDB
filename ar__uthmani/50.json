ory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4668, "text": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u0652 \u062e\u064e\u0644\u064e\u0642\u0652\u0646\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u0652\u0623\u064e\u0631\u0652\u0636\u064e \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u0652\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0641\u0650\u0649 \u0633\u0650\u062a\u0651\u064e\u0629\u0650 \u0623\u064e\u064a\u0651\u064e\u0627\u0645\u064d\u06e2 \u0648\u064e\u0645\u064e\u0627 \u0645\u064e\u0633\u0651\u064e\u0646\u064e\u0627 \u0645\u0650\u0646 \u0644\u0651\u064f\u063a\u064f\u0648\u0628\u064d\u06e2"}, {"id": 4535, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 454, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ar", "name": "Uthamani", "transelator_en_name": "Uthamani", "format": "text", "type": "quran", "ayah_number": 4669, "text": "\u0641\u064e\u0671\u0635\u0652\u0628\u0650\u0631\u0652 \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0648\u064e\u0633\u064e\u0628\u0651\u0650\u062d\u0652 \u0628\u0650\u062d\u064e\u0645\u0652\u062f\u0650 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0642\u064e\u0628\u0652\u0644\u064e \u0637\u064f\u0644\u064f\u0648\u0639\u0650 \u0671\u0644\u0634\u0651\u064e\u0645\u0652\u0633\u0650 \u0648\u064e\u0642\u064e\u0628\u0652\u0644\u064e \u0671\u0644\u0652\u063a\u064f\u0631\u064f\u0648\u0628\u0650"}, {"id": 4536, "identifier": "quran-uthmani", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "su