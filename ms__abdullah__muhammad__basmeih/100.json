[{"id": 405218, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6147, "text": "Demi Kuda Perang yang tangkas berlari dengan kedengaran kencang nafasnya,"}, {"id": 405219, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6148, "text": "Serta mencetuskan api dari telapak kakinya,"}, {"id": 405220, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6149, "text": "Dan meluru menyerbu musuh pada waktu subuh,"}, {"id": 405221, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6150, "text": "Sehingga menghamburkan debu pada waktu itu,"}, {"id": 405330, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6151, "text": "Lalu menggempur ketika itu di tengah-tengah kumpulan musuh;"}, {"id": 405222, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6152, "text": "Sesungguhnya manusia sangat tidak bersyukur akan nikmat Tuhannya."}, {"id": 405223, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6153, "text": "Dan sesungguhnya ia (dengan bawaannya) menerangkan dengan jelas keadaan yang demikian;"}, {"id": 405224, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6154, "text": "Dan sesungguhnya ia melampau sangat sayangkan harta (secara tamak haloba)."}, {"id": 405225, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6155, "text": "(Patutkah ia bersikap demikian?) Tidakkah ia mengetahui (bagaimana keadaan) ketika dibongkarkan segala yang ada dalam kubur?"}, {"id": 405226, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6156, "text": "Dan dikumpul serta didedahkan segala yang terpendam dalam dada?"}, {"id": 405227, "identifier": "ms.basmeih", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6157, "text": "Sesungguhnya Tuhan mereka Maha Mengetahui dengan mendalam tentang (balasan yang diberikanNya kepada) mereka - pada hari itu."}]