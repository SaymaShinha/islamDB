[{"id": 405013, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5932, "text": "Demi langit dan \"At-Taariq\"; -"}, {"id": 405014, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5933, "text": "Dan apa jalannya engkau dapat mengetahui apa dia \"At-Taariq\" itu? -"}, {"id": 405015, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5934, "text": "(At-Taariq) ialah bintang yang menembusi (sinaran cahayanya);"}, {"id": 405016, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5935, "text": "Tiada sesuatu diri juapun melainkan ada malaikat yang menjaga (keadaannya serta menyimpan catitan mengenai segala bawaannya)."}, {"id": 405017, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5936, "text": "(Setelah mengetahui yang demikian), maka hendaklah manusia memikirkan: dari apa ia diciptakan."}, {"id": 405018, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5937, "text": "Ia diciptakan dari air (mani) yang memancut (ke dalam rahim) -"}, {"id": 405019, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5938, "text": "Yang keluar dari \"tulang sulbi\" lelaki dan \"tulang dada\" perempuan."}, {"id": 405020, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5939, "text": "Sesungguhnya Allah Maha Kuasa untuk mengembalikannya (hidup semula sesudah mati),"}, {"id": 405021, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5940, "text": "Pada hari didedahkan segala yang terpendam di hati (dari iktiqad, niat, dan lain-lainnya),"}, {"id": 405022, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5941, "text": "Maka (pada saat itu) tidak ada bagi manusia sebarang kekuatan (untuk membela diri), dan tidak ada penolong (yang dapat memberikan pertolongan)."}, {"id": 400511, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5942, "text": "Demi langit yang berulang-ulang mencurahkan hujan,"}, {"id": 400731, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5943, "text": "Dan bumi yang merekah mengeluarkan tumbuh-tumbuhan,"}, {"id": 405023, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5944, "text": "Sesungguhnya keterangan Al-Quran adalah kata-kata pemutus (yang sebenar),"}, {"id": 405024, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5945, "text": "Dan bukanlah ia kata-kata yang olok-olok."}, {"id": 405025, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Sesungguhnya mereka (yang menentangmu, wahai Muhammad) bermati-mati menjalankan rancangan jahat,"}, {"id": 405026, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5947, "text": "Dan Aku pula tetap bertindak membalas rancangan jahat (mereka, dan menggagalkannya)."}, {"id": 405027, "identifier": "ms.basmeih", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5948, "text": "Oleh itu janganlah engkau hendakkan segera kebinasaan orang-orang kafir itu, berilah tempoh kepada mereka sedikit masa."}]