[{"id": 405072, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5994, "text": "Demi waktu fajar;"}, {"id": 405073, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5995, "text": "Dan malam yang sepuluh (yang mempunyai kelebihan di sisi Allah);"}, {"id": 405074, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5996, "text": "Dan bilangan yang genap serta yang ganjil;"}, {"id": 405075, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5997, "text": "Dan malam, apabila ia berlalu; -"}, {"id": 405076, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5998, "text": "Bukankah yang demikian itu mengandungi sumpah (yang diakui kebenarannya) oleh orang yang berakal sempurna?"}, {"id": 405077, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 5999, "text": "(Kami tetap akan membinasakan orang-orang yang menentangmu wahai Muhammad), tidakkah engkau perhatikan, bagaimana Tuhanmu telah melakukan terhadap kaum Aad (yang kufur derhaka)"}, {"id": 405078, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6000, "text": "Iaitu penduduk \"Iram\" yang mempunyai bangunan-bangunan yang tinggi tiangnya, (sesuai dengan penduduknya)"}, {"id": 405079, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6001, "text": "Yang belum pernah diciptakan sepertinya (tentang besar dan kukuhnya) di segala negeri (pada zamannya)?"}, {"id": 405080, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6002, "text": "Dan (terhadap) kaum Thamud yang memahat batu-batu besar di lembah (Wadil-Qura iaitu tempat tinggalnya)?"}, {"id": 405081, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6003, "text": "Dan (terhadap) Firaun yang menguasai bangunan-bangunan yang handal kukuh?"}, {"id": 405082, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6004, "text": "(Semuanya itu ialah) orang-orang yang telah bermaharajalela di negeri (masing-masing)."}, {"id": 405083, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6005, "text": "Lalu mereka melakukan dengan banyaknya kerosakan dan bencana di negeri-negeri itu."}, {"id": 405084, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6006, "text": "Dengan yang demikian, Tuhanmu mencurahkan ke atas mereka berbagai paluan azab;"}, {"id": 405085, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6007, "text": "Sesungguhnya Tuhanmu tetap mengawas dan membalas, (terutama balasan akhirat)"}, {"id": 405086, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6008, "text": "(Dalam pada itu manusia tidak menghiraukan balasan akhirat), oleh yang demikian, maka kebanyakan manusia apabila diuji oleh Tuhannya dengan dimuliakan dan dimewahkan hidupnya, (ia tidak mahu bersyukur tetapi terus bersikap takbur) serta berkata dengan sombongnya: \"Tuhanku telah memuliakan daku!\""}, {"id": 405087, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6009, "text": "Dan sebaliknya apabila ia diuji oleh Tuhannya, dengan disempitkan rezekinya, (ia tidak bersabar bahkan ia resah gelisah) serta merepek dengan katanya: \"Tuhanku telah menghinakan daku!\""}, {"id": 405088, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6010, "text": "Jangan demikian, (sebenarnya kata-kata kamu itu salah). Bahkan (perbuatan kamu wahai orang-orang yang hidup mewah, lebih salah lagi kerana) kamu tidak memuliakan anak yatim, (malah kamu menahan apa yang Ia berhak menerimanya);"}, {"id": 405089, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6011, "text": "Dan kamu tidak menggalakkan untuk memberi makanan (yang berhak diterima oleh) orang miskin;"}, {"id": 405090, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6012, "text": "Dan kamu sentiasa makan harta pusaka secara rakus (dengan tidak membezakan halal haramnya),"}, {"id": 405091, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6013, "text": "Serta kamu pula sayangkan harta secara tamak haloba!"}, {"id": 405092, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6014, "text": "Jangan sekali-kali bersikap demikian! (Sebenarnya) apabila bumi (dihancurkan segala yang ada di atasnya dan) diratakan serata-ratanya,"}, {"id": 405093, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6015, "text": "Dan (perintah) Tuhanmu pun datang, sedang malaikat berbaris-baris (siap sedia menjalankan perintah),"}, {"id": 405094, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6016, "text": "Serta diperlihatkan neraka Jahannam pada hari itu, (maka) pada saat itu manusia akan ingat (hendak berlaku baik), dan bagaimana ingatan itu akan berguna lagi kepadanya?"}, {"id": 405095, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6017, "text": "Ia akan berkata: \"Alangkah baiknya kalau aku dahulu sediakan amal-amal baik untuk hidupku (di sini)!\""}, {"id": 405096, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6018, "text": "Maka pada hari itu tiada sesiapapun yang dapat menyeksa seperti azab (yang ditimpakan oleh) Allah."}, {"id": 405097, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6019, "text": "Dan tiada sesiapapun yang dapat mengikat serta membelenggu seperti ikatan dan belengguNya."}, {"id": 405098, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6020, "text": "(Setelah menerangkan akibat orang-orang yang tidak menghiraukan akhirat, Tuhan menyatakan bahawa orang-orang yang beriman dan beramal soleh akan disambut dengan kata-kata): \"Wahai orang yang mempunyai jiwa yang sentiasa tenang tetap dengan kepercayaan dan bawaan baiknya! -"}, {"id": 405099, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6021, "text": "\"Kembalilah kepada Tuhanmu dengan keadaan engkau berpuas hati (dengan segala nikmat yang diberikan) lagi diredhai (di sisi Tuhanmu) ! -"}, {"id": 405100, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6022, "text": "\"Serta masuklah engkau dalam kumpulan hamba-hambaku yang berbahagia -"}, {"id": 405101, "identifier": "ms.basmeih", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6023, "text": "\"Dan masuklah ke dalam SyurgaKu! \""}]