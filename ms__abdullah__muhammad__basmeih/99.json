[{"id": 405210, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6139, "text": "Apabila bumi digegarkan dengan gegaran yang sedahsyat-dahsyatnya,"}, {"id": 405211, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6140, "text": "Serta bumi itu mengeluarkan segala isinya,"}, {"id": 405212, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6141, "text": "Dan berkatalah manusia (dengan perasaan gerun); \"Apa yang sudah terjadi kepada bumi?\""}, {"id": 405213, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6142, "text": "Pada hari itu bumipun menceritakan khabar beritanya:"}, {"id": 405214, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6143, "text": "Bahawa Tuhanmu telah memerintahnya (berlaku demikian)."}, {"id": 405215, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6144, "text": "Pada hari itu manusia akan keluar berselerak (dari kubur masing-masing) - untuk diperlihatkan kepada mereka (balasan) amal-amal mereka."}, {"id": 405216, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6145, "text": "Maka sesiapa berbuat kebajikan seberat zarah, nescaya akan dilihatnya (dalam surat amalnya)!"}, {"id": 405217, "identifier": "ms.basmeih", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6146, "text": "Dan sesiapa berbuat kejahatan seberat zarah, nescaya akan dilihatnya (dalam surat amalnya)!"}]