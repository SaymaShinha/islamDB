[{"id": 405102, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Aku bersumpah dengan negeri (Makkah) ini;"}, {"id": 405103, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6025, "text": "Sedang engkau (wahai Muhammad) tinggal di negeri ini (sentiasa ditindas),"}, {"id": 405104, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6026, "text": "Demi manusia yang melahirkan zuriat, dan zuriat yang dilahirkannya;"}, {"id": 405105, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Sesungguhnya Kami telah jadikan manusia sentiasa dalam keadaan menghadapi kesulitan dan kesukaran (jasmani dan rohaninya);"}, {"id": 405106, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6028, "text": "Patutkah manusia yang demikian keadaannya (terpedaya dengan kekuasaan yang ada padanya dan) menyangka bahawa tidak ada sesiapapun yang dapat mengatasi kekuasaannya (dan menyeksakannya)?"}, {"id": 405107, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6029, "text": "Manusia yang demikian keadaannya (tidaklah patut ia bermegah-megah dengan kekayaannya dan) berkata: \"Aku telah habiskan harta benda yang banyak (dalam usaha menegakkan nama dan bangsa)."}, {"id": 405108, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6030, "text": "Adakah ia menyangka bahawa tidak ada sesiapapun yang melihatnya (dan mengetahui tujuannya menghabiskan harta bendanya itu?"}, {"id": 405109, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6031, "text": "(Mengapa manusia terpedaya dan bermegah-megah?) Tidakkah Kami telah menjadikan baginya: dua mata (untuk ia memerhatikan kekuasaan dan kekayaan Kami?) -"}, {"id": 405110, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6032, "text": "Dan lidah serta dua bibir (untuk ia menyempurnakan sebahagian besar dari hajat-hajatnya)?"}, {"id": 405111, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6033, "text": "Dan Kami telah menunjukkan kepadanya dua jalan, (jalan kebaikan untuk dijalaninya, dan jalan kejahatan untuk dijauhi)?"}, {"id": 405112, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Dalam pada itu manusia tidak (memilih jalan kebaikan) merempuh masuk mengerjakan amal-amal yang tinggi darjatnya di sisi Tuhan;"}, {"id": 405113, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6035, "text": "Dan apa jalannya engkau dapat mengetahui: apa dia amal-amal yang tinggi darjatnya di sisi Tuhan itu?"}, {"id": 405114, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6036, "text": "(Di antara amal-amal itu - bagi orang yang mampu) ialah: memerdekakan hamba abdi;"}, {"id": 405115, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6037, "text": "Atau memberi makan pada hari kelaparan -"}, {"id": 405116, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6038, "text": "Kepada anak yatim dari kaum kerabat,"}, {"id": 405117, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6039, "text": "Atau kepada orang miskin yang terlantar di atas tanah."}, {"id": 405118, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6040, "text": "Selain dari (tidak mengerjakan amal-amal) itu, ia (tidak pula) menjadi dari orang-orang yang beriman dan berpesan-pesan dengan sabar serta berpesan-pesan dengan kasih sayang."}, {"id": 405119, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6041, "text": "(Ketahuilah! Bahawa orang-orang yang beriman serta berusaha mengerjakan amal-amal yang tinggi darjatnya di sisi Tuhan), merekalah golongan pihak kanan (yang akan beroleh Syurga)."}, {"id": 405120, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6042, "text": "Dan (sebaliknya) orang-orang yang kufur ingkar akan ayat-ayat keterangan Kami, merekalah golongan pihak kiri. -"}, {"id": 405121, "identifier": "ms.basmeih", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Mereka ditimpakan (azab seksa) neraka yang ditutup rapat (supaya kuat bakarannya)."}]