[{"id": 405263, "identifier": "ms.basmeih", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6194, "text": "Kerana kebiasaan aman tenteram kaum Quraisy (penduduk Makkah)"}, {"id": 405264, "identifier": "ms.basmeih", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6195, "text": "(laitu) kebiasaan aman tenteram perjalanan mereka (menjalankan perniagaan) pada musim sejuk (ke negeri Yaman), dan pada musim panas (ke negeri Syam),"}, {"id": 405265, "identifier": "ms.basmeih", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6196, "text": "Maka hendaklah mereka menyembah Tuhan yang menguasai rumah (Kaabah) ini,"}, {"id": 405266, "identifier": "ms.basmeih", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ms", "name": "Basmeih", "transelator_en_name": "Abdullah Muhammad Basmeih", "format": "text", "type": "translation", "ayah_number": 6197, "text": "Tuhan yang memberi mereka penghidupan: menyelamatkan mereka dari kelaparan, dan mengamankan mereka dari ketakutan."}]