tifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Indeed yes, We are able to shape his fingerstips yet again!"}, {"id": 204932, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5556, "text": "Rather, the human desires to continue in his immorality,"}, {"id": 204933, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5557, "text": "'When will the Day of Resurrection be' he asks,"}, {"id": 204934, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5558, "text": "but when the sight is dazed"}, {"id": 204935, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5559, "text": "and the moon eclipsed,"}, {"id": 204936, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5560, "text": "when the sun and moon are gathered together"}, {"id": 204937, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5561, "text": "on that Day the human will ask: 'To (which place) shall I flee'"}, {"id": 204938, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5562, "text": "No, there shall be no (place of) refuge."}, {"id": 204939, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5563, "text": "For on that Day the refuge shall be to your Lord."}, {"id": 204940, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5564, "text": "On that Day the human shall be informed of his former and latter deeds."}, {"id": 204941, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5565, "text": "He shall be a clear proof against himself,"}, {"id": 204942, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5566, "text": "even though he offers his excuses."}, {"id": 204943, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5567, "text": "Do not move your tongue with it (the Revelation) to hasten it."}, {"id": 204944, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5568, "text": "It's gathering and reciting is upon Us."}, {"id": 204945, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5569, "text": "When We recite it, follow its recitation."}, {"id": 204946, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5570, "text": "It's explanation is upon Us."}, {"id": 204947, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5571, "text": "Yet you love this hasty world"}, {"id": 204948, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5572, "text": "and are heedless of the Everlasting Life."}, {"id": 204949, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5573, "text": "On that Day there shall be radiant faces,"}, {"id": 204950, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5574, "text": "gazing towards their Lord."}, {"id": 204951, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5575, "text": "And on that Day there shall be scowling faces,"}, {"id": 204952, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5576, "text": "so they might think the Calamity had been inflicted upon them."}, {"id": 204953, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5577, "text": "But, when (the soul) reaches the collar bone"}, {"id": 204954, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5578, "text": "and it is said: 'Who is a healing chanter'"}, {"id": 204955, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5579, "text": "and when he thinks it is the time of departure"}, {"id": 204956, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5580, "text": "and when leg is intertwined with leg,"}, {"id": 204957, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5581, "text": "upon that Day the driving shall be to your Lord."}, {"id": 204958, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5582, "text": "Because, he neither believed nor prayed;"}, {"id": 204959, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5583, "text": "he belied the truth and turned away;"}, {"id": 204960, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5584, "text": "then went arrogantly to his household."}, {"id": 204961, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5585, "text": "Near to you and nearer,"}, {"id": 204962, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5586, "text": "then nearer to you and nearer!"}, {"id": 204963, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5587, "text": "Does the human think he will be left to roam at will?"}, {"id": 204964, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5588, "text": "What, was he not an ejaculated drop (of sperm)?"}, {"id": 204965, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Then he was a clot of blood, then He created and formed him"}, {"id": 204966, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5590, "text": "and made from him two kinds, male and female."}, {"id": 204967, "identifier": "en.qaribullah", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5591, "text": "What, is He then unable to revive the dead?"}]