[{"id": 205499, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6131, "text": "The unbelievers among the People of the Book and the idolaters would never desist until the Clear Proof came to them."}, {"id": 205500, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6132, "text": "A Messenger from Allah reciting Purified Pages"}, {"id": 205501, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6133, "text": "in which there are valuable Books."}, {"id": 205502, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6134, "text": "Those to whom the Book was given did not divide themselves until the Clear Proof came to them."}, {"id": 205503, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6135, "text": "Yet they were ordered to worship Allah alone, making the Religion His sincerely, upright, and to establish their prayers and to pay the obligatory charity. That is indeed the Religion of Straightness."}, {"id": 205504, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6136, "text": "The unbelievers among the People of the Book and the idolaters shall be for ever in the Fire of Gehenna (Hell). They are the worst of all creatures."}, {"id": 205505, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6137, "text": "But those who believe and do good deeds are the best of all creatures."}, {"id": 205506, "identifier": "en.qaribullah", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 6138, "text": "Their recompense is with their Lord; Gardens of Eden, underneath which rivers flow, where they shall live for ever. Allah is wellpleased with them, and theyare wellpleased with Him. That is for he who fears his Lord!"}]