[{"id": 204825, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5448, "text": "Say: 'It is revealed to me that a party of jinn listened then said: \"We have indeed heard a wonderful Koran,"}, {"id": 204826, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5449, "text": "that guides to the Right Path. We believe in it and we will not associate anyone with our Lord."}, {"id": 204827, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5450, "text": "He exalted be the Majesty of our Lord, who has neither taken to Himself a wife, nor a son!"}, {"id": 204828, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5451, "text": "The ignorant fool among us has spoken outrageously against Allah,"}, {"id": 204829, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5452, "text": "we never thought that either human or jinn would ever tell a lie against Allah!'"}, {"id": 204830, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5453, "text": "But there were certain men from mankind who would take refuge with certain males from the jinn and they increased them in tyranny."}, {"id": 204831, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5454, "text": "Like you, they thought that Allah would never raise the dead."}, {"id": 204832, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5455, "text": "(The jinn continued saying): \"We made our way towards heaven, but we found it filled with stern guards and a flame."}, {"id": 204833, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5456, "text": "There, we would sit to eavesdrop, but now an eavesdropper finds a flame in wait for him."}, {"id": 204834, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5457, "text": "And so we do not know whether evil is intended for those on earth, or whether their Lord intends to guide them."}, {"id": 204835, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5458, "text": "Some of us are righteous, but some are otherwise, we are sects that differ."}, {"id": 204836, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5459, "text": "We know that we cannot frustrate Allah in the earth, nor can we frustrate Him by flight."}, {"id": 204837, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5460, "text": "When we heard the Guidance, we believed in it, and whosoever believes in his Lord shall fear neither shortage nor injustice."}, {"id": 204838, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5461, "text": "Some of us have surrendered (Muslims) and some of us have deviated. Those who surrendered sought the Right Path,"}, {"id": 204839, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5462, "text": "but those who have deviated shall become the fuel of Gehenna (Hell)."}, {"id": 204840, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5463, "text": "If they had followed the Path We would give to them abundant water to drink,"}, {"id": 204841, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5464, "text": "and tested them with it. And whosoever turns away from his Lord's Remembrance, He will hurl him into a stern punishment."}, {"id": 204842, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5465, "text": "Mosques belong to Allah, so do not call to anyone else, other than Allah."}, {"id": 204843, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5466, "text": "When the worshiper of Allah stood supplicating to Him, they swarmed around him (Prophet Muhammad)."}, {"id": 204844, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5467, "text": "Say: 'I supplicate only to my Lord and I do not associate any with Him'"}, {"id": 204845, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5468, "text": "Say: 'Indeed, I possess no power over you, either for harm or for guidance'"}, {"id": 204846, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5469, "text": "Say: 'None shall protect me from Allah, and other than Him, I will find no refuge,"}, {"id": 204848, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5470, "text": "except a deliverance from Allah and His Messages. He who disobeys Allah and His Messenger, for him is the Fire of Gehenna, which he will be in for ever."}, {"id": 204849, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5471, "text": "When they see that which they were promised they shall know who is weaker in helpers and fewer in numbers."}, {"id": 204850, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5472, "text": "Say: 'I do not know whether that which you are promised is near, or whether my Lord will set for it a time."}, {"id": 204851, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5473, "text": "He is the Knower of the Unseen and does not disclose His Unseen to anyone,"}, {"id": 204852, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5474, "text": "except only to a Messenger that He has chosen, and He sends guardians before him and behind him,"}, {"id": 204853, "identifier": "en.qaribullah", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 573, "ruku": 507, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5475, "text": "so that He may know that they have delivered the Messages of their Lord and He encompasses all that is with them, and He has counted everything innumbers'"}]