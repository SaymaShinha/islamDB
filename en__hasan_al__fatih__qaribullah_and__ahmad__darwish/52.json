[{"id": 204122, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4736, "text": "By the Mount"}, {"id": 204123, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4737, "text": "and by the Book in lines"}, {"id": 204124, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4738, "text": "in an exposed parchment"}, {"id": 204125, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4739, "text": "by the Visited House,"}, {"id": 204126, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4740, "text": "and the uplifted roof"}, {"id": 204127, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4741, "text": "and the sea that is full"}, {"id": 204128, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4742, "text": "surely, the punishment of your Lord is about to come,"}, {"id": 204129, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4743, "text": "there is none to prevent it."}, {"id": 204130, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4744, "text": "Upon the Day when the heaven spins dizzily"}, {"id": 204131, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4745, "text": "and the mountains move, moving."}, {"id": 204132, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4746, "text": "On that Day woe to those who belied"}, {"id": 204133, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4747, "text": "those who are in plunging, playing."}, {"id": 204134, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4748, "text": "On that Day when they shall be pitched into the Fire of Gehenna (Hell),"}, {"id": 204135, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4749, "text": "(it will be said to them): 'This is the Fire which you belied!"}, {"id": 204136, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4750, "text": "Is this magic, or is it that you do you not see?"}, {"id": 204137, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4751, "text": "Roast in it, bear it with or without patience, it is the same, you are only being recompensed for that which you used to do'"}, {"id": 204138, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4752, "text": "But in the Gardens the righteous shall live in bliss,"}, {"id": 204139, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4753, "text": "rejoicing in all their Lord has given them, and their Lord will guard them against the punishment of Hell."}, {"id": 204140, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4754, "text": "(It will be said): 'Eat and drink with a good appetite because of that which you did'"}, {"id": 204141, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4755, "text": "(They shall be) reclining on couches ranged in rows and We shall wed them to houris (virgins of Paradise) with large wide eyes."}, {"id": 204142, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4756, "text": "Those who believe, and whose descendants follow in belief, We will join their descendants to them. And We will not reduce them of anything of their deeds. Every one is pledged for what he has earned."}, {"id": 204143, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4757, "text": "We shall give them fruits and meat as they desire."}, {"id": 204144, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4758, "text": "There they will pass a goblet to one another with neither idle talk nor sin,"}, {"id": 204145, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4759, "text": "and youths, of their own, shall pass among them as if they were hidden pearls."}, {"id": 204146, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4760, "text": "They will go to one another asking each other questions:"}, {"id": 204147, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4761, "text": "'When we were among our people' they will say, 'we were ever fearful,"}, {"id": 204119, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4762, "text": "but Allah has been gracious to us and has protected us from the punishment of the burning wind."}, {"id": 204148, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 458, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4763, "text": "Before, we were supplicating to Him. He is the Giving, the Most Merciful'"}, {"id": 204149, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4764, "text": "Therefore, remind. By the Favor of Allah, you are neither a soothsayer, nor mad."}, {"id": 204150, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4765, "text": "Or do they say: 'He is a poet, we are waiting for some misfortune to befall him'"}, {"id": 204151, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 524, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4766, "text": "Say: 'Wait if you will; I shall be waiting with you'"}, {"id": 204152, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4767, "text": "Or, do their intellects order them to do this? Or, are they an insolent people?"}, {"id": 204153, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4768, "text": "Do they say: 'He has invented it' No, they do not believe."}, {"id": 204154, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4769, "text": "Let them produce a phrase like it, if what they say is true!"}, {"id": 204155, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4770, "text": "Or, were they created out of nothing? Or, were they their own creators?"}, {"id": 204156, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4771, "text": "Or, did they create the heavens and the earth? No, their belief is not certain!"}, {"id": 204157, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4772, "text": "Or, are the treasures of your Lord in their keeping? Or, are they the controllers?"}, {"id": 204158, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4773, "text": "Or, do they have a ladder on which they listen? Then let any of them that has listened bring a clear authority."}, {"id": 204159, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4774, "text": "Or, has He daughters, and they sons?"}, {"id": 204160, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4775, "text": "Or, do you ask them for a wage, so that they become weighed down in debt?"}, {"id": 204161, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4776, "text": "Or, is the Unseen in their keeping, so they are writing it down?"}, {"id": 204162, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4777, "text": "Or, do they desire to outwit? The unbelievers are the outwitted."}, {"id": 204163, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4778, "text": "Or, do they have a god, other than Allah? Exaltations to Allah above that which they associate!"}, {"id": 204164, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4779, "text": "Even if they saw lumps falling from the sky they would say: 'A massed cloud'"}, {"id": 204165, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4780, "text": "So leave them till they encounter their Day in which they shall be thunderstruck."}, {"id": 204166, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4781, "text": "The Day when their guile shall not relieve them a thing, and they shall not be helped."}, {"id": 204167, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4782, "text": "For the harmdoers there is indeed, a punishment before that, but most of them do not know."}, {"id": 204168, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4783, "text": "And be patient under the Judgement of your Lord, surely, you are before Our Eyes. And exalt with the praise of your Lord when you arise,"}, {"id": 204169, "identifier": "en.qaribullah", "surah_number": 52, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0648\u0631", "surah_en_name": "At-Tur", "surah_en_name_translation": "The Mount", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 525, "ruku": 459, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 4784, "text": "and exalt Him in the night and at the declining of the stars."}]