[{"id": 204703, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5324, "text": "The Resurrection Verifier;"}, {"id": 204704, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5325, "text": "and what is the Resurrection Verifier?"}, {"id": 204705, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5326, "text": "What makes you to know what the Resurrection Verifier is?"}, {"id": 204706, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5327, "text": "Thamood and Aad belied the Clatterer."}, {"id": 204707, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5328, "text": "Thamood, they were destroyed by the violent shout (of Gabriel),"}, {"id": 204708, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5329, "text": "as for Aad, they were destroyed by a howling, violent wind"}, {"id": 204709, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5330, "text": "that He subjected upon them for seven nights and eight days consecutively and you might have seen them struck down as if they were the stumps of palm trees that had fallen down."}, {"id": 204710, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5331, "text": "Can you see any remnant of them now?"}, {"id": 204711, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5332, "text": "Similarly, Pharaoh, and those before him, and the ruined villages, sinned"}, {"id": 204712, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5333, "text": "and rebelled against their Lord's Messenger. So He took them with a stern taking."}, {"id": 204713, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5334, "text": "And when the waters rose (high) We carried you in the sailing (Ark),"}, {"id": 204714, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5335, "text": "making it a Reminder for you, for all attentive ears to retain."}, {"id": 204715, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5336, "text": "When a single blow is blown on the Horn,"}, {"id": 204716, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5337, "text": "when the earth with all its mountains is lifted up and crushed with a single blow,"}, {"id": 204717, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5338, "text": "on that Day, the Event occurs."}, {"id": 204718, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5339, "text": "The heaven will be split; because on that Day it will be frail."}, {"id": 204719, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5340, "text": "The angels will stand on all its sides. And on that Day, eight (of them) will carry the Throne of your Lord above them."}, {"id": 204720, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5341, "text": "On that Day you shall be exposed, and no secret of yours will remain hidden."}, {"id": 204721, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5342, "text": "Then, he who is given his book in his right hand will say: 'Here, take and read my book!"}, {"id": 204722, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5343, "text": "Indeed, I knew that I should come to my reckoning'"}, {"id": 200227, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5344, "text": "His shall be a pleasing life"}, {"id": 200612, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5345, "text": "in a high Garden,"}, {"id": 200690, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5346, "text": "its fruits are near."}, {"id": 204723, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5347, "text": "(It will be said): 'Eat and drink with a good appetite because of what you did in days long passed'"}, {"id": 204724, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5348, "text": "But, he who is given his book in his left hand will say: 'Woe to me, would that my book had not been given to me!"}, {"id": 204725, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5349, "text": "Nor that I knew my reckoning!"}, {"id": 204726, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5350, "text": "Would that it (my death) had ended it all!"}, {"id": 204727, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5351, "text": "My wealth has not helped me a thing"}, {"id": 204728, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5352, "text": "and my authority has been destroyed'"}, {"id": 204729, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5353, "text": "(It will be said): 'Take him and bind him."}, {"id": 204730, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5354, "text": "Roast him in Hell,"}, {"id": 204731, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5355, "text": "then in a chain seventy arm's lengths long insert him."}, {"id": 204732, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5356, "text": "He did not believe in Allah, the Great,"}, {"id": 204733, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5357, "text": "nor did he urge the feeding of the needy."}, {"id": 204734, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5358, "text": "Today he shall have no loyal friend here,"}, {"id": 204735, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5359, "text": "nor any food except foul pus"}, {"id": 204736, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5360, "text": "that none but sinners eat'"}, {"id": 204737, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5361, "text": "I swear by all that you can see,"}, {"id": 204738, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5362, "text": "and all that you do not see,"}, {"id": 204739, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5363, "text": "that this is the speech of a noble Messenger."}, {"id": 204740, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5364, "text": "It is not the speech of a poet little do you believe"}, {"id": 204741, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5365, "text": "nor is it the speech of a soothsayer little do you remember."}, {"id": 204742, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5366, "text": "(It is) a sending down from the Lord of all the Worlds."}, {"id": 204743, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5367, "text": "Had he invented sayings against Us,"}, {"id": 204744, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5368, "text": "We would have seized him by the right hand"}, {"id": 204745, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5369, "text": "then, We would surely have cut from him the aorta (vein)"}, {"id": 204746, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5370, "text": "not one of you could have prevented it from him."}, {"id": 204747, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5371, "text": "Indeed, it is a Reminder to those who fear Allah,"}, {"id": 204748, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5372, "text": "We know that there are some among you who will belie."}, {"id": 204749, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5373, "text": "Indeed, it is a sorrow to the unbelievers"}, {"id": 204750, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5374, "text": "yet it is a certain truth."}, {"id": 204751, "identifier": "en.qaribullah", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Qaribullah & Darwish", "transelator_en_name": "Hasan al-Fatih Qaribullah and Ahmad Darwish", "format": "text", "type": "translation", "ayah_number": 5375, "text": "Exalt the Name of your Lord, the Great."}]