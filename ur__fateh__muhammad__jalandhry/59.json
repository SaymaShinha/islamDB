[{"id": 666004, "identifier": "ur.jalandhry", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u062c\u0627\u0644\u0646\u062f\u06c1\u0631\u06cc", "transelator_en_name": "Fateh Muhammad Jalandhry", "format": "text", "type": "translation", "ayah_number": 5127, "text": "\u062c\u0648 \u0686\u06cc\u0632\u06cc\u06ba \u0622\u0633\u0645\u0627\u0646\u0648\u06ba \u0645\u06cc\u06ba \u06c1\u06cc\u06ba \u0627\u0648\u0631 \u062c\u0648 \u0686\u06cc\u0632\u06cc\u06ba \u0632\u0645\u06cc\u0646 \u0645\u06cc\u06ba \u06c1\u06cc\u06ba (\u0633\u0628) \u062e\u062f\u0627 \u06a9\u06cc \u062a\u0633\u0628\u06cc\u062d \u06a9\u0631\u062a\u06cc \u06c1\u06cc\u06ba\u06d4 \u0627\u0648\u0631 \u0648\u06c1 \u063a\u0627\u0644\u0628 \u062d\u06a9\u0645\u062a \u0648\u0627\u0644\u0627 \u06c1\u06d2"}, {"id": 666006, "identifier": "ur.jalandhry", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u062c\u0627\u0644\u0646\u062f\u06c1\u0631\u06cc", "transelator_en_name": "Fateh Muhammad Jalandhry", "format": "text", "type": "translation", "ayah_number": 5128, "text": "\u0648\u06c1\u06cc \u062a\u0648 \u06c1\u06d2 \u062c\u0633 \u0646\u06d2 \u06a9\u0641\u0627\u0631 \u0627\u06c1\u0644 \u06a9\u062a\u0627\u0628 \u06a9\u0648 \u062d\u0634\u0631 \u0627\u0648\u0644 \u06a9\u06d2 \u0648\u0642\u062a \u0627\u0646 \u06a9\u06d2 \u06af\u06be\u0631\u0648\u06ba \u0633\u06d2 \u0646\u06a9\u0627\u0644 \u062f\u06cc\u0627\u06d4 \u062a\u0645\u06c1\u0627\u0631\u06d2 \u062e\u06cc\u0627\u0644 \u0645\u06cc\u06ba \u0628\u06be\u06cc \u0646\u06c1 \u062a\u06be\u0627 \u06a9\u06c1 \u0648\u06c1 \u0646\u06a9\u0644 \u062c\u0627\u0626\u06cc\u06ba \u06af\u06d2 \u0627\u0648\u0631 \u0648\u06c1 \u0644\u0648\u06af \u06cc\u06c1 \u0633\u0645\u062c\u06be\u06d2 \u06c1\u0648\u0626\u06d2 \u062a\u06be\u06d2 \u06a9\u06c1 \u0627\u0646 \u06a9\u06d2 \u0642\u0644\u0639\u06d2 \u0627\u0646 \u06a9\u0648 \u062e\u062f\u0627 (\u06a9\u06d2 \u0639\u0630\u0627\u0628) \u0633\u06d2 \u0628\u0686\u0627 \u0644\u06cc\u06ba \u06af\u06d2\u06d4 \u0645\u06af\u0631 \u062e\u062f\u0627 \u0646\u06d2 \u0627\u0646 \u06a9\u0648 \u0648\u06c1\u0627\u06ba \u0633\u06d2 \u0622 \u0644\u06cc\u0627 \u062c\u06c1\u0627\u06ba \u0633\u06d2 \u0627\u0646 \u06a9\u0648 \u06af\u0645\u0627\u0646 \u0628\u06be\u06cc \u0646\u06c1 \u062a\u06be\u0627\u06d4 \u0627\u0648\u0631 \u0627\u0646 \u06a9\u06d2 \u062f\u0644\u0648\u06ba \u0645\u06cc\u06ba \u062f\u06c1\u0634\u062a \u0688\u0627\u0644 \u062f\u06cc \u06a9\u06c1 \u0627\u067e\u0646\u06d2 \u06af\u06be\u0631\u0648\u06ba \u06a9\u0648 \u062e\u0648\u062f \u0627\u067e\u0646\u06d2 \u06c1\u0627\u062a\u06be\u0648\u06ba \u0627\u0648\u0631 \u0645\u0648\u0645\u0646\u0648\u06ba \u06a9\u06d2 \u06c1\u0627\u062a\u06be\u0648\u06ba \u0633\u06d2 \u0627\u064f\u062c\u0627\u0691\u0646\u06d2 \u0644\u06af\u06d2 \u062a\u0648 \u0627\u06d2 (\u0628\u0635\u06cc\u0631\u062a \u06a9\u06cc) \u0622\u0646\u06a9\u06be\u06cc\u06ba \u0631\u06a9\u06be\u0646\u06d2 \u0648\u0627\u0644\u0648 \u0639\u0628\u0631\u062a \u067e\u06a9\u0691\u0648"}, {"id": 666007, "identifier": "ur.jalandhry", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_reco