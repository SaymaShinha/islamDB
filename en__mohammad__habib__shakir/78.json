[{"id": 211296, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5673, "text": "Of what do they ask one another?"}, {"id": 211297, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5674, "text": "About the great event,"}, {"id": 205807, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5675, "text": "About which they differ?"}, {"id": 205805, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5676, "text": "Nay! they shall soon come to know"}, {"id": 205813, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5677, "text": "Nay! Nay! they shall soon know."}, {"id": 205815, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5678, "text": "Have We not made the earth an even expanse?"}, {"id": 205816, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5679, "text": "And the mountains as projections (thereon)?"}, {"id": 205814, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5680, "text": "And We created you in pairs,"}, {"id": 205817, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5681, "text": "And We made your sleep to be rest (to you),"}, {"id": 205820, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5682, "text": "And We made the night to be a covering,"}, {"id": 205818, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5683, "text": "And We made the day for seeking livelihood."}, {"id": 205819, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5684, "text": "And We made above you seven strong ones,"}, {"id": 205821, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5685, "text": "And We made a shining lamp,"}, {"id": 211298, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5686, "text": "And We send down from the clouds water pouring forth abundantly,"}, {"id": 211299, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5687, "text": "That We may bring forth thereby corn and herbs,"}, {"id": 211300, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5688, "text": "And gardens dense and luxuriant."}, {"id": 211301, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5689, "text": "Surely the day of decision is (a day) appointed:"}, {"id": 211302, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5690, "text": "The day on which the trumpet shall be blown so you shall come forth in hosts,"}, {"id": 211303, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5691, "text": "And the heaven shall be opened so that it shall be all openings,"}, {"id": 211304, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5692, "text": "And the mountains shall be moved off so that they shall remain a mere semblance."}, {"id": 211305, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5693, "text": "Surely hell lies in wait,"}, {"id": 211306, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5694, "text": "A place of resort for the inordinate,"}, {"id": 211307, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5695, "text": "Living therein for ages."}, {"id": 211308, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5696, "text": "They shall not taste therein cool nor drink"}, {"id": 211309, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5697, "text": "But boiling and intensely cold water,"}, {"id": 211310, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5698, "text": "Requital corresponding."}, {"id": 211311, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5699, "text": "Surely they feared not the account,"}, {"id": 211312, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5700, "text": "And called Our communications a lie, giving the lie (to the truth)."}, {"id": 211313, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5701, "text": "And We have recorded everything in a book,"}, {"id": 211314, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 582, "ruku": 518, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5702, "text": "So taste! for We will not add to you aught but chastisement."}, {"id": 211315, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5703, "text": "Surely for those who guard (against evil) is achievement,"}, {"id": 211316, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5704, "text": "Gardens and vineyards,"}, {"id": 211317, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5705, "text": "And voluptuous women of equal age;"}, {"id": 211318, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5706, "text": "And a pure cup."}, {"id": 211319, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5707, "text": "They shall not hear therein any vain words nor lying."}, {"id": 211320, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5708, "text": "A reward from your Lord, a gift according to a reckoning:"}, {"id": 211321, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5709, "text": "The Lord of the heavens and the earth and what is between them, the Beneficent Allah, they shall not be able to address Him."}, {"id": 211322, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5710, "text": "The day on which the spirit and the angels shall stand in ranks; they shall not speak except he whom the Beneficent Allah permits and who speaks the right thing."}, {"id": 211323, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5711, "text": "That is the sure day, so whoever desires may take refuge with his Lord."}, {"id": 211324, "identifier": "en.shakir", "surah_number": 78, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0628\u0623", "surah_en_name": "An-Naba", "surah_en_name_translation": "The Announcement", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 519, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5712, "text": "Surely We have warned you of a chastisement near at hand: the day when man shall see what his two hands have sent before, and the unbeliever shall say: O! would that I were dust!"}]