[{"id": 210753, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5127, "text": "Whatever is in the heavens and whatever is in the earth declares the glory of Allah, and He is the Mighty, the Wise."}, {"id": 210754, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5128, "text": "He it is Who caused those who disbelieved of the followers of the Book to go forth from their homes at the first banishment you did not think that they would go forth, while they were certain that their fortresses would defend them against Allah; but Allah came to them whence they did not expect, and cast terror into their hearts; they demolished their houses with their own hands and the hands of the believers; therefore take a lesson, O you who have eyes!"}, {"id": 210755, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5129, "text": "And had it not been that Allah had decreed for them the exile, He would certainly have punished them in this world, and in the hereafter they shall have chastisement of the fire."}, {"id": 210756, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5130, "text": "That is because they acted in opposition to Allah and His Apostle, and whoever acts in opposition to Allah, then surely Allah is severe in retributing (evil)."}, {"id": 210757, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5131, "text": "Whatever palm-tree you cut down or leave standing upon its roots, It is by Allah's command, and that He may abase the transgressors."}, {"id": 210758, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5132, "text": "And whatever Allah restored to His Apostle from them you did not press forward against it any horse or a riding camel but Allah gives authority to His apostles against whom He pleases, and Allah has power over all things."}, {"id": 210759, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5133, "text": "Whatever Allah has restored to His Apostle from the people of the towns, it is for Allah and for the Apostle, and for the near of kin and the orphans and the needy and the wayfarer, so that it may not be a thing taken by turns among the rich of you, and whatever the Apostle gives you, accept it, and from whatever he forbids you, keep back, and be careful of (your duty to) Allah; surely Allah is severe in retributing (evil):"}, {"id": 210760, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5134, "text": "(It is) for the poor who fled their homes and their possessions, seeking grace of Allah and (His) pleasure, and assisting Allah and His Apostle: these it is that are the truthful."}, {"id": 210761, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 546, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5135, "text": "And those who made their abode in the city and in the faith before them love those who have fled to them, and do not find in their hearts a need of what they are given, and prefer (them) before themselves though poverty may afflict them, and whoever is preserved from the niggardliness of his soul, these it is that are the successful ones."}, {"id": 210763, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5136, "text": "And those who come after them say: Our Lord! forgive us and those of our brethren who had precedence of us in faith, and do not allow any spite to remain in our hearts towards those who believe, our Lord! surely Thou art Kind, Merciful."}, {"id": 210764, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5137, "text": "Have you not seen those who have become hypocrites? They say to those of their brethren who disbelieve from among the followers of the Book: If you are driven forth, we shall certainly go forth with you, and we will never obey any one concerning you, and if you are fought against, we will certainly help you, and Allah bears witness that they are most surely liars."}, {"id": 210765, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5138, "text": "Certainly if these are driven forth, they will not go forth with them, and if they are fought against, they will not help them, and even if they help-them, they will certainly turn (their) backs, then they shall not be helped."}, {"id": 210766, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5139, "text": "You are certainly greater in being feared in their hearts than Allah; that is because they are a people who do not understand"}, {"id": 210767, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5140, "text": "They will not fight against you in a body save in fortified towns or from behind walls; their fighting between them is severe, you may think them as one body, and their hearts are disunited; that is because they are a people who have no sense."}, {"id": 210768, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5141, "text": "Like those before them shortly; they tasted the evil result of their affair, and they shall have a painful punishment."}, {"id": 210769, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 547, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5142, "text": "Like the Shaitan when he says to man: Disbelieve, but when he disbelieves, he says: I am surely clear of you; surely I fear Allah, the Lord of the worlds."}, {"id": 210770, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 480, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5143, "text": "Therefore the end of both of them is that they are both in the fire to abide therein, and that is the reward of the unjust."}, {"id": 210771, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5144, "text": "O you who believe! be careful of (your duty to) Allah, and let every soul consider what it has sent on for the morrow, and be careful of (your duty to) Allah; surely Allah is Aware of what you do."}, {"id": 210772, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5145, "text": "And be not like those who forsook Allah, so He made them forsake their own souls: these it is that are the transgressors."}, {"id": 210773, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5146, "text": "Not alike are the inmates of the fire and the dwellers of the garden: the dwellers of the garden are they that are the achievers."}, {"id": 210774, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5147, "text": "Had We sent down this Quran on a mountain, you would certainly have seen it falling down, splitting asunder because of the fear of Allah, and We set forth these parables to men that they may reflect."}, {"id": 210775, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5148, "text": "He is Allah besides Whom there is no god; the Knower of the unseen and the seen; He is the Beneficent, the Merciful"}, {"id": 210776, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5149, "text": "He is Allah, besides Whom there is no god; the King, the Holy, the Giver of peace, the Granter of security, Guardian over all, the Mighty, the Supreme, the Possessor of every greatness Glory be to Allah from what they set up (with Him)."}, {"id": 210777, "identifier": "en.shakir", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 548, "ruku": 481, "hizb_quarter": 219, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5150, "text": "He is Allah the Creator, the Maker, the Fashioner; His are the most excellent names; whatever is in the heavens and the earth declares His glory; and He is the Mighty, the Wise."}]