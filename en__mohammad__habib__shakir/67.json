[{"id": 210869, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5242, "text": "Blessed is He in Whose hand is the kingdom, and He has power over all things,"}, {"id": 210870, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5243, "text": "Who created death and life that He may try you-- which of you is best in deeds; and He is the Mighty, the Forgiving,"}, {"id": 210871, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5244, "text": "Who created the seven heavens one above another; you see no incongruity in the creation of the Beneficent Allah; then look again, can you see any disorder?"}, {"id": 210872, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5245, "text": "Then turn back the eye again and again; your look shall come back to you confused while it is fatigued."}, {"id": 210873, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5246, "text": "And certainly We have adorned this lower heaven with lamps and We have made these missiles for the Shaitans, and We have prepared for them the chastisement of burning."}, {"id": 210874, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5247, "text": "And for those who disbelieve in their Lord is the punishment of hell, and evil is the resort."}, {"id": 210875, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5248, "text": "When they shall be cast therein, they shall hear a loud moaning of it as it heaves,"}, {"id": 210876, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5249, "text": "Almost bursting for fury. Whenever a group is cast into it, its keeper shall ask them: Did there not come to you a warner?"}, {"id": 210877, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5250, "text": "They shall say: Yea! indeed there came to us a warner, but we rejected (him) and said: Allah has not revealed anything, you are only in a great error."}, {"id": 210878, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5251, "text": "And they shall say: Had we but listened or pondered, we should not have been among the inmates of the burning fire."}, {"id": 210879, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5252, "text": "So they shall acknowledge their sins, but far will be (forgiveness) from the inmates of the burning fire."}, {"id": 210880, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5253, "text": "(As for) those who fear their Lord in secret, they shall surely have forgiveness and a great reward."}, {"id": 210881, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5254, "text": "And conceal your word or manifest it; surely He is Cognizant of what is in the hearts."}, {"id": 210882, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5255, "text": "Does He not know, Who created? And He is the Knower of the subtleties, the Aware."}, {"id": 210883, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5256, "text": "He it is Who made the earth smooth for you, therefore go about in the spacious sides thereof, and eat of His sustenance, and to Him is the return after death."}, {"id": 210884, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5257, "text": "Are you secure of those in the heaven that He should not make the earth to swallow you up? Then lo! it shall be in a state of commotion."}, {"id": 210885, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5258, "text": "Or are you secure of those in the heaven that He should not send down upon you a punishment? Then shall you know how was My warning."}, {"id": 210886, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5259, "text": "And certainly those before them rejected (the truth), then how was My disapproval."}, {"id": 210887, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5260, "text": "Have they not seen the birds above them expanding (their wings) and contracting (them)? What is it that withholds them save the Beneficent Allah? Surely He sees everything."}, {"id": 210888, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5261, "text": "Or who is it that will be a host for you to assist you besides the Beneficent Allah? The unbelievers are only in deception."}, {"id": 210889, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5262, "text": "Or who is it that will give you sustenance if He should withhold His sustenance? Nay! they persist in disdain and aversion."}, {"id": 210890, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5263, "text": "What! is he who goes prone upon his face better guided or he who walks upright upon a straight path?"}, {"id": 210824, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5264, "text": "Say: He it is Who brought you into being and made for you the ears and the eyes and the hearts: little is it that you give thanks."}, {"id": 210891, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5265, "text": "Say: He it is Who multiplied you in the earth and to Him you shall be gathered."}, {"id": 210892, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5266, "text": "And they say: When shall this threat be (executed) if you are truthful?"}, {"id": 210893, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5267, "text": "Say: The knowledge (thereof is only with Allah and I am only a plain warner."}, {"id": 210894, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5268, "text": "But when they shall see it nigh, the faces of those who disbelieve shall be sorry, and it shall be said; This is that which you used to call for."}, {"id": 210895, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5269, "text": "Say: Have you considered if Allah should destroy me and those with me-- rather He will have mercy on us; yet who will protect the unbelievers from a painful punishment?"}, {"id": 210896, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5270, "text": "Say: He is the Beneficent Allah, we believe in Him and on Him do we rely, so you shall come to know who it is that is in clear error."}, {"id": 210897, "identifier": "en.shakir", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5271, "text": "Say: Have you considered if your water should go down, who is it then that will bring you flowing water?"}]