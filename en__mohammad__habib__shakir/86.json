[{"id": 211533, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5932, "text": "I swear by the heaven and the comer by night;"}, {"id": 211534, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5933, "text": "And what will make you know what the comer by night is?"}, {"id": 211535, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5934, "text": "The star of piercing brightness;"}, {"id": 211536, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5935, "text": "There is not a soul but over it is a keeper."}, {"id": 211537, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5936, "text": "So let man consider of what he is created:"}, {"id": 211538, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5937, "text": "He is created of water pouring forth,"}, {"id": 211539, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5938, "text": "Coming from between the back and the ribs."}, {"id": 211540, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5939, "text": "Most surely He is able to return him (to life)."}, {"id": 211541, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5940, "text": "On the day when hidden things shall be made manifest,"}, {"id": 211542, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5941, "text": "He shall have neither strength nor helper."}, {"id": 211543, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5942, "text": "I swear by the raingiving heavens,"}, {"id": 211544, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5943, "text": "And the earth splitting (with plants);"}, {"id": 211545, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5944, "text": "Most surely it is a decisive word,"}, {"id": 211546, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5945, "text": "And it is no joke."}, {"id": 211547, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Surely they will make a scheme,"}, {"id": 211548, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5947, "text": "And I (too) will make a scheme."}, {"id": 211549, "identifier": "en.shakir", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5948, "text": "So grant the unbelievers a respite: let them alone for a while."}]