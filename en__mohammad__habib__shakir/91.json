[{"id": 211640, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6044, "text": "I swear by the sun and its brilliance,"}, {"id": 211641, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6045, "text": "And the moon when it follows the sun,"}, {"id": 211642, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6046, "text": "And the day when it shows it,"}, {"id": 211643, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6047, "text": "And the night when it draws a veil over it,"}, {"id": 211644, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6048, "text": "And the heaven and Him Who made it,"}, {"id": 211645, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6049, "text": "And the earth and Him Who extended it,"}, {"id": 211646, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6050, "text": "And the soul and Him Who made it perfect,"}, {"id": 211647, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6051, "text": "Then He inspired it to understand what is right and wrong for it;"}, {"id": 211648, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6052, "text": "He will indeed be successful who purifies it,"}, {"id": 211649, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6053, "text": "And he will indeed fail who corrupts it."}, {"id": 211650, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6054, "text": "Samood gave the lie (to the truth) in their inordinacy,"}, {"id": 211651, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6055, "text": "When the most unfortunate of them broke forth with"}, {"id": 211652, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6056, "text": "So Allah's apostle said to them (Leave alone) Allah's she-camel, and (give) her (to) drink."}, {"id": 211653, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6057, "text": "But they called him a liar and slaughtered her, therefore their Lord crushed them for their sin and levelled them (with the ground)."}, {"id": 211654, "identifier": "en.shakir", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 6058, "text": "And He fears not its consequence."}]