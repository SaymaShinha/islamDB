[{"id": 210827, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5200, "text": "Whatever is in the heavens and whatever is in the earth declares the glory of Allah; to Him belongs the kingdom, and to Him is due (all) praise, and He has power over all things."}, {"id": 210828, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5201, "text": "He it is Who created you, but one of you is an unbeliever and another of you is a believer; and Allah sees what you do."}, {"id": 210829, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5202, "text": "He created the heavens and the earth with truth, and He formed you, then made goodly your forms, and to Him is the ultimate resort."}, {"id": 210830, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5203, "text": "He knows what is in the heavens and the earth, and He knows what you hide and what you manifest; and Allah is Cognizant of what is in the hearts."}, {"id": 210831, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5204, "text": "Has there not come to you the story of those who disbelieved before, then tasted the evil result of their conduct, and they had a painful punishment?"}, {"id": 210832, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5205, "text": "That is because there came to them their apostles with clear arguments, but they said: Shall mortals guide us? So they disbelieved and turned back, and Allah does not stand in need (of anything), and Allah is Self-sufficient, Praised."}, {"id": 210833, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5206, "text": "Those who disbelieve think that they shall never be raised. Say: Aye! by my Lord! you shall most certainly be raised, then you shall most certainly be informed of what you did; and that is easy to Allah."}, {"id": 210834, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5207, "text": "Therefore believe in Allah and His Apostle and the Light which We have revealed; and Allah is Aware of what you do."}, {"id": 210835, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5208, "text": "On the day that He will gather you for the day of gathering, that is the day of loss and gain; and whoever believes in Allah and does good, He will remove from him his evil and cause him to enter gardens beneath which rivers flow, to abide therein forever; that is the great achievement."}, {"id": 210836, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5209, "text": "And (as for) those who disbelieve and reject Our communications, they are the inmates of the fire, to abide therein and evil is the resort."}, {"id": 210837, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5210, "text": "No affliction comes about but by Allah's permission; and whoever believes in Allah, He guides aright his heart; and Allah is Cognizant of all things."}, {"id": 210838, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5211, "text": "And obey Allah and obey the Apostle, but if you turn back, then upon Our Apostle devolves only the clear delivery (of the message)."}, {"id": 210839, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5212, "text": "Allah, there is no god but He; and upon Allah, then, let the believers rely."}, {"id": 210840, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5213, "text": "O you who believe! surely from among your wives and your children there is an enemy to you; therefore beware of them; and if you pardon and forbear and forgive, then surely Allah is Forgiving, Merciful."}, {"id": 210841, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5214, "text": "Your possessions and your children are only a trial, and Allah it is with Whom is a great reward."}, {"id": 210842, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5215, "text": "Therefore be careful of (your duty to) Allah as much as you can, and hear and obey and spend, it is better for your souls; and whoever is saved from the greediness of his soul, these it is that are the successful."}, {"id": 210843, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5216, "text": "If you set apart for Allah a goodly portion, He will double it for you and forgive you; and Allah is the Multiplier (of rewards), Forbearing,"}, {"id": 210844, "identifier": "en.shakir", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5217, "text": "The Knower of the unseen and the seen, the Mighty, the Wise."}]