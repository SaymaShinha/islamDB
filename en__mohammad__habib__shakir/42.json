[{"id": 209918, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4273, "text": "Ha Mim."}, {"id": 209919, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4274, "text": "Ain Sin Qaf."}, {"id": 209920, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4275, "text": "Thus does Allah, the Mighty, the Wise, reveal to you, and (thus He revealed) to those before you."}, {"id": 209921, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4276, "text": "His is what is in the heavens and what is in the earth, and He is the High, the Great."}, {"id": 209922, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4277, "text": "The heavens may almost rend asunder from above them and the angels sing the praise of their Lord and ask forgiveness for those on earth; now surely Allah is the Forgiving, the Merciful."}, {"id": 209923, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4278, "text": "And (as for) those who take guardians besides Him, Allah watches over them, and you have not charge over them."}, {"id": 209925, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4279, "text": "And thus have We revealed to you an Arabic Quran, that you may warn the mother city and those around it, and that you may give warning of the day of gathering together wherein is no doubt; a party shall be in the garden and (another) party in the burning fire."}, {"id": 209926, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4280, "text": "And if Allah had pleased He would surely have made them a single community, but He makes whom He pleases enter into His mercy, and the unjust it is that shall have no guardian or helper."}, {"id": 209927, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4281, "text": "Or have they taken guardians besides Him? But Allah is the Guardian, and He gives life to the dead, and He has power over all things."}, {"id": 209928, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4282, "text": "And in whatever thing you disagree, the judgment thereof is (in) Allah's (hand); that is Allah, my Lord, on Him do I rely and to Him do I turn time after time."}, {"id": 209929, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4283, "text": "The Originator of the heavens and the earth; He made mates for you from among yourselves, and mates of the cattle too, multiplying you thereby; nothing like a likeness of Him; and He is the Hearing, the Seeing."}, {"id": 209930, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4284, "text": "His are the treasures of the heavens and the earth; He makes ample and straitens the means of subsistence for whom He pleases; surely He is Cognizant of all things."}, {"id": 209931, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4285, "text": "He has made plain to you of the religion what He enjoined upon Nuh and that which We have revealed to you and that which We enjoined upon Ibrahim and Musa and Isa that keep to obedience and be not divided therein; hard to the unbelievers is that which you call them to; Allah chooses for Himself whom He pleases, and guides to Himself him who turns (to Him), frequently."}, {"id": 209932, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4286, "text": "And they did not become divided until after knowledge had come to them out of envy among themselves; and had not a word gone forth from your Lord till an appointed term, certainly judgment would have been given between them; and those who were made to inherit the Book after them are most surely in disquieting doubt concerning it."}, {"id": 209933, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4287, "text": "To this then go on inviting, and go on steadfastly on the right way as you are commanded, and do not follow their low desires, and say: I believe in what Allah has revealed of the Book, and I am commanded to do justice between you: Allah is our Lord and your Lord; we shall have our deeds and you shall have your deeds; no plea need there be (now) between us and you: Allah will gather us together, and to Him is the return."}, {"id": 209934, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4288, "text": "And (as for) those who dispute about Allah after that obedience has been rendered to Him, their plea is null with their Lord, and upon them is wrath, and for them is severe punishment."}, {"id": 209935, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4289, "text": "Allah it is Who revealed the Book with truth, and the balance, and what shall make you know that haply the hour be nigh?"}, {"id": 209936, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4290, "text": "Those who do not believe in it would hasten it on, and those who believe are in fear from it, and they know that it is the truth. Now most surely those who dispute obstinately concerning the hour are in a great error."}, {"id": 209937, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4291, "text": "Allah is Benignant to His servants; He gives sustenance to whom He pleases, and He is the Strong, the Mighty."}, {"id": 209938, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4292, "text": "Whoever desires the gain of the hereafter, We will give him more of that gain; and whoever desires the gain of this world, We give him of it, and in the hereafter he has no portion."}, {"id": 209939, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4293, "text": "Or have they associates who have prescribed for them any religion that Allah does not sanction? And were it not for the word of judgment, decision would have certainly been given between them; and surely the unjust shall have a painful punishment."}, {"id": 209940, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4294, "text": "You will see the unjust fearing on account of what they have earned, and it must befall them; and those who believe and do good shall be in the meadows of the gardens; they shall have what they please with their Lord: that is the great grace."}, {"id": 209941, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4295, "text": "That is of which Allah gives the good news to His servants, (to) those who believe and do good deeds. Say: I do not ask of you any reward for it but love for my near relatives; and whoever earns good, We give him more of good therein; surely Allah is Forgiving, Grateful."}, {"id": 209942, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4296, "text": "Or do they say: He has forged a lie against Allah? But if Allah pleased, He would seal your heart; and Allah will blot out the falsehood and confirm the truth with His words; surely He is Cognizant of what is in the breasts."}, {"id": 209943, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4297, "text": "And He it is Who accepts repentance from His servants and pardons the evil deeds and He knows what you do;"}, {"id": 209944, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4298, "text": "And He answers those who believe and do good deeds, and gives them more out of His grace; and (as for) the unbelievers, they shall have a severe punishment."}, {"id": 209945, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4299, "text": "And if Allah should amplify the provision for His servants they would certainly revolt in the earth; but He sends it down according to a measure as He pleases; surely He is Aware of, Seeing, His servants."}, {"id": 209946, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4300, "text": "And He it is Who sends down the rain after they have despaired, and He unfolds His mercy; and He is the Guardian, the Praised One."}, {"id": 209947, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4301, "text": "And one of His signs is the creation of the heavens and the earth and what He has spread forth in both of them of living beings; and when He pleases He is all-powerful to gather them together."}, {"id": 209948, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4302, "text": "And whatever affliction befalls you, it is on account of what your hands have wrought, and (yet) He pardons most (of your faults)."}, {"id": 209949, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4303, "text": "And you cannot escape in the earth, and you shall not have a guardian or a helper besides Allah."}, {"id": 209950, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4304, "text": "And among His signs are the ships in the sea like mountains."}, {"id": 209951, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4305, "text": "If He pleases, He causes the wind to become still so that they lie motionless on its back; most surely there are signs in this for every patient, grateful one,"}, {"id": 209952, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4306, "text": "Or He may make them founder for what they have earned, and (even then) pardon most;"}, {"id": 209953, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4307, "text": "And (that) those who dispute about Our communications may know; there is no place of refuge for them."}, {"id": 209954, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4308, "text": "So whatever thing you are given, that is only a provision of this world's life, and what is with Allah is better and more lasting for those who believe and rely on their Lord."}, {"id": 209955, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4309, "text": "And those who. shun the great sins and indecencies, and whenever they are angry they forgive."}, {"id": 209956, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4310, "text": "And those who respond to their Lord and keep up prayer, and their rule is to take counsel among themselves, and who spend out of what We have given them."}, {"id": 209957, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4311, "text": "And those who, when great wrong afflicts them, defend themselves."}, {"id": 209958, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4312, "text": "And the recompense of evil is punishment like it, but whoever forgives and amends, he shall have his reward from Allah; surely He does not love the unjust."}, {"id": 209959, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4313, "text": "And whoever defends himself after his being oppressed, these it is against whom there is no way (to blame)."}, {"id": 209960, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4314, "text": "The way (to blame) is only against those who oppress men and revolt in the earth unjustly; these shall have a painful punishment."}, {"id": 209961, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4315, "text": "And whoever is patient and forgiving, these most surely are actions due to courage."}, {"id": 209962, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4316, "text": "And whomsoever Allah makes err, he has no guardian after Him; and you shall see the unjust, when they see the punishment, saying: Is there any way to return?"}, {"id": 209963, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4317, "text": "And you shall see them brought before it humbling themselves because of the abasements, looking with a faint glance. And those who believe shall say: Surely the losers are they who have lost themselves and their followers on the resurrection day. Now surely the iniquitous shall remain in lasting chastisement."}, {"id": 209964, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4318, "text": "And they shall have no friends to help them besides Allah; and-- whomsoever Allah makes err, he shall have no way."}, {"id": 209965, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4319, "text": "Hearken to your Lord before there comes the day from Allah for which there shall be no averting; you shall have no refuge on that day, nor shall it be yours to make a denial."}, {"id": 209966, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4320, "text": "But if they turn aside, We have not sent you as a watcher over them; on you is only to deliver (the message); and surely when We make man taste mercy from Us, he rejoices thereat; and if an evil afflicts them on account of what their hands have already done, then-surely man is ungrateful."}, {"id": 209967, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4321, "text": "Allah's is the kingdom of the heavens and the earth; He creates what He pleases; He grants to whom He pleases daughters and grants to whom He pleases sons."}, {"id": 209968, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4322, "text": "Or He makes them of both sorts, male and female; and He makes whom He pleases barren; surely He is the Knowing, the Powerful."}, {"id": 209969, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4323, "text": "And it is not for any mortal that Allah should speak to him except by revelation or from behind a veil, or by sending a messenger and revealing by His permission what He pleases; surely He is High, Wise."}, {"id": 209970, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 489, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4324, "text": "And thus did We reveal to you an inspired book by Our command. You did not know what the Book was, nor (what) the faith (was), but We made it a light, guiding thereby whom We please of Our servants; and most surely you show the way to the right path:"}, {"id": 209971, "identifier": "en.shakir", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 489, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4325, "text": "The path of Allah, Whose is whatsoever is in the heavens and whatsoever is in the earth; now surely to Allah do all affairs eventually come."}]