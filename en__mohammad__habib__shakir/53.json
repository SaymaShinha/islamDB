[{"id": 210425, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4785, "text": "I swear by the star when it goes down."}, {"id": 210426, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Your companion does not err, nor does he go astray;"}, {"id": 210427, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4787, "text": "Nor does he speak out of desire."}, {"id": 210428, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4788, "text": "It is naught but revelation that is revealed,"}, {"id": 210429, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4789, "text": "The Lord of Mighty Power has taught him,"}, {"id": 210430, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4790, "text": "The Lord of Strength; so he attained completion,"}, {"id": 210431, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4791, "text": "And he is in the highest part of the horizon."}, {"id": 210432, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4792, "text": "Then he drew near, then he bowed"}, {"id": 210433, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4793, "text": "So he was the measure of two bows or closer still."}, {"id": 210434, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4794, "text": "And He revealed to His servant what He revealed."}, {"id": 210435, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4795, "text": "The heart was not untrue in (making him see) what he saw."}, {"id": 210436, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4796, "text": "What! do you then dispute with him as to what he saw?"}, {"id": 210437, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4797, "text": "And certainly he saw him in another descent,"}, {"id": 210438, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4798, "text": "At the farthest lote-tree;"}, {"id": 210439, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4799, "text": "Near which is the garden, the place to be resorted to."}, {"id": 210440, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4800, "text": "When that which covers covered the lote-tree;"}, {"id": 210441, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4801, "text": "The eye did not turn aside, nor did it exceed the limit."}, {"id": 210442, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Certainly he saw of the greatest signs of his Lord."}, {"id": 210443, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Have you then considered the Lat and the Uzza,"}, {"id": 210444, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4804, "text": "And Manat, the third, the last?"}, {"id": 210445, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4805, "text": "What! for you the males and for Him the females!"}, {"id": 210446, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4806, "text": "This indeed is an unjust division!"}, {"id": 210447, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4807, "text": "They are naught but names which you have named, you and your fathers; Allah has not sent for them any authority. They follow naught but conjecture and the low desires which (their) souls incline to; and certainly the guidance has come to them from their Lord."}, {"id": 210448, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Or shall man have what he wishes?"}, {"id": 210449, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Nay! for Allah is the hereafter and the former (life)."}, {"id": 210450, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4810, "text": "And how many an angel is there in the heavens whose intercession does not avail at all except after Allah has given permission to whom He pleases and chooses."}, {"id": 210451, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Most surely they who do not believe in the hereafter name the angels with female names."}, {"id": 210452, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4812, "text": "And they have no knowledge of it; they do not follow anything but conjecture, and surely conjecture does not avail against the truth at all."}, {"id": 210453, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Therefore turn aside from him who turns his back upon Our reminder and does not desire anything but this world's life."}, {"id": 210454, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4814, "text": "That is their goal of knowledge; surely your Lord knows best him who goes astray from His path and He knows best him who follows the right direction."}, {"id": 210455, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4815, "text": "And Allah's is what is in the heavens and what is in the earth, that He may reward those who do evil according to what they do, and (that) He may reward those who do good with goodness."}, {"id": 210456, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Those who keep aloof from the great sins and the indecencies but the passing idea; surely your Lord is liberal in forgiving. He knows you best when He brings you forth from the earth and when you are embryos in the wombs of your mothers; therefore do not attribute purity to your souls; He knows him best who guards (against evil)."}, {"id": 210457, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Have you then seen him who turns his back?"}, {"id": 210458, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4818, "text": "And gives a little and (then) withholds."}, {"id": 210459, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Has he the knowledge of the unseen so that he can see?"}, {"id": 210460, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Or, has he not been informed of what is in the scriptures of Musa?"}, {"id": 210461, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4821, "text": "And (of) Ibrahim who fulfilled (the commandments):"}, {"id": 210462, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4822, "text": "That no bearer of burden shall bear the burden of another-"}, {"id": 210463, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4823, "text": "And that man shall have nothing but what he strives for-"}, {"id": 210464, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4824, "text": "And that his striving shall soon be seen-"}, {"id": 210465, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4825, "text": "Then shall he be rewarded for it with the fullest reward-"}, {"id": 210466, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4826, "text": "And that to your Lord is the goal-"}, {"id": 210467, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4827, "text": "And that He it is Who makes (men) laugh and makes (them) weep;"}, {"id": 210468, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4828, "text": "And that He it is Who causes death and gives life-"}, {"id": 210469, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4829, "text": "And that He created pairs, the male and the female"}, {"id": 210470, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4830, "text": "From the small seed when it is adapted"}, {"id": 210471, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4831, "text": "And that on Him is the bringing forth a second time;"}, {"id": 210472, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4832, "text": "And that He it is Who enriches and gives to hold;"}, {"id": 210473, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4833, "text": "And that He is the Lord of the Sirius;"}, {"id": 210474, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4834, "text": "And that He did destroy the Ad of old"}, {"id": 210475, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4835, "text": "And Samood, so He spared not"}, {"id": 210476, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4836, "text": "And the people of Nuh before; surely they were most unjust and inordinate;"}, {"id": 210477, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4837, "text": "And the overthrown cities did He overthrow,"}, {"id": 205796, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4838, "text": "So there covered them that which covered."}, {"id": 210412, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4839, "text": "Which of your Lord's benefits will you then dispute about?"}, {"id": 205797, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4840, "text": "This is a warner of the warners of old."}, {"id": 205794, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4841, "text": "The near event draws nigh."}, {"id": 205798, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4842, "text": "There shall be none besides Allah to remove it."}, {"id": 205800, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Do you then wonder at this announcement?"}, {"id": 205799, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4844, "text": "And will you laugh and not weep?"}, {"id": 205802, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4845, "text": "While you are indulging in varieties."}, {"id": 205803, "identifier": "en.shakir", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4846, "text": "So make obeisance to Allah and serve (Him)."}]