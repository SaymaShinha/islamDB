[{"id": 211119, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5496, "text": "O you who are clothed!"}, {"id": 211120, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5497, "text": "Arise and warn,"}, {"id": 211121, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5498, "text": "And your Lord do magnify,"}, {"id": 211122, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5499, "text": "And your garments do purify,"}, {"id": 211123, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5500, "text": "And uncleanness do shun,"}, {"id": 211124, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5501, "text": "And bestow not favors that you may receive again with increase,"}, {"id": 211125, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5502, "text": "And for the sake of your Lord, be patient."}, {"id": 211126, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5503, "text": "For when the trumpet is sounded,"}, {"id": 211127, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5504, "text": "That, at that time, shall be a difficult day,"}, {"id": 211128, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5505, "text": "For the unbelievers, anything but easy."}, {"id": 211129, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5506, "text": "Leave Me and him whom I created alone,"}, {"id": 211130, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5507, "text": "And give him vast riches,"}, {"id": 211131, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5508, "text": "And sons dwelling in his presence,"}, {"id": 211132, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5509, "text": "And I adjusted affairs for him adjustably;"}, {"id": 211133, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5510, "text": "And yet he desires that I should add more!"}, {"id": 211134, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5511, "text": "By no means! surely he offers opposition to Our communications."}, {"id": 211135, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5512, "text": "I will make a distressing punishment overtake him."}, {"id": 211136, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5513, "text": "Surely he reflected and guessed,"}, {"id": 211137, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5514, "text": "But may he be cursed how he plotted;"}, {"id": 211138, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5515, "text": "Again, may he be cursed how he plotted;"}, {"id": 211139, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5516, "text": "Then he looked,"}, {"id": 211140, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5517, "text": "Then he frowned and scowled,"}, {"id": 211141, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5518, "text": "Then he turned back and was big with pride,"}, {"id": 211142, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5519, "text": "~Then he said: This is naught but enchantment, narrated (from others);"}, {"id": 211143, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5520, "text": "This is naught but the word of a mortal."}, {"id": 211144, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5521, "text": "I will cast him into hell."}, {"id": 211145, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5522, "text": "And what will make you realize what hell is?"}, {"id": 211146, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5523, "text": "It leaves naught nor does it spare aught."}, {"id": 211147, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5524, "text": "It scorches the mortal."}, {"id": 211148, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5525, "text": "Over it are nineteen."}, {"id": 211149, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5526, "text": "And We have not made the wardens of the fire others than angels, and We have not made their number but as a trial for those who disbelieve, that those who have been given the book may be certain and those who believe may increase in faith, and those who have been given the book and the believers may not doubt, and that those in whose hearts is a disease and the unbelievers may say: What does Allah mean by this parable? Thus does Allah make err whom He pleases, and He guides whom He pleases, and none knows the hosts of your Lord but He Himself; and this is naught but a reminder to the mortals."}, {"id": 211150, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5527, "text": "Nay; I swear by the moon,"}, {"id": 211151, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5528, "text": "And the night when it departs,"}, {"id": 211152, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5529, "text": "And the daybreak when it shines;"}, {"id": 211153, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5530, "text": "Surely it (hell) is one of the gravest (misfortunes),"}, {"id": 211154, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5531, "text": "A warning to mortals,"}, {"id": 211155, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5532, "text": "To him among you who wishes to go forward or remain behind."}, {"id": 211156, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5533, "text": "Every soul is held in pledge for what it earns,"}, {"id": 211157, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5534, "text": "Except the people of the right hand,"}, {"id": 211158, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5535, "text": "In gardens, they shall ask each other"}, {"id": 211159, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5536, "text": "About the guilty:"}, {"id": 211160, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5537, "text": "What has brought you into hell?"}, {"id": 211161, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5538, "text": "They shall say: We were not of those who prayed;"}, {"id": 211162, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5539, "text": "And we used not to feed the poor;"}, {"id": 211163, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5540, "text": "And we used to enter into vain discourse with those who entered into vain discourses."}, {"id": 211164, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5541, "text": "And we used to call the day of judgment a lie;"}, {"id": 211165, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5542, "text": "Till death overtook us."}, {"id": 211166, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5543, "text": "So the intercession of intercessors shall not avail them."}, {"id": 211167, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5544, "text": "What is then the matter with them, that they turn away from the admonition"}, {"id": 211168, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5545, "text": "As if they were asses taking fright"}, {"id": 211169, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5546, "text": "That had fled from a lion?"}, {"id": 211170, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5547, "text": "Nay; every one of them desires that he may be given pages spread out;"}, {"id": 211171, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5548, "text": "Nay! but they do not fear the hereafter."}, {"id": 211172, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5549, "text": "Nay! it is surely an admonition."}, {"id": 211173, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5550, "text": "So whoever pleases may mind it."}, {"id": 211174, "identifier": "en.shakir", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5551, "text": "And they will not mind unless Allah please. He is worthy to be feared and worthy to forgive."}]