[{"id": 210153, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4511, "text": "Ha Mim."}, {"id": 210154, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4512, "text": "The revelation of the Book is from Allah, the Mighty, the Wise."}, {"id": 210155, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4513, "text": "We did not create the heavens and the earth and what is between them two save with truth and (for) an appointed term; and those who disbelieve turn aside from what they are warned of."}, {"id": 210156, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4514, "text": "Say: Have you considered what you call upon besides Allah? Show me what they have created of the earth, or have they a share in the heavens? Bring me a book before this or traces of knowledge, if you are truthful."}, {"id": 210157, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4515, "text": "And who is in greater error than he who calls besides Allah upon those that will not answer him till the day of resurrection and they are heedless of their call?"}, {"id": 210158, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4516, "text": "And when men are gathered together they shall be their enemies, and shall be deniers of their worshipping (them)."}, {"id": 210159, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4517, "text": "And when Our clear communications are recited to them, those who disbelieve say with regard to the truth when it comes to them: This is clear magic."}, {"id": 210160, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4518, "text": "Nay! they say: He has forged it. Say: If I have forged it, you do not control anything for me from Allah; He knows best what you utter concerning it; He is enough as a witness between me and you, and He is the Forgiving, the Merciful."}, {"id": 210161, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4519, "text": "Say: I am not the first of the apostles, and I do not know what will be done with me or with you: I do not follow anything but that which is revealed to me, and I am nothing but a plain warner."}, {"id": 210162, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4520, "text": "Say: Have you considered if it is from Allah, and you disbelieve in it, and a witness from among the children of Israel has borne witness of one like it, so he believed, while you are big with pride; surely Allah does not guide the unjust people."}, {"id": 210163, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4521, "text": "And those who disbelieve say concerning those who believe: If it had been a good, they would not have gone ahead of us therein. And as they do not seek to be rightly directed thereby, they say: It is an old lie."}, {"id": 210164, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4522, "text": "And before it the Book of Musa was a guide and a mercy: and this is a Book verifying (it) in the Arabic language that it may warn those who are unjust and as good news for the doers of good."}, {"id": 210165, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4523, "text": "Surely those who say, Our Lord is Allah, then they continue on the right way, they shall have no fear nor shall they grieve."}, {"id": 210166, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4524, "text": "These are the dwellers of the garden, abiding therein: a reward for what they did."}, {"id": 210167, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4525, "text": "And We have enjoined on man doing of good to his parents; with trouble did his mother bear him and with trouble did she bring him forth; and the bearing of him and the weaning of him was thirty months; until when he attains his maturity and reaches forty years, he says: My Lord! grant me that I may give thanks for Thy favor which Thou hast bestowed on me and on my parents, and that I may do good which pleases Thee and do good to me in respect of my offspring; surely I turn to Thee, and surely I am of those who submit."}, {"id": 210168, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4526, "text": "These are they from whom We accept the best of what they have done and pass over their evil deeds, among the dwellers of the garden; the promise of truth which they were promised."}, {"id": 210169, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4527, "text": "And he who says to his parents: Fie on you! do you threaten me that I shall be brought forth when generations have already passed away before me? And they both call for Allah's aid: Woe to you! believe, surely the promise of Allah is true. But he says: This is nothing but stories of the ancients."}, {"id": 210170, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4528, "text": "These are they against whom the word has proved true among nations of the jinn and the men that have already passed away before them; surely they are losers."}, {"id": 210171, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4529, "text": "And for all are degrees according to what they did, and that He may pay them back fully their deeds and they shall not be wronged."}, {"id": 210172, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4530, "text": "And on the day when those who disbelieve shall be brought before the fire: You did away with your good things in your life of the world and you enjoyed them for a while, so today you shall be rewarded with the punishment of abasement because you were unjustly proud in the land and because you transgressed."}, {"id": 210173, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4531, "text": "And mention the brother of Ad; when he warned his people in the sandy plains,-- and indeed warners came before him and after him-- saying Serve none but Allah; surely I fear for you the punishment of a grievous day."}, {"id": 210174, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4532, "text": "They said: Have you come to us to turn us away from our gods; then bring us what you threaten us with, if you are of the truthful ones."}, {"id": 210175, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4533, "text": "He said: The knowledge is only with Allah, and I deliver to you the message with which I am sent, but I see you are a people who are ignorant."}, {"id": 210176, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4534, "text": "So when they saw it as a cloud appearing in the sky advancing towards their valleys, they said: This is a cloud which will give us rain. Nay! it is what you sought to hasten on, a blast of wind in which is a painful punishment,"}, {"id": 210177, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4535, "text": "Destroying everything by the command of its Lord, so they became such that naught could be seen except their dwellings. Thus do We reward the guilty people."}, {"id": 210178, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4536, "text": "And certainly We had established them in what We have not established you in, and We had given-- them ears and eyes and hearts, but neither their ears, nor their eyes, nor their hearts availed them aught, since they denied the communications of Allah, and that which they mocked encompassed them."}, {"id": 210179, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4537, "text": "And certainly We destroyed the towns which are around you, and We repeat the communications that they might turn."}, {"id": 210180, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4538, "text": "Why did not then those help them whom they took for gods besides Allah to draw (them) nigh (to Him)? Nay! they were lost to them; and this was their lie and what they forged."}, {"id": 210181, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4539, "text": "And when We turned towards you a party of the jinn who listened to the Quran; so when they came to it, they said: Be silent; then when it was finished, they turned back to their people warning (them)."}, {"id": 210182, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4540, "text": "They said: O our people! we have listened to a Book revealed after Musa verifying that which is before it, guiding to the truth and to a right path:"}, {"id": 210183, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4541, "text": "O our people! accept the Divine caller and believe in Him, He will forgive you of your faults and protect you from a painful punishment."}, {"id": 210184, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4542, "text": "And whoever does not accept the-Divine caller, he shall not escape in the earth and he shall not have guardians besides Him, these are in manifest error."}, {"id": 210185, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4543, "text": "Have they not considered that Allah, Who created the heavens and the earth and was not tired by their creation, is able to give life to the dead? Aye! He has surely power over all things."}, {"id": 210186, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4544, "text": "And on the day when those who disbelieve shall be brought before the fire: Is it not true? They shall say: Aye! by our Lord! He will say: Then taste the punishment, because you disbelieved."}, {"id": 210187, "identifier": "en.shakir", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 4545, "text": "Therefore bear up patiently as did the apostles endowed with constancy bear up with patience and do not seek to hasten for them (their doom). On the day that they shall see what they are promised they shall be as if they had not tarried save an hour of the day. A sufficient exposition! Shall then any be destroyed save the transgressing people?"}]