[{"id": 211371, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5759, "text": "He frowned and turned (his) back,"}, {"id": 211372, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5760, "text": "Because there came to him the blind man."}, {"id": 211373, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5761, "text": "And what would make you know that he would purify himself,"}, {"id": 211374, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5762, "text": "Or become reminded so that the reminder should profit him?"}, {"id": 211375, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5763, "text": "As for him who considers himself free from need (of you),"}, {"id": 211376, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5764, "text": "To him do you address yourself."}, {"id": 211377, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5765, "text": "And no blame is on you if he would not purify himself"}, {"id": 211378, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5766, "text": "And as to him who comes to you striving hard,"}, {"id": 211379, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5767, "text": "And he fears,"}, {"id": 211380, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5768, "text": "From him will you divert yourself."}, {"id": 211381, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5769, "text": "Nay! surely it is an admonishment."}, {"id": 211382, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5770, "text": "So let him who pleases mind it."}, {"id": 211383, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5771, "text": "In honored books,"}, {"id": 211384, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5772, "text": "Exalted, purified,"}, {"id": 211385, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5773, "text": "In the hands of scribes"}, {"id": 211386, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5774, "text": "Noble, virtuous."}, {"id": 211387, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5775, "text": "Cursed be man! how ungrateful is he!"}, {"id": 211388, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5776, "text": "Of what thing did He create him?"}, {"id": 211389, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5777, "text": "Of a small seed; He created him, then He made him according to a measure,"}, {"id": 211390, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5778, "text": "Then (as for) the way-- He has made it easy (for him)"}, {"id": 211391, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5779, "text": "Then He causes him to die, then assigns to him a grave,"}, {"id": 211392, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5780, "text": "Then when He pleases, He will raise him to life again."}, {"id": 211393, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5781, "text": "Nay; but he has not done what He bade him."}, {"id": 211394, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5782, "text": "Then let man look to his food,"}, {"id": 211395, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5783, "text": "That We pour down the water, pouring (it) down in abundance,"}, {"id": 211396, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5784, "text": "Then We cleave the earth, cleaving (it) asunder,"}, {"id": 205823, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5785, "text": "Then We cause to grow therein the grain,"}, {"id": 205822, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5786, "text": "And grapes and clover,"}, {"id": 205825, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5787, "text": "And the olive and the palm,"}, {"id": 205824, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5788, "text": "And thick gardens,"}, {"id": 205827, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5789, "text": "And fruits and herbage"}, {"id": 205828, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5790, "text": "A provision for you and for your cattle."}, {"id": 205826, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5791, "text": "But when the deafening cry comes,"}, {"id": 205830, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5792, "text": "The day on which a man shall fly from his brother,"}, {"id": 205831, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5793, "text": "And his mother and his father,"}, {"id": 205829, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5794, "text": "And his spouse and his son--"}, {"id": 211397, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5795, "text": "Every man of them shall on that day have an affair which will occupy him."}, {"id": 211398, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5796, "text": "(Many) faces on that day shall be bright,"}, {"id": 211399, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5797, "text": "Laughing, joyous."}, {"id": 211400, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5798, "text": "And (many) faces on that day, on them shall be dust,"}, {"id": 211401, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5799, "text": "Darkness shall cover them."}, {"id": 211402, "identifier": "en.shakir", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Shakir", "transelator_en_name": "Mohammad Habib Shakir", "format": "text", "type": "translation", "ayah_number": 5800, "text": "These are they who are unbelievers, the wicked."}]