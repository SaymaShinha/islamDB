[{"id": 361663, "identifier": "id.muntakhab", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Quraish Shihab", "transelator_en_name": "Muhammad Quraish Shihab et al.", "format": "text", "type": "translation", "ayah_number": 6226, "text": "[[113 ~ AL-FALAQ (WAKTU SUBUH) Pendahuluan: Makkiyyah, 5 ayat ~ Surat ini memerintahkan Nabi Muhammad saw. untuk kembali pada Tuhannya dan meminta perlindungan dari kejahatan setiap makhluk yang mempunyai kejahatan; dari kejahatan malam ketika mulai gelap, yang membuat jiwa menjadi takut dan tidak mampu untuk mencegahnya; dari segala kejahatan orang yang berusaha untuk melepaskan hubungan setiap manusia; dan dari kejahatan orang yang dengki yang mengharapkan hilangnya nikmat yang diberikan Allah kepada hamba-Nya.]] Katakan, aku berlindung kepada Tuhan (yang menguasai) waktu subuh, dengan hilangnya waktu malam."}, {"id": 361664, "identifier": "id.muntakhab", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Quraish Shihab", "transelator_en_name": "Muhammad Quraish Shihab et al.", "format": "text", "type": "translation", "ayah_number": 6227, "text": "Dari kejahatan setiap makhluk yang berniat jahat, yang tidak dapat mencegahnya selain Tuhan Sang Maha Penguasa atas segala perkara."}, {"id": 361665, "identifier": "id.muntakhab", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Quraish Shihab", "transelator_en_name": "Muhammad Quraish Shihab et al.", "format": "text", "type": "translation", "ayah_number": 6228, "text": "Dari kejahatan malam ketika semakin gelap."}, {"id": 361666, "identifier": "id.muntakhab", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Quraish Shihab", "transelator_en_name": "Muhammad Quraish Shihab et al.", "format": "text", "type": "translation", "ayah_number": 6229, "text": "Dari kejahatan yang berusaha membuat kerusakan diantara manusia."}, {"id": 361667, "identifier": "id.muntakhab", "surah_number": 113, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u0644\u0642", "surah_en_name": "Al-Falaq", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 604, "ruku": 555, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Quraish Shihab", "transelator_en_name": "Muhammad Quraish Shihab et al.", "format": "text", "type": "translation", "ayah_number": 6230, "text": "Dari kejahatan orang yang dengki yang mengharapkan hilangnya nikmat dari orang selain dirinya."}]