[{"id": 372559, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Per la stella quando tramonta."}, {"id": 372560, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Il vostro compagno non \u00e8 traviato, non \u00e8 in errore;"}, {"id": 372561, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4787, "text": "e neppure parla d'impulso:"}, {"id": 372562, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4788, "text": "non \u00e8 che una Rivelazione ispirata."}, {"id": 372563, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4789, "text": "Gliel'ha insegnata un fortissimo,"}, {"id": 372564, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4790, "text": "di saggezza dotato, che compostamente comparve:"}, {"id": 372565, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4791, "text": "[si trovava] all'orizzonte pi\u00f9 elevato,"}, {"id": 372566, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4792, "text": "poi s'avvicin\u00f2 scendendo ancora pi\u00f9 in basso,"}, {"id": 372567, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4793, "text": "[finch\u00e9] fu alla distanza di due archi o meno."}, {"id": 372568, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4794, "text": "Rivel\u00f2 al Suo servo quello che rivel\u00f2."}, {"id": 372569, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Il cuore non ment\u00ec su quel che vide."}, {"id": 372570, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Vorreste dunque polemizzare su quel che vide?"}, {"id": 372571, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4797, "text": "E invero lo vide in un'altra discesa,"}, {"id": 372572, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4798, "text": "vicino al Loto del limite,"}, {"id": 367930, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4799, "text": "presso il quale c'\u00e8 il Giardino di Ma'wa,"}, {"id": 372573, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4800, "text": "nel momento in cui il Loto era coperto da quel che lo copriva."}, {"id": 372574, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4801, "text": "Non distolse lo sguardo e non and\u00f2 oltre."}, {"id": 372575, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Vide davvero i segni pi\u00f9 grandi del suo Signore."}, {"id": 372576, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Cosa ne dite di al-L\u00e2t e al-Uzz\u00e2,"}, {"id": 372577, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4804, "text": "e di Man\u00e2t, la terza ?"}, {"id": 372578, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Avrete voi il maschio e Lui la femmina ?"}, {"id": 372579, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Che ingiusta spartizione!"}, {"id": 372581, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Non sono altro che nomi menzionati da voi e dai vostri antenati, a proposito dei quali Allah non fece scendere nessuna autorit\u00e0. Essi si abbandonano alle congetture e a quello che affascina gli animi loro, nonostante sia giunta loro una guida del loro Signore."}, {"id": 372582, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4808, "text": "L'uomo otterr\u00e0 forse tutto quel che desidera?"}, {"id": 372583, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4809, "text": "Appartengono ad Allah l'altra vita e questa vita."}, {"id": 372584, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4810, "text": "Quanti angeli nel cielo, la cui intercessione sar\u00e0 vana finch\u00e9 Allah non l'abbia permessa, e in favore di chi Egli voglia e di chi Gli aggrada."}, {"id": 372585, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Invero quelli che non credono nell'altra vita danno agli angeli nomi femminili,"}, {"id": 372586, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4812, "text": "mentre non posseggono alcuna scienza: si abbandonano alle congetture, ma la congettura non pu\u00f2 nulla contro la verit\u00e0."}, {"id": 372587, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Dunque non ti curare di chi volge le spalle al Nostro monito e non brama che la vita terrena."}, {"id": 372588, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Ecco tutta la portata della loro scienza: in verit\u00e0 il tuo Signore ben conosce chi si allontana dalla Sua via e conosce chi \u00e8 ben diretto."}, {"id": 372589, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Appartiene ad Allah tutto quello che c'\u00e8 nei cieli e tutto quello che c'\u00e8 sulla terra, s\u00ec che compensi coloro che agiscono male per ci\u00f2 che avranno fatto e compensi coloro che agiscono bene con quanto ci sia di pi\u00f9 bello."}, {"id": 372590, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Essi sono coloro che evitano i peccati pi\u00f9 gravi e le perversit\u00e0 e non commettono che le colpe pi\u00f9 lievi. Invero il perdono del tuo Signore \u00e8 immenso. Egli vi conosce meglio [di chiunque altro] quando vi ha prodotti dalla terra e quando eravate ancora embrioni nel ventre delle vostre madri. Non vantatevi di essere puri : Egli conosce meglio [di chiunque altro] coloro che[Lo] temono."}, {"id": 372591, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Hai visto colui che ti volge le spalle,"}, {"id": 372592, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4818, "text": "che d\u00e0 poco e poi smette [di dare]?"}, {"id": 372593, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Possiede conoscenza dell'invisibile e riesce a vederlo?"}, {"id": 372594, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Non \u00e8 stato informato di quello che contengono i fogli di Mos\u00e8"}, {"id": 372595, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4821, "text": "e quelli di Abramo, uomo fedele?"}, {"id": 372596, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4822, "text": "Che nessuno porter\u00e0 il fardello di un altro,"}, {"id": 372597, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4823, "text": "e che invero, l'uomo non ottiene che il [frutto dei] suoi sforzi ;"}, {"id": 372598, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4824, "text": "e che il suo sforzo gli sar\u00e0 presentato [nel Giorno del Giudizio]"}, {"id": 372599, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4825, "text": "e gli sar\u00e0 dato pieno compenso,"}, {"id": 372600, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4826, "text": "e che in verit\u00e0 tutto conduce verso il tuo Signore,"}, {"id": 372601, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4827, "text": "e che Egli \u00e8 Colui Che fa ridere e fa piangere,"}, {"id": 372602, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4828, "text": "e che Egli \u00e8 Colui Che d\u00e0 la vita e d\u00e0 la morte,"}, {"id": 372603, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4829, "text": "e che Egli \u00e8 Colui Che ha creato i due generi, il maschio e la femmina,"}, {"id": 367926, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4830, "text": "da una goccia di sperma quand'\u00e8 eiaculata,"}, {"id": 367934, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4831, "text": "e che a Lui incombe l'altra creazione,"}, {"id": 367940, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4832, "text": "e che invero \u00e8 Lui Che arricchisce e provvede,"}, {"id": 367936, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4833, "text": "e che \u00e8 Lui il Signore di Sirio,"}, {"id": 367937, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4834, "text": "e che Lui annient\u00f2 gli antichi \u0091\u00c2d,"}, {"id": 367938, "identifier": "it.piccardo", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4835, "text": "e i Tham\u00fbd, s\u00ec che non ne rimase nessuno,"}, {"id": 372604, "identifier": "it.piccardo", "surah_number": 53, "surah