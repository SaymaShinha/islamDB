[{"id": 373086, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5324, "text": "L'Inevitabile!"}, {"id": 373087, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5325, "text": "Cos'\u00e8 l'Inevitabile?"}, {"id": 373088, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5326, "text": "Chi mai ti dir\u00e0 che cos'\u00e8 l'Inevitabile?"}, {"id": 373089, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5327, "text": "I Tham\u00fbd e gli \u0091\u00c2d tacciarono di menzogna il cataclisma,"}, {"id": 373090, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5328, "text": "ma i Tham\u00fbd furono sterminati da un Grido tremendo,"}, {"id": 373091, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5329, "text": "mentre gli \u0091\u00c2d furono distrutti da un vento mugghiante, furioso,"}, {"id": 373092, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5330, "text": "che Allah scaten\u00f2 contro di loro per sette notti e otto giorni consecutivi. Allora avresti visto quella gente riversa in terra come tronchi cavi di palma."}, {"id": 373093, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5331, "text": "Ne vedi forse uno sopravvissuto?"}, {"id": 373094, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5332, "text": "Anche Faraone e coloro che lo precedettero e le [citt\u00e0] stravolte commisero peccati."}, {"id": 373095, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5333, "text": "Disobbedirono al Messaggero del loro Signore ed Egli li afferr\u00f2 con estrema energia."}, {"id": 373096, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5334, "text": "Quando l'acqua dilag\u00f2 vi caricammo sull'Arca,"}, {"id": 373097, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5335, "text": "affinch\u00e9 essa diventasse un monito ricordato da ogni orecchio che ricorda."}, {"id": 373098, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5336, "text": "Quando sar\u00e0 soffiato nel Corno la prima volta,"}, {"id": 373099, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5337, "text": "e la terra e le montagne saranno sollevate e polverizzate in un sol colpo,"}, {"id": 373100, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5338, "text": "in quel Giorno avverr\u00e0 l'Evento,"}, {"id": 373101, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5339, "text": "e si spaccher\u00e0 il cielo, cos\u00ec fragile in quel Giorno."}, {"id": 373102, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5340, "text": "Staranno gli angeli ai suoi orizzonti e in quel Giorno otto [di loro] porteranno il Trono del tuo Signore."}, {"id": 373103, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5341, "text": "Sfilerete [davanti ad Allah] in quel Giorno e niente di quel che celate potr\u00e0 essere nascosto."}, {"id": 373104, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5342, "text": "Chi avr\u00e0 ricevuto il libro nella mano destra, dir\u00e0: \u201cPrendete, leggete il mio libro."}, {"id": 373105, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5343, "text": "Invero sapevo che avrei avuto il mio rendiconto!\u201d."}, {"id": 373106, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5344, "text": "Egli avr\u00e0 piacevole esistenza"}, {"id": 373107, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5345, "text": "in un Giardino elevato,"}, {"id": 373108, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5346, "text": "i cui frutti saranno a portata di mano."}, {"id": 373109, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5347, "text": "\u201cMangiate e bevete in pace, \u00e8 il premio per quel che avete fatto nei giorni passati.\u201d"}, {"id": 373110, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5348, "text": "Chi invece avr\u00e0 ricevuto il suo libro nella mano sinistra, dir\u00e0: \u201cAhim\u00e8, se solo non mi fosse stato consegnato il mio libro"}, {"id": 373111, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5349, "text": "e non avessi conosciuto il mio rendiconto!"}, {"id": 373112, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5350, "text": "Ahim\u00e8, quanto vorrei che essa fosse stata definitiva!"}, {"id": 373113, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5351, "text": "Quel che possedevo non mi ha giovato affatto."}, {"id": 373114, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5352, "text": "Ho perso il mio potere\u201d."}, {"id": 373115, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5353, "text": "[Diranno:] \u201cAfferratelo e mettetelo nei ceppi,"}, {"id": 373116, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5354, "text": "quindi sia precipitato nella Fornace,"}, {"id": 373117, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5355, "text": "e poi legatelo con una catena di settanta cubiti."}, {"id": 373118, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5356, "text": "Non credeva in Allah, il Supremo,"}, {"id": 373119, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5357, "text": "e non esortava a nutrire il povero."}, {"id": 373120, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5358, "text": "Oggi non avr\u00e0 qui nessun amico sincero,"}, {"id": 373121, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5359, "text": "n\u00e9 altro cibo che sanie"}, {"id": 373122, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5360, "text": "che solo i colpevoli mangeranno."}, {"id": 373123, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5361, "text": "Lo giuro per quel che vedete,"}, {"id": 373124, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5362, "text": "e per quel che non vedete!\u201d."}, {"id": 373125, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5363, "text": "Questa \u00e8 in verit\u00e0 la parola di un Messaggero nobilissimo;"}, {"id": 373126, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5364, "text": "non \u00e8 la parola di un poeta - [credetelo] per quanto poco crediate-"}, {"id": 373127, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5365, "text": "e neanche la parola di un indovino _ per quanto poco riflettiate!"}, {"id": 373128, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5366, "text": "\u00c8 una Rivelazione venuta dal Signore dei mondi."}, {"id": 373129, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5367, "text": "Se [Muhammad] Ci avesse mendacemente attribuito qualche discorso,"}, {"id": 373130, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5368, "text": "lo avremmo certo afferrato per la mano destra,"}, {"id": 373131, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5369, "text": "e quindi gli avremmo reciso l'aorta"}, {"id": 373132, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5370, "text": "e nessuno di voi avrebbe potuto impedirCelo."}, {"id": 373133, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5371, "text": "In verit\u00e0 questo \u00e8 un Monito per i timorati [di Allah]!"}, {"id": 373134, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5372, "text": "Sappiamo che tra voi c'\u00e8 chi lo tratta da bugiardo;"}, {"id": 373135, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5373, "text": "in verit\u00e0 ci\u00f2 sar\u00e0 un rimpianto per i miscredenti;"}, {"id": 373136, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5374, "text": "questa \u00e8 l'assoluta certezza."}, {"id": 373137, "identifier": "it.piccardo", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 5375, "text": "Allora glorifica il Nome del tuo Signore, il Supremo."}]