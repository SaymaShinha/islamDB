[{"id": 372614, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4847, "text": "L'Ora si avvicina e la luna si spacca."}, {"id": 372615, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Se vedono un segno, si sottraggono e dicono: \u201c\u00c8 una magia continua!\u201d."}, {"id": 372616, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4849, "text": "Tacciano di menzogna e seguono le loro passioni, ma ogni Decreto \u00e8 prefissato."}, {"id": 372617, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4850, "text": "Certamente sono giunte a loro storie che dovrebbero dissuaderli [dal male],"}, {"id": 372618, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4851, "text": "consolidata saggezza. Ma gli avvertimenti non giovano [loro]."}, {"id": 372619, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Distogliti da loro [o Muhammad]. Il Giorno in cui l'Araldo chiamer\u00e0 a qualcosa di orribile,"}, {"id": 372620, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4853, "text": "usciranno dalle tombe con gli occhi bassi, come locuste disperse"}, {"id": 372621, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4854, "text": "e si precipiteranno impauriti verso l'Araldo. Diranno i miscredenti: \u201cEcco un Giorno difficile\u201d."}, {"id": 372622, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4855, "text": "Prima di loro il popolo di No\u00e8 gi\u00e0 tacci\u00f2 di menzogna. Tacciarono di menzogna il Nostro servo e dissero: \u201c\u00c8 un pazzo\u201d. Lo diffidarono [dal predicare]."}, {"id": 372623, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4856, "text": "Invoc\u00f2 il suo Signore: \u201cSono sopraffatto: fa' trionfare la Tua causa\u201d."}, {"id": 372624, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4857, "text": "Spalancammo le porte del cielo ad un'acqua torrenziale,"}, {"id": 372625, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4858, "text": "e da tutta la terra scaturirono sorgenti e le acque si mescolarono in un ordine prestabilito."}, {"id": 372626, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4859, "text": "E lo portammo su [quella fatta di] tavole e chiodi."}, {"id": 372627, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4860, "text": "Navig\u00f2 sotto i Nostri occhi: fu il compenso per colui che era stato rinnegato."}, {"id": 372628, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4861, "text": "La lasciammo come segno. C'\u00e8 qualcuno che se ne ricorda?"}, {"id": 372629, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4862, "text": "Quale fu il Mio castigo, quali i Miei moniti !"}, {"id": 372630, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4863, "text": "Invero abbiamo reso facile il Corano, che vi servisse da Monito. C'\u00e8 qualcuno che rifletta [su di esso]?"}, {"id": 372631, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4864, "text": "Gli \u0091\u00c2d tacciarono di menzogna. Quale fu il Mio castigo, quali i Miei moniti!"}, {"id": 372632, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4865, "text": "Scatenammo contro di loro un vento tempestoso, durante un giorno nefasto e interminabile;"}, {"id": 372633, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4866, "text": "strappava gli uomini come fossero tronchi di palme sradicate."}, {"id": 372634, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4867, "text": "Quale fu il Mio castigo! Quali i Miei moniti!"}, {"id": 372635, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4868, "text": "Invero abbiamo reso facile il Corano, che vi servisse da Monito. C'\u00e8 qualcuno che rifletta [su di esso]?"}, {"id": 372636, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4869, "text": "I Tham\u00fbd tacciarono di menzogna gli ammonimenti;"}, {"id": 372637, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4870, "text": "dissero: \u201cDovremmo seguire un solo mortale fra di noi? Davvero in tal caso saremmo traviati e folli!"}, {"id": 372638, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4871, "text": "Ma come? Il Monito \u00e8 stato affidato solo a lui tra [tutti] noi? \u00c8 un gran bugiardo, uno sfrontato!\u201d."}, {"id": 372639, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4872, "text": "Domani sapranno chi \u00e8 il gran bugiardo, lo sfrontato!"}, {"id": 372640, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4873, "text": "Manderemo loro la cammella come tentazione: osservali e sii paziente."}, {"id": 372641, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4874, "text": "Informali che devono dividere l'acqua [con la cammella]: ognuno il suo turno per bere."}, {"id": 372642, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4875, "text": "Chiamarono uno dei loro che impugn\u00f2 [la spada] e le tagli\u00f2 i garretti."}, {"id": 372643, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4876, "text": "Quale fu il Mio castigo, quali i Miei moniti!"}, {"id": 372644, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4877, "text": "Mandammo contro di loro un solo Grido, rimasero come erba disseccata per gli stabbi."}, {"id": 372645, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4878, "text": "Invero abbiamo reso facile il Corano, che vi servisse da Monito. C'e qualcuno che rifletta [su di esso]?"}, {"id": 372646, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Il popolo di Lot tacci\u00f2 di menzogna i moniti."}, {"id": 372647, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4880, "text": "Mandammo contro di loro una tempesta di pietre, eccezion fatta per la famiglia di Lot che salvammo sul far dell'alba,"}, {"id": 372648, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4881, "text": "favore da parte Nostra: cos\u00ec compensiamo chi Ci \u00e8 riconoscente."}, {"id": 372649, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4882, "text": "Egli li aveva avvisati del Nostro castigo, ma dubitarono di questi moniti."}, {"id": 372650, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4883, "text": "Pretendevano i suoi ospiti, ma accecammo i loro occhi [dicendo]: \u201cProvate allora il Mio castigo e [la veridicit\u00e0 de] i Miei moniti!\u201d."}, {"id": 372651, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4884, "text": "E invero di buon'ora li sorprese un durevole castigo."}, {"id": 372652, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4885, "text": "Provate allora il Mio castigo e [la veridicit\u00e0 de]i Miei moniti!"}, {"id": 372653, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4886, "text": "Invero abbiamo reso facile il Corano, che vi servisse da monito. C'e qualcuno che rifletta [su di esso]?"}, {"id": 372654, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4887, "text": "E invero giunsero ammonimenti alla gente di Faraone."}, {"id": 372655, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4888, "text": "Smentirono tutti quanti i Nostri segni, perci\u00f2 Li afferrammo con la presa di un possente, onnipotente."}, {"id": 372656, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4889, "text": "I vostri miscredenti [o meccani] sono migliori di quelli? Ci son forse nelle Scritture delle immunit\u00e0 a vostro favore?"}, {"id": 372657, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4890, "text": "Oppure diranno: \u201cSiamo una moltitudine capace di vincere\u201d."}, {"id": 372658, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4891, "text": "Presto sar\u00e0 dispersa la moltitudine e volgeranno in fuga."}, {"id": 372659, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4892, "text": "Sar\u00e0 piuttosto l'Ora il loro appuntamento. L'Ora sar\u00e0 pi\u00f9 atroce e pi\u00f9 amara."}, {"id": 372660, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4893, "text": "In verit\u00e0 i malvagi sono nello smarrimento e nella follia."}, {"id": 372661, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4894, "text": "Il Giorno in cui saranno trascinati sui loro volti fino al Fuoco [sar\u00e0 detto loro]: \u201cGustate il contatto del Calore che brucia!\u201d."}, {"id": 372662, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4895, "text": "Ogni cosa creammo in giusta misura,"}, {"id": 372663, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4896, "text": "e il Nostro ordine \u00e8 una sola parola, [istantaneo] come battito di ciglia."}, {"id": 372664, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4897, "text": "Invero gi\u00e0 annientammo faziosi della vostra specie. C'\u00e8 forse qualcuno che rifletta in proposito?"}, {"id": 367939, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4898, "text": "Tutto quel che fecero \u00e8 nei registri:"}, {"id": 367935, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4899, "text": "ogni cosa piccola o grande vi \u00e8 segnata."}, {"id": 367941, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4900, "text": "I timorati saranno tra Giardini e ruscelli,"}, {"id": 372665, "identifier": "it.piccardo", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 4901, "text": "in un luogo di verit\u00e0, presso un Re onnipotente."}]