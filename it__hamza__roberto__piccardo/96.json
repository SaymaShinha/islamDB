[{"id": 373853, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6107, "text": "Leggi! In nome del tuo Signore che ha creato,"}, {"id": 373854, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6108, "text": "ha creato l'uomo da un'aderenza."}, {"id": 373855, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6109, "text": "Leggi, ch\u00e9 il tuo Signore \u00e8 il Generosissimo,"}, {"id": 373856, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6110, "text": "Colui Che ha insegnato mediante il calamo,"}, {"id": 373857, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6111, "text": "che ha insegnato all'uomo quello che non sapeva."}, {"id": 373858, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Invece no! Invero l'uomo si ribella,"}, {"id": 373859, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6113, "text": "appena ritiene di bastare a se stesso."}, {"id": 373860, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6114, "text": "In verit\u00e0 il ritorno \u00e8 verso il tuo Signore."}, {"id": 373861, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6115, "text": "Hai visto colui che proibisce"}, {"id": 373862, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6116, "text": "al servo di eseguire l'orazione?"}, {"id": 373863, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6117, "text": "Pensi che segua la guida,"}, {"id": 373864, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6118, "text": "che comandi il timore [di Allah]?"}, {"id": 373865, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6119, "text": "Non pensi piuttosto che rinneghi e volga le spalle?"}, {"id": 373866, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6120, "text": "Non sa che, invero, Allah vede?"}, {"id": 373867, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6121, "text": "Stia in guardia: se non smette, Noi lo afferreremo per il ciuffo,"}, {"id": 373868, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6122, "text": "il ciuffo mendace peccaminoso."}, {"id": 373869, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6123, "text": "Chiami pure il suo clan:"}, {"id": 373870, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6124, "text": "Noi chiameremo i guardiani."}, {"id": 373871, "identifier": "it.piccardo", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6125, "text": "No, non gli obbedire, ma prosternati e avvicinati."}]