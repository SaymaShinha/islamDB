[{"id": 373792, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6044, "text": "Per il sole e il suo fulgore,"}, {"id": 373793, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6045, "text": "per la luna quando lo segue,"}, {"id": 373794, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6046, "text": "per il giorno quando rischiara [la terra],"}, {"id": 373795, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6047, "text": "per la notte quando la copre,"}, {"id": 373796, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6048, "text": "per il cielo e Ci\u00f2 che lo ha edificato,"}, {"id": 373797, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6049, "text": "per la terra e Ci\u00f2 che l'ha distesa,"}, {"id": 373798, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6050, "text": "per l'anima e Ci\u00f2 che l'ha formata armoniosamente"}, {"id": 373799, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6051, "text": "ispirandole empiet\u00e0 e devozione."}, {"id": 373800, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6052, "text": "Ha successo invero chi la purifica,"}, {"id": 373801, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6053, "text": "\u00e8 perduto chi la corrompe."}, {"id": 373802, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6054, "text": "I Tham\u00fbd, per arroganza, tacciarono di menzogna,"}, {"id": 373803, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6055, "text": "quando si alz\u00f2 il pi\u00f9 miserabile di loro."}, {"id": 373804, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6056, "text": "Il Messaggero di Allah aveva detto loro: \u201c[Questa] \u00e8 la cammella di Allah, [abbia] il suo turno per bere\u201d. [Il profeta S\u00e2lih_ aveva stabilito che la cammella, che Allah (gloria a Lui l'Altissimo) aveva miracolosamente tratto da una roccia, avesse diritto di bere, un giorno su due, all'unica fonte di cui disponevano i Tham\u00fbd. In seguito, per\u00f2, quelli rinnegarono il patto e uccisero la cammella, suscitando la collera e il castigo di Allah. (Vedi anche XI, 64)]"}, {"id": 373805, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6057, "text": "Lo tacciarono di impostura e le tagliarono i garetti: li annient\u00f2 il loro Signore per il loro peccato,"}, {"id": 373806, "identifier": "it.piccardo", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "it", "name": "Piccardo", "transelator_en_name": "Hamza Roberto Piccardo", "format": "text", "type": "translation", "ayah_number": 6058, "text": "senza temere [di ci\u00f2] alcuna conseguenza."}]