[{"id": 342634, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5910, "text": "In\u00e3 rantsuwa da sama mai taur\u00e3rin lissafin shekara."}, {"id": 342635, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5911, "text": "Da yinin da aka yi alkawarin zuwansa,"}, {"id": 342636, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5912, "text": "Da yini mai shaidu, da yini da ake halarta a cikinsa"}, {"id": 342637, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5913, "text": "An la'ani mut\u00e3nen r\u00e3mi."}, {"id": 342638, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Wato wuta wadda aka hura."}, {"id": 342639, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5915, "text": "A l\u00f5kacin da suke a kan (gefen) ta a zazzaune."}, {"id": 342640, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5916, "text": "Alh\u00e3li s\u0169, bisa ga abin da suke aikat\u00e3wa ga m\u0169minai, sun\u00e3 halarce."}, {"id": 342641, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5917, "text": "Kuma ba su tuhumce su ba, f\u00e3ce kawai domin sun yi \u0129m\u00e3ni da Allah Mabuw\u00e3yi, wanda ake g\u00f5dewa."}, {"id": 342642, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Wanda Yake shi ne da mulkin sammai da \u0199asa, kuma Allah a kan k\u00f5me halarce Yake."}, {"id": 342643, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Lalle ne wa\u0257anda suka fitini m\u0169minai maza da m\u0169minai m\u00e3ta, sa'an nan ba su t\u0169ba ba to, sun\u00e3 da az\u00e3bar Jahannama, kuma sun\u00e3 da az\u00e3bar g\u00f5bara."}, {"id": 342644, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5920, "text": "Lalle ne wa\u0257anda suka yi \u0129m\u00e3ni, kuma suka aikata ayyukan \u0199warai, sun\u00e3 da gid\u00e3jen Aljanna, \u0199oramu na gud\u00e3na daga \u0199ar\u0199ashin gid\u00e3jen. Wancan abu fa shi ne rabo babba."}, {"id": 342645, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Lalle ne dam\u0199ar Ubangijinka mai tsanani ce \u0199warai."}, {"id": 342646, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Lalle ne Sh\u0129, Shi ne Mai \u0199\u00e3ga halitta, kuma Ya mayar da ita (b\u00e3yan mutuwa)."}, {"id": 342647, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Kuma Shi ne Mai g\u00e3fara, Mai bayyana s\u00f5yayya."}, {"id": 342648, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5924, "text": "Mai Al'arshi mai girma"}, {"id": 342649, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Mai aikat\u00e3wa ga abin da Yake nufi."}, {"id": 342650, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Ko l\u00e3b\u00e3rin rundan\u00f5ni y\u00e3 zo maka."}, {"id": 342651, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5927, "text": "Fir'auna da sam\u0169d\u00e3wa?"}, {"id": 342652, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5928, "text": "\u00c3'aha! wa\u0257anda suka k\u00e3firta sun\u00e3 cikin \u0199aryat\u00e3wa."}, {"id": 342653, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5929, "text": "Alh\u00e3li, Allah daga b\u00e3yansu, Mai k\u1ebdwaye su ne (da saninSa)."}, {"id": 342654, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5930, "text": "\u00c3'aha! Shi Al\u0199ur'\u00e3ni ne mai girma."}, {"id": 342655, "identifier": "ha.gumi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5931, "text": "A cikin Allo tsararre."}]