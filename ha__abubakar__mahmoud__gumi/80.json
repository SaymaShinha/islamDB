[{"id": 342487, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5759, "text": "Y\u00e3 game huska kuma ya j\u0169ya b\u00e3ya."}, {"id": 342488, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5760, "text": "Sab\u00f5da mak\u00e3ho y\u00e3 je masa."}, {"id": 342489, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5761, "text": "To, me ya sanar da kai c\u1ebdwa watakila shi ne zai tsarkaka."}, {"id": 342490, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5762, "text": "Ko ya tuna, d\u00f5min tun\u00e3war ta amfane shi?"}, {"id": 342491, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5763, "text": "Amma wanda ya wad\u00e3tu da d\u0169kiya."}, {"id": 342492, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5764, "text": "Sa'an nan kai kuma ka \u0257ora bijira zuwa gare shi!"}, {"id": 342493, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5765, "text": "To, me zai c\u0169ce ka idan bai tsarkaka ba?"}, {"id": 342494, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5766, "text": "Kuma, amma wanda ya zomaka yana gaug\u00e3wa."}, {"id": 342495, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5767, "text": "Alh\u00e3li sh\u0129 yan\u00e3 jin ts\u00f5r\u00f5n Allah."}, {"id": 342496, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5768, "text": "Kai kuma k\u00e3 shagala ga barinsa!"}, {"id": 342497, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5769, "text": "A'aha! Lalle ne, wannan tun\u00e3tarwa ce."}, {"id": 342498, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5770, "text": "Sab\u00f5da wanda ya so ya tunaShi (Allah)."}, {"id": 342499, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5771, "text": "(Tun\u00e3tarwa ce) ta cikin littafai ab\u00e3ban girmam\u00e3wa,"}, {"id": 342500, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5772, "text": "Ab\u00e3ban \u0257aukak\u00e3wa, ab\u00e3ban tsarkak\u1ebdwa."}, {"id": 342501, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5773, "text": "A cikin hann\u00e3yen mala'iku marub\u0169ta."}, {"id": 342502, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5774, "text": "M\u00e3su daraja, m\u00e3su \u0257\u00e3'a ga Allah."}, {"id": 342183, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5775, "text": "An la'ani mutum (kafiri). M\u1ebd y\u00e3 yi k\u00e3fircinsa!"}, {"id": 342213, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5776, "text": "Daga wane abu, (Allah) Ya halitta shi?"}, {"id": 342503, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5777, "text": "Daga \u0257igon maniyyi, Ya halitta shi sa'an nan Ya \u0199addar\u00e3 shi (ga halaye)."}, {"id": 342504, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5778, "text": "Sa'an nan, hanyarsa ta fita Ya sau\u0199a\u0199e masa."}, {"id": 342505, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5779, "text": "Sa'an nan, Ya matar da shi sai Ya sanya shi a cikin kabari."}, {"id": 342506, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5780, "text": "Sa'an nan, idan Ya so lalle ne zai t\u00e3yar da shi."}, {"id": 342507, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5781, "text": "Ha\u0199\u0129\u0199a bai i da aikata abin da Allah Ya umurce shi ba (l\u00f5kacin sanya shi a cikin kabari)."}, {"id": 342508, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5782, "text": "To, mutum ya d\u0169ba zuwa ga abincinsa."}, {"id": 342509, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5783, "text": "Lalle ne M\u0169, Mun zuo ruwa, zub\u00f5wa."}, {"id": 342510, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5784, "text": "Sa'an nan, Muka tsatts\u00e3ge \u0199asa tsattsagewa."}, {"id": 342511, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5785, "text": "Sa'an nan, Muka tsirar da \u0199waya, a cikinta."}, {"id": 342512, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5786, "text": "Da inabi da ciy\u00e3wa."}, {"id": 342513, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5787, "text": "Da zait\u0169ni da it\u00e3cen dab\u0129no."}, {"id": 342514, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5788, "text": "Da lambuna, m\u00e3su yawan it\u00e3ce."}, {"id": 342515, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5789, "text": "Da 'y\u00e3'yan it\u00e3cen marmari, da makiy\u00e3y\u00e3 ta dabb\u00f5bi."}, {"id": 342516, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5790, "text": "Domin jin d\u00e3\u0257i a gare ku, ku da dabbobinku."}, {"id": 342517, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5791, "text": "To, idan mai ts\u00e3wa (busa ta biyu) ta zo."}, {"id": 342518, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5792, "text": "R\u00e3nar da mutum yake gudu daga \u0257an'uwansa."}, {"id": 342519, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5793, "text": "Da uwarsa da ubansa."}, {"id": 342046, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5794, "text": "Da m\u00e3tarsa da \u0257iyansa."}, {"id": 342520, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5795, "text": "Ga k\u00f5wane mutum daga cikinsu, a r\u00e3nar nan akwai wani sha'ani da ya ishe shi."}, {"id": 342521, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5796, "text": "Wasu husk\u00f5ki, a r\u00e3nar nan, m\u00e3su haske ne."}, {"id": 342522, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5797, "text": "M\u00e3su d\u00e3riya ne, m\u00e3su bush\u00e3ra."}, {"id": 342523, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5798, "text": "Wasu husk\u00f5ki, a r\u00e3nar nan, akwai \u0199\u0169ra a kansu."}, {"id": 342524, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5799, "text": "Ba\u0199i zai rufe su."}, {"id": 342525, "identifier": "ha.gumi", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5800, "text": "Wa\u0257annan s\u0169 ne k\u00e3firai f\u00e3jirai (ga ayyukansu)."}]