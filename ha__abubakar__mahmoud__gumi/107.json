[{"id": 342921, "identifier": "ha.gumi", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6198, "text": "Shin, ka ga wanda ke \u0199aryat\u00e3wa game da sakamako?"}, {"id": 342922, "identifier": "ha.gumi", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6199, "text": "To, wannan shi ne ke tunkue mar\u00e3ya (daga ha\u0199\u0199insa)."}, {"id": 342923, "identifier": "ha.gumi", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6200, "text": "Kuma b\u00e3 ya kwaaitarwa bisa b\u00e3yar da abinci ga matalauci."}, {"id": 342924, "identifier": "ha.gumi", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u06