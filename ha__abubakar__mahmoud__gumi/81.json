[{"id": 342526, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5801, "text": "Idan r\u00e3na aka shafe haskenta"}, {"id": 342527, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5802, "text": "Kuma idan taur\u00e3ri suka gur\u0253\u00e3ce (wani ya shiga a cikin wani)."}, {"id": 342528, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5803, "text": "Kuma idan duw\u00e3tsu aka tafiyar da su."}, {"id": 342529, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5804, "text": "Kuma idan r\u00e3\u0199uma m\u00e3su cikunna aka sak\u1ebd su w\u00e3wai, b\u00e3 ga k\u00f5wa ba."}, {"id": 342530, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5805, "text": "Kuma idan dabb\u00f5bin d\u00e3ji aka tattara su."}, {"id": 342531, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5806, "text": "Kuma idan t\u1ebdkuna aka mayar da su wuta."}, {"id": 342532, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5807, "text": "Kuma idan r\u00e3yuka aka ha\u0257a su da jikunkunansu."}, {"id": 342533, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5808, "text": "Kuma idan wadda aka turbu\u0257e ta da rai aka tambaye ta."}, {"id": 342534, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5809, "text": "\"Sab\u00f5da wane laifi ne aka kashe ta?\""}, {"id": 342535, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5810, "text": "Idan takardun ayyuka aka w\u00e3tsa su (ga m\u00e3su su)."}, {"id": 342536, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5811, "text": "Kuma idan sama aka f\u1ebd\u0257e ta."}, {"id": 342537, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5812, "text": "Kuma idan Jah\u0129m aka h\u0169ra ta"}, {"id": 341391, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5813, "text": "Kuma idan Aljanna aka kusantar da ita."}, {"id": 342538, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5814, "text": "Rai ya san abin da ya halartar (a r\u00e3nar nan)."}, {"id": 342539, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5815, "text": "To, ba sai Na yi rantsuwa da taur\u00e3ri mataf\u00e3 ba."}, {"id": 342540, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5816, "text": "M\u00e3su gudu suna \u0253\u0169ya."}, {"id": 342541, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5817, "text": "Da dare idan ya b\u00e3yar da b\u00e3ya."}, {"id": 342542, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5818, "text": "Da s\u00e3fiya idan ta yi lumfashi."}, {"id": 342543, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5819, "text": "Lalle ne shi (Al\u0199ur'\u00e3ni) ha\u0199\u0199an, maganar wani manzon (Allah) ne mai girma ga Allah."}, {"id": 342544, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5820, "text": "Mai \u0199arfi, mai daraja a wurin Mai Al'arshi."}, {"id": 342545, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5821, "text": "Wanda ake yi wa \u0257\u00e3'a (wato shugaban mal\u00e3'iku) ne a can, amintacce."}, {"id": 342546, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5822, "text": "Kuma abokinku ba mahaukaci ba ne."}, {"id": 342547, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5823, "text": "Kuma lalle ne, y\u00e3 gan shi a cikin sararin sama mabayyani."}, {"id": 342548, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5824, "text": "Kuma shi, ga gaibi b\u00e3 mai rowa ba ne."}, {"id": 342549, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5825, "text": "Kuma shi (Al\u0199ur'ani) b\u00e3 maganar shai\u0257ani, wanda aka la'ana, ba ce."}, {"id": 342550, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5826, "text": "Shin, a in\u00e3 z\u00e3 ku tafi?"}, {"id": 342551, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5827, "text": "Lalle ne shi (Al\u0199ur'\u00e3ni), b\u00e3 k\u00f5me ba ne f\u00e3ce garga\u0257i ga talikai."}, {"id": 342552, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5828, "text": "Ga wanda ya so, daga cikinku, ya shiryu."}, {"id": 342553, "identifier": "ha.gumi", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5829, "text": "Kuma b\u00e3 z\u00e3 ku so ba sai idan Allah Ubangijin halitta Y\u00e3 yarda."}]