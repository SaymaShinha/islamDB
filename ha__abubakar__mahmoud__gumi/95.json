[{"id": 342822, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6099, "text": "In\u00e3 rantsuwa da Attinu da Azzait\u0169n."}, {"id": 342823, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6100, "text": "Da D\u0169r S\u0129n\u0129na."}, {"id": 342824, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6101, "text": "Da wannan gari amintacce."}, {"id": 342825, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6102, "text": "Lalle ne, Mun halitta mutum a cikin mafi kyawon tsayuwa."}, {"id": 342826, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6103, "text": "Sa'an nan, Muka mayar da shi mafi \u0199as\u0199antar masu rauni"}, {"id": 342827, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6104, "text": "Face wa\u0257anda suka yi imani, kuma suka aikata ayyukan \u0199warai, saboda haka suna da sakamako wanda b\u00e3 ya yankewa."}, {"id": 342828, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6105, "text": "To, b\u00e3yan haka m\u1ebd ya sanya ka ka \u0199aryata sakamako (wanda za a yi a b\u00e3yan T\u00e3shin \u00a1iy\u00e3ma)?"}, {"id": 342829, "identifier": "ha.gumi", "surah_number": 95, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u064a\u0646", "surah_en_name": "At-Tin", "surah_en_name_translation": "The Fig", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 537, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6106, "text": "Ashe Allah b\u00e3 Shi ne Mafi m\u00e3su hukunci ky\u00e3won hukunci ba?"}]