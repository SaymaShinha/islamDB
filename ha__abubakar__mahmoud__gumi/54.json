[{"id": 339791, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4847, "text": "S\u00e3'a ta yi kusa, kuma wata ya ts\u00e3ge."}, {"id": 341577, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Kuma idan sun ga wata \u00e3y\u00e3, sai su juya baya su ce: \"Sihiri ne mai d\u00f5g\u1ebdwa!\""}, {"id": 341578, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4849, "text": "Kuma suka \u0199aryata, kuma suka bi son z\u0169ciyarsu, alh\u00e3li kuwa k\u00f5wane al'amari (wanda suke son su t\u0169re daga Annabi) an tabbatar da shi."}, {"id": 341579, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4850, "text": "Kuma lalle, abin da yake akwai ts\u00e3watarwa a cikinsa na l\u00e3b\u00e3raiya zo musu."}, {"id": 341580, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4851, "text": "Hikima cikakka! Sai dai ab\u0169buwan garga\u0257i b\u00e3 su amf\u00e3ni."}, {"id": 341581, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Sab\u00f5da haka, ka bar su! R\u00e3nar da mai kiran zai yi kira zuwa ga wani abu abin \u0199yama."}, {"id": 341582, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4853, "text": "\u00a1as\u0199antattu ga idanunsu z\u00e3 su fito daga kaburburansu, kamar dai s\u0169 f\u00e3ri ne wa\u0257andasuka w\u00e3tse."}, {"id": 341583, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4854, "text": "Sun\u00e3 gagg\u00e3war tafiya zuwa ga mai kiran, k\u00e3firai na c\u1ebdwa, \"Wannan yini ne mai wuya!\""}, {"id": 341584, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4855, "text": "Mut\u00e3nen N\u0169hu sun \u0199aryata, a gab\u00e3ninsu, sai suka \u0199aryata BawanMu, kuma suka ce: \"Shi mahaukaci ne.\" Kuma aka ts\u00e3wace shi."}, {"id": 341585, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4856, "text": "Sab\u00f5da haka, ya kira Ubangijinsa (ya ce), \"Lalle n\u0129, an rinj\u00e3ye ni, sai Ka yi taimako.\""}, {"id": 341586, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4857, "text": "Sai Muka b\u0169\u0257e k\u00f5f\u00f5fin sama da ruwa mai zuba."}, {"id": 341587, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4858, "text": "Kuma Muka \u0253u\u0253\u0253ugar da \u0199as\u00e3 ta zama id\u00e3nun ruwa, da\u0257a ruwa ya ha\u0257u a kan wani umurni da aka riga aka \u0199addara shi."}, {"id": 341588, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4859, "text": "Kuma Muka \u0257auke N\u0169hu a kan (jirgi) na alluna da \u0199\u0169s\u00f5shi."}, {"id": 341589, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4860, "text": "Tan\u00e3 gud\u00e3na, a kan id\u00e3nunMu, d\u00f5min sakamako ga wanda aka yi wa k\u00e3fircin."}, {"id": 341590, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4861, "text": "Kuma lalle, Mun bar ta ta zama \u00e3y\u00e3. To, Shin, akwai mai tun\u00e3ni"}, {"id": 341591, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4862, "text": "To, y\u00e3y\u00e3 az\u00e3b\u00e3Ta take da garga\u0257iNa?"}, {"id": 341592, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4863, "text": "Kuma lalle ne, ha\u0199\u0129k\u0199a, Mun sau\u0199a\u0199e Al\u0199ur'\u00e3ni, d\u00f5min tun\u00e3wa. To, shin, akwai mai tun\u00e3wa?"}, {"id": 341593, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4864, "text": "\u00c3d\u00e3wa sun \u0199aryata, to, y\u00e3ya az\u00e3b\u00e3Ta take, da garga\u0257iNa?"}, {"id": 341594, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4865, "text": "Lalle M\u0169, Mun aika da iska mai tsananin sauti a kansu, a cikin wani yinin nah\u0129sa mai d\u00f5g\u1ebdwa."}, {"id": 341595, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4866, "text": "Tan\u00e3 fizgar mut\u00e3ne kamar dai s\u0169 kututturan dab\u0129no tum\u0253ukakku ne."}, {"id": 341596, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4867, "text": "To, y\u00e3ya az\u00e3b\u00e3Ta take da garga\u0257iNa?"}, {"id": 341597, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4868, "text": "Kuma lalle ne, ha\u0199i\u0199a Mun sau\u0199a\u0199e Al\u0199ur'ani, d\u00f5min tun\u00e3wa. To, shin, akwai mai tun\u00e3wa?"}, {"id": 341598, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4869, "text": "Sam\u0169d\u00e3wa sun \u0199aryata game da garga\u0257in."}, {"id": 341599, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4870, "text": "Sai suka ce: \"Wani mutum daga cikinmu, shi ka\u0257ai, wai mu b\u0129 shi! Lalle m\u0169 a l\u00f5kacin, ha\u0199\u0129\u0199a mun shiga wata \u0253ata da hauk\u00e3."}, {"id": 341600, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4871, "text": "\"Shin, an j\u1ebdfa masa Manzancin ne, a tsak\u00e3ninmu? \u00c3'a, sh\u0129 dai gawurtaccen ma\u0199aryaci ne mai girman kai!\""}, {"id": 341601, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4872, "text": "Z\u00e3 su sani a g\u00f5be, w\u00e3ne ne gawurtaccen mai \u0199aryar, mai girman kan?"}, {"id": 341602, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4873, "text": "Lalle M\u0169, m\u00e3su aik\u00e3wa da r\u00e3\u0199umar ne, ta zame musu fitina, sai ka tsare su da kallo, kuma ka yi ha\u0199uri."}, {"id": 341603, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4874, "text": "Kuma ka b\u00e3 su l\u00e3b\u00e3ri c\u1ebdwa ruwa rababbe ne a tsak\u00e3ninsu (da r\u00e3\u0199umar), k\u00f5wane sha, mai shi yan\u00e3 halartar sa."}, {"id": 341604, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4875, "text": "Sai suka kira abokinsu, sai ya kar\u0253a, sa'an nan ya s\u00f5ke ta,"}, {"id": 341605, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4876, "text": "To, y\u00e3ya az\u00e3baTa take da garga\u0257iNa?"}, {"id": 341606, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4877, "text": "Lalle M\u0169, Mun aika ts\u00e3wa guda a kansu, sai suka kasance kamar y\u00e3yin mai shinge."}, {"id": 341608, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4878, "text": "Kuma lalle ne, ha\u0199\u0129\u0199a, Mun sau\u0199a\u0199e Al\u0199ur'ani, d\u00f5min tun\u00e3wa. To, shin, akwai mai tun\u00e3wa?"}, {"id": 341609, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Mut\u00e3nen L\u0169\u0257u sun \u0199aryata, game da garga\u0257i."}, {"id": 341610, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4880, "text": "Lalle Mun aika iskar tsakuwa a kansu, f\u00e3ce mabiyan L\u0169\u0257u, Mun tsirar da su a l\u00f5kacin asuba."}, {"id": 341611, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4881, "text": "Sab\u00f5da wata ni'ima ta daga gare Mu. Kamar haka Muke s\u00e3ka wa wanda ya g\u00f5de."}, {"id": 341612, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4882, "text": "Kuma lalle, ha\u0199\u0129\u0199a, ya yi musu garga\u0257in dam\u0199arMu, sai suka yi musu game da garga\u0257in."}, {"id": 341613, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4883, "text": "Kuma lalle ha\u0199\u0129\u0199a, su, sun n\u1ebdme shi ta wajen b\u00e3\u0199insa, sai Muka sh\u00e3fe id\u00e3nunsu. \"To, ku \u0257an\u0257ani az\u00e3baTa da garga\u0257\u0129Na.\""}, {"id": 341614, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4884, "text": "Kuma lalle, ha\u0199\u0129\u0199a, wata az\u00e3ba matabbaciya t\u00e3 w\u00e3ye musu gari da y\u00e3\u0199i, tun da s\u00e3fe."}, {"id": 341615, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4885, "text": "To, ku \u0257an\u0257ani az\u00e3b\u00e3Ta da garga\u0257\u0129Na."}, {"id": 341616, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4886, "text": "Kuma lalle ne, ha\u0199\u0129\u0199a, Mun sau\u0199a\u0199e Al\u0199ur, ani d\u00f5min tun\u00e3wa. To, shin, akwai mai tun\u00e3wa?"}, {"id": 341617, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4887, "text": "Kuma lalle, ha\u0199\u0129\u0199a, garga\u0257in ya j\u1ebd wa mabiyan Fir'auna."}, {"id": 341618, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4888, "text": "Sun \u0199aryata game da \u00e3y\u00f5yinMu, dukansu sai Muka k\u00e3ma su, irin k\u00e3mun Mabuw\u00e3yi, Mai \u0129kon yi."}, {"id": 341619, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4889, "text": "Shin, k\u00e3firanku ne mafi alh\u1ebdri daga wa\u0257ancan, ko kuwa kun\u00e3 da wata bar\u00e3'a a cikin littattafai?"}, {"id": 341620, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4890, "text": "K\u00f5 z\u00e3 su ce: \"M\u0169 duka m\u00e3su ha\u0257a \u0199arfi ne d\u00f5min cin nasara?\""}, {"id": 341621, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4891, "text": "Z\u00e3 a karya t\u00e3ron, kuma su j\u0169ya b\u00e3ya d\u00f5min gudu."}, {"id": 341622, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4892, "text": "\u00c3'a, S\u00e3'a ita c\u1ebd l\u00f5kacin wa'adinsu, kuma S\u00e3'ar t\u00e3 fi tsananin mas\u0129fa, kuma ta fi\u0257\u00e3ci."}, {"id": 341623, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4893, "text": "Lalle ne, m\u00e3su laifi sun\u00e3 a cikin \u0253ata da hauka."}, {"id": 341624, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4894, "text": "R\u00e3nar da z\u00e3 a j\u00e3 su a cikin wuta a kan fusk\u00f5kinsu. \"Ku \u0257an\u0257ani sh\u00e3far wutar Sa\u0199ar.\""}, {"id": 341625, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4895, "text": "Lalle M\u0169, k\u00f5wane irin abu Mun halitta shi a kan ts\u00e3ri."}, {"id": 341626, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4896, "text": "Kuma umurninMu bai zamo ba f\u00e3ce da kalma \u0257aya, kamar wal\u0199\u00e3war ido."}, {"id": 341627, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4897, "text": "Kuma lalle ne ha\u0199\u0129\u0199a, Mun halaka irin gayyarku. To, shin, akwai mai tun\u00e3ni?"}, {"id": 341628, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4898, "text": "Kuma k\u00f5wane abu, da suka aikata shi, yan\u00e3 a cikin littattafai."}, {"id": 341629, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4899, "text": "Kuma dukkan \u0199arami da babba, rubutacce ne."}, {"id": 341630, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4900, "text": "Lalle m\u00e3su ta\u0199awa sun\u00e3 a cikin gid\u00e3jen Aljanna da k\u00f5guna."}, {"id": 341631, "identifier": "ha.gumi", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4901, "text": "A cikin mazaunin gaskiya, wurin Mai ikon yi, Mai iya zartaswa."}]