[{"id": 340970, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4415, "text": "H\u0323. M\u0303."}, {"id": 339998, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4416, "text": "In\u00e3 rantsuwa da Litt\u00e3fi Mabayyani."}, {"id": 340851, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4417, "text": "Lalle ne, M\u0169, Muka saukar da shi a cikin wani dare mai albarka. Lalle M\u0169' Mun kasance M\u00e3su yin garga\u0257i."}, {"id": 340998, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4418, "text": "A cikinsa (shi daren) ake rarrabe k\u00f5wane umurui bayyananne."}, {"id": 340999, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4419, "text": "Umurni na daga wurinMu. Lalle M\u0169 ne Muka kasanceM\u00e3su aik\u00e3w\u00e3."}, {"id": 341000, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4420, "text": "Sab\u00f5da rahama daga Ubangijinka. Lalle Sh\u0129, Shi ne Mai ji, M\u00e3sani."}, {"id": 341001, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4421, "text": "(Sh\u0129 ne) Ubangijin sammai da \u0199asa da abin da ke a tsak\u00e3ninsu, idan kun kasance m\u00e3su ya\u0199\u0129ni (za ku g\u00e3ne haka)."}, {"id": 341002, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4422, "text": "Babu abin baut\u00e3wa f\u00e3ce Shi. Yana r\u00e3yarwa Kuma Yana kash\u1ebdwa. (Shi ne) Ubangijinku, kuma Ubangijin ubanninku na farko."}, {"id": 341003, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4423, "text": "A'a s\u0169, sun\u00e3 w\u00e3s\u00e3 a cikin shakka."}, {"id": 341004, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4424, "text": "Sab\u00f5da haka, ka dakata r\u00e3nar da sama z\u00e3 t\u00e3 zo da hay\u00e3\u0199i bayyananne."}, {"id": 341005, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4425, "text": "Yan\u00e3 rufe mut\u00e3ne. Wannan wata az\u00e3ba ce mai ra\u0257a\u0257i."}, {"id": 341006, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4426, "text": "Ya Ubangijinmu! Ka kuranye mana az\u00e3ba. Lalle M\u0169, m\u00e3su \u0129m\u00e3ni ne."}, {"id": 341007, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4427, "text": "In\u00e3 tun\u00e3wa take a gare su, alh\u00e3li kuwa, ha\u0199\u0129\u0199a, Manzo mai bayyan\u00e3wa Ya je musu (da garga\u0257in saukar az\u00e3bar, ba su kar\u0253a ba)?"}, {"id": 341008, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4428, "text": "Sa'an nan suka j\u0169ya baya daga gare shi, Kuma suka ce: \"Wanda ake gay\u00e3wa ne, mahaukaci.\""}, {"id": 341009, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4429, "text": "Lalle M\u0169, M\u00e3su kurany\u1ebdwar az\u00e3ba ne, a \u0257an l\u00f5kaci ka\u0257an, lalle k\u0169, m\u00e3su k\u00f5m\u00e3wa ne (ga laifin)."}, {"id": 341010, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 198, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4430, "text": "R\u00e3nar da Muke dam\u0199a, dam\u0199a mafi girma, lalle ne M\u0169 m\u00e3su az\u00e3bar r\u00e3muwa ne."}, {"id": 341011, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4431, "text": "Kuma lalle ne ha\u0199\u0129\u0199a a gabaninsu, Mun fitini mut\u00e3nen Fir'auna, kuma wani Manzo kar\u0129mi ya j\u1ebd musu."}, {"id": 341012, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 496, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4432, "text": "(Mazon ya ce): \"Ku k\u00e3wo mini (\u0129m\u00e3ninku) ya bayin Allah! Lalle ni, Manzo ne amintacce zuwa gare ku.\""}, {"id": 341013, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4433, "text": "\"Kuma kada ku n\u1ebdmi \u0257aukaka a kan Allah. Lalle n\u0129 mai zo muku ne da dal\u0129li bayyananne.\""}, {"id": 341014, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4434, "text": "\"Kuma lalle n\u0129 na n\u1ebdmi tsari da Ubangij\u0129na, kuma Ubangijinku, d\u00f5min kada ku j\u1ebdfe ni.\""}, {"id": 341015, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4435, "text": "\"Kuma idan ba ku yi \u0129m\u00e3ni sab\u00f5da n\u0129 ba, to, ku n\u0129sance ni.\""}, {"id": 341016, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4436, "text": "Sai ya kirayi Ubangjinsa c\u1ebdwa wa\u0257annan mut\u00e3nene m\u00e3su laifi."}, {"id": 341017, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4437, "text": "(Allah Ya ce): \"To, ka yi tafiyar dare da bayiNa, da dare. Lalle k\u0169, wa\u0257anda ake b\u0129 ne (d\u00f5min a k\u00e3ma ku.)\""}, {"id": 341018, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4438, "text": "\"Kuma ka bar t\u1ebdku rarrabe. Lalle s\u0169, runduna ce abar nutsarwa.\""}, {"id": 341019, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4439, "text": "Da yawa suka bar g\u00f5naki da mar\u1ebdmari."}, {"id": 341020, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4440, "text": "Da shuke-shuke da matsayi mai kyau."}, {"id": 341021, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4441, "text": "Da wata ni'ima da suka kasance a cikinta sun\u00e3 m\u00e3su hutu."}, {"id": 341022, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4442, "text": "Kamar haka! Kuma Muka g\u00e3dar da ita ga wa\u0257ansu mut\u00e3ne na dabam."}, {"id": 341023, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 431, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4443, "text": "Sa'an nan sam\u00e3 da \u0199as\u00e3 ba su yi k\u0169ka a kansu ba, Kuma ba su kasance wa\u0257anda ake yi wa jinkiri ba."}, {"id": 341024, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4444, "text": "Kuma lalle ne, ha\u0199\u0129\u0199a Mun ts\u0129rar da Ban\u0129 Isr\u00e3'\u0129la daga, az\u00e3ba mai wul\u00e3kant\u00e3wa."}, {"id": 341025, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4445, "text": "Daga Fir'auna. Lalle shi, ya kasance ma\u0257aukaki daga cikin m\u00e3su \u0253arna."}, {"id": 341026, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4446, "text": "Kuma lalle ne, ha\u0199\u0129\u0199a, Mun z\u00e3\u0253e su sab\u00f5da wani ilmi (na Taurata) a kan mut\u00e3ne."}, {"id": 341027, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4447, "text": "Kuma Muka b\u00e3 su, daga \u00e3y\u00f5yin mu'ujiz\u00f5ji, abin da yake a cikinsa, akwai ni'ima bayyananna"}, {"id": 341028, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4448, "text": "Lalle wa\u0257annan mut\u00e3ne, ha\u0199\u0129ka, sun\u00e3 c\u1ebdwa,"}, {"id": 341029, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4449, "text": "\"Babu wani abu sai mutuwarmu ta farko, kuma ba mu zama wa\u0257anda ake t\u00e3yarwa ba.\""}, {"id": 341030, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4450, "text": "\"Sai ku zo da ubanninmu, idan kun kasance m\u00e3su gaskiya.\""}, {"id": 341031, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4451, "text": "shin, s\u0169 ne maf\u0129f\u0129ta k\u00f5 kuwa mut\u00e3nen Tubba'u, kuma da wa\u0257anda ke a gab\u00e3ninsu? Mun halaka su, lalle s\u0169, sun kasance m\u00e3su laifi."}, {"id": 341032, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4452, "text": "Kuma ba Mu halitta slmmai da \u0199asa da abin da ke a tsak\u00e3ninsu ba, alh\u00e3li kuwa Mun\u00e3 m\u00e3su w\u00e3s\u00e3."}, {"id": 341033, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 497, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4453, "text": "Ba Mu halitta su ba f\u00e3ce da manufa ta gaskiya, kuma amma mafi yawansu, ba su sani ba."}, {"id": 341034, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4454, "text": "Lalle r\u00e3nar rarrab\u1ebdwa, ita ce l\u00f5kacin wa'adinsu gab\u00e3 \u0257aya."}, {"id": 341035, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4455, "text": "R\u00e3nar da wani zumu b\u00e3 ya amf\u00e3nin wani zumu da k\u00f5me kuma ba su zama an\u00e3 taimakon su ba."}, {"id": 341036, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 432, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4456, "text": "f\u00e3ce wanda Allah Ya yi wa rahama. Lalle Shi (Allah), sh\u0129 ne Mabuw\u00e3yi, Mai jin \u0199ai."}, {"id": 341037, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4457, "text": "Lalle it\u00e3ciyar za\u0199\u0199\u0169m (\u0257anyen wut\u00e3),"}, {"id": 341038, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4458, "text": "Ita ce abincin mai laifi."}, {"id": 341039, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4459, "text": "Kamar narkakken kwalta yan\u00e3 tafasa a cikin cikunna."}, {"id": 341040, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4460, "text": "Kamar tafasar ruwan z\u00e3fi."}, {"id": 341041, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4461, "text": "(A c\u1ebd wa mal\u00e3'ikun wut\u00e3), \"Ku k\u00e3m\u00e3 shi, sa'an nan ku fizge shi zuwa ga tsakiyar Jah\u0129m.\""}, {"id": 341042, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4462, "text": "\"Sa'an nan ku zuba, a kansa, daga az\u00e3bar ruwan z\u00e3fi.\""}, {"id": 341043, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4463, "text": "(A ce mas\u00e3), \"Ka \u0257an\u0257ana! Lalle kai, kai ne mabuw\u00e3yi mai girma!\""}, {"id": 341044, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4464, "text": "\"Lalle wannan, shi ne abin da kuka kasance kun\u00e3 shakka game da shi.\""}, {"id": 341045, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4465, "text": "Lalle m\u00e3su ta\u0199awa sun\u00e3 cikin matsayi amintacce."}, {"id": 341046, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4466, "text": "A cikin gid\u00e3jen Aljanna da mar\u1ebdmari."}, {"id": 341047, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4467, "text": "Sun\u00e3 tufanta daga tuf\u00e3fin alhar\u0129ni ra\u0199\u0129\u0199i, da mai kauri, sun\u00e3 m\u00e3su zaman fuskantar j\u0169na."}, {"id": 341048, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4468, "text": "Kamar haka, kuma Mun aurar da su da m\u00e3t\u00e3 m\u00e3su kyaun id\u00e3nu, m\u00e3su girmansu."}, {"id": 341049, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4469, "text": "Sun\u00e3 kira, a cikinsu (gid\u1ebdjen) ga dukan 'ya'yan it\u00e3cen marmari, sun\u00e3 amintattu (daga dukan abin ts\u00f5ro)."}, {"id": 341050, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4470, "text": "B\u00e3 su \u0257an\u0257anar mutuwa, a cikinsu, f\u00e3ce mutuwar farko, kuma (Allah), Ya tsare musu az\u00e3bar Jah\u0129m."}, {"id": 341051, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4471, "text": "Sab\u00f5da falala daga Ubangijinka. Wancan shi ne babban rabo, mai girma."}, {"id": 341052, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4472, "text": "D\u00f5min haka kawai Muka sau\u0199a\u0199ar da shi (Al\u0199ur'\u00e3ni) da harshenka, tsamm\u00e3ninsu, su ri\u0199a tun\u00e3wa."}, {"id": 341053, "identifier": "ha.gumi", "surah_number": 44, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062f\u062e\u0627\u0646", "surah_en_name": "Ad-Dukhaan", "surah_en_name_translation": "The Smoke", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 498, "ruku": 433, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4473, "text": "Sai ka yi jira. Lalle s\u0169, m\u00e3su jira ne."}]