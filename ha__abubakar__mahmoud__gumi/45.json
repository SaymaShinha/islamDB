[{"id": 341054, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4474, "text": "H\u0323. M\u0303."}, {"id": 341055, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4475, "text": "Saukar Litt\u00e3fi daga Allah Mabuw\u00e3yi Mai hikima yake."}, {"id": 341056, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4476, "text": "Lalle ne a cikin sammai da \u0199asa akwai \u00e3y\u00f5yi ga m\u00e3su \u0129m\u00e3ni."}, {"id": 341057, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4477, "text": "Kuma a cikin halittarku da abin da ke watsuwa na dabba akwai \u00e3y\u00f5yi ga mut\u00e3ne m\u00e3su ya\u0199\u0129ni."}, {"id": 341060, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4478, "text": "Kuma da s\u00e3\u0253awar dare da yini da abin da Allah Ya saukar daga sama na arziki, sa'an nan Ya r\u00e3yar da \u0199asa game da shi a b\u00e3yan mutuwarta, da j\u0169yawar isk\u00f5ki, akwai \u00e3y\u00f5yi ga mut\u00e3ne m\u00e3su yin hankali."}, {"id": 341061, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4479, "text": "Wa\u0257ancan \u00e3y\u00f5yin Allah ne, Mun\u00e3 karanta su gare ka da gaskiya. To, da wane l\u00e3b\u00e3ri b\u00e3yan Allah da \u00e3y\u00f5yinSa suke yin \u0129m\u00e3ni?"}, {"id": 341062, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4480, "text": "Bone ya tabbata ga dukan mai yawan \u0199iren \u0199arya, mai laifi."}, {"id": 341063, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4481, "text": "Yan\u00e3 jin \u00e3y\u00f5yin Allah an\u00e3 karanta su a kansa, sa'an nan ya d\u00f5ge yan\u00e3 mak\u00e3ngari, kamar bai j\u0129 su ba. To, ka yi masa bushara da az\u00e3ba mai ra\u0257a\u0257i."}, {"id": 341064, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4482, "text": "Kuma har idan ya san wani abu daga \u00e3y\u00f5yinMu, sai ya rika su da izgili. Wa\u0257ancan sun\u00e3 da wata az\u00e3ba mai wul\u00e3kant\u00e3wa (a d\u0169niya)."}, {"id": 341065, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4483, "text": "Gaba gare su (a L\u00e3hira) akwai Jahannama, kuma abin da suka san\u00e3'anta b\u00e3 ya wad\u00e3tar da su daga k\u00f5me, kuma ab\u0169buwan da suka ri\u0199a maji\u0253inta, baicin Allah, b\u00e3 su w\u00e3datar da su daga k\u00f5me. Kuma sun\u00e3 da wata az\u00e3ba mai girma."}, {"id": 341066, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4484, "text": "Wannan (Al\u0199ur'\u00e3ni) shi ne shiryuwa. Kuma wa\u0257anda suka k\u00e3firta game da \u00e3y\u00f5yin Ubangijinsu, sun\u00e3 da wata az\u00e3ba ta wul\u00e3kanci mai ra\u0257a\u0257i."}, {"id": 341067, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4485, "text": "Allah ne wanda Ya h\u00f5re muku t\u1ebdku d\u00f5min jirgi ya gud\u00e3na a cikinta da umurninSa, kuma d\u00f5min ku n\u1ebdma daga falalarSa, kuma tsamm\u00e3ninku z\u00e3 ku g\u00f5de."}, {"id": 341068, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4486, "text": "Kuma Ya h\u00f5re muku abin da ke a cikin sammai da abin da ke a cikin \u0199asa, gab\u00e3 \u0257aya daga gare Shi yake. Lalle ne, a cikin wancan, ha\u0199\u0129\u0199a, akwai \u00e3y\u00f5yi ga mut\u00e3ne wa\u0257anda ke yin tun\u00e3ni."}, {"id": 341069, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4487, "text": "Ka ce wa wa\u0257anda suka yi \u0129m\u00e3ni, su yi g\u00e3fara ga wa\u0257anda b\u00e3 su f\u00e3tan rahama ga kw\u00e3nukan Allah, d\u00f5min (Allah) Ya s\u00e3ka wa mut\u00e3ne da abin da suka kasance sun\u00e3 aikat\u00e3wa."}, {"id": 341070, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4488, "text": "Wanda ya aikata aikin \u0199warai, to, d\u00f5min kans\u00e3, kuma wanda ya m\u0169nana aki, to, a kansa. Sa'an nan zuwa ga Ubangijinku ake mayar da ku."}, {"id": 341071, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4489, "text": "Kuma lalle ne, ha\u0199\u0129\u0199a, Mun bai (wa B\u00e3n\u0129 Isr\u00e3'\u0129la Litt\u00e3fi da hukunci da Annabci, Kuma Mun azurta su daga ab\u0169buwa m\u00e3sud\u00e3\u0257i, Kuma Mun f\u0129fta su a kan mut\u00e3nen d\u0169niya (a z\u00e3maninsu)."}, {"id": 341072, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4490, "text": "Kuma Muka b\u00e3 su hujj\u00f5ji na umurni. Ba su s\u00e3\u0253a ba f\u00e3ce b\u00e3yan ilmi ya j\u1ebd musu, sab\u00f5da z\u00e3lunci a tsak\u00e3ninsu. Lalle ne, Ubangijinka zai yi hukunci a tsak\u00e3ninsu, a R\u00e3nar \u0199iy\u00e3ma a cikin abin da suka kasance sun\u00e3 s\u00e3\u0253a wa (j\u0169na)."}, {"id": 341073, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4491, "text": "Sa'an nan Muka sanya ka a kan wata shar\u0129'a ta al'amarin. Sai ka b\u0129 ta, Kuma kada k\u00e3 bi son z\u0169ciy\u00f5yin wa\u0257annan daba su sani ba."}, {"id": 341074, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4492, "text": "Lalle ne s\u0169, b\u00e3 z\u00e3 su wad\u00e3tar da kai da k\u00f5me ba daga Allah. Kuma lalle ne azz\u00e3lumai s\u00e3shensu maji\u0253intan s\u00e3she ne. Kuma Allah ne Maji\u0253incin m\u00e3su ta\u0199awa."}, {"id": 341075, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4493, "text": "wannan (Alkur'\u00e3ni) hukunce-hukuncen natsuwa ne ga mut\u00e3ne, da shiryuwa, da rahama, ga mut\u00e3ne wa\u0257anda ke da ya\u0199\u0129ni."}, {"id": 341076, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4494, "text": "K\u00f5 wa\u0257anda suka y\u00e3gi miy\u00e3gun ayyuka sun\u00e3 zaton Mu sany\u00e3 su kamar wa\u0257anda suka yi \u0129m\u00e3ni, kuma suka aikata ayyukan \u0199warai, su zama daidai ga r\u00e3yuwarsu da mutuwarsu? Abin da suke hukunt\u00e3wa y\u00e3 m\u0169nan\u00e3!"}, {"id": 341077, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4495, "text": "Alh\u00e3li kuwa Allah ne Ya halitta sammai da \u0199as\u00e3 sab\u00f5da gaskiya, Kuma d\u00f5min a s\u00e3ka wa k\u00f5wane rai da abin da ya san\u00e3'anta, kuma su, b\u00e3 z\u00e3 a z\u00e3lunce su ba."}, {"id": 341078, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4496, "text": "Shin, k\u00e3 ga wanda ya ri\u0199i son z\u0169ciyarsa s\u0129h ne abin bautawarsa, kuma Allah Ya \u0253atar da shi a kan wani ilmi, Kuma Ya sa hatini a kan jinsa, da z\u0169ciyarsa, kuma Ya sa wata y\u00e3n\u00e3 \u00e3 kan ganinsa? To, w\u00e3ne ne zai shiryar da shi b\u00e3yan Allah? Shin to, b\u00e3 z\u00e3 ku yi tun\u00e3ni ba?"}, {"id": 341079, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4497, "text": "Kuma suka ce: \"B\u00e3bu k\u00f5me f\u00e3ce r\u00e3yuwarmu ta d\u0169niya; mun\u00e3 mutuwa kuma mun\u00e3 r\u00e3yuwa (da haihuwa) kuma b\u00e3bu abin da ke halaka mu sai z\u00e3mani.\" Alh\u00e3li kuwa (ko da suke fa\u0257ar maganar) b\u00e3 su da wani ilmi game da wannan, b\u00e3 su bin k\u00f5me face zato."}, {"id": 341080, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4498, "text": "Kuma idan an\u00e3 karanta \u00e3y\u00f5yinMu bayyanannu a kansu, b\u00e3bu abin da ya kasance hujjarsu f\u00e3ce suka ce: \"Ku zo mana da ubanninmu, idan kun kasance m\u00e3su gaskiya.\""}, {"id": 341081, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4499, "text": "Ka ce: \"Allah ne ke r\u00e3yar da ku kuma Sh\u0129 ne ke matarda ku, sa'an nan Ya t\u00e3ra ku zuwa ga R\u00e3nar \u00a1iy\u00e3ma, b\u00e3bu shakka a gare ta, kuma amma mafi yawan mut\u00e3ne ba su sani ba.\""}, {"id": 341082, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4500, "text": "\"Kuma mulkin sammai da \u0199asa na Allah ne, Sh\u0129 ka\u0257ai. Kuma r\u00e3nar da Sa'a ke tsayuwa, a r\u00e3nar nan m\u00e3su \u0253\u00e3t\u00e3w\u00e3 (ga hujj\u00f5jin Allah d\u00f5min su ki bin shar\u0129'arSa) z\u00e3 su yi has\u00e3ra.\""}, {"id": 341083, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4501, "text": "Kuma z\u00e3 ka ga k\u00f5wace al'umma tan\u00e3 gurf\u00e3ne, k\u00f5wace al'umma an\u00e3 kiran ta zuwa ga litt\u00e3finta. (A ce musu) \"A yau an\u00e3 s\u00e3ka muku da abin da kuka kasance kun\u00e3 aikat\u00e3wa.\""}, {"id": 341084, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4502, "text": "\"Wannan litt\u00e3finMu ne yan\u00e3 yin magana a kanku da gas, kiya. Lalle M\u0169, Mun kasanceMuna sauya rub\u0169tun tamkar abin dakuka kasance kun\u00e3 aik\u00e3t\u00e3wa.\""}, {"id": 341085, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4503, "text": "To, amma wa\u0257anda suka yi \u0129m\u00e3ni kuma suka aikata ayyukan \u0199warai, to, Ubangijinsu zai sanya su a cikin rahamarsa. Wannan sh\u0129 ne babban rabo bayyananne."}, {"id": 341086, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4504, "text": "Kuma amma wa\u0257anda suka k\u00e3firt\u00e3 (Allah zai ce musu): \"Shin, \u00e3y\u00f5y\u0129N\u00e3 ba su kasance an\u00e3 kar\u00e3nta su \u00e3 kanku ba sai kuka kangare, Kuma kuka kasance mut\u00e3ne m\u00e3su laifi?\""}, {"id": 341087, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4505, "text": "\"Kuma idan aka ce, lalle wa'\u00e3din Allah gaskiya ne, kuma Sa'a, b\u00e3bu shakka a cikinta sai kuka ce, 'Ba mu san abin da ake c\u1ebdwa Sa'a ba, b\u00e3 mu z\u00e3to (game da ita) f\u00e3ce zato mai rauni, Kuma ba mu zama m\u00e3su ya\u0199ni ba.\"'"}, {"id": 341088, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4506, "text": "Kuma m\u0169nanan abin da suka aikata ya bayyana a gare su, kuma abin da suka kasance sun\u00e3 yi na izgili ya wajaba a kansu."}, {"id": 341089, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4507, "text": "Kuma aka ce: \"A yau z\u00e3 Mu manta da ku, kamar yadda kuka manta da gamuwa da yininku wannan. Kuma m\u00e3k\u00f5marku wut\u00e3 ce, Kuma b\u00e3 ku da wa\u0257ansu m\u00e3su taimako."}, {"id": 341090, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4508, "text": "\"Wancan d\u00f5min lalle k\u0169, kun ri\u0199i \u00e3y\u00f5yin Allah da izgili, Kuma r\u00e3yuwar d\u0169niya ta r\u0169\u0257e ku. To, a yau b\u00e3 z\u00e3 su fita daga gare ta ba, Kuma b\u00e3 z\u00e3 su zama wa\u0257anda ake n\u1ebdman yardarsu ba.\""}, {"id": 341091, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4509, "text": "Sab\u00f5da haka, g\u00f5diya ta tabbata ga Allah Ubangijin sammai, kuma Ubangijin \u0199as\u00e3, Ubangijin halittu."}, {"id": 341092, "identifier": "ha.gumi", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 4510, "text": "Kuma gare Shi girma yake, a cikin sammai da \u0199as\u00e3, kuma Sh\u0129 ne Mabuw\u00e3yi, Mai Hikima."}]