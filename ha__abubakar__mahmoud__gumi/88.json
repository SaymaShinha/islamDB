[{"id": 342692, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5968, "text": "Lalle ne lab\u00e3rin (\u00a1iy\u00e3ma) mai rufe mut\u00e3ne da tsoronta y\u00e3 zo maka?"}, {"id": 342693, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5969, "text": "Wasu husk\u00f5ki a r\u00e3nar nan \u0199as\u0199antattu ne."}, {"id": 342694, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5970, "text": "M\u00e3su aikin wahala ne, m\u00e3su gajiya."}, {"id": 342695, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5971, "text": "Z\u00e3 su shiga wata wuta mai z\u00e3fi."}, {"id": 342696, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5972, "text": "Ana sh\u00e3yar da su daga wani marmaro mai z\u00e3fin ruwa."}, {"id": 342697, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5973, "text": "Ba su da wani abinci f\u00e3ce dai daga danyi."}, {"id": 342698, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5974, "text": "B\u00e3 ya sanya \u0199iba, kuma b\u00e3 zai wad\u00e3tar daga yunwa ba."}, {"id": 342699, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5975, "text": "Wasu husk\u00f5ki a r\u00e3nar nan m\u00e3su ni'ima ne."}, {"id": 342700, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5976, "text": "Game da aikinsu, masu yarda ne."}, {"id": 342701, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5977, "text": "(Suna) a cikin Aljanna ma\u0257aukakiya."}, {"id": 342702, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5978, "text": "B\u00e3 z\u00e3 su ji y\u00e3sassar magana ba, a cikinta."}, {"id": 342703, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5979, "text": "A cikinta akwai marmaro mai gud\u00e3na."}, {"id": 342704, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5980, "text": "A cikinta akwai gad\u00e3je ma\u0257aukaka."}, {"id": 342705, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5981, "text": "Da k\u00f5funa ar'aje."}, {"id": 342706, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5982, "text": "Da fil\u00f5li j\u1ebdre,"}, {"id": 342707, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5983, "text": "Da katifu shimfi\u0257e."}, {"id": 342708, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5984, "text": "Ashe to b\u00e3 z\u00e3 su d\u0169b\u00e3wa ba ga r\u00e3\u0199um\u00e3 yadda aka halitta su?"}, {"id": 342709, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5985, "text": "Da zuwa ga sama yadda aka \u0257aukaka ta?"}, {"id": 342710, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5986, "text": "Da zuwa ga duw\u00e3tsu yadda aka kafa su?"}, {"id": 342711, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5987, "text": "Da zuwa ga \u0199asa yadda aka shimfi\u0257\u00e3 ta?"}, {"id": 342712, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5988, "text": "sab\u00f5da haka, ka yi wa'azi, kai mai yin wa'azi ne kawai."}, {"id": 342713, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5989, "text": "Ba ka zama mai \u0129kon tan\u0199was\u00e3wa a kansu ba."}, {"id": 342714, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5990, "text": "F\u00e3ce dai duk wanda ya j\u0169ya b\u00e3ya, kuma ya k\u00e3firta."}, {"id": 342715, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5991, "text": "To, Allah zai yi masa az\u00e3ba, az\u00e3bar nan da take mafi girma."}, {"id": 342716, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5992, "text": "Lalle ne, zuwa gare Mu k\u00f5m\u00f5warsu take."}, {"id": 342717, "identifier": "ha.gumi", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5993, "text": "Sa'an nan lalle ne aikinMu ne Mu yi musu his\u00e3bi."}]