[{"id": 342870, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6147, "text": "Ina rantsuwa da daw\u00e3ki m\u00e3su gudu suna fitar da k\u0169kan ciki."}, {"id": 342871, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6148, "text": "M\u00e3su \u0199yasta wuta (da k\u00f5fatansu a kan duwatsu) \u0199yastawa."}, {"id": 342872, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6149, "text": "Sa'an nan m\u00e3su yin hari a lokacin asuba."}, {"id": 342873, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6150, "text": "Sai su motsar da \u0199\u0169ra game da shi."}, {"id": 342874, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6151, "text": "Sai su shiga, game da ita (\u0199\u0169rar), a tsak\u00e3nin jama'ar ma\u0199iya."}, {"id": 342875, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6152, "text": "Lalle ne mutum mai tsananin butulci ne ga Ubangijinsa."}, {"id": 342876, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6153, "text": "Lalle ne shi mai shaida ne a kan laifinsa dl haka."}, {"id": 342877, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6154, "text": "Kuma 1alle ne ga d\u0169kiya shi mai tsananin so ne."}, {"id": 342878, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 599, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6155, "text": "Shin, b\u00e3 ya da sanin l\u00f5kacin da aka t\u00f5ne abin da yake cikin kaburbura."}, {"id": 342879, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6156, "text": "Aka bayyana abin da ke cikin zukata."}, {"id": 342880, "identifier": "ha.gumi", "surah_number": 100, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0627\u062a", "surah_en_name": "Al-Aadiyaat", "surah_en_name_translation": "The Chargers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 542, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 6157, "text": "Lalle ne Ubangijinsu, game da su a ranar nan Mai \u0199ididdigewa ne?"}]