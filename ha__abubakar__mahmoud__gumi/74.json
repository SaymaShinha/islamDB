[{"id": 342225, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5496, "text": "Y\u00e3 wanda ya lullu\u0253a da may\u00e3fi."}, {"id": 342226, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5497, "text": "Ka t\u00e3shi d\u00f5min ka yi garga\u0257i"}, {"id": 342227, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5498, "text": "Kuma Ubangijinka, sai ka girmama Shi,"}, {"id": 342228, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5499, "text": "Kuma tut\u00e3finka, sai ka tsarkake su,"}, {"id": 342229, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5500, "text": "Kuma gum\u00e3ka, sai ka \u0199aurace musu."}, {"id": 342230, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5501, "text": "Kada ka yi kyauta kana n\u1ebdman \u0199\u00e3ri"}, {"id": 342231, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5502, "text": "Kuma sab\u00f5da Ubangijinka? Sai ka yi ha\u0199ure"}, {"id": 342232, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5503, "text": "To, idan aka yi b\u0169sa a cikin \u0199aho."}, {"id": 342233, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5504, "text": "To, wannan, a r\u00e3nar nan, yini ne mai wuya"}, {"id": 342234, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5505, "text": "A kan k\u00e3firai, b\u00e3 mai sau\u0199i ba ne."}, {"id": 342235, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5506, "text": "Ka bar Ni da wanda Na halitta, yana shi ka\u0257ai,"}, {"id": 342236, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5507, "text": "Kuma Na sanya masa d\u0169kiya shimfi\u0257a\u0257\u0257iya"}, {"id": 342237, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5508, "text": "Da \u0257iy\u00e3 halartattu,"}, {"id": 342238, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5509, "text": "Kuma Na shimfi\u0257a? masa k\u00f5me shimfi\u0257\u00e3wa."}, {"id": 342239, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5510, "text": "Sa'an nan, yan\u00e3 kwa\u0257ayin in yi masa \u0199\u00e3ri!"}, {"id": 342240, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5511, "text": "Faufau! Lalle ne, sh\u0129 y\u00e3 kasance, ga \u00e3y\u00f5yinMu, mai tsaurin kai."}, {"id": 342241, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5512, "text": "Z\u00e3 Ni kallafa masa wahala mai hauhawa."}, {"id": 342242, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5513, "text": "Lalle ne, Shi, y\u00e3 yi tun\u00e3ni, kuma y\u00e3 \u0199addara (abin da zai fa\u0257\u00e3 game da Al\u0199ur'\u00e3ni)"}, {"id": 342243, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5514, "text": "Sab\u00f5da haka, aka la'ane shi, kamar yadda ya \u0199addara."}, {"id": 342244, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5515, "text": "Sa'an nan, aka la'ane shi kamar yadda ya \u0199addara."}, {"id": 342245, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5516, "text": "Sa'an nan, ya yi tun\u00e3ni"}, {"id": 342246, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5517, "text": "Sa'an nan, ya gintse huska? kuma ya yi murtuk."}, {"id": 342247, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5518, "text": "Sa'an nan, ya j\u0169ya b\u00e3ya, kuma ya bun\u0199\u00e3sa,"}, {"id": 342248, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5519, "text": "Sai ya ce: \"Wannan abu dai b\u00e3 k\u00f5me ba ne f\u00e3ce wani sihiri, wanda aka ruwaito.\""}, {"id": 342249, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5520, "text": "\"Wannan maganar mutum dai ce.\""}, {"id": 342250, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5521, "text": "Z\u00e3 Ni \u0199\u00f5na shi da Sa\u0199ar."}, {"id": 342251, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5522, "text": "Kuma m\u1ebd ya sanar da kai abin da ak\u1ebd c\u1ebdwa Sa\u0199ar!"}, {"id": 342252, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5523, "text": "B\u00e3 ta rag\u1ebdwa, kuma b\u00e3 ta bari."}, {"id": 342253, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5524, "text": "Mai n\u00e3c\u1ebdwa ga jiki ce (da (\u0199\u0169na)."}, {"id": 342254, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5525, "text": "A kanta akwai (matsara) g\u00f5ma sh\u00e3 tara."}, {"id": 342255, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5526, "text": "Kuma ba Mu sanya ma'ab\u0169ta wut\u00e3 (w\u00e3to matsaranta) ba, f\u00e3ce mal\u00e3'iku, kuma ba Mu sanya adadinsu (g\u00f5ma sha tara) ba, f\u00e3ce d\u00f5min fitina ga wa\u0257anda suka k\u00e3firta domin wa\u0257anda aka bai wa litt\u00e3fi su s\u00e3mi ya\u0199\u0129ni kuma wa\u0257anda suka yi \u0129m\u00e3ni su \u0199\u00e3ra \u0129m\u00e3ni, kuma wa\u0257anda aka bai wa litt\u00e3fi da m\u0169minai b\u00e3 z\u00e3 su yi shakka ba, kuma domin wa \u0257anda a cikin zukatansu akwai wata cuta da k\u00e3firai su ce: \"M\u1ebd Allah Yake nufi da wannan, ya kasance mis\u00e3li?\" Haka dai Allah ke \u0253atar da wanda Ya so, kuma Ya shiryar da wanda ya so. Kuma b\u00e3bu wanda ya san may\u00e3\u0199an Ubangijinka f\u00e3ce Shi, kuma ita (wutar) ba ta kasance ba f\u00e3ce wata tun\u00e3tarwa ce ga mutum."}, {"id": 342256, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5527, "text": "A'aha! Ina rantsuwa da wat\u00e3."}, {"id": 342257, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5528, "text": "Da dare a l\u00f5kacin da ya j\u0169yar da baya."}, {"id": 342258, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5529, "text": "Da s\u00e3fiya idan ta w\u00e3ye."}, {"id": 342259, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5530, "text": "Ita (wutar) \u0257ayan manyan mas\u0129f\u0169? ce."}, {"id": 342260, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5531, "text": "Mai garga\u0257\u0129 ce ga mutum."}, {"id": 342261, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5532, "text": "Ga wanda ya so, daga cikinku, ya gab\u00e3ta ko ya jinkirta."}, {"id": 342262, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5533, "text": "K\u00f5wane rai ga abin da ya aikata jingina ce."}, {"id": 342263, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5534, "text": "F\u00e3ce mut\u00e3nen d\u00e3ma."}, {"id": 342264, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5535, "text": "A cikin Aljanna suna tambayar j\u0169na."}, {"id": 342265, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5536, "text": "Game da m\u00e3su laifi."}, {"id": 342266, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5537, "text": "(Su ce musu) \"Me ya shigar da ku a cikin Sa\u0199ar?\""}, {"id": 342267, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5538, "text": "Suka ce: \"Ba mu kasance mun\u00e3 a cikin m\u00e3su salla ba.\""}, {"id": 342268, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5539, "text": "\"Kuma ba mu kasance muna ciyar da matalaut\u00e3 ba.\""}, {"id": 342269, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5540, "text": "\"Kuma mun kasance muna k\u0169ts\u00e3wa t\u00e3re da m\u00e3su k\u0169ts\u00e3wa.\""}, {"id": 342270, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5541, "text": "\"Mun kasance mun\u00e3 \u0199aryata r\u00e3nar s\u00e3kamako.\""}, {"id": 342271, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5542, "text": "\"Har gaskiya (w\u00e3to mutuwa) ta zo mana.\""}, {"id": 342272, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5543, "text": "Sab\u00f5da haka c\u1ebdton m\u00e3su c\u1ebdto b\u00e3 zai amf\u00e3ne su ba."}, {"id": 342273, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5544, "text": "Haba! Me ya same su, suka zama m\u00e3su bijirewa daga wa'azin gaskiya."}, {"id": 342274, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5545, "text": "Kamar dai s\u0169, j\u00e3kuna firgitattu ne."}, {"id": 342061, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5546, "text": "Sun gudu daga z\u00e3ki."}, {"id": 342275, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5547, "text": "A'aha! K\u00f5w\u00e3ne mutum daga cikinsu yan\u00e3 son a zo masa da takardu (da s\u0169nansa) ana w\u00e3ts\u00e3wa"}, {"id": 342276, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5548, "text": "A'aha! Kai dai, b\u00e3 su jin ts\u00f5ron L\u00e3hira."}, {"id": 342277, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5549, "text": "A'aha! Lalle ne, shi (Al\u0199ur'\u00e3ni) tun\u00e3tarwa ce."}, {"id": 342278, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5550, "text": "D\u00f5min wanda ya so, ya tuna."}, {"id": 342279, "identifier": "ha.gumi", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ha", "name": "Gumi", "transelator_en_name": "Abubakar Mahmoud Gumi", "format": "text", "type": "translation", "ayah_number": 5551, "text": "Kuma b\u00e3 z\u00e3 su tuna ba f\u00e3ce idan Allah Ya so, Shi ne Ya cancanta a bi Shi da ta\u0199awa kuma Ya cancanta ga Ya yi g\u00e3fara."}]