[{"id": 223201, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5552, "text": "NAY! I call to witness the Day of Resurrection!"}, {"id": 223638, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5553, "text": "But nay! I call to witness the accusing voice of man's own conscience!"}, {"id": 223639, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5554, "text": "Does man think that We cannot [resurrect him and] bring his bones together again?"}, {"id": 223640, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Yea indeed, We are able to make whole his very finger-tips!"}, {"id": 223641, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5556, "text": "None the less man chooses to deny what lies ahead of him,"}, {"id": 223642, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5557, "text": "asking [derisively], \"When is that Resurrection Day to be?\""}, {"id": 223643, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5558, "text": "But [on that Day,] when the eyesight is by fear confounded,"}, {"id": 223644, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5559, "text": "and the moon is darkened,"}, {"id": 223645, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5560, "text": "and the sun and the moon are brought together"}, {"id": 223646, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5561, "text": "on that Day will man exclaim \"Whither to flee?\""}, {"id": 223647, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5562, "text": "But nay: no refuge [for thee, O man]!"}, {"id": 223648, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5563, "text": "With thy Sustainer, on that Day, the journey's end will be!"}, {"id": 223649, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5564, "text": "Man will be apprised, on that Day, of what he has done and what he has left undone:"}, {"id": 223650, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5565, "text": "nay, but man shall against himself be an eye-witness,"}, {"id": 223651, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5566, "text": "even though he may veil himself in excuses."}, {"id": 223652, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5567, "text": "MOVE NOT thy tongue in haste, [repeating the words of the revelation:]"}, {"id": 223653, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5568, "text": "for, behold, it is for Us to gather it [in thy heart,] and to cause it to be read [as it ought to be read]."}, {"id": 223654, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5569, "text": "Thus, when We recite it, follow thou its wording [with all thy mind]:"}, {"id": 223655, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5570, "text": "and then, behold, it will be for Us to make its meaning clear."}, {"id": 223656, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5571, "text": "NAY, but [most of] you love this fleeting life,"}, {"id": 223657, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5572, "text": "and give no thought to the life to come [and to Judgment Day]!"}, {"id": 223658, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5573, "text": "Some faces will on that Day be bright with happiness,"}, {"id": 223659, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5574, "text": "looking up to their Sustainer;"}, {"id": 223660, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5575, "text": "and some faces will on that Day be overcast with despair,"}, {"id": 223661, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5576, "text": "knowing that a crushing calamity is about to befall them."}, {"id": 223662, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5577, "text": "NAY, but when [the last breath] comes up to the throat [of a dying man],"}, {"id": 223663, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5578, "text": "and people ask, \"Is there any wizard [that could save him]?\""}, {"id": 223664, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5579, "text": "the while he [himself] knows that this is the parting,"}, {"id": 223665, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5580, "text": "and is enwrapped in the pangs of death"}, {"id": 223666, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5581, "text": "at that time towards thy Sustainer does he feel impelled to turn!"}, {"id": 223667, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5582, "text": "[Useless, though, will be his repentance: for [as long as he was alive] he did not accept the truth, nor did he pray [for enlightenment],"}, {"id": 223668, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5583, "text": "but, on the contrary, he gave the lie to the truth and turned away [from it],"}, {"id": 223669, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5584, "text": "and then went arrogantly back to what he had come from."}, {"id": 223670, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5585, "text": "[And yet, O man, thine end comes hourly] nearer unto thee, and nearer \u2013"}, {"id": 223671, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5586, "text": "and ever nearer unto thee, and nearer!"}, {"id": 223672, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5587, "text": "DOES MAN, then, think that he is to be left to himself to go about at will?"}, {"id": 223673, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5588, "text": "Was he not once a [mere] drop of sperm that had been spilt,"}, {"id": 223674, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5589, "text": "and thereafter became a germ-cell - whereupon He created and formed [it] in accordance with what [it] was meant to be,"}, {"id": 223675, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5590, "text": "and fashioned out of it the two sexes, the male and the female?"}, {"id": 223676, "identifier": "en.asad", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5591, "text": "Is not He, then; able to bring the dead back to life?"}]