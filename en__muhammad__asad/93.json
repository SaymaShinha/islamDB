[{"id": 224162, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6080, "text": "CONSIDER the bright morning hours,"}, {"id": 224163, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6081, "text": "and the night when it grows still and dark."}, {"id": 224164, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Thy Sustainer has not forsaken thee, nor does He scorn thee:"}, {"id": 224165, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6083, "text": "for, indeed, the life to come will be better for thee than this earlier part [of thy life]!"}, {"id": 224166, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6084, "text": "And, indeed, in time will thy Sustainer grant thee [what thy heart desires], and thou shalt be well-pleased."}, {"id": 224167, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Has He not found thee an orphan, and given thee shelter?"}, {"id": 224168, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6086, "text": "And found thee lost on thy way, and guided thee?"}, {"id": 224169, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6087, "text": "And found thee in want, and given thee sufficiency?"}, {"id": 224170, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6088, "text": "Therefore, the orphan shalt thou never wrong,"}, {"id": 224171, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6089, "text": "and him that seeks [thy] help shalt thou never chide,"}, {"id": 224172, "identifier": "en.asad", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6090, "text": "and of thy Sustainer's blessings shalt thou [ever] speak."}]