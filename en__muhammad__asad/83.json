[{"id": 223931, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5849, "text": "WOE UNTO THOSE who give short measure:"}, {"id": 223932, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5850, "text": "those who, when they are to receive their due from [other] people, demand that it be given in full \u2013"}, {"id": 223933, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5851, "text": "but when they have to measure or weigh whatever they owe to others, give less than what is due!"}, {"id": 223934, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Do they not know that they are bound to be raised from the dead"}, {"id": 223935, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5853, "text": "[and called to account] on an awesome Day \u2013"}, {"id": 223936, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5854, "text": "the Day when all men shall stand before the Sustainer of all the worlds?"}, {"id": 223937, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5855, "text": "NAY, VERILY, the record of the wicked is indeed [set down] in a mode inescapable!"}, {"id": 223938, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5856, "text": "And what could make thee conceive what that that mode inescapable will be?"}, {"id": 223939, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5857, "text": "A record [indelibly] inscribed!"}, {"id": 223940, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5858, "text": "Woe on that Day unto those who give the lie to the truth \u2013"}, {"id": 223941, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5859, "text": "those who give the lie to the [coming of] Judgment Day:"}, {"id": 223942, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5860, "text": "for, none gives the lie to it but such as are wont to transgress against all that is [and are] immersed in sin:"}, {"id": 223943, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5861, "text": "[and so,] whenever Our messages are conveyed to them, they but say, \"Fables of ancient times!\""}, {"id": 223944, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5862, "text": "Nay, but their hearts are corroded by all [the evil] that they were wont to do!"}, {"id": 223945, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5863, "text": "Nay, verily, from [the grace of] their Sustainer shall they on that Day be debarred;"}, {"id": 223946, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5864, "text": "and then, behold, they shall enter the blazing fire"}, {"id": 223947, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5865, "text": "and be told: \"This is the [very thing] to which you were wont to give the lie!\""}, {"id": 223948, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5866, "text": "NAY, VERILY - the record of the truly virtuous is [set down] in a mode most lofty!"}, {"id": 223949, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5867, "text": "And what could make thee conceive what that mode most lofty will be?"}, {"id": 223950, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5868, "text": "A record [indelibly] inscribed,"}, {"id": 223951, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5869, "text": "witnessed~ by all who have [ever] been drawn close unto God."}, {"id": 223952, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5870, "text": "Behold, [in the life to come] the truly virtuous will indeed be in bliss:"}, {"id": 223953, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5871, "text": "[resting on couches, they will look up [to God]:"}, {"id": 223954, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5872, "text": "upon their faces thou wilt see the brightness of bliss."}, {"id": 223955, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5873, "text": "They will be given a drink of pure wine whereon the seal [of God] will have been set,"}, {"id": 223956, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5874, "text": "pouring forth with a fragrance of musk. To that [wine of paradise], then, let all such aspire as [are willing to] aspire to things of high account:"}, {"id": 223957, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5875, "text": "for it is composed of all that is most exalting"}, {"id": 223958, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5876, "text": "a source [of bliss] whereof those who are drawn close unto God shall drink."}, {"id": 223959, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5877, "text": "BEHOLD, those who have abandoned themselves to sin are wont to laugh at such as have attained to faith"}, {"id": 223960, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5878, "text": "and whenever they pass by them, they wink at one another [derisively];"}, {"id": 223961, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5879, "text": "and whenever they return to people of their own kind, they return full of jests;"}, {"id": 223962, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5880, "text": "and whenever they see those [who believe,] they say, \"Behold, these [people] have indeed gone astray!\""}, {"id": 223963, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5881, "text": "And, withal, they have no call to watch over [the beliefs of] others..."}, {"id": 223964, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5882, "text": "But on the Day [of Judgment], they who had attained to faith will [be able to] laugh at the [erstwhile] deniers of the truth:"}, {"id": 223965, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5883, "text": "[for, resting in paradise] on couches, they will look on [and say to themselves]:"}, {"id": 223966, "identifier": "en.asad", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 5884, "text": "\"Are these deniers of the truth being [thus] requited for [aught but] what they were wont to do?\""}]