[{"id": 224251, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6169, "text": "YOU ARE OBSESSED by greed for more and more"}, {"id": 224252, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6170, "text": "until you go down to your graves."}, {"id": 224253, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6171, "text": "Nay, in time you will come to understand!"}, {"id": 224254, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6172, "text": "And once again: Nay, in time you will come to understand!"}, {"id": 224255, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6173, "text": "Nay, if you could but understand [it] with an understanding [born] of certainty,"}, {"id": 224256, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6174, "text": "you would indeed, most surely, behold the blazing fire [of hell]!"}, {"id": 224257, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6175, "text": "In the end you will indeed, most surely, behold it with the eye of certainty:"}, {"id": 224258, "identifier": "en.asad", "surah_number": 102, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0627\u062b\u0631", "surah_en_name": "At-Takaathur", "surah_en_name_translation": "Competition", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 544, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Asad", "transelator_en_name": "Muhammad Asad", "format": "text", "type": "translation", "ayah_number": 6176, "text": "and on that Day you will most surely be called to account for [what you did with] the boon of life!"}]