[{"id": 367614, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6080, "text": "Demi waktu matahari sepenggalahan naik,"}, {"id": 367615, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6081, "text": "dan demi malam apabila telah sunyi (gelap),"}, {"id": 367616, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Tuhanmu tiada meninggalkan kamu dan tiada (pula) benci kepadamu."}, {"id": 367617, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6083, "text": "Dan sesungguhnya hari kemudian itu lebih baik bagimu daripada yang sekarang (permulaan)."}, {"id": 367618, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6084, "text": "Dan kelak Tuhanmu pasti memberikan karunia-Nya kepadamu, lalu (hati) kamu menjadi puas."}, {"id": 367619, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Bukankah Dia mendapatimu sebagai seorang yatim, lalu Dia melindungimu?"}, {"id": 367620, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6086, "text": "Dan Dia mendapatimu sebagai seorang yang bingung, lalu Dia memberikan petunjuk."}, {"id": 367621, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6087, "text": "Dan Dia mendapatimu sebagai seorang yang kekurangan, lalu Dia memberikan kecukupan."}, {"id": 367622, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6088, "text": "Sebab itu, terhadap anak yatim janganlah kamu berlaku sewenang-wenang."}, {"id": 367623, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6089, "text": "Dan terhadap orang yang minta-minta, janganlah kamu menghardiknya."}, {"id": 367624, "identifier": "id.indonesian", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6090, "text": "Dan terhadap nikmat Tuhanmu, maka hendaklah kamu siarkan."}]