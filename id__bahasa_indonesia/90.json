[{"id": 367562, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Aku benar-benar bersumpah dengan kota ini (Mekah),"}, {"id": 367563, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6025, "text": "dan kamu (Muhammad) bertempat di kota Mekah ini,"}, {"id": 361713, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6026, "text": "dan demi bapak dan anaknya."}, {"id": 367564, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6027, "text": "Sesungguhnya Kami telah menciptakan manusia berada dalam susah payah."}, {"id": 367565, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6028, "text": "Apakah manusia itu menyangka bahwa sekali-kali tiada seorangpun yang berkuasa atasnya?"}, {"id": 367566, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6029, "text": "Dan mengatakan: \"Aku telah menghabiskan harta yang banyak\"."}, {"id": 367567, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6030, "text": "Apakah dia menyangka bahwa tiada seorangpun yang melihatnya?"}, {"id": 367568, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Bukankah Kami telah memberikan kepadanya dua buah mata,"}, {"id": 367569, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6032, "text": "lidah dan dua buah bibir."}, {"id": 367570, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6033, "text": "Dan Kami telah menunjukkan kepadanya dua jalan,"}, {"id": 367571, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Tetapi dia tiada menempuh jalan yang mendaki lagi sukar."}, {"id": 367572, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6035, "text": "Tahukah kamu apakah jalan yang mendaki lagi sukar itu?"}, {"id": 367573, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6036, "text": "(yaitu) melepaskan budak dari perbudakan,"}, {"id": 367574, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6037, "text": "atau memberi makan pada hari kelaparan,"}, {"id": 367575, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6038, "text": "(kepada) anak yatim yang ada hubungan kerabat,"}, {"id": 367576, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6039, "text": "atau kepada orang miskin yang sangat fakir."}, {"id": 367577, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6040, "text": "Dan dia (tidak pula) termasuk orang-orang yang beriman dan saling berpesan untuk bersabar dan saling berpesan untuk berkasih sayang."}, {"id": 367578, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6041, "text": "Mereka (orang-orang yang beriman dan saling berpesan itu) adalah golongan kanan."}, {"id": 367579, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6042, "text": "Dan orang-orang yang kafir kepada ayat-ayat Kami, mereka itu adalah golongan kiri."}, {"id": 367580, "identifier": "id.indonesian", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Mereka berada dalam neraka yang ditutup rapat."}]