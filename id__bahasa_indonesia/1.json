[{"id": 367770, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 1, "text": "Dengan menyebut nama Allah Yang Maha Pemurah lagi Maha Penyayang."}, {"id": 367771, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 2, "text": "Segala puji bagi Allah, Tuhan semesta alam."}, {"id": 367772, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3, "text": "Maha Pemurah lagi Maha Penyayang."}, {"id": 367773, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4, "text": "Yang menguasai di Hari Pembalasan."}, {"id": 367774, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5, "text": "Hanya Engkaulah yang kami sembah, dan hanya kepada Engkaulah kami meminta pertolongan."}, {"id": 367775, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6, "text": "Tunjukilah kami jalan yang lurus,"}, {"id": 367776, "identifier": "id.indonesian", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 7, "text": "(yaitu) Jalan orang-orang yang telah Engkau beri nikmat kepada mereka; bukan (jalan) mereka yang dimurkai dan bukan (pula jalan) mereka yang sesat."}]