[{"id": 366474, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4902, "text": "(Tuhan) Yang Maha Pemurah,"}, {"id": 366475, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4903, "text": "Yang telah mengajarkan al Quran."}, {"id": 366476, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4904, "text": "Dia menciptakan manusia."}, {"id": 366477, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4905, "text": "Mengajarnya pandai berbicara."}, {"id": 366478, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4906, "text": "Matahari dan bulan (beredar) menurut perhitungan."}, {"id": 366479, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4907, "text": "Dan tumbuh-tumbuhan dan pohon-pohonan kedua-duanya tunduk kepada-Nya."}, {"id": 366480, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4908, "text": "Dan Allah telah meninggikan langit dan Dia meletakkan neraca (keadilan)."}, {"id": 366481, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4909, "text": "Supaya kamu jangan melampaui batas tentang neraca itu."}, {"id": 366482, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4910, "text": "Dan tegakkanlah timbangan itu dengan adil dan janganlah kamu mengurangi neraca itu."}, {"id": 366483, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4911, "text": "Dan Allah telah meratakan bumi untuk makhluk(Nya)."}, {"id": 366484, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4912, "text": "Di bumi itu ada buah-buahan dan pohon kurma yang mempunyai kelopak mayang."}, {"id": 366485, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4913, "text": "Dan biji-bijian yang berkulit dan bunga-bunga yang harum baunya."}, {"id": 366486, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4914, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366487, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4915, "text": "Dia menciptakan manusia dari tanah kering seperti tembikar,"}, {"id": 366488, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4916, "text": "dan Dia menciptakan jin dari nyala api."}, {"id": 366489, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 531, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4917, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366490, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4918, "text": "Tuhan yang memelihara kedua tempat terbit matahari dan Tuhan yang memelihara kedua tempat terbenamnya"}, {"id": 366491, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4919, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366492, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4920, "text": "Dia membiarkan dua lautan mengalir yang keduanya kemudian bertemu,"}, {"id": 366493, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4921, "text": "antara keduanya ada batas yang tidak dilampaui masing-masing."}, {"id": 366494, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4922, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366495, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4923, "text": "Dari keduanya keluar mutiara dan marjan."}, {"id": 366496, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4924, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366497, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4925, "text": "Dan kepunyaan-Nya lah bahtera-bahtera yang tinggi layarnya di lautan laksana gunung-gunung."}, {"id": 364296, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 466, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4926, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 364369, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4927, "text": "Semua yang ada di bumi itu akan binasa."}, {"id": 364394, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4928, "text": "Dan tetap kekal Dzat Tuhanmu yang mempunyai kebesaran dan kemuliaan."}, {"id": 365040, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4929, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366498, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4930, "text": "Semua yang ada di langit dan bumi selalu meminta kepada-Nya. Setiap waktu Dia dalam kesibukan."}, {"id": 366499, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4931, "text": "Maka nikmat Rabb-mu yang manakah yang kamu dustakan?"}, {"id": 363246, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4932, "text": "Kami akan memperhatikan sepenuhnya kepadamu hai manusia dan jin."}, {"id": 363484, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4933, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366500, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4934, "text": "Hai jama'ah jin dan manusia, jika kamu sanggup menembus (melintasi) penjuru langit dan bumi, maka lintasilah, kamu tidak dapat menembusnya kecuali dengan kekuatan."}, {"id": 366501, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4935, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366502, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4936, "text": "Kepada kamu, (jin dan manusia) dilepaskan nyala api dan cairan tembaga maka kamu tidak dapat menyelamatkan diri (dari padanya)."}, {"id": 366503, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4937, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366504, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4938, "text": "Maka apabila langit telah terbelah dan menjadi merah mawar seperti (kilapan) minyak."}, {"id": 366505, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4939, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366506, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4940, "text": "Pada waktu itu manusia dan jin tidak ditanya tentang dosanya."}, {"id": 366507, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 532, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4941, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366508, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4942, "text": "Orang-orang yang berdosa dikenal dengan tanda-tandannya, lalu dipegang ubun-ubun dan kaki mereka."}, {"id": 366509, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4943, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366510, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4944, "text": "Inilah neraka Jahannam yang didustakan oleh orang-orang berdosa."}, {"id": 366511, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4945, "text": "Mereka berkeliling di antaranya dan di antara air mendidih yang memuncak panasnya."}, {"id": 366512, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 467, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4946, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366513, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4947, "text": "Dan bagi orang yang takut akan saat menghadap Tuhannya ada dua surga."}, {"id": 366514, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4948, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?,"}, {"id": 366515, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4949, "text": "kedua surga itu mempunyai pohon-pohonan dan buah-buahan."}, {"id": 366516, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4950, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366517, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4951, "text": "Di dalam kedua surga itu ada dua buah mata air yang mengalir"}, {"id": 366518, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4952, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366519, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4953, "text": "Di dalam kedua surga itu terdapat segala macam buah-buahan yang berpasangan."}, {"id": 366520, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4954, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366521, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4955, "text": "Mereka bertelekan di atas permadani yang sebelah dalamnya dari sutera. Dan buah-buahan di kedua surga itu dapat (dipetik) dari dekat."}, {"id": 366522, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4956, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366523, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4957, "text": "Di dalam surga itu ada bidadari-bidadari yang sopan menundukkan pandangannya, tidak pernah disentuh oleh manusia sebelum mereka (penghuni-penghuni surga yang menjadi suami mereka), dan tidak pula oleh jin."}, {"id": 366524, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4958, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366525, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4959, "text": "Seakan-akan bidadari itu permata yakut dan marjan."}, {"id": 366526, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4960, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366527, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4961, "text": "Tidak ada balasan kebaikan kecuali kebaikan (pula)."}, {"id": 365101, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4962, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 365162, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4963, "text": "Dan selain dari dua surga itu ada dua surga lagi"}, {"id": 363537, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4964, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 363643, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4965, "text": "Kedua surga itu (kelihatan) hijau tua warnanya."}, {"id": 364225, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4966, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 364284, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4967, "text": "Di dalam kedua surga itu ada dua buah mata air yang memancar."}, {"id": 364756, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 533, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4968, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 365025, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4969, "text": "Di dalam keduanya (ada macam-macam) buah-buahan dan kurma serta delima."}, {"id": 365466, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4970, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366528, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4971, "text": "Di dalam surga itu ada bidadari-bidadari yang baik-baik lagi cantik-cantik."}, {"id": 366529, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4972, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366530, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4973, "text": "(Bidadari-bidadari) yang jelita, putih bersih, dipingit dalam rumah."}, {"id": 366531, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4974, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366532, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4975, "text": "Mereka tidak pernah disentuh oleh manusia sebelum mereka (penghuni-penghuni surga yang menjadi suami mereka), dan tidak pula oleh jin."}, {"id": 366533, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4976, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366534, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4977, "text": "Mereka bertelekan pada bantal-bantal yang hijau dan permadani-permadani yang indah."}, {"id": 366535, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4978, "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?"}, {"id": 366536, "identifier": "id.indonesian", "surah_number": 55, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u062d\u0645\u0646", "surah_en_name": "Ar-Rahmaan", "surah_en_name_translation": "The Beneficent", "revelation_type": "Medinan", "juz": 27, "manzil": 7, "page": 534, "ruku": 468, "hizb_quarter": 213, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4979, "text": "Maha Agung nama Tuhanmu Yang Mempunyai Kebesaran dan Karunia."}]