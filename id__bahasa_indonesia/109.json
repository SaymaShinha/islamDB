[{"id": 367742, "identifier": "id.indonesian", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6208, "text": "Katakanlah: \"Hai orang-orang kafir,"}, {"id": 367743, "identifier": "id.indonesian", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6209, "text": "Aku tidak akan menyembah apa yang kamu sembah."}, {"id": 367744, "identifier": "id.indonesian", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6210, "text": "Dan kamu bukan penyembah Tuhan yang aku sembah."}, {"id": 367745, "identifier": "id.indonesian", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6211, "text": "Dan aku tidak pernah menjadi penyembah apa yang kamu sembah,"}, {"id": 367746, "identifier": "id.indonesian", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6212, "text": "dan kamu tidak pernah (pula) menjadi penyembah Tuhan yang aku sembah."}, {"id": 367747, "identifier": "id.indonesian", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6213, "text": "Untukmu agamamu, dan untukkulah, agamaku\"."}]