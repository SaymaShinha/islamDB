[{"id": 367053, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5496, "text": "Hai orang yang berkemul (berselimut),"}, {"id": 367054, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5497, "text": "bangunlah, lalu berilah peringatan!"}, {"id": 367055, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5498, "text": "dan Tuhanmu agungkanlah!"}, {"id": 367056, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5499, "text": "dan pakaianmu bersihkanlah,"}, {"id": 367057, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5500, "text": "dan perbuatan dosa tinggalkanlah,"}, {"id": 367058, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5501, "text": "dan janganlah kamu memberi (dengan maksud) memperoleh (balasan) yang lebih banyak."}, {"id": 367059, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5502, "text": "Dan untuk (memenuhi perintah) Tuhanmu, bersabarlah."}, {"id": 367060, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5503, "text": "Apabila ditiup sangkakala,"}, {"id": 367061, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5504, "text": "maka waktu itu adalah waktu (datangnya) hari yang sulit,"}, {"id": 367062, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5505, "text": "bagi orang-orang kafir lagi tidak mudah."}, {"id": 367063, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5506, "text": "Biarkanlah Aku bertindak terhadap orang yang Aku telah menciptakannya sendirian."}, {"id": 367064, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5507, "text": "Dan Aku jadikan baginya harta benda yang banyak,"}, {"id": 367065, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5508, "text": "dan anak-anak yang selalu bersama dia,"}, {"id": 367066, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5509, "text": "dan Ku-lapangkan baginya (rezeki dan kekuasaan) dengan selapang-lapangnya,"}, {"id": 367067, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5510, "text": "kemudian dia ingin sekali supaya Aku menambahnya."}, {"id": 367068, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5511, "text": "Sekali-kali tidak (akan Aku tambah), karena sesungguhnya dia menentang ayat-ayat Kami (Al Quran)."}, {"id": 367069, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5512, "text": "Aku akan membebaninya mendaki pendakian yang memayahkan."}, {"id": 367070, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5513, "text": "Sesungguhnya dia telah memikirkan dan menetapkan (apa yang ditetapkannya),"}, {"id": 367071, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5514, "text": "maka celakalah dia! Bagaimana dia menetapkan?,"}, {"id": 367072, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5515, "text": "kemudian celakalah dia! Bagaimanakah dia menetapkan?,"}, {"id": 367073, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5516, "text": "kemudian dia memikirkan,"}, {"id": 367074, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5517, "text": "sesudah itu dia bermasam muka dan merengut,"}, {"id": 367075, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5518, "text": "kemudian dia berpaling (dari kebenaran) dan menyombongkan diri,"}, {"id": 367076, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5519, "text": "lalu dia berkata: \"(Al Quran) ini tidak lain hanyalah sihir yang dipelajari (dari orang-orang dahulu),"}, {"id": 367077, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5520, "text": "ini tidak lain hanyalah perkataan manusia\"."}, {"id": 367078, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5521, "text": "Aku akan memasukkannya ke dalam (neraka) Saqar."}, {"id": 367079, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5522, "text": "Tahukah kamu apakah (neraka) Saqar itu?"}, {"id": 367080, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5523, "text": "Saqar itu tidak meninggalkan dan tidak membiarkan."}, {"id": 366999, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5524, "text": "(Neraka Saqar) adalah pembakar kulit manusia."}, {"id": 367048, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5525, "text": "Dan di atasnya ada sembilan belas (malaikat penjaga)."}, {"id": 367081, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 510, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5526, "text": "Dan tiada Kami jadikan penjaga neraka itu melainkan dari malaikat: dan tidaklah Kami menjadikan bilangan mereka itu melainkan untuk jadi cobaan bagi orang-orang kafir, supaya orang-orang yang diberi Al-Kitab menjadi yakin dan supaya orang yang beriman bertambah imannya dan supaya orang-orang yang diberi Al Kitab dan orng-orang mukmin itu tidak ragu-ragu dan supaya orang-orang yang di dalam hatinya ada penyakit dan orang-orang kafir (mengatakan): \"Apakah yang dikehendaki Allah dengan bilangan ini sebagai suatu perumpamaan?\" Demikianlah Allah membiarkan sesat orang-orang yang dikehendaki-Nya dan memberi petunjuk kepada siapa yang dikehendaki-Nya. Dan tidak ada yang mengetahui tentara Tuhanmu melainkan Dia sendiri. Dan Saqar itu tiada lain hanyalah peringatan bagi manusia."}, {"id": 367082, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5527, "text": "Sekali-kali tidak, demi bulan,"}, {"id": 367083, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5528, "text": "dan malam ketika telah berlalu,"}, {"id": 367084, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5529, "text": "dan subuh apabila mulai terang."}, {"id": 367085, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5530, "text": "Sesungguhnya Saqar itu adalah salah satu bencana yang amat besar,"}, {"id": 367086, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5531, "text": "sebagai ancaman bagi manusia."}, {"id": 367087, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5532, "text": "(Yaitu) bagi siapa di antaramu yang berkehendak akan maju atau mundur."}, {"id": 367088, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5533, "text": "Tiap-tiap diri bertanggung jawab atas apa yang telah diperbuatnya,"}, {"id": 367089, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5534, "text": "kecuali golongan kanan,"}, {"id": 367090, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5535, "text": "berada di dalam surga, mereka tanya menanya,"}, {"id": 367091, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5536, "text": "tentang (keadaan) orang-orang yang berdosa,"}, {"id": 367092, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5537, "text": "\"Apakah yang memasukkan kamu ke dalam Saqar (neraka)?\""}, {"id": 367093, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5538, "text": "Mereka menjawab: \"Kami dahulu tidak termasuk orang-orang yang mengerjakan shalat,"}, {"id": 367094, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5539, "text": "dan kami tidak (pula) memberi makan orang miskin,"}, {"id": 367095, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5540, "text": "dan adalah kami membicarakan yang bathil, bersama dengan orang-orang yang membicarakannya,"}, {"id": 367096, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5541, "text": "dan adalah kami mendustakan hari pembalasan,"}, {"id": 367097, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 576, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5542, "text": "hingga datang kepada kami kematian\"."}, {"id": 367098, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5543, "text": "Maka tidak berguna lagi bagi mereka syafa'at dari orang-orang yang memberikan syafa'at."}, {"id": 367099, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5544, "text": "Maka mengapa mereka (orang-orang kafir) berpaling dari peringatan (Allah)?,"}, {"id": 367100, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5545, "text": "seakan-akan mereka itu keledai liar yang lari terkejut,"}, {"id": 367101, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5546, "text": "lari daripada singa."}, {"id": 367102, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5547, "text": "Bahkan tiap-tiap orang dari mereka berkehendak supaya diberikan kepadanya lembaran-lembaran yang terbuka."}, {"id": 367103, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5548, "text": "Sekali-kali tidak. Sebenarnya mereka tidak takut kepada negeri akhirat."}, {"id": 367104, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5549, "text": "Sekali-kali tidak demikian halnya. Sesungguhnya Al Quran itu adalah peringatan."}, {"id": 367105, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5550, "text": "Maka barangsiapa menghendaki, niscaya dia mengambil pelajaran daripadanya (Al Quran)."}, {"id": 367106, "identifier": "id.indonesian", "surah_number": 74, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062f\u062b\u0631", "surah_en_name": "Al-Muddaththir", "surah_en_name_translation": "The Cloaked One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 511, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5551, "text": "Dan mereka tidak akan mengambil pelajaran daripadanya kecuali (jika) Allah menghendakinya. Dia (Allah) adalah Tuhan Yang patut (kita) bertakwa kepada-Nya dan berhak memberi ampun."}]