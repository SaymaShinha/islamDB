[{"id": 367532, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5994, "text": "Demi fajar,"}, {"id": 367533, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5995, "text": "dan malam yang sepuluh,"}, {"id": 367534, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5996, "text": "dan yang genap dan yang ganjil,"}, {"id": 367535, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5997, "text": "dan malam bila berlalu."}, {"id": 367536, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5998, "text": "Pada yang demikian itu terdapat sumpah (yang dapat diterima) oleh orang-orang yang berakal."}, {"id": 367537, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5999, "text": "Apakah kamu tidak memperhatikan bagaimana Tuhanmu berbuat terhadap kaum 'Aad?"}, {"id": 367538, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6000, "text": "(yaitu) penduduk Iram yang mempunyai bangunan-bangunan yang tinggi,"}, {"id": 367539, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6001, "text": "yang belum pernah dibangun (suatu kota) seperti itu, di negeri-negeri lain,"}, {"id": 367540, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6002, "text": "dan kaum Tsamud yang memotong batu-batu besar di lembah,"}, {"id": 367541, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6003, "text": "dan kaum Fir'aun yang mempunyai pasak-pasak (tentara yang banyak),"}, {"id": 367542, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6004, "text": "yang berbuat sewenang-wenang dalam negeri,"}, {"id": 367543, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6005, "text": "lalu mereka berbuat banyak kerusakan dalam negeri itu,"}, {"id": 367544, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6006, "text": "karena itu Tuhanmu menimpakan kepada mereka cemeti azab,"}, {"id": 367545, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6007, "text": "sesungguhnya Tuhanmu benar-benar mengawasi."}, {"id": 367546, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6008, "text": "Adapun manusia apabila Tuhannya mengujinya lalu dia dimuliakan-Nya dan diberi-Nya kesenangan, maka dia akan berkata: \"Tuhanku telah memuliakanku\"."}, {"id": 367547, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6009, "text": "Adapun bila Tuhannya mengujinya lalu membatasi rezekinya maka dia berkata: \"Tuhanku menghinakanku\"."}, {"id": 367548, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6010, "text": "Sekali-kali tidak (demikian), sebenarnya kamu tidak memuliakan anak yatim,"}, {"id": 367549, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6011, "text": "dan kamu tidak saling mengajak memberi makan orang miskin,"}, {"id": 367550, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6012, "text": "dan kamu memakan harta pusaka dengan cara mencampur baurkan (yang halal dan yang bathil),"}, {"id": 367551, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6013, "text": "dan kamu mencintai harta benda dengan kecintaan yang berlebihan."}, {"id": 367552, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6014, "text": "Jangan (berbuat demikian). Apabila bumi digoncangkan berturut-turut,"}, {"id": 367553, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6015, "text": "dan datanglah Tuhanmu; sedang malaikat berbaris-baris."}, {"id": 367554, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6016, "text": "Dan pada hari itu diperlihatkan neraka Jahannam; dan pada hari itu ingatlah manusia, akan tetapi tidak berguna lagi mengingat itu baginya."}, {"id": 367555, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6017, "text": "Dia mengatakan: \"Alangkah baiknya kiranya aku dahulu mengerjakan (amal saleh) untuk hidupku ini\"."}, {"id": 367556, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6018, "text": "Maka pada hari itu tiada seorangpun yang menyiksa seperti siksa-Nya."}, {"id": 367557, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6019, "text": "dan tiada seorangpun yang mengikat seperti ikatan-Nya."}, {"id": 367558, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6020, "text": "Hai jiwa yang tenang."}, {"id": 367559, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6021, "text": "Kembalilah kepada Tuhanmu dengan hati yang puas lagi diridhai-Nya."}, {"id": 367560, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6022, "text": "Maka masuklah ke dalam jama'ah hamba-hamba-Ku,"}, {"id": 367561, "identifier": "id.indonesian", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6023, "text": "masuklah ke dalam surga-Ku."}]