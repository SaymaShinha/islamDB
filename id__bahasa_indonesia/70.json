[{"id": 366926, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5376, "text": "Seseorang telah meminta kedatangan azab yang akan menimpa,"}, {"id": 366927, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5377, "text": "orang-orang kafir, yang tidak seorangpun dapat menolaknya,"}, {"id": 366928, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5378, "text": "(yang datang) dari Allah, Yang mempunyai tempat-tempat naik."}, {"id": 366929, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5379, "text": "Malaikat-malaikat dan Jibril naik (menghadap) kepada Tuhan dalam sehari yang kadarnya limapuluh ribu tahun."}, {"id": 366930, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5380, "text": "Maka bersabarlah kamu dengan sabar yang baik."}, {"id": 366931, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5381, "text": "Sesungguhnya mereka memandang siksaaan itu jauh (mustahil)."}, {"id": 366932, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5382, "text": "Sedangkan Kami memandangnya dekat (mungkin terjadi)."}, {"id": 366933, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5383, "text": "Pada hari ketika langit menjadi seperti luluhan perak,"}, {"id": 366934, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5384, "text": "dan gunung-gunung menjadi seperti bulu (yang berterbangan),"}, {"id": 366935, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5385, "text": "dan tidak ada seorang teman akrabpun menanyakan temannya,"}, {"id": 366936, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5386, "text": "sedang mereka saling memandang. Orang kafir ingin kalau sekiranya dia dapat menebus (dirinya) dari azab hari itu dengan anak-anaknya,"}, {"id": 366937, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5387, "text": "dan isterinya dan saudaranya,"}, {"id": 366938, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5388, "text": "dan kaum familinya yang melindunginya (di dunia)."}, {"id": 366939, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5389, "text": "Dan orang-orang di atas bumi seluruhnya kemudian (mengharapkan) tebusan itu dapat menyelamatkannya."}, {"id": 366940, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5390, "text": "Sekali-kali tidak dapat, sesungguhnya neraka itu adalah api yang bergolak,"}, {"id": 366941, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5391, "text": "yang mengelupas kulit kepala,"}, {"id": 366942, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5392, "text": "yang memanggil orang yang membelakang dan yang berpaling (dari agama),"}, {"id": 366943, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5393, "text": "serta mengumpulkan (harta benda) lalu menyimpannya."}, {"id": 366944, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5394, "text": "Sesungguhnya manusia diciptakan bersifat keluh kesah lagi kikir."}, {"id": 366945, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5395, "text": "Apabila ia ditimpa kesusahan ia berkeluh kesah,"}, {"id": 366946, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5396, "text": "dan apabila ia mendapat kebaikan ia amat kikir,"}, {"id": 366947, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5397, "text": "kecuali orang-orang yang mengerjakan shalat,"}, {"id": 366948, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5398, "text": "yang mereka itu tetap mengerjakan shalatnya,"}, {"id": 366949, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5399, "text": "dan orang-orang yang dalam hartanya tersedia bagian tertentu,"}, {"id": 366950, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5400, "text": "bagi orang (miskin) yang meminta dan orang yang tidak mempunyai apa-apa (yang tidak mau meminta),"}, {"id": 366951, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5401, "text": "dan orang-orang yang mempercayai hari pembalasan,"}, {"id": 366952, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5402, "text": "dan orang-orang yang takut terhadap azab Tuhannya."}, {"id": 366953, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5403, "text": "Karena sesungguhnya azab Tuhan mereka tidak dapat orang merasa aman (dari kedatangannya)."}, {"id": 366954, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5404, "text": "Dan orang-orang yang memelihara kemaluannya,"}, {"id": 366955, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5405, "text": "kecuali terhadap isteri-isteri mereka atau budak-budak yang mereka miliki, maka sesungguhnya mereka dalam hal ini tiada tercela."}, {"id": 366956, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5406, "text": "Barangsiapa mencari yang di balik itu, maka mereka itulah orang-orang yang melampaui batas."}, {"id": 366957, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5407, "text": "Dan orang-orang yang memelihara amanat-amanat (yang dipikulnya) dan janjinya."}, {"id": 366958, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5408, "text": "Dan orang-orang yang memberikan kesaksiannya."}, {"id": 366959, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5409, "text": "Dan orang-orang yang memelihara shalatnya."}, {"id": 366960, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 502, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5410, "text": "Mereka itu (kekal) di surga lagi dimuliakan."}, {"id": 366961, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5411, "text": "Mengapakah orang-orang kafir itu bersegera datang ke arahmu,"}, {"id": 366962, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5412, "text": "dari kanan dan dari kiri dengan berkelompok-kelompok."}, {"id": 366963, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5413, "text": "Adakah setiap orang dari orang-orang kafir itu ingin masuk ke dalam surga yang penuh kenikmatan?,"}, {"id": 366964, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 569, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5414, "text": "sekali-kali tidak! Sesungguhnya Kami ciptakan mereka dari apa yang mereka ketahui (air mani)."}, {"id": 366965, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5415, "text": "Maka aku bersumpah dengan Tuhan Yang memiliki timur dan barat, sesungguhnya Kami benar-benar Maha Kuasa."}, {"id": 366966, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5416, "text": "Untuk mengganti (mereka) dengan kaum yang lebih baik dari mereka, dan Kami sekali-kali tidak dapat dikalahkan."}, {"id": 366967, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5417, "text": "Maka biarkanlah mereka tenggelam (dalam kebatilan) dan bermain-main sampai mereka menjumpai hari yang diancamkan kepada mereka,"}, {"id": 366968, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5418, "text": "(yaitu) pada hari mereka keluar dari kubur dengan cepat seakan-akan mereka pergi dengan segera kepada berhala-berhala (sewaktu di dunia),"}, {"id": 366969, "identifier": "id.indonesian", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 570, "ruku": 503, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5419, "text": "dalam keadaan mereka menekurkan pandangannya (serta) diliputi kehinaan. Itulah hari yang dahulunya diancamkan kepada mereka."}]