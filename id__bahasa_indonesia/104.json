uku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6186, "text": "yang (membakar) sampai ke hati."}, {"id": 367721, "identifier": "id.indonesian", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6187, "text": "Sesungguhnya api itu ditutup rapat atas mereka,"}, {"id": 367722, "identifier": "id.indonesian", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "id", "name": "Bahasa Indonesia", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6188, "text": "(sedang mereka itu) diikat pada tiang-tiang yang panjang."}]