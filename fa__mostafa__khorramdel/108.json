[{"id": 311769, "identifier": "fa.khorramdel", "surah_number": 108, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0648\u062b\u0631", "surah_en_name": "Al-Kawthar", "surah_en_name_translation": "Abundance", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 550, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u062e\u0631\u0645\u062f\u0644", "transelator_en_name": "Mostafa Khorramdel", "format": "text", "type": "translation", "ayah_number": 6205, "text": "\u0645\u0627 \u0628\u0647 \u062a\u0648 \u062e\u06cc\u0631 \u0648 \u062e\u0648\u0628\u06cc \u0628\u06cc\u200c\u0646\u0647\u0627\u06cc\u062a \u0641\u0631\u0627\u0648\u0627\u0646\u06cc \u0631\u0627 \u0639\u0637\u0627\u0621 \u06a9\u0631\u062f\u0647\u200c\u0627\u06cc\u0645 (\u06a9\u0647 \u0646\u0628\u0648\u0651\u062a \u0648 \u062f\u06cc\u0646 \u062d\u0642 \u0648 \u0647\u062f\u0627\u06cc\u062a\u060c \u0648 \u0647\u0631 \u0622\u0646 \u0686\u06cc\u0632\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0633\u0639\u0627\u062f\u062a \u062f\u0648 \u062c\u0647\u0627\u0646 \u0631\u0627 \u0628\u0647 \u0647\u0645\u0631\u0627\u0647 \u062f\u0627\u0631\u062f). [[\u00ab\u0627\u0644\u0652\u06a9\u064e\u0648\u0652\u062b\u064e\u0631\u064e\u00bb: \u0645\u0628\u0627\u0644\u063a\u0647 \u062f\u0631 \u06a9\u062b\u0631\u062a \u0627\u0633\u062a\u060c \u06cc\u0639\u0646\u06cc \u062e\u06cc\u0631 \u0648 \u062e\u0648\u0628\u06cc \u0628\u06cc\u200c\u0646\u0647\u0627\u06cc\u062a \u0641\u0631\u0627\u0648\u0627\u0646.]]"}, {"id": 311770, "identifier": "fa.khorramdel", "surah_number": 108, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0648\u062b\u0631", "surah_en_name": "Al-Kawthar", "surah_en_name_translation": "Abundance", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 550, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u062e\u0631\u0645\u062f\u0644", "transelator_en_name": "Mostafa Khorramdel", "format": "text", "type": "translation", "ayah_number": 6206, "text": "\u062d\u0627\u0644 \u06a9\u0647 \u0686\u0646\u06cc\u0646 \u0627\u0633\u062a \u062a\u0646\u0647\u0627 \u0628\u0631\u0627\u06cc \u067e\u0631\u0648\u0631\u062f\u06af\u0627\u0631 \u062e\u0648\u062f \u0646\u0645\u0627\u0632 \u0628\u062e\u0648\u0627\u0646 \u0648 \u0642\u0631\u0628\u0627\u0646\u06cc \u0628\u06a9\u0646. [[\u00ab\u0635\u064e\u0644\u0650\u0651 \u0644\u0650\u0631\u064e\u0628\u0650\u0651\u06a9\u064e\u00bb: \u062a\u0646\u0647\u0627 \u0628\u0631\u0627\u06cc \u067e\u0631\u0648\u0631\u062f\u06af\u0627\u0631\u062a \u0646\u0645\u0627\u0632 \u0628\u062e\u0648\u0627\u0646. \u0645\u0631\u0627\u062f \u0646\u0645\u0627\u0632 \u062e\u0627\u0644\u06cc \u0627\u0632 \u0631\u06cc\u0627 \u0627\u0633\u062a. \u00ab\u0625\u0646\u0652\u062d\u064e\u0631\u0652\u00bb: \u062a\u0646\u0647\u0627 \u0628\u0631\u0627\u06cc \u067e\u0631\u0648\u0631\u062f\u06af\u0627\u0631\u062a\u060c \u0648 \u062a\u0646\u0647\u0627 \u0628\u0647 \u0646\u0627\u0645 \u0627\u0648\u060c \u0646\u0647 \u06a9\u0633 \u0648 \u0686\u06cc\u0632 \u062f\u06cc\u06af\u0631\u06cc\u060c \u0642\u0631\u0628\u0627\u0646\u06cc \u06a9\u0646.]]"}, {"id": 311771, "identifier": "fa.khorramdel", "surah_number": 108, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0648\u062b\u0631", "surah_en_name": "Al-Kawthar", "surah_en_name_translation": "Abundance", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 550, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "fa", "name": "\u062e\u0631\u0645\u062f\u0644", "transelator_en_name": "Mostafa Khorramdel", "format": "text", "type": "translation", "ayah_number": 6207, "text": "\u0628\u062f\u0648\u0646 \u0634\u06a9\u0651\u060c \u062f\u0634\u0645\u0646 \u06a9\u06cc\u0646\u0647\u200c\u062a\u0648\u0632 \u062a\u0648 \u0628\u06cc\u200c\u062e\u06cc\u0631 \u0648 \u0628\u0631\u06a9\u062a \u0648 \u0628\u06cc\u200c\u0646\u0627\u0645 \u0648 \u0646\u0634\u0627\u0646 \u062e\u0648\u0627\u0647\u062f \u0628\u0648\u062f. [[\u00ab\u0634\u064e\u0627\u0646\u0650\u0626\u200c\u064e\u00bb: \u062f\u0634\u0645\u0646 \u06a9\u06cc\u0646\u0647\u200c\u062a\u0648\u0632. \u0627\u0632 \u0645\u0635\u062f\u0631 (\u0634\u064e\u0646\u064e\u0622\u0646) \u0628\u0647 \u0645\u0639\u0646\u06cc\u200c\u06a9\u06cc\u0646\u0647\u200c\u062a\u0648\u0632\u06cc (\u0646\u06af\u0627: \u0645\u0627\u0626\u062f\u0647 / 2 \u0648 8). \u00ab\u0627\u0644\u0623\u0628\u0652\u062a\u064e\u0631\u064f\u00bb: \u0628\u06cc\u200c\u0646\u0627\u0645 \u0648 \u0646\u0634\u0627\u0646. \u0628\u06cc\u200c\u062e\u06cc\u0631 \u0648 \u0628\u0631\u06a9\u062a.]]"}]