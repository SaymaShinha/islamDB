[{"id": 504692, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5994, "text": "Pasha agimin,"}, {"id": 504693, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5995, "text": "Pasha dhjet\u00eb net\u00ebt,"}, {"id": 504694, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5996, "text": "Pasha \u00e7iftin edhe tek\u00ebt,"}, {"id": 504695, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5997, "text": "Pasha nat\u00ebn kur kalon,"}, {"id": 504696, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5998, "text": "A \u00ebsht\u00eb ky betim p\u00ebr t\u00eb men\u00e7urin?"}, {"id": 504697, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5999, "text": "A nuk e di se \u00e7\u2019b\u00ebri Zoti yt me Ad-in?"}, {"id": 504698, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6000, "text": "Banor\u00ebt e Irem-it, me shtylla (pallatet),"}, {"id": 504699, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6001, "text": "Q\u00eb nuk ka pasur shoq askund,"}, {"id": 504700, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6002, "text": "Edhe Themud-in t\u00eb cil\u00ebt skalit\u00ebn shkrepat n\u00eb lugin\u00eb,"}, {"id": 504701, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6003, "text": "Edhe me Faraonin q\u00eb kishte tenda,"}, {"id": 504702, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6004, "text": "T\u00eb cil\u00ebt b\u00ebn\u00eb krime n\u00eb tok\u00eb,"}, {"id": 504703, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6005, "text": "Dhe n\u00eb t\u00eb e shtuan shkat\u00ebrrimin,"}, {"id": 504704, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6006, "text": "Andaj Zoti yt i goditi me shufr\u00ebn e d\u00ebnimit,"}, {"id": 502272, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6007, "text": "Sepse Zoti yt \u00ebsht\u00eb nj\u00ebmend n\u00eb prit\u00eb,"}, {"id": 504705, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6008, "text": "Nd\u00ebrsa njeriu, kur do Zoti i tij ta sprovoj\u00eb, e nderon dhe e shp\u00ebrblen me dhunti, thot: \u201cZoti im m\u00eb ka respektuar\u201d."}, {"id": 504706, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6009, "text": "Por kur, p\u00ebr t\u00eb sprovuar, ia mungon furnizmin e tij, thot: \u201cZoti im m\u00eb ka l\u00ebn\u00eb anash (n\u00eb\u00e7muar)\u201d"}, {"id": 504707, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6010, "text": "Assesi! Por ju nuk e respektoni bonjakun,"}, {"id": 504708, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6011, "text": "Dhe nuk nxiteni q\u00eb ta ushqeni varfnjakun,"}, {"id": 504709, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6012, "text": "Dhe me gryk\u00ebsi e hani trash\u00ebgimin,"}, {"id": 504710, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6013, "text": "Dhe p\u00ebr s\u00eb tep\u00ebrmi e doni pasurimin."}, {"id": 504711, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6014, "text": "Por, ani! Se kur t\u00eb d\u00ebrmohet toka cop\u00eb e grim\u00eb,"}, {"id": 504712, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6015, "text": "Dhe t\u00eb vij\u00eb Zoti yt edhe engjujt radh\u00eb-radh\u00eb,"}, {"id": 504713, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6016, "text": "Dhe at\u00ebher\u00eb kur t\u00eb sillet xhehennemi \u2013 at\u00eb dit\u00eb njeriu do t\u00eb kujtohet, por \u00e7\u2019i duhet kujtimi?!"}, {"id": 504714, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6017, "text": "Do t\u00eb thot\u00eb: \u201cKu \u00ebsht\u00eb ai fat t\u00eb isha p\u00ebrgatitur n\u00ebjet\u00ebn time!\u201d"}, {"id": 504715, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6018, "text": "At\u00eb dit\u00eb askush nuk d\u00ebnon sikur Ai."}, {"id": 504716, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6019, "text": "Dhe askush sikur ai nuk farkon n\u00eb pranga!"}, {"id": 504717, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6020, "text": "O shpirt i qet\u00ebsuar,"}, {"id": 504718, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6021, "text": "Kthehu te Zoti yt si k\u00ebnaq\u00ebs e i k\u00ebnaqur,"}, {"id": 504719, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6022, "text": "Hyr n\u00eb gjiun e rob\u00ebve t\u00eb mi,"}, {"id": 504720, "identifier": "sq.mehdiu", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6023, "text": "Dhe hyr n\u00eb xhennetin tim!"}]