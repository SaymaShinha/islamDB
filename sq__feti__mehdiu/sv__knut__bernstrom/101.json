[{"id": 523560, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6158, "text": "[EN DAG skall ni h\u00f6ra] det dundrande slaget!"}, {"id": 523561, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6159, "text": "Vad betyder det dundrande slaget"}, {"id": 523562, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6160, "text": "Vad kan l\u00e5ta dig f\u00f6rst\u00e5 vad detta dundrande slag betyder"}, {"id": 523563, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6161, "text": "[Det skall d\u00e5na] en Dag d\u00e5 m\u00e4nniskorna myllrar och sv\u00e4rmar som mott och mal,"}, {"id": 523564, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6162, "text": "och d\u00e5 bergen [f\u00f6rvandlas] till h\u00f6gar av kardad ull."}, {"id": 523565, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6163, "text": "D\u00e5 skall den vars v\u00e5gsk\u00e5l [med goda handlingar] v\u00e4ger tungt"}, {"id": 523566, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6164, "text": "g\u00e5 till ett liv i lycksalighet,"}, {"id": 523567, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6165, "text": "men den vars v\u00e5gsk\u00e5l v\u00e4ger l\u00e4tt"}, {"id": 523568, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6166, "text": "skall se sig innesluten i en avgrund."}, {"id": 523569, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6167, "text": "Och vad kan l\u00e5ta dig f\u00f6rst\u00e5 vad denna [avgrund] betyder"}, {"id": 523570, "identifier": "sv.bernstrom", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6168, "text": "En [famn av] het eld!"}]