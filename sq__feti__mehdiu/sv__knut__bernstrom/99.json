[{"id": 523541, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6139, "text": "N\u00c4R JORDEN sk\u00e4lver i sitt [sista] skalv"}, {"id": 523542, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6140, "text": "och jorden kastar upp sina b\u00f6rdor"}, {"id": 523543, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6141, "text": "och m\u00e4nniskan ropar: \"Vad har h\u00e4nt med den?\""}, {"id": 523544, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6142, "text": "Den Dagen skall [jorden] ber\u00e4tta vad den har bevittnat [av m\u00e4nniskornas handlingar];"}, {"id": 523545, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6143, "text": "det g\u00f6r den p\u00e5 din Herres ingivelse."}, {"id": 523546, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6144, "text": "Den Dagen skall m\u00e4nniskorna stiga fram i spridda skaror f\u00f6r att se sina handlingar."}, {"id": 523547, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6145, "text": "Och den som har gjort s\u00e5 litet som ett stoftkorns vikt av gott skall se det,"}, {"id": 523548, "identifier": "sv.bernstrom", "surah_number": 99, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0632\u0644\u0632\u0644\u0629", "surah_en_name": "Az-Zalzala", "surah_en_name_translation": "The Earthquake", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 541, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6146, "text": "och den som har gjort s\u00e5 litet som ett stoftkorns vikt av ont skall se det."}]