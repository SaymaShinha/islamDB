[{"id": 523233, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5830, "text": "N\u00c4R HIMLEN r\u00e4mnar,"}, {"id": 523234, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5831, "text": "och stj\u00e4rnorna skingras,"}, {"id": 523235, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5832, "text": "och haven sv\u00e4mmar \u00f6ver sina br\u00e4ddar,"}, {"id": 523236, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5833, "text": "och gravarna st\u00f6rtas \u00f6ver \u00e4nda -"}, {"id": 523237, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5834, "text": "[d\u00e5] skall varje sj\u00e4l klart inse vad den har s\u00e4nt framf\u00f6r sig och [vad den] har skjutit upp."}, {"id": 523238, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5835, "text": "VAD KOM dig, m\u00e4nniska, att missta dig i fr\u00e5ga om din Herre, den Givmilde?"}, {"id": 523239, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5836, "text": "[Han] som har skapat dig och format dig [med h\u00e4nsyn till den uppgift du skall fylla] och gett din natur dess j\u00e4mvikt;"}, {"id": 523240, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5837, "text": "skapat dig i enlighet med Sin vilja."}, {"id": 523241, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5838, "text": "Nej, [i er otacksamhet] f\u00f6rnekar ni domen!"}, {"id": 523242, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5839, "text": "Och det finns de som vakar \u00f6ver er,"}, {"id": 523243, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5840, "text": "\u00e4dla skrivare,"}, {"id": 523244, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5841, "text": "som [k\u00e4nner er och] vet vad ni g\u00f6r."}, {"id": 523245, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5842, "text": "De gudfruktiga skall helt visst finna salighet"}, {"id": 523246, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5843, "text": "och de som har sjunkit djupt i synd skall f\u00f6rvisso finna helvetets eld;"}, {"id": 523247, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5844, "text": "d\u00e4r skall de brinna p\u00e5 Domens dag"}, {"id": 523248, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5845, "text": "och de skall inte komma undan [straffet]."}, {"id": 523249, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5846, "text": "Och vad kan l\u00e5ta dig f\u00f6rst\u00e5 vad Domens dag betyder"}, {"id": 523250, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5847, "text": "Ja, vad kan l\u00e5ta dig f\u00f6rst\u00e5 vad Domens dag betyder?"}, {"id": 523251, "identifier": "sv.bernstrom", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 5848, "text": "Det \u00e4r den Dag d\u00e5 ingen m\u00e4nniska kan hj\u00e4lpa en annan; den Dag d\u00e5 [det blir uppenbart att] all makt och all myndighet tillh\u00f6r Gud."}]