[{"id": 523483, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6080, "text": "VID DET klara morgonljuset"}, {"id": 523484, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6081, "text": "och vid natten, n\u00e4r dess m\u00f6rker t\u00e4tnar!"}, {"id": 523485, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Din Herre har inte \u00f6vergett dig och du har inte v\u00e4ckt Hans missn\u00f6je."}, {"id": 523486, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6083, "text": "Det liv som ligger framf\u00f6r dig skall f\u00f6rvisso bli l\u00e4ttare \u00e4n det som hittills varit!"}, {"id": 523487, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6084, "text": "Och din Herre skall sk\u00e4nka dig [Sitt goda i \u00f6verfl\u00f6d] och du skall bli helt n\u00f6jd."}, {"id": 523488, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Fann Han dig inte faderl\u00f6s och gav dig ett hem?"}, {"id": 523489, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6086, "text": "Och s\u00e5g Han dig inte vilsen och v\u00e4gledde dig"}, {"id": 523490, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6087, "text": "Och s\u00e5g Han dig inte lida n\u00f6d och sk\u00e4nkte dig allt vad du beh\u00f6vde"}, {"id": 523491, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6088, "text": "Var d\u00e4rf\u00f6r inte h\u00e5rd mot den faderl\u00f6se"}, {"id": 523492, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6089, "text": "och st\u00f6t inte bort tiggaren med barska ord."}, {"id": 523493, "identifier": "sv.bernstrom", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 6090, "text": "Men ber\u00e4tta [f\u00f6r alla] om din Herres v\u00e4lg\u00e4rningar!"}]