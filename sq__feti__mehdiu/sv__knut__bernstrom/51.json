[{"id": 521959, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4676, "text": "VID [vindarna] som virvlar upp [damm och vissna l\u00f6v];"}, {"id": 522090, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4677, "text": "vid [molnen] som f\u00f6r med sig regn;"}, {"id": 522091, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4678, "text": "vid [skeppen] som glider fram med l\u00e4tthet;"}, {"id": 522092, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4679, "text": "vid [\u00e4nglarna] som p\u00e5 [Guds] befallning f\u00f6rdelar [Hans g\u00e5vor]!"}, {"id": 522093, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4680, "text": "[De vittnar att] det l\u00f6fte ni f\u00e5tt \u00e4r sanning"}, {"id": 521942, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4681, "text": "[och att] r\u00e4kenskapen och domen skall komma!"}, {"id": 522024, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4682, "text": "VID himlens valv, korsat av stj\u00e4rnornas str\u00e5k!"}, {"id": 522094, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4683, "text": "[M\u00e4nniskor!] Era \u00e5sikter [om trons inneh\u00e5ll] g\u00e5r sannerligen vitt is\u00e4r! -"}, {"id": 522095, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Den som vill l\u00e5ta sig f\u00f6ras bakom ljuset har redan f\u00e5tt synen f\u00f6rv\u00e4nd!"}, {"id": 522096, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4685, "text": "M\u00e5tte [Gud] f\u00f6rg\u00f6ra alla l\u00f6gnare som p\u00e5st\u00e5r och gissar vad de inte kan veta,"}, {"id": 522097, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4686, "text": "de som [vadar] i f\u00f6rvirring och sj\u00e4lvbedr\u00e4geri"}, {"id": 522098, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4687, "text": "och [h\u00e5nfullt] fr\u00e5gar: \"Och n\u00e4r skall denna Domens dag komma?\""}, {"id": 522099, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4688, "text": "Den Dagen skall de pl\u00e5gas \u00f6ver elden"}, {"id": 522100, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4689, "text": "[och en r\u00f6st skall s\u00e4ga:] \"Smaka eldprovet som ni [nu skall underg\u00e5]! Det \u00e4r detta som ni ville p\u00e5skynda.\""}, {"id": 522101, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Men de som fruktade Gud skall vistas i lustg\u00e5rdar med [porlande] k\u00e4llor"}, {"id": 522102, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4691, "text": "och d\u00e4r njuta av det som deras Herre har sk\u00e4nkt dem, d\u00e4rf\u00f6r att de [p\u00e5 jorden] gjorde de det goda och det r\u00e4tta;"}, {"id": 522103, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4692, "text": "de \u00e4gnade inte mera \u00e4n en kort del av natten \u00e5t s\u00f6mn"}, {"id": 522104, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4693, "text": "och i den tidiga gryningen bad de om Guds f\u00f6rl\u00e5telse f\u00f6r sina synder."}, {"id": 522105, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Och av det som de \u00e4gde gav de tiggarna och dem som led n\u00f6d [i tysthet] deras r\u00e4ttm\u00e4tiga andel."}, {"id": 522106, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4695, "text": "F\u00d6R DEM som har visshet i tron finns tecken [p\u00e5 Guds allmakt \u00f6verallt] p\u00e5 jorden,"}, {"id": 522107, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4696, "text": "och inom er sj\u00e4lva. Varf\u00f6r inte [\u00f6ppna \u00f6gonen och] se"}, {"id": 522108, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4697, "text": "I himlen finns det som [hj\u00e4lper er] att uppeh\u00e5lla livet och d\u00e4r finns det som ni f\u00e5tt l\u00f6fte om [i n\u00e4sta liv];"}, {"id": 522109, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4698, "text": "ja, vid himlarnas och jordens Herre! - allt detta \u00e4r sanning, [lika sant] som att ni har f\u00f6rm\u00e5gan att tala!"}, {"id": 522110, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4699, "text": "HAR DU h\u00f6rt ber\u00e4ttelsen om Abrahams himmelska g\u00e4ster?"}, {"id": 522111, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4700, "text": "N\u00e4r de kom till honom och h\u00e4lsade \"Fred!\", svarade han: \"Fred [vare med er]! Ni \u00e4r [tydligtvis] fr\u00e4mlingar [h\u00e4r]?\""}, {"id": 522112, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4701, "text": "Efter ett kort samr\u00e5d med husfolket kom han ut med en [helstekt] g\u00f6dkalv;"}, {"id": 522113, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4702, "text": "som han satte framf\u00f6r dem och bad dem ta f\u00f6r sig."}, {"id": 522114, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4703, "text": "[N\u00e4r han s\u00e5g att de inte \u00e5t] blev han r\u00e4dd att de [hade ont i sinnet]. Men d\u00e5 sade de: \"Du har ingenting att frukta!\" - och de gav honom det glada budskapet om en son [som skulle f\u00f6das \u00e5t honom och bli en man] med stor visdom."}, {"id": 522115, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4704, "text": "D\u00e5 n\u00e4rmade sig hans hustru och slog sig f\u00f6r pannan med ett j\u00e4mmerrop och utbrast: \"En gammal orkesl\u00f6s och ofruktsam kvinna [som jag]!\""}, {"id": 522116, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4705, "text": "De svarade: \"S\u00e5 har din Herre beslutat. Han \u00e4r den Allvise, den Allvetande!\""}, {"id": 522117, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4706, "text": "[Abraham] sade: \"Vad \u00e4r [annars] sk\u00e4let till ert bes\u00f6k, \u00e4rade budb\u00e4rare?\""}, {"id": 522118, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4707, "text": "De svarade: \"Vi har s\u00e4nts [f\u00f6r att straffa] ett folk av obotf\u00e4rdiga, trotsiga syndare;"}, {"id": 522119, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4708, "text": "vi skall l\u00e5ta ett regn av stenblock falla \u00f6ver dem,"}, {"id": 522120, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4709, "text": "[ett straff] fastst\u00e4llt i din Herres beslut f\u00f6r dem som har drivit sina utsv\u00e4vningar f\u00f6r l\u00e5ngt."}, {"id": 522121, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Vi skulle f\u00f6ra bort de troende [som fanns i st\u00e4derna som skulle f\u00f6rst\u00f6ras]"}, {"id": 522122, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4711, "text": "men, fr\u00e5nsett ett hus, fann vi ingen som hade underkastat sig Guds vilja.\""}, {"id": 522123, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Och i [de \u00f6delagda st\u00e4derna] l\u00e4mnade Vi ett budskap till dem som fruktar det pl\u00e5gsamma straff [som \u00e4r f\u00f6ljden av att framh\u00e4rda i det onda]."}, {"id": 522124, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4713, "text": "OCH I [ber\u00e4ttelsen om] Moses ligger [samma budskap]. Vi s\u00e4nde honom till Farao med ett klart bevis p\u00e5 hans myndighet,"}, {"id": 522125, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4714, "text": "men den h\u00f6ge [fursten], st\u00f6dd p\u00e5 sina m\u00e4ktiga resurser, v\u00e4nde [honom] ryggen och sade: \"Antingen \u00e4r han en trollkarl eller en galning.\""}, {"id": 522126, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Och s\u00e5 kastade Vi honom och hans h\u00e4r i havet; och han bar [sj\u00e4lv] skulden [till allas underg\u00e5ng]."}, {"id": 522127, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4716, "text": "Och i [ber\u00e4ttelsen om vad som h\u00e4nde stammen] Aad ligger [samma budskap]: Vi s\u00e4nde mot dem en d\u00f6dsbringande vind"}, {"id": 522128, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4717, "text": "som, d\u00e4r den drar fram, inte l\u00e4mnar n\u00e5got levande efter sig utan f\u00f6rvandlar allt till multnande ben."}, {"id": 522129, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4718, "text": "[Samma budskap] ligger i [ber\u00e4ttelsen om stammen] Thamud. [Deras profet] sade till dem: \"Ni skall f\u00e5 gl\u00e4djas \u00e5t livet [\u00e4nnu] en liten tid\" -"}, {"id": 522130, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4719, "text": "detta sedan de hade satt sig upp emot sin Herres befallning - och s\u00e5 drabbade [straffets] blixtar dem mitt f\u00f6r deras \u00f6gon."}, {"id": 522131, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4720, "text": "De kunde inte resa sig p\u00e5 nytt och ingen fanns som kunde hj\u00e4lpa dem."}, {"id": 522132, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4721, "text": "Och f\u00f6re dem l\u00e4t Vi Noas folk, f\u00f6rh\u00e4rdade i synd och trots, g\u00e5 under."}, {"id": 522133, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4722, "text": "VI HAR byggt v\u00e4rldsalltet och gett det en fast struktur, och f\u00f6rvisso \u00e4r det Vi som utvidgar det."}, {"id": 522134, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Och Vi har brett ut jordens [yta] - kunde n\u00e5gon ha brett ut den p\u00e5 b\u00e4ttre s\u00e4tt?"}, {"id": 522135, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Allt som Vi har skapat har Vi skapat i motsatspar; kanske skall ni minnas [att En Gud \u00e4r upphovet till denna v\u00e4l planerade m\u00e5ngfald]."}, {"id": 522136, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4725, "text": "[S\u00e4g, Muhammad:] \u201dFly undan synden till Gud! Jag kommer till er, [s\u00e4nd] av Honom, f\u00f6r att varna er klart och entydigt:"}, {"id": 522137, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4726, "text": "'S\u00e4tt inte en annan gud vid Guds sida!' - Ja, jag kommer till er, [s\u00e4nd] av Honom, f\u00f6r att varna er klart och entydigt.\u201d"}, {"id": 522138, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4727, "text": "[Men som de upptr\u00e4der mot dig] s\u00e5 har det alltid varit. Inget s\u00e4ndebud kom till dem som levde i g\u00e5ngna tider utan att de kallade honom trollkarl eller galning."}, {"id": 522139, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4728, "text": "L\u00e4t de orden g\u00e5 i arv? Nej, men dessa [sentida f\u00f6rnekare] \u00e4r h\u00e5rdnackade m\u00e4nniskor vilkas onda handlingar inte k\u00e4nner n\u00e5gra gr\u00e4nser."}, {"id": 522140, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4729, "text": "H\u00e5ll dig d\u00e4rf\u00f6r p\u00e5 avst\u00e5nd fr\u00e5n dem; d\u00e5 kan ingen klandra dig!"}, {"id": 522141, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Men [forts\u00e4tt att] p\u00e5minna [och varna]; p\u00e5minnelsen \u00e4r till nytta f\u00f6r dem som tror."}, {"id": 522142, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Jag har skapat de osynliga v\u00e4sendena och m\u00e4nniskorna enbart f\u00f6r att de skall [k\u00e4nna Mig och] dyrka Mig."}, {"id": 522143, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4732, "text": "Jag beg\u00e4r ingen f\u00f6rs\u00f6rjning och ingen f\u00f6da av dem."}, {"id": 522144, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Det \u00e4r Gud som drar f\u00f6rsorg [om skapelsen], den Starke, den Orubblige."}, {"id": 522145, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4734, "text": "De som beg\u00e5r or\u00e4tt skall f\u00e5 sin andel av det straff som drabbade deras likasinnade f\u00f6reg\u00e5ngare. De beh\u00f6ver inte be Mig att p\u00e5skynda [domen]!"}, {"id": 522147, "identifier": "sv.bernstrom", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sv", "name": "Bernstr\u00f6m", "transelator_en_name": "Knut Bernstr\u00f6m", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Den Dag som f\u00f6rnekarna av sanningen har f\u00e5tt l\u00f6fte om skall bli en Dag av fasa och el\u00e4nde!"}]