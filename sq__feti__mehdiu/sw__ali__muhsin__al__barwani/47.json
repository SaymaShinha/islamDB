[{"id": 528202, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4546, "text": "Walio kufuru na wakazuilia njia ya Mwenyezi Mungu, Yeye atavipotoa vitendo vyao."}, {"id": 528203, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4547, "text": "Na walio amini, na wakatenda mema, na wakaamini aliyo teremshiwa Muhammad - nayo ni Haki iliyo toka kwa Mola wao Mlezi - atawafutia makosa yao na ataitengeneza hali yao."}, {"id": 528204, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4548, "text": "Hayo ni kwa sababu walio kufuru wamefuata upotovu, na walio amini wamefuata Haki iliyo toka kwa Mola wao Mlezi. Hivyo ndivyo Mwenyezi Mungu anavyo wapigia watu mifano yao."}, {"id": 528205, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4549, "text": "Basi mnapo wakuta walio kufuru wapigeni shingoni mwao, mpaka mkiwadhoofisha wafungeni pingu. Tena waachilieni kwa hisani au wajikomboe, mpaka vita vipoe. Ndio hivyo. Na lau angeli taka Mwenyezi Mungu angeli washinda mwenyewe, lakini haya ni kwa sababu akutieni mitihani nyinyi kwa nyinyi. Na walio uliwa katika njia ya Mwenyezi Mungu hatazipoteza a'mali zao."}, {"id": 528206, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4550, "text": "Atawaongoza na awatengezee hali yao."}, {"id": 528207, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4551, "text": "Na atawaingiza katika Pepo aliyo wajuulisha."}, {"id": 528208, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4552, "text": "Enyi mlio amini! Mkimnusuru Mwenyezi Mungu naye atakunusuruni na ataithibitisha miguu yenu."}, {"id": 528209, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4553, "text": "Na walio kufuru, basi kwao ni maangamizo, na atavipoteza vitendo vyao."}, {"id": 528210, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4554, "text": "Hayo ni kwa sababu waliyachukia aliyo yateremsha Mwenyezi Mungu, basi akaviangusha vitendo vyao."}, {"id": 528211, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4555, "text": "Kwani hawakutembea katika ardhi wakaona ulikuwaje mwisho wa walio kuwa kabla yao? Mwenyezi Mungu aliwaangamiza, na kwa makafiri hawa itakuwa mfano wa hayo."}, {"id": 528212, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4556, "text": "Hayo ni kwa sababu Mwenyezi Mungu ni Mlinzi wa walio amini. Na makafiri hawana mlinzi."}, {"id": 528213, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4557, "text": "Hakika Mwenyezi Mungu atawaingiza walio amini na wakatenda mema katika Bustani zipitiwazo na mito kati yake. Na walio kufuru hujifurahisha na hula kama walavyo wanyama, na Moto ndio makaazi yao."}, {"id": 528214, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4558, "text": "Na miji mingapi iliyo kuwa na nguvu zaidi kuliko mji wako huu ulio kutoa; nao tuliwateketeza na wala hawakuwa wa kuwanusuru."}, {"id": 528215, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4559, "text": "Je! Mwenye kuwa na bayana kutoka kwa Mola wake Mlezi ni kama aliye pambiwa uovu wa vitendo vyake, na wakafuata pumbao lao?"}, {"id": 528216, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4560, "text": "Mfano wa Pepo walio ahidiwa wachamngu ina mito ya maji yasiyo vunda, na mito ya maziwa isiyo haribika ladha yake, na mito ya mvinyo yenye ladha kwa wanywao, na mito ya asali iliyo safishwa. Na wao humo watakuwa na kila namna ya matunda, na maghfira kutoka kwa Mola wao Mlezi. Basi hao ni kama watakao dumu Motoni na wakinyweshwa maji yanayo chemka ya kuwakata matumbo yao?"}, {"id": 528217, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4561, "text": "Na wapo miongoni mwao wanao kusikiliza, mpaka wakiondoka kwako huwauliza walio pewa ilimu: Amesema nini sasa hivi? Hao ndio ambao Mwenyezi Mungu amepiga muhuri juu ya nyoyo zao, na wakafuata pumbao lao."}, {"id": 528218, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4562, "text": "Na wale walio ongoka anawazidishia uongofu na anawapa uchamngu wao."}, {"id": 528220, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4563, "text": "Kwani wanangojea jengine isipo kuwa iwafikie Saa (ya Kiyama) kwa ghafla? Na hakika alama zake zimekwisha kuja. Na itapo wajia kutawafaa wapi kukumbuka hapo?"}, {"id": 528221, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4564, "text": "Basi jua ya kwamba hapana mungu ila Mwenyezi Mungu, na omba maghufira kwa dhambi zako na za Waumini wanaume na Waumini wanawake. Na Mwenyezi Mungu anajua mahali penu pa kwenda na kurudi, na mahali penu pa kukaa."}, {"id": 528222, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4565, "text": "Na Waumini wanasema: Kwa nini haiteremshwi Sura? Na inapo teremshwa Sura madhubuti na ikatajwa ndani yake khabari ya vita, utawaona wenye maradhi katika nyoyo zao wanakutazama mtazamo wa anaye zimia kwa mauti. Basi ni bora kwao"}, {"id": 528223, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4566, "text": "Ut'iifu na kauli njema. Na jambo likisha azimiwa, basi wakiwa ni wakweli kwa Mwenyezi Mungu, bila ya shaka itakuwa ni kheri kwao."}, {"id": 528224, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4567, "text": "Basi yanayo tarajiwa kwenu mkitawala ndio mfisidi katika nchi na mwatupe jamaa zenu?"}, {"id": 528225, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4568, "text": "Hao ndio Mwenyezi Mungu alio walaani, na akawatia uziwi, na akawapofoa macho yao."}, {"id": 528226, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4569, "text": "Je! Hawaizingatii hii Qur'ani? Au kwenye nyoyo (zao) zipo kufuli?"}, {"id": 528227, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4570, "text": "Kwa hakika wanao rudi nyuma baada ya kwisha wabainikia uwongofu, Shetani huyo amewashawishi na amewaghuri."}, {"id": 528228, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4571, "text": "Hayo ni kwa sababu waliwaambia walio chukia aliyo yateremsha Mwenyezi Mungu: Tutakut'iini katika baadhi ya mambo. Na Mwenyezi Mungu anajua siri zao."}, {"id": 528229, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4572, "text": "Basi itakuwaje Malaika watakapo wafisha wakawa wanawapiga nyuso zao na migongo yao!"}, {"id": 528230, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4573, "text": "Hayo ni kwa sababu wao waliyafuata yaliyo mchukiza Mwenyezi Mungu na wakachukia yanayo mridhisha, basi akaviangusha vitendo vyao."}, {"id": 528231, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4574, "text": "Je! Wenye maradhi nyoyoni mwao wanadhani kwamba Mwenyezi Mungu hatazidhihirisha chuki zao?"}, {"id": 528232, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4575, "text": "Na tungeli penda tungeli kuonyesha hao na ungeli watambua kwa alama zao; lakini bila ya shaka utawajua kwa namna ya msemo wao. Na Mwenyezi Mungu anavijua vitendo vyenu."}, {"id": 528233, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4576, "text": "Na bila ya shaka tutakujaribuni mpaka tuwadhihirishe wapignao Jihadi katika nyinyi na wanao subiri. Nasi tutazifanyia mtihani khabari zenu."}, {"id": 528234, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4577, "text": "Kwa hakika walio kufuru na wakazuilia njia ya Mwenyezi Mungu na wakapinzana na Mtume baada ya kwisha wabainikia uwongofu, hao hawatamdhuru Mwenyezi Mungu kwa lolote. Naye ataviangusha vitendo vyao."}, {"id": 528235, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4578, "text": "Enyi mlio amini! Mt'iini Mwenyezi Mungu, na mt'iini Mtume, wala msiviharibu vitendo vyenu."}, {"id": 528236, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4579, "text": "Hakika walio kufuru na wakazuilia njia ya Mwenyezi Mungu, kisha wakafa na hali ni makafiri, Mwenyezi Mungu hatawasamehe makosa yao."}, {"id": 528237, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4580, "text": "Basi msiregee na kutaka suluhu, maana nyinyi ndio mtakao shinda. Na Mwenyezi Mungu yu pamoja nanyi, wala hatakunyimeni malipo ya vitendo vyenu."}, {"id": 528238, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4581, "text": "Hakika uhai wa duniani ni mchezo na pumbao. Na mkiamini na mkamchamngu Mwenyezi Mungu atakupeni ujira wenu, wala hakutakeni mali yenu."}, {"id": 528239, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4582, "text": "Na akikutakeni hayo na kukushikilieni mtafanya ubakhili, na atatoa undani wa chuki zenu."}, {"id": 528240, "identifier": "sw.barwani", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 510, "ruku": 445, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4583, "text": "Angalieni! Nyinyi mnaitwa mtumie katika njia ya Mwenyezi Mungu, na wapo katika nyinyi wanao fanya ubakhili. Na anaye fanya ubakhili basi anajifanyia ubakhili mwenyewe. Na Mwenyezi Mungu si mhitaji, na nyinyi ndio wahitaji. Na mkigeuka atawaleta watu wengine badala yenu, nao hawatakuwa kama nyinyi."}]