[{"id": 529223, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5592, "text": "Hakika kilimpitia binaadamu kipindi katika zama ambacho kwamba hakuwa kitu kinacho tajwa."}, {"id": 529224, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5593, "text": "Hakika Sisi tumemuumba mtu kutokana na mbegu ya uhai iliyo changanyika, tumfanyie mtihani. Kwa hivyo tukamfanya mwenye kusikia, mwenye kuona."}, {"id": 529225, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5594, "text": "Hakika Sisi tumembainishia Njia. Ama ni mwenye kushukuru, au mwenye kukufuru."}, {"id": 529226, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5595, "text": "Hakika Sisi tumewaandalia makafiri minyororo na pingu na Moto mkali."}, {"id": 529227, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5596, "text": "Hakika watu wema watakunywa katika vinywaji vilivyo changanyika na kafuri,"}, {"id": 529228, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5597, "text": "Ni chemchem watakao inywa waja wa Mwenyezi Mungu, wakiifanya imiminike kwa wingi."}, {"id": 529229, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5598, "text": "Wanatimiza ahadi, na wanaiogopa siku ambayo shari yake inaenea sana,"}, {"id": 529230, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5599, "text": "Na huwalisha chakula, juu ya kukipenda kwake, masikini, na yatima, na wafungwa."}, {"id": 529231, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5600, "text": "Hakika sisi tunakulisheni kwa wajihi wa Mwenyezi Mungu. Hatutaki kwenu malipo wala shukrani."}, {"id": 529232, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5601, "text": "Hakika sisi tunaiogopa kwa Mola wetu Mlezi hiyo siku yenye shida na taabu."}, {"id": 529233, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5602, "text": "Basi Mwenyezi Mungu atawalinda na shari ya siku hiyo, na atawakutanisha na raha na furaha."}, {"id": 529234, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5603, "text": "Na atawajazi Bustani za Peponi na maguo ya hariri kwa vile walivyo subiri."}, {"id": 529235, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5604, "text": "Humo wataegemea juu ya viti vya enzi, hawataona humo jua kali wala baridi kali."}, {"id": 529236, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5605, "text": "Na vivuli vyake vitakuwa karibu yao, na mashada ya matunda yataning'inia mpaka chini."}, {"id": 529237, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5606, "text": "Na watapitishiwa vyombo vya fedha na vikombe vya vigae,"}, {"id": 529238, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5607, "text": "Vya fedha safi kama kiyoo, wamevipima kwa vipimo."}, {"id": 529239, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5608, "text": "Na humo watanyweshwa kinywaji kilicho changanyika na tangawizi."}, {"id": 529240, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5609, "text": "Hiyo ni chemchem iliyo humo inaitwa Salsabil."}, {"id": 529241, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5610, "text": "Na watawazungukia kuwatumikia wavulana wasio pevuka, ukiwaona utawafikiri ni lulu zilizo tawanywa."}, {"id": 529242, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5611, "text": "Na utakapo yaona, utakuwa umeona neema na ufalme mkubwa."}, {"id": 529243, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5612, "text": "Juu yao zipo nguo za hariri laini za kijani kibichi, na hariri nzito ya at'ilasi. Na watavikwa vikuku vya fedha, na Mola wao Mlezi atawanywesha kinywaji safi kabisa."}, {"id": 529244, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5613, "text": "Hakika haya ni malipo yenu; na juhudi zenu zimekubaliwa."}, {"id": 529245, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5614, "text": "Hakika Sisi tumekuteremshia Qur'ani kidogo kidogo."}, {"id": 529246, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5615, "text": "Basi ngojea hukumu ya Mola wako Mlezi wala usimt'ii miongoni mwao mwenye dhambi au mwenye kufuru."}, {"id": 529247, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5616, "text": "Na likumbuke jina la Mola wako Mlezi asubuhi na jioni;"}, {"id": 529248, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5617, "text": "Na usiku msujudie Yeye, na umtakase usiku, wakati mrefu."}, {"id": 529249, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5618, "text": "Kwa hakika watu hawa wanapenda maisha ya kidunia, na wanaiacha nyuma yao siku nzito."}, {"id": 529250, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5619, "text": "Sisi tumewaumba, na tukavitia nguvu viungo vyao. Na tukitaka tutawabadilisha mfano wao wawe badala yao."}, {"id": 529251, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5620, "text": "Hakika haya ni mawaidha; basi anaye taka atashika Njia ya kwenda kwa Mola wake Mlezi."}, {"id": 529252, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5621, "text": "Wala hamwezi kutaka ila atakapo Mwenyezi Mungu. Hakika Mwenyezi Mungu ni Mwenye ilimu, Mwenye hikima."}, {"id": 529253, "identifier": "sw.barwani", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5622, "text": "Humuingiza amtakaye katika rehema yake. Na wenye kudhulumu amewawekea adhabu iliyo chungu."}]