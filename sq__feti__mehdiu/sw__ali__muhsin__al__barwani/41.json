[{"id": 527871, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4219, "text": "H'a Mim"}, {"id": 527872, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4220, "text": "Uteremsho huu umetoka kwa Mwingi wa rehema, Mwenye kurehemu."}, {"id": 527873, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4221, "text": "Hichi ni Kitabu kilicho pambanuliwa Aya zake, cha kusomwa kwa Kiarabu kwa watu wanao jua."}, {"id": 527874, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4222, "text": "Kitoacho khabari njema, na chenye kuonya. Lakini wengi katika wao wamepuuza; kwa hivyo hawasikii."}, {"id": 527875, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4223, "text": "Na wakasema: Nyoyo zetu zimo katika vifuniko kwa hayo unayo tuitia, na masikio yetu yana uziwi, na baina yetu na wewe lipo pazia. Basi wewe tenda, nasi tunatenda."}, {"id": 527876, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4224, "text": "Sema: Hakika mimi ni mtu kama nyinyi; inafunuliwa kwangu ya kwamba Mungu wenu ni Mungu Mmoja tu. Basi nyookeni sawa kumwendea Yeye, na mumtake msamaha, na ole wao wanao mshirikisha,"}, {"id": 527877, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4225, "text": "Ambao hawatoi Zaka na wanaikataa Akhera."}, {"id": 527878, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4226, "text": "Hakika wanao amini na wakatenda mema watakuwa na ujira usio na ukomo."}, {"id": 527879, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4227, "text": "Sema: Hivyo nyinyi mnamkataa aliye umba ardhi katika siku mbili, na mnamfanyia washirika? Huyu ndiye Mola Mlezi wa walimwengu wote."}, {"id": 527880, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4228, "text": "Na akaweka humo milima juu yake, na akabarikia humo na akakadiria humo chakula chake katika siku nne. Haya ni sawa kabisa kwa wanao uliza."}, {"id": 527881, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4229, "text": "Kisha akazielekea mbingu, na zilikuwa moshi, akaziambia hizo na ardhi: Njooni, kwa khiari au kwa nguvu! Vyote viwili vikasema: Tumekuja nasi ni wat'iifu."}, {"id": 527882, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4230, "text": "Basi akazifanya mbingu saba kwa siku mbili, na akazipangia kila mbingu mambo yake. Na tukaipamba mbingu ya chini kwa mataa na kwa ulinzi. Hichi ndicho kipimo cha Mwenyezi Mungu Mwenye Kujua."}, {"id": 527883, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4231, "text": "Basi wakipuuza wewe sema: Nakuhadharisheni adhabu mfano wa adhabu ya A'di na Thamudi,"}, {"id": 527884, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4232, "text": "Walipo wajia Mitume mbele yao na nyuma yao wakawaambia: Msimuabudu ila Mwenyezi Mungu! Wakasema: Angeli taka Mola wetu Mlezi bila ya shaka angeli wateremsha Malaika. Basi sisi hakika tunayakataa hayo mliyo tumwa."}, {"id": 527885, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4233, "text": "Ama kina A'di walijivuna katika nchi bila ya haki, na wakasema: Nani aliye kuwa na nguvu kushinda sisi? Kwani wao hawakuona kwamba Mwenyezi Mungu aliye waumba ni Mwenye nguvu kushinda wao? Na wao wakawa wanazikataa Ishara zetu!"}, {"id": 527886, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4234, "text": "Basi tuliwapelekea upepo wa kimbunga katika siku za ukorofi, ili tuwaonjeshe adhabu ya kuwahizi katika uhai wa duniani, na bila ya shaka adhabu ya Akhera ina hizaya zaidi, na wala wao hawatanusuriwa."}, {"id": 527887, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4235, "text": "Na ama Thamudi tuliwaongoza, lakini walipenda upofu kuliko uwongofu. Basi uliwachukua moto wa adhabu ya kuwafedhehi kwa sababu ya yale waliyo kuwa wakiyachuma."}, {"id": 527888, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4236, "text": "Na tukawaokoa walio amini na wakawa wanamcha-mngu."}, {"id": 527889, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4237, "text": "Na siku watakapo kusanywa maadui wa Mwenyezi Mungu kwenye Moto, nao wakikgawanywa kwa makundi."}, {"id": 527890, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4238, "text": "Hata watakapo ufikia Moto yatawashuhudia masikio yao, na macho yao na ngozi zao kwa waliyo kuwa wakiyatenda."}, {"id": 527891, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4239, "text": "Na wao wataziambia ngozi zao: Kwa nini mmetushuhudilia? Nazo zitasema: Ametutamkisha Mwenyezi Mungu ambaye ametamkisha kila kitu! Na Yeye ndiye aliye kuumbeni mara ya kwanza, na kwake Yeye tu mtarejeshwa."}, {"id": 527892, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4240, "text": "Na hamkuwa wenye kujificha hata masikio yenu, na macho yenu, na ngozi zenu, zisikushuhudilieni. Lakini mlidhani kwamba Mwenyezi Mungu hayajui mengi katika mliyo kuwa mkiyatenda."}, {"id": 527893, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4241, "text": "Basi hiyo dhana yenu mliyo kuwa mkimdhania Mola wenu Mlezi. Imekuangamizeni; na mmekuwa miongoni mwa walio khasiri."}, {"id": 527894, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4242, "text": "Basi wakisubiri Moto ndio maskani yao. Na wakiomba radhi hawakubaliwi."}, {"id": 527895, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4243, "text": "Na tukawawekea marafiki, na wakawapambia yaliyo mbele yao na nyuma yao. Basi ikawathibitikia kauli wawe pamoja na mataifa yaliyo pita kabla yao, miongoni mwa majini na watu. Hakika hao wamekuwa ni wenye kukhasirika."}, {"id": 527896, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4244, "text": "Na walio kufuru walisema: Msiisikilize Qur'ani hii, na timueni zogo, huenda mkashinda."}, {"id": 527897, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4245, "text": "Basi hapana shaka tutawaonjesha hao walio kufuru adhabu kali, na hapana shaka tutawalipa malipo mabaya ya yale waliyo kuwa wakiyafanya."}, {"id": 527898, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4246, "text": "Hayo ndiyo malipo ya Maadui wa Mwenyezi Mungu - Moto! Humo watakuwa na maskani ya kudumu, ndiyo malipo yao kwa sababu walikuwa wakizikataa Ishara zetu."}, {"id": 527899, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4247, "text": "Na walio kufuru watasema: Mola wetu Mlezi! Tuonyeshe walio tupoteza miongoni mwa majini na watu ili tuwaweke chini ya miguu yetu, wapate kuwa miongoni wa walio chini kabisa."}, {"id": 527900, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4248, "text": "Hakika walio sema: Mola wetu Mlezi ni Mwenyezi Mungu! Kisha wakanyooka sawa, hao huwateremkia Malaika wakawaambia: Msiogope, wala msihuzunike; nanyi furahini kwa Pepo mliyo kuwa mkiahidiwa."}, {"id": 527901, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4249, "text": "Sisi ni vipenzi vyenu katika maisha ya dunia na katika Akhera, na humo mtapata kinacho tamaniwa na nafsi zenu, na humo mtapata mtakavyo vitaka."}, {"id": 527902, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4250, "text": "Ni takrima itokayo kwa Mwenye kusamehe, Mwenye kurehemu."}, {"id": 527903, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4251, "text": "Na ni nani mbora wa kusema kuliko aitaye kwa Mwenyezi Mungu na akatenda mema, na akasema: Hakika mimi ni katika Waislamu?"}, {"id": 527904, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4252, "text": "Mema na maovu hayalingani. Pinga uovu kwa lilio jema zaidi. Hapo yule ambaye baina yako naye pana uadui atakuwa kama rafiki jamaa wa kukuonea uchungu."}, {"id": 527905, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4253, "text": "Lakini hawapewi wema huu ila wanao subiri, wala hawapewi ila wenye bahati kubwa."}, {"id": 527906, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4254, "text": "Na Shet'ani akikuchochea kwa uchochezi wewe jikinge kwa Mwenyezi Mungu. Hakika Yeye ni Mwenye kusikia, Mwenye kujua."}, {"id": 527907, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4255, "text": "Na katika Ishara zake ni usiku na mchana, na jua na mwezi. Basi msilisujudie jua wala mwezi, bali msujudieni Mwenyezi Mungu aliye viumba, ikiwa nyinyi mnamuabudu Yeye tu."}, {"id": 527908, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": true, "sajda_id": 12, "sajda_obligatory": true, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4256, "text": "Na ikiwa watajivuna, basi hao walioko kwa Mola wako Mlezi wanamtakasa Yeye usiku na mchana, na wao hawachoki."}, {"id": 527909, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4257, "text": "Na katika Ishara zake ni kwamba unaiona ardhi nyonge, lakini tunapo iteremshia maji mara unaiona inataharaki na kuumuka. Bila ya shaka aliye ihuisha ardhi ndiye atakaye huisha wafu. Hakika Yeye ni Muweza wa kila kitu."}, {"id": 527910, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4258, "text": "Hakika wale wanao upotoa ukweli uliomo katika Ishara zetu hawatufichikii Sisi. Je! Atakaye tupwa Motoni ni bora au atakaye kuja kwa amani Siku ya Kiyama? Tendeni mpendavyo, kwa hakika Yeye anayaona mnayo yatenda."}, {"id": 527911, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4259, "text": "Kwa hakika wanayo yakataa mawaidha haya yanapo wajia (wataangamia), na haya bila ya shaka ni Kitabu chenye nguvu na utukufu."}, {"id": 527912, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4260, "text": "Hautakifikia upotovu mbele yake wala nyuma yake. Kimeteremshwa na Mwenye hikima, Msifiwa."}, {"id": 527913, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4261, "text": "Huambiwi ila yale yale waliyo ambiwa Mitume wa kabla yako. Hakika Mola wako Mlezi bila ya shaka ni Mwenye maghfira, na ni Mwenye adhabu chungu."}, {"id": 527914, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4262, "text": "Na lau tungeli ifanya Qur'ani kwa lugha ya kigeni wangeli sema: Kwanini Aya zake haziku pambanuliwa? Yawaje lugha ya kigeni na Mtume Mwaarabu? Sema: Hii Qur'ani ni uwongofu na poza kwa wenye kuamini. Na wasio amini umo uziwi katika masikio yao, nayo kwao imezibwa hawaioni. Hao wanaitwa nao wako pahala mbali."}, {"id": 527915, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4263, "text": "Na hakika tulimpa Musa Kitabu, lakini pakatokea khitilafu kati yake. Na lau kuwa halikwisha tangulia neno la Mola wako Mlezi wangeli hukumiwa. Na hakika wao wamo katika shaka yenye kuwatia wasiwasi."}, {"id": 527916, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4264, "text": "Anaye tenda mema basi anajitendea mwenyewe nafsi yake, na mwenye kutenda uovu basi ni juu ya nafsi yake vile vile; wala Mola wako Mlezi si mwenye kuwadhulumu waja."}, {"id": 527917, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4265, "text": "UJUZI wa kuijua Saa ya Kiyama unarudishwa kwake tu Mwenyezi Mungu. Na matunda hayatoki katika mafumba yake, wala mwanamke hachukui mimba, wala hazai, ila kwa kujua kwake. Na siku atakayo waita, akawaambia: Wako wapi washirika wangu? Hapo watasema: Tunakiri kwako ya kwamba hapana miongoni mwetu anaye shuhudia hayo."}, {"id": 527918, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4266, "text": "Na wakawapotea wale walio kuwa wakiwaomba hapo kwanza, na wakawa na yakini kuwa hawana pa kukimbilia."}, {"id": 527919, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4267, "text": "Mwanaadamu hachoki kuomba dua za kheri, na inapo mpata shari, mara huvunjika moyo akakata tamaa."}, {"id": 527921, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4268, "text": "Na pindi tukimwonjesha rehema itokayo kwetu baada ya kumgusa dhara, bila ya shaka husema: Haya nayastahiki mimi, wala sidhani kuwa Kiyama kitakuja. Na hata nikirudishwa kwa Mola wangu Mlezi bila ya shaka nina mema yangu kwake! Basi kwa yakini tutawaelezea walio kufuru hayo waliyo yatenda, na kwa yakini tutawaonjesha adhabu ngumu."}, {"id": 527922, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4269, "text": "Na tukimneemesha mwanaadamu hugeuka na kujitenga upande, na inapo mgusa shari, huwa na madua marefu marefu."}, {"id": 527923, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4270, "text": "Sema: Mnaonaje! Ikiwa haya yanatoka kwa Mwenyezi Mungu, nanyi ikawa ndio mmeyakataa, ni nani aliye potea zaidi kuliko aliye katika upinzani wa mbali?"}, {"id": 527924, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4271, "text": "Tutawaonyesha Ishara zetu katika upeo wa mbali na katika nafsi zao wenyewe mpaka iwabainikie kwamba haya ni kweli. Je! Haikutoshi kwamba Mola wako Mlezi kuwa Yeye ni Shahidi wa kila kitu?"}, {"id": 527925, "identifier": "sw.barwani", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4272, "text": "Ama wao wamo katika shaka ya kukutana na Mola wao Mlezi! Ama hakika Yeye amekizunguka kila kitu."}]