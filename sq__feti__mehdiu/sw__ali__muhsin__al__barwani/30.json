[{"id": 527067, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3410, "text": "Alif Lam Mim (A. L. M.)"}, {"id": 527068, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3411, "text": "Warumi wameshindwa,"}, {"id": 526850, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3412, "text": "Katika nchi iliyo karibu. Nao baada ya kushindwa kwao watashinda"}, {"id": 527043, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3413, "text": "Katika miaka michache. Amri ni ya Mwenyezi Mungu kabla yake na baada yake. Na siku hiyo Waumini watafurahi."}, {"id": 527069, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3414, "text": "Kwa nusura ya Mwenyezi Mungu humnusuru amtakaye. Naye ndiye Mwenye nguvu Mwenye kurehemu."}, {"id": 527070, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3415, "text": "Ni ahadi ya Mwenyezi Mungu. Mwenyezi Mungu havunji ahadi yake. Lakini watu wengi hawajui."}, {"id": 527071, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3416, "text": "Wanajua hali ya dhaahiri ya maisha ya dunia, na wameghafilika na Akhera."}, {"id": 527072, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3417, "text": "Je! Hawajifikirii nafsi zao? Mwenyezi Mungu hakuumba mbingu na ardhi na viliomo ndani yake ila kwa Haki na kwa muda maalumu. Na hakika watu wengi bila ya shaka ni wenye kukataa kuwa watakutana na Mola wao Mlezi."}, {"id": 527073, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3418, "text": "Je! Hawatembei katika ardhi wakaona ulivyo kuwa mwisho wa walio kuwa kabla yao? Nao walikuwa na nguvu zaidi kuliko wao, na wakaitimbua ardhi na wakaistawisha zaidi kuliko walivyo istawisha wao. Na Mitume wao wakawajia na dalili zilizo wazi. Basi hakuwa Mwenyezi Mungu ni mwenye kuwadhulumu, lakini walikuwa wakijidhulumu nafsi zao."}, {"id": 527074, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3419, "text": "Kisha ulikuwa mwisho wa walio fanya ubaya ni kuzikanusha Ishara za Mwenyezi Mungu, na wakawa wanazifanyia maskhara."}, {"id": 527075, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3420, "text": "Mwenyezi Mungu huanzisha uumbaji, tena akaurudisha mara ya pili, na kisha mtarejeshwa kwake."}, {"id": 527076, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3421, "text": "Na itapo simama Saa wakosefu watakata tamaa."}, {"id": 527077, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3422, "text": "Wala hawatakuwa na waombezi miongoni mwa walio kuwa wakiwashirikisha na Mungu; na wao wenyewe watawakataa hao washirikishwa wao."}, {"id": 527078, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3423, "text": "Na siku itapo simama Saa, siku hiyo watagawanyika."}, {"id": 527079, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3424, "text": "Ama walio amini na wakatenda mema watafurahishwa katika Bustani."}, {"id": 527080, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3425, "text": "Na ama walio kufuru na wakazikanusha Ishara zetu na mkutano wa Akhera, hao basi watahudhurishwa katika adhabu."}, {"id": 527081, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3426, "text": "Basi Subhanallah! Mtakaseni Mwenyezi Mungu jioni na asubuhi,"}, {"id": 527082, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3427, "text": "Na sifa zote njema ni zake katika mbingu na ardhi, na alasiri na adhuhuri."}, {"id": 527083, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3428, "text": "Hukitoa kilicho hai kutokana na kilicho kufa, na hukitoa kilicho kufa kutokana na kilicho hai. Na huihuisha ardhi baada ya kufa kwake. Na kama hivyo ndivyo mtavyo fufuliwa."}, {"id": 527084, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3429, "text": "Na katika Ishara zake ni kuwa kakuumbeni kwa udongo. Kisha mmekuwa watu mlio tawanyika kote kote."}, {"id": 527085, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3430, "text": "Na katika Ishara zake ni kuwa amekuumbieni wake zenu kutokana na nafsi zenu ili mpate utulivu kwao. Naye amejaalia mapenzi na huruma baina yenu. Hakika katika haya bila ya shaka zipo Ishara kwa watu wanao fikiri."}, {"id": 527087, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3431, "text": "Na katika Ishara zake ni kuumba mbingu na ardhi, na kutafautiana ndimi zenu na rangi zenu. Hakika katika hayo zipo Ishara kwa wajuzi."}, {"id": 527088, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3432, "text": "Na katika Ishara zake ni kulala kwenu usiku na mchana, na kutafuta kwenu fadhila zake. Hakika katika haya bila ya shaka zipo Ishara kwa watu wanao sikia."}, {"id": 527089, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3433, "text": "Na katika Ishara zake hukuonyesheni umeme kwa kukutieni khofu na tamaa. Na hukuteremshieni maji kutoka mbinguni, na kwa hayo huifufua ardhi baada ya kufa kwake. Hakika katika hayo zipo Ishara kwa watu wanao zingatia."}, {"id": 527090, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3434, "text": "Na katika Ishara zake ni kuwa mbingu na ardhi zimesimama kwa amri yake. Kisha atakapo kuiteni mwito mmoja tu, hapo mtatoka kwenye ardhi."}, {"id": 527091, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3435, "text": "Na vyake Yeye vilivyo mbinguni na katika ardhi. Vyote vinamt'ii Yeye."}, {"id": 527092, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3436, "text": "Na Yeye ndiye anaye anzisha uumbaji, kisha ataurudisha mara nyengine. Na jambo hili ni jepesi zaidi kwake. Naye ndiye mwenye mfano bora katika mbingu na ardhi. Na Yeye ndiye Mwenye nguvu, Mwenye hikima."}, {"id": 527093, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3437, "text": "Amekupigieni mfano kutokana na nafsi zenu. Je! Katika hao iliyo wamiliki mikono yenu ya kulia mnao washirika katika hivyo tulivyo kuruzukuni, mkawa nyinyi katika hivyo ni sawasawa, mnawaogopa kama mnavyo ogopana wenyewe kwa wenyewe? Namna hivi tunazipambanua Aya zetu kwa watu wenye akili."}, {"id": 527094, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3438, "text": "Bali walio dhulumu wamefuata mapendo ya nafsi zao pasina kujua. Basi nani atamwongoa ambaye Mwenyezi Mungu amemuacha apotee? Wala hao hawatakuwa na wa kuwanusuru."}, {"id": 527095, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3439, "text": "Basi uelekeze uso wako sawasawa kwenye Dini - ndilo umbile la Mwenyezi Mungu alilo waumbia watu. Hapana mabadiliko katika uumbaji wa Mwenyezi Mungu. Hiyo ndiyo Dini iliyo nyooka sawa. Lakini watu wengi hawajui."}, {"id": 527096, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3440, "text": "Muwe wenye kutubia kwake, na mcheni Yeye, na shikeni Sala, na wala msiwe katika washirikina."}, {"id": 527097, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3441, "text": "Katika wale ambao kwamba wameigawanya dini yao, na wakawa makundi makundi, kila kikundi kinafurahia kilicho nacho."}, {"id": 527098, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3442, "text": "Yakiwapata watu madhara humwomba Mola wao Mlezi nao wametubu kwake. Kisha akiwaonjesha rehema itokayo kwake, mara kikundi miongoni mwao humshirikisha Mola wao Mlezi,"}, {"id": 527099, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3443, "text": "Kwa kuyakataa tuliyo wapa. Basi stareheni. Mtakuja jua!"}, {"id": 527100, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3444, "text": "Au tumewateremshia hoja ambayo kwayo ndio wanafanya ushirikina?"}, {"id": 527101, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3445, "text": "Na watu tunapo waonjesha rehema huifurahia, na ukiwasibu uovu kwa iliyo yatanguliza mikono yao wenyewe, mara wanakata tamaa."}, {"id": 527102, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3446, "text": "Kwani hawaoni kwamba Mwenyezi Mungu humkunjulia riziki amtakaye na humdhikisha amtakaye? Hakika katika hayo zipo Ishara kwa watu wanao amini."}, {"id": 527103, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3447, "text": "Basi mpe jamaa haki yake, na masikini, na msafiri. Hayo ni kheri kwa watakao radhi ya Mwenyezi Mungu. Na hao ndio wenye kufanikiwa."}, {"id": 527104, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3448, "text": "Na mnacho kitoa kwa riba ili kiongezeke katika mali ya watu, basi hakiongezeki mbele ya Mwenyezi Mungu. Lakini mnacho kitoa kwa Zaka kwa kutaka radhi ya Mwenyezi Mungu, basi hao ndio watao zidishiwa."}, {"id": 527105, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3449, "text": "Mwenyezi Mungu ndiye aliye kuumbeni, kisha akakuruzukuni, kisha anakufisheni, na kisha anakufufueni. Je! Yupo yeyote katika hao mnao washirikisha na Mwenyezi Mungu afanyae lolote katika hayo? Yeye ametakasika, na ametukuka na hayo mnayo mshirikisha naye."}, {"id": 527106, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3450, "text": "Ufisadi umedhihiri bara na baharini kwa iliyo yafanya mikono ya watu, ili Mwenyezi Mungu awaonyeshe baadhi ya waliyo yatenda. Huenda wakarejea kwa kutubu."}, {"id": 527107, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3451, "text": "Sema: Safirini duniani muangalie ulikuwaje mwisho wa walio tangulia. Wengi wao walikuwa washirikina."}, {"id": 527108, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3452, "text": "Basi uelekeze uso wako kwenye Dini Iliyo Nyooka kabla ya kufika Siku hiyo isiyo zuilika, inayo toka kwa Mwenyezi Mungu. Siku hiyo watu watatengana."}, {"id": 527109, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3453, "text": "Anaye kufuru basi ukafiri wake utakuwa juu yake mwenyewe, na anaye tenda mema, basi wanazitengezea nafsi zao."}, {"id": 527110, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3454, "text": "Ili awalipe walio amini na wakatenda mema kutokana na fadhila yake. Hakika Yeye hawapendi makafiri."}, {"id": 527111, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3455, "text": "Na katika Ishara zake ni kuzituma pepo ziletazo bishara njema, na ili kukuonjesheni baadhi ya rehema zake, na ili majahazi yende kwa amri yake; na mtafute kutokana na fadhila yake, na ili mumshukuru."}, {"id": 527112, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3456, "text": "Na hakika tulikwisha watuma Mitume kwa kaumu zao, na wakawajia kwa hoja zilizo wazi. Tukawaadhibu wale walio fanya ukosefu. Na ilikuwa ni haki juu yetu kuwanusuru Waumini."}, {"id": 527113, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3457, "text": "Mwenyezi Mungu ndiye anaye zituma pepo zikayatimua mawingu, kisha akayatandaza mbinguni kama apendavyo. Na akayafanya mapande mapande, basi ukaiona mvua inatoka ndani yake. Anapo wafikishia awatakao katika waja wake, mara hao huwa na furaha."}, {"id": 527114, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3458, "text": "Na ingawa kabla ya kuteremshiwa walikuwa wenye kukata tamaa."}, {"id": 527115, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3459, "text": "Basi ziangalie athari za rehema ya Mwenyezi Mungu, jinsi anavyo ihuisha ardhi baada ya kufa kwake. Basi hakika huyo ndiye bila ya shaka Mwenye kuhuisha wafu. Na Yeye ni Muweza wa kila kitu."}, {"id": 527116, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3460, "text": "Na tunge utuma upepo na wakauona umekuwa rangi ya manjano, juu ya hayo wange endelea kukufuru."}, {"id": 527117, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3461, "text": "Kwani wewe hakika huwafanyi maiti wakasikia, wala viziwi wakasikia wito wakisha geuka kukupa mgongo."}, {"id": 527118, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3462, "text": "Wala wewe huwaongoi vipofu na upotovu wao. Huwasikilizishi ila wanao ziamini Aya zetu. Hao ndio Waislamu, walio nyenyekea."}, {"id": 527119, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3463, "text": "Mwenyezi Mungu ndiye aliye kuumbeni kutokana na udhaifu kisha akajaalia nguvu baada ya unyonge, kisha akajaalia baada ya nguvu unyonge tena na ukongwe. Anaumba atakavyo. Naye ndiye Mjuzi, Mwenye uweza."}, {"id": 527120, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3464, "text": "Na siku itapo simama Saa wakosefu wataapa kwamba hawakukaa duniani ila saa moja tu. Hivyo ndivyo walivyo kuwa wakipotozwa."}, {"id": 527121, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3465, "text": "Na walio pewa ilimu na Imani watasema: Hakika nyinyi mlikwisha kaa katika hukumu ya Mwenyezi Mungu mpaka Siku ya kufufuliwa. Basi hii ndiyo Siku ya kufufuliwa! Lakini nyinyi mlikuwa hamuijui."}, {"id": 527122, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3466, "text": "Basi Siku hiyo hautawafaa walio dhulumu udhuru wao, wala haitotakiwa toba yao."}, {"id": 527123, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3467, "text": "Na hakika tulikwisha wapigia watu kila mfano katika hii Qur'ani. Na ukiwaletea Ishara yoyote hapana shaka walio kufuru watasema: Nyinyi si chochote ila ni waongo."}, {"id": 527124, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3468, "text": "Hivi ndivyo Mwenyezi Mungu anavyo piga muhuri kwenye nyoyo za wasio jua."}, {"id": 527125, "identifier": "sw.barwani", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3469, "text": "Basi subiri. Hakika ahadi ya Mwenyezi Mungu ni ya haki. Wala wasikupaparishe hao wasio kuwa na yakini."}]