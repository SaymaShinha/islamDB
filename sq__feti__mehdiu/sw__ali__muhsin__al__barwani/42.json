[{"id": 527926, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4273, "text": "H'a Mim"}, {"id": 527927, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4274, "text": "A'yn Sin Qaf"}, {"id": 527928, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4275, "text": "Hivi ndivyo Mwenyezi Mungu Mwenye nguvu na Mwenye hikima anavyo kuletea Wahyi wewe na walio kabla yako."}, {"id": 527929, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4276, "text": "Ni vyake viliomo mbinguni, na viliomo katika ardhi. Na Yeye ndiye Mtukufu, Mkuu."}, {"id": 527930, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4277, "text": "Zinakaribia mbingu kupasuka juu huko, na Malaika wakimtakasa Mola wao Mlezi na kumhimidi, na wakiwaombea maghfira waliomo kwenye ardhi. Ama hakika Mwenyezi Mungu ni Mwenye kusamehe, Mwenye kurehemu."}, {"id": 527931, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4278, "text": "Na wale walio wafanya walinzi wengine badala yake Yeye, Mwenyezi Mungu ni Mwangalizi juu yao. Wala wewe si wakili juu yao."}, {"id": 527932, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4279, "text": "Na namna hivi tumekufunulia Qur'ani kwa Kiarabu ili uwaonye watu wa Mama wa Miji na walio pembezoni mwake. Na uhadharishe na Siku ya Mkutano, haina shaka hiyo. Kundi moja litakuwa Peponi, na kundi jengine Motoni."}, {"id": 527933, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4280, "text": "Na Mwenyezi Mungu angeli penda ange wafanya wote umma mmoja, lakini anamwingiza katika rehema yake amtakaye. Na wenye kudhulumu hawana mlinzi wala msaidizi."}, {"id": 527934, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4281, "text": "Au wamechukua walinzi wengine badala yake! Lakini Mwenyezi Mungu ndiye Mlinzi khasa. Na Yeye ndiye anaye huisha wafu. Na Yeye ndiye Mweza wa kila kitu."}, {"id": 527935, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4282, "text": "Na mkikhitalifiana katika jambo lolote, basi hukumu yake iko kwa Mwenyezi Mungu. Huyo Mwenyezi Mungu ndiye Mola wangu Mlezi, ninaye mtegemea na kwake Yeye narejea."}, {"id": 527936, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4283, "text": "Yeye ndiye Muumba mbingu na ardhi, amekujaalieni mke na mume katika nafsi zenu, na katika nyama hoa dume na jike, anakuzidishieni namna hii. Hapana kitu kama mfano wake. Naye ni Mwenye kusikia Mwenye kuona."}, {"id": 527937, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4284, "text": "Yeye ndiye Mwenye funguo za mbingu na ardhi. Humkunjulia riziki amtakaye, na humpimia amtakaye. Hakika Yeye ni Mjuzi wa kila kitu."}, {"id": 527938, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4285, "text": "Amekuamrisheni Dini ile ile aliyo muusia Nuhu na tuliyo kufunulia wewe, na tuliyo wausia Ibrahim na Musa na Isa, kwamba shikeni Dini wala msifarikiane kwayo. Ni magumu kwa washirikina hayo unayo waitia. Mwenyezi Mungu humteua kwake amtakaye, na humwongoa kwake aelekeaye."}, {"id": 527939, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4286, "text": "Na hawakufarikiana ila baada ya kuwajia ilimu kwa sababu ya husuda iliyo kuwa baina yao. Na lau kuwa haikwisha tangulia kauli kutoka kwa Mola wako Mlezi ya kuakhirisha mpaka muda maalumu, basi bila ya shaka palingeli hukumiwa baina yao. Na hakika walio rithishwa Kitabu baada yao wanakitilia shaka inayo wahangaisha."}, {"id": 527940, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4287, "text": "Basi kwa haya waite! Nawe simama sawa sawa kama ulivyo amrishwa, wala usiyafuate matamanio yao. Na sema: Naamini aliyo teremsha Mwenyezi Mungu katika Vitabu. Na nimeamrishwa nifanye uadilifu baina yenu. Mwenyezi Mungu ni Mola wetu Mlezi, na Mola wenu Mlezi. Sisi tuna jukumu kwa vitendo vyetu, na nyinyi mna jukumu kwa vitendo vyenu. Hapana kuhojiana baina yetu na nyinyi. Na Mwenyezi Mungu atatukusanya pamoja, na marejeo ni kwake."}, {"id": 527941, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4288, "text": "Na wanao hojiana juu ya Mwenyezi Mungu baada ya kukubaliwa, hoja za hawa ni baat'ili mbele ya Mola wao Mlezi, na juu yao ipo ghadhabu, na itakuwa kwao adhabu kali."}, {"id": 527942, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4289, "text": "Mwenyezi Mungu ndiye aliye teremsha Kitabu kwa Haki, na Mizani. Na nini kitakacho kujuulisha ya kwamba pengine Saa ya Kiyama ipo karibu?"}, {"id": 527943, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4290, "text": "Wasio amini huihimiza hiyo Saa ifike upesi; lakini wanao amini wanaiogopa, na wanajua kwamba hakika hiyo ni kweli. Ama kwa hakika hao wanao bishana katika khabari za hiyo Saa wamo katika upotofu wa mbali kabisa."}, {"id": 527944, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4291, "text": "Mwenyezi Mungu ni Mpole kwa waja wake. Humruzuku ampendaye. Naye ni Mwenye nguvu, Mtukufu."}, {"id": 527945, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4292, "text": "Mwenye kutaka mavuno ya Akhera tutamzidishia katika mavuno yake, na mwenye kutaka mavuno ya duniani tutampa katika hayo, lakini katika Akhera hana fungu."}, {"id": 527946, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4293, "text": "Au hao wana miungu ya kishirikina walio watungia dini asiyo itolea idhini Mwenyezi Mungu? Na lau lisinge kuwako neno la kupambanua basi wangeli katiwa hukumu baina yao. Na hakika wenye kudhulumu watapata adhabu chungu."}, {"id": 527947, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4294, "text": "Utawaona hao madhaalimu wanavyo kuwa na khofu kwa sababu ya waliyo yachuma, nayo yatawafika tu. Na walio amini na wakatenda mema watakuwamo katika mabustani ya Peponi. Humo watapata wayatakayo kwa Mola wao Mlezi. Hiyo ndiyo fadhila kubwa."}, {"id": 527948, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4295, "text": "Hayo ndiyo aliyo wabashiria Mwenyezi Mungu waja wake walio amini na wakatenda mema. Sema: Sikuombeni malipo yoyote kwenu ila mapenzi katika kujikurubisha. Na anaye fanya wema tutamzidishia wema. Hakika Mwenyezi Mungu ni Msamehevu Mwenye shukrani."}, {"id": 527949, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4296, "text": "Ati wanasema: Amemzulia Mwenyezi Mungu uwongo? Mwenyezi Mungu akipenda atapiga muhuri juu ya moyo wako. Na Mwenyezi Mungu anaufuta upotofu na anaithibitisha Haki kwa maneno yake. Hakika Yeye anayajua yaliomo vifuani."}, {"id": 527950, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4297, "text": "Naye ndiye anaye pokea toba kwa waja wake, na anasamehe makosa, na anayajua mnayo yatenda."}, {"id": 527951, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4298, "text": "Na anawaitikia wanao amini na wakatenda mema, na anawazidishia fadhila zake. Na makafiri watakuwa na adhabu chungu."}, {"id": 527952, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4299, "text": "Na lau kuwa Mwenyezi Mungu angeli wakunjulia riziki waja wake, basi bila ya shaka wangeli panda kiburi katika dunia. Lakini anaiteremsha kwa kipimo akitakacho. Hakika Yeye anawajua vyema waja wake na anawaona."}, {"id": 527953, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4300, "text": "Naye ndiye anaye iteremsha mvua baada ya wao kwisha kata tamaa, na hueneza rehema yake. Naye ndiye Mlinzi Mwenye kuhimidiwa."}, {"id": 527954, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4301, "text": "Na katika Ishara zake ni kuumba mbingu na ardhi, na wanyama alio waeneza. Naye ni Mwenye uweza wa kuwakusanya apendapo."}, {"id": 527955, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4302, "text": "Na misiba inayo kusibuni ni kwa sababu ya vitendo vya mikono yenu. Naye anasamehe mengi."}, {"id": 527956, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4303, "text": "Na nyinyi hamwezi kushinda katika ardhi. Na wala nyinyi hamna mlinzi wala msaidizi badala ya Mwenyezi Mungu."}, {"id": 527957, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4304, "text": "Na katika Ishara zake ni vyombo vinavyo kwenda na kurejea baharini kama vilima."}, {"id": 527958, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4305, "text": "Akitaka, huutuliza upepo navyo hivyo vyombo husimama tutwe juu ya bahari. Hakika katika hayo zipo Ishara kwa kila mwenye kusubiri akashukuru."}, {"id": 527959, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4306, "text": "Au huviangamiza hivyo vyombo kwa sababu ya waliyo yatenda hao watu. Naye husamehe mengi."}, {"id": 527960, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4307, "text": "Na ili wajue wanao jadiliana katika Ishara zetu kwamba hawana pahala pa kukimbilia."}, {"id": 527961, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4308, "text": "Basi vyote mlivyo pewa ni starehe ya maisha ya dunia tu, lakini kilichoko kwa Mwenyezi Mungu ni bora, na cha kudumu kwa ajili ya walio amini, na wakawa wanamtegemea Mola wao Mlezi,"}, {"id": 527962, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4309, "text": "Na wanayo yaepuka madhambi makubwa na mambo machafu, na wanapo kasirika wao husamehe,"}, {"id": 527963, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4310, "text": "Na wanao mwitikia Mola wao Mlezi, na wakashika Sala, na mambo yao yakawa ni kwa kushauriana baina yao, na kutokana na tulicho waruzuku wakawa wanatoa,"}, {"id": 527964, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4311, "text": "Na ambao wanapo fanyiwa jeuri hujitetea."}, {"id": 527965, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4312, "text": "Na malipo ya uovu ni uovu mfano wa huo. Lakini mwenye kusamehe, na akasuluhisha, basi huyo malipo yake yapo kwa Mwenyezi Mungu. Hakika Yeye hawapendi wenye kudhulumu."}, {"id": 527966, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4313, "text": "Na wanao jitetea baada ya kudhulumiwa, hao hapana njia ya kuwalaumu."}, {"id": 527920, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4314, "text": "Bali lawama ipo kwa wale wanao wadhulumu watu, na wanafanya jeuri katika nchi bila ya haki. Hao watapata adhabu iliyo chungu."}, {"id": 527967, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4315, "text": "Na anaye subiri, na akasamehe, hakika hayo ni katika mambo ya kuazimiwa."}, {"id": 527968, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4316, "text": "Na ambaye Mwenyezi Mungu amemwacha kupotea, hana mlinzi baada yake. Na utawaona wenye kudhulumu watakapo iona adhabu wanasema: Je! Ipo njia ya kurudi?"}, {"id": 527969, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4317, "text": "Na utawaona wanapelekwa kwenye Moto, nao wamenyenyekea kwa unyonge, wanatazama kwa mtazamo wa kificho. Na walio amini watasema: Hakika wapatao khasara ni hao walio khasiri nafsi zao na ahali zao Siku ya Kiyama. Ama hakika wenye kudhulumu watakuwa katika adhabu ya daima."}, {"id": 527970, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4318, "text": "Wala hawatakuwa na walinzi wa kuwanusuru mbele ya Mwenyezi Mungu. Na ambaye Mwenyezi Mungu amemwacha kupotea, hana njia yoyote."}, {"id": 527971, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4319, "text": "Muitikieni Mola wenu Mlezi kabla haijafika siku isiyo epukika itokayo kwa Mwenyezi Mungu. Siku hiyo hamtakuwa pa kukimbilia wala hamtakuwa na cha kukingia."}, {"id": 527972, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4320, "text": "Na wakipuuza, basi Sisi hatukukupeleka ili uwe mwangalizi wao. Si juu yako ila kufikisha Ujumbe tu. Na hakika Sisi tukimwonjesha mtu rehema kutoka kwetu, huifurahia. Na akisibiwa na ovu kwa sababu ya iliyo yatanguliza mikono yao, basi hakika mtu huyu anakufuru."}, {"id": 527973, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4321, "text": "Ufalme wa mbingu na ardhi ni wa Mwenyezi Mungu; anaumba apendavyo, anamtunukia amtakaye watoto wa kike, na anamtunukia amtakaye watoto wa kiume,"}, {"id": 527974, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4322, "text": "Au huwachanganya wanaume na wanawake, na akamfanya amtakaye tasa. Hakika Yeye ni Mjuzi Mwenye uweza."}, {"id": 527975, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4323, "text": "Na haikuwa kwa mwanaadamu kwamba Mwenyezi Mungu amsemeze ila kwa Wahyi (Ufunuo), au kwa nyuma ya pazia, au kumtuma Mjumbe. Naye humfunulia ayatakayo kwa idhini yake. Hakika Yeye ni Mtukufu, Mwenye hikima."}, {"id": 527977, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 489, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4324, "text": "Na namna hivi tumekufunulia Qur'ani kwa amri yetu. Ulikuwa hujui Kitabu ni nini, wala Imani. Lakini tumekifanya kuwa ni Nuru ambayo kwayo tunammwongoa tumtakaye katika waja wetu. Na hakika wewe unaongoa kwenye Njia Iliyo Nyooka."}, {"id": 527978, "identifier": "sw.barwani", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 489, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4325, "text": "Njia ya Mwenyezi Mungu ambaye ni vyake Yeye tu viliomo mbinguni na katika ardhi. Jueni kuwa mambo yote yanarudi kwa Mwenyezi Mungu."}]