[{"id": 529254, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5623, "text": "Naapa kwa zinazo tumwa kwa upole!"}, {"id": 529255, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5624, "text": "Na zinazo vuma kwa kasi!"}, {"id": 529256, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5625, "text": "Na zikaeneza maeneo yote!"}, {"id": 529257, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5626, "text": "Na zinazo farikisha zikatawanya!"}, {"id": 526840, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5627, "text": "Na zinazo peleka mawaidha!"}, {"id": 527728, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5628, "text": "Kwa kuudhuru au kuonya,"}, {"id": 529258, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5629, "text": "Hakika mnayo ahidiwa bila ya shaka yatakuwa!"}, {"id": 529259, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5630, "text": "Wakati nyota zitakapo futwa,"}, {"id": 529260, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5631, "text": "Na mbingu zitakapo pasuliwa,"}, {"id": 529261, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5632, "text": "Na milima itakapo peperushwa,"}, {"id": 529262, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5633, "text": "Na Mitume watakapo wekewa wakati wao,"}, {"id": 529263, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5634, "text": "Kwa siku gani hiyo wamewekewa muda huo?"}, {"id": 529264, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5635, "text": "Kwa siku ya kupambanua!"}, {"id": 529265, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5636, "text": "Na nini kitakacho kujuulisha siku ya kupambanua ni nini?"}, {"id": 529266, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5637, "text": "Ole wao, siku hiyo hao wanao kanusha!"}, {"id": 529267, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5638, "text": "Kwani hatukuwaangamiza walio tangulia?"}, {"id": 529268, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5639, "text": "Kisha tukawafuatilizia walio fuatia?"}, {"id": 529269, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5640, "text": "Ndio kama hivi tuwatendavyo wakosefu!"}, {"id": 529270, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5641, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529271, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5642, "text": "Kwani hatukukuumbeni kwa maji ya kudharauliwa?"}, {"id": 529272, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5643, "text": "Kisha tukayaweka mahali pa utulivu madhubuti?"}, {"id": 529273, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5644, "text": "Mpaka muda maalumu?"}, {"id": 529274, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5645, "text": "Tukakadiria na Sisi ni wabora wa kukadiria."}, {"id": 529275, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5646, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529276, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5647, "text": "Kwani hatukuifanya ardhi yenye kukusanya"}, {"id": 529277, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5648, "text": "Walio hai na maiti?"}, {"id": 529278, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5649, "text": "Na ndani yake tukaweka milima mirefu yenye kuthibiti, na tunakunywesheni maji matamu?"}, {"id": 529279, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5650, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529280, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5651, "text": "Nendeni kwenye adhabu mliyo kuwa mkiikanusha!"}, {"id": 529281, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5652, "text": "Nendeni kwenye kivuli chenye mapande matatu!"}, {"id": 529282, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5653, "text": "Hakikingi moto, wala hakiwaepushi na mwako."}, {"id": 529283, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5654, "text": "Hakika Moto huo unatoa macheche kama majumba!"}, {"id": 529284, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5655, "text": "Kama kwamba ni ngamia wa rangi ya manjano!"}, {"id": 529285, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5656, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529286, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5657, "text": "Hii ni siku ambayo hawatatamka kitu,"}, {"id": 529287, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5658, "text": "Wala hawataruhusiwa kutoa udhuru."}, {"id": 529288, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5659, "text": "Ole wao siku hiyo hao walio kanusha!"}, {"id": 529289, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5660, "text": "Hii ndiyo siku ya kupambanua. Tumekukusanyeni nyinyi na walio tangulia."}, {"id": 529290, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5661, "text": "Ikiwa mnayo hila, nifanyieni hila Mimi!"}, {"id": 529291, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5662, "text": "Ole wao siku hiyo hao walio kanusha!"}, {"id": 529292, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5663, "text": "Hakika wachamngu watakuwa katika vivuli na chemchem,"}, {"id": 529293, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5664, "text": "Na matunda wanayo yapenda,"}, {"id": 529294, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5665, "text": "Kuleni na kunyweni kwa furaha kwa yale mliyo kuwa mkiyatenda."}, {"id": 529295, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5666, "text": "Hakika ndio kama hivyo tunavyo walipa watendao mema."}, {"id": 529296, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5667, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529297, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5668, "text": "Kuleni na mjifurahishe kidogo tu. Hakika nyinyi ni wakosefu!"}, {"id": 529298, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5669, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529299, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5670, "text": "Na wakiambiwa: Inameni! Hawainami."}, {"id": 529300, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5671, "text": "Ole wao siku hiyo hao wanao kanusha!"}, {"id": 529301, "identifier": "sw.barwani", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 517, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5672, "text": "Basi maneno gani baada ya haya watayaamini?"}]