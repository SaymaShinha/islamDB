[{"id": 528818, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5178, "text": "Viliomo mbinguni na viliomo katika ardhi vinamtakasa Mwenyezi Mungu, Mfalme, Mtakatifu, Mwenye nguvu, Mwenye hikima."}, {"id": 528819, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5179, "text": "Yeye ndiye aliye mpeleka Mtume kwenye watu wasio jua kusoma, awasomee Aya zake na awatakase, na awafunze Kitabu na hikima, ijapo kuwa kabla ya haya walikuwa katika upotofu ulio dhaahiri."}, {"id": 528798, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5180, "text": "Na kwa wengine ambao bado hawajaungana nao. Na Yeye ndiye Mwenye nguvu, Mwenye hikima."}, {"id": 528820, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5181, "text": "Hiyo ni fadhila ya Mwenyezi Mungu anayo mpa amtakaye. Na Mwenyezi Mungu ni Mwenye fadhila kubwa."}, {"id": 528821, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5182, "text": "Mfano wa walio bebeshwa Taurati kisha wasiibebe, ni mfano wa punda anaye beba vitabu vikubwa vikubwa. Mfano muovu mno wa watu walio kadhibisha Ishara za Mwenyezi Mungu, na Mwenyezi Mungu hawaongoi watu madhaalimu."}, {"id": 528822, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5183, "text": "Sema: Enyi mlio Mayahudi! Ikiwa nyinyi mnadai kuwa ni vipenzi vya Mwenyezi Mungu pasipo kuwa watu wengine, basi yatamanini mauti, ikiwa mnasema kweli."}, {"id": 528823, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5184, "text": "Wala hawatayatamani kabisa, kwa sababu ya iliyo kwisha yatanguliza mikono yao. Na Mwenyezi Mungu anawajua walio dhulumu."}, {"id": 528824, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 553, "ruku": 486, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5185, "text": "Sema: Hayo mauti mnayo yakimbia, bila ya shaka yatakukuteni. Kisha mtarudishwa kwa Mwenye kuyajua yaliyo fichikana na yanayo onekana. Hapo atakwambieni mliyo kuwa mkiyatenda."}, {"id": 528825, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 487, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5186, "text": "Enyi mlio amini! Ikiadhiniwa Sala siku ya Ijumaa, nendeni upesi kwenye dhikri ya Mwenyezi Mungu, na wacheni biashara. Hayo ni bora kwenu, lau kama mnajua."}, {"id": 528826, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 487, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5187, "text": "Na itakapo kwisha Sala, tawanyikeni katika nchi mtafute fadhila za Mwenyezi Mungu, na mkumbukeni Mwenyezi Mungu kwa wingi ili mpate kufanikiwa."}, {"id": 528827, "identifier": "sw.barwani", "surah_number": 62, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0645\u0639\u0629", "surah_en_name": "Al-Jumu'a", "surah_en_name_translation": "Friday", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 487, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5188, "text": "Na wanapo iona biashara au pumbao wanayakimbilia hayo na wakakuacha umesimama. Sema: Yaliyoko kwa Mwenyezi Mungu ni bora kuliko pumbao na biashara. Na Mwenyezi Mungu ni Mbora wa watoa riziki."}]