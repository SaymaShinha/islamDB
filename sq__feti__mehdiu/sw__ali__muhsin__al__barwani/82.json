[{"id": 529456, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5830, "text": "Mbingu itapo chanika,"}, {"id": 529457, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5831, "text": "Na nyota zitapo tawanyika,"}, {"id": 529458, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5832, "text": "Na bahari zitakapo pasuliwa,"}, {"id": 529459, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5833, "text": "Na makaburi yatapo fukuliwa,"}, {"id": 529460, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5834, "text": "Hapo kila nafsi itajua ilicho tanguliza, na ilicho bakisha nyuma."}, {"id": 529461, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5835, "text": "Ewe mwanaadamu! Nini kilicho kughuri ukamwacha Mola wako Mlezi Mtukufu?"}, {"id": 529462, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5836, "text": "Aliye kuumba, akakuweka sawa, akakunyoosha,"}, {"id": 529463, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5837, "text": "Katika sura yoyote aliyo ipenda akakujenga."}, {"id": 529464, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5838, "text": "Sivyo hivyo! Bali nyinyi mnaikanusha Siku ya Malipo."}, {"id": 529465, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5839, "text": "Na hakika bila ya shaka wapo walinzi juu yenu,"}, {"id": 529466, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5840, "text": "Waandishi wenye hishima,"}, {"id": 529467, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5841, "text": "Wanayajua mnayo yatenda."}, {"id": 529468, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5842, "text": "Hakika wema bila ya shaka watakuwa katika neema,"}, {"id": 529469, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5843, "text": "Na hakika waovu bila ya shaka watakuwa Motoni;"}, {"id": 529470, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5844, "text": "Wataingia humo Siku ya Malipo."}, {"id": 529471, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5845, "text": "Na hawatoacha kuwamo humo."}, {"id": 529472, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5846, "text": "Na nini kitakacho kujuulisha Siku ya Malipo ni siku gani?"}, {"id": 529473, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5847, "text": "Tena nini kitakacho kujuulisha Siku ya Malipo ni siku gani?"}, {"id": 529474, "identifier": "sw.barwani", "surah_number": 82, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0641\u0637\u0627\u0631", "surah_en_name": "Al-Infitaar", "surah_en_name_translation": "The Cleaving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 524, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5848, "text": "Siku ambayo nafsi haitakuwa na madaraka yoyote juu ya nafsi; na amri yote siku hiyo ni ya Mwenyezi Mungu tu."}]