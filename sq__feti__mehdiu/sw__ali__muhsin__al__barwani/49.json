[{"id": 528271, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4613, "text": "Enyi mlio amini! Msitangulie mbele ya Mwenyezi Mungu na Mtume wake, na mcheni Mwenyezi Mungu. Hakika Mwenyezi Mungu ni Mwenye kusikia, Mwenye kujua."}, {"id": 528272, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4614, "text": "Enyi mlio amini! Msinyanyue sauti zenu kuliko sauti ya Nabii, wala msiseme naye kwa kelele kama mnavyo semezana nyinyi kwa nyinyi, visije vitendo vyenu vikaharibika, na hali hamtambui."}, {"id": 528273, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4615, "text": "Kwa hakika wanao teremsha sauti zao mbele ya Mtume wa Mwenyezi Mungu, hao ndio Mwenyezi Mungu amezisafisha nyoyo zao kwa uchamngu. Hao watakuwa na maghfira na ujira mkubwa."}, {"id": 528274, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4616, "text": "Hakika hao wanao kuita nawe uko nyuma ya vyumba, wengi wao hawana akili."}, {"id": 528275, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4617, "text": "Na lau wao wangeli ngojea mpaka uwatokee ingeli kuwa kheri kwao. Na Mwenyezi Mungu ni Mwenye maghfira, Mwenye kurehemu."}, {"id": 528276, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4618, "text": "Enyi mlio amini! Akikujieni mpotovu na khabari yoyote, ichunguzeni, msije mkawasibu watu kwa kuto jua, na mkawa tena wenye kujuta kwa mliyo yatenda."}, {"id": 528277, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4619, "text": "Na jueni kuwa Mtume wa Mwenyezi Mungu yuko nanyi. Lau angeli kut'iini katika mambo mengi, bila ya shaka mngeli taabika. Lakini Mwenyezi Mungu amekupendezeeni Imani, na akaipamba katika nyoyo zenu, na amekufanyeni muuchukie ukafiri, na upotovu, na uasi. Hao ndio walio ongoka,"}, {"id": 528278, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4620, "text": "Kwa fadhila za Mwenyezi Mungu na neema zake. Na Mwenyezi Mungu ni Mwenye kujua, Mwenye hikima."}, {"id": 528280, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4621, "text": "Na ikiwa makundi mawili katika Waumini yanapigana, basi yapatanisheni. Na likiwa moja la hao linamdhulumu mwenzie, basi lipigeni linalo dhulumu mpaka lirejee kwenye amri ya Mwenyezi Mungu. Na likirudi basi yapatanisheni kwa uadilifu. Na hukumuni kwa haki. Hakika Mwenyezi Mungu anawapenda wanao hukumu kwa haki."}, {"id": 528281, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 450, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4622, "text": "Hakika Waumini ni ndugu, basi patanisheni baina ya ndugu zenu, na mcheni Mwenyezi Mungu ili mrehemewe."}, {"id": 528282, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 516, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4623, "text": "Enyi mlio amini! Wanaume wasiwadharau wanaume wenzao, wala wanawake wasiwadharau wanawake wenzao. Huenda hao wakawa bora kuliko wao. Wala msivunjiane hishima, wala msiitane kwa majina ya kejeli. Ni uovu kutumia jina baya baada ya kwisha amini. Na wasio tubu, hao ndio wenye kudhulumu."}, {"id": 528283, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4624, "text": "Enyi mlio amini! Jiepusheni na dhana nyingi, kwani baadhi ya dhana ni dhambi. Wala msipelelezane, wala msisengenyane nyinyi kwa nyinyi. Je! Yupo katika nyinyi anaye penda kuila nyama ya nduguye aliye kufa? Mnalichukia hilo! Na mcheni Mwenyezi Mungu. Hakika Mwenyezi Mungu ni Mwenye kupokea toba, Mwenye kurehemu."}, {"id": 528284, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 206, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4625, "text": "Enyi watu! Hakika Sisi tumekuumbeni kutokana na mwanamume na mwanamke. Na tumekujaalieni kuwa ni mataifa na makabila ili mjuane. Hakika aliye mtukufu zaidi kati yenu kwa Mwenyezi Mungu ni huyo aliye mchamngu zaidi katika nyinyi. Hakika Mwenyezi Mungu ni Mwenye kujua, Mwenye khabari."}, {"id": 528285, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4626, "text": "Mabedui wamesema: Tumeamini. Sema: Hamjaamini, lakini semeni: Tumesilimu. Kwani Imani haijaingia katika nyoyo zenu. Na mkimt'ii Mwenyezi Mungu na Mtume wake, hatakupunguzieni chochote katika vitendo vyenu. Hakika Mwenyezi Mungu ni Mwenye kusamehe, Mwenye kurehemu."}, {"id": 528286, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4627, "text": "Hakika Waumini ni wale walio muamini Mwenyezi Mungu na Mtume wake tena wasitie shaka kabisa, na wakapigana Jihadi katika Njia ya Mwenyezi Mungu kwa mali yao na nafsi zao. Hao ndio wakweli."}, {"id": 528287, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4628, "text": "Sema: Ati ndio mnamfundisha Mwenyezi Mungu Dini yenu, na hali Mwenyezi Mungu anayajua ya katika mbingu na ardhi, na Mwenyezi Mungu anajua kila kitu?"}, {"id": 528288, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4629, "text": "Wanakusimbulia kuwa wamesilimu! Sema: Msinisimbulie kwa kusilimu kwenu. Bali Mwenyezi Mungu ndiye aliye kufanyieni hisani kwa kukuongoeni kwenye Imani, ikiwa nyinyi ni wakweli."}, {"id": 528289, "identifier": "sw.barwani", "surah_number": 49, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u062c\u0631\u0627\u062a", "surah_en_name": "Al-Hujuraat", "surah_en_name_translation": "The Inner Apartments", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 517, "ruku": 451, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4630, "text": "Hakika Mwenyezi Mungu anajua siri za mbingu na ardhi. Na Mwenyezi Mungu anayaona myatendayo."}]