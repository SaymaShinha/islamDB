[{"id": 525410, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1751, "text": "Alif Lam Ra. (A. L. R.) Hichi ni Kitabu tulicho kiteremsha kwako ili uwatowe watu kwenye giza uwapeleke kwenye muwangaza, kwa idhini ya Mola wao Mlezi, uwafikishe kwenye Njia ya Mwenye nguvu, Msifiwa,"}, {"id": 525411, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1752, "text": "Naye ni Mwenyezi Mungu ambaye ni vyake Yeye vyote viliomo katika mbingu na ardhi. Na ole wao makafiri kwa adhabu kali!"}, {"id": 525412, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1753, "text": "Wale wanao fadhilisha maisha ya dunia kuliko Akhera, na wanawazuilia watu wasifuate Njia ya Mwenyezi Mungu, na wanataka kuipotosha. Hao wamepotelea mbali."}, {"id": 525413, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1754, "text": "Na hatukumtuma Mtume ila kwa ulimi wa kaumu yake ili apate kuwabainishia. Basi Mwenyezi Mungu humuacha akapotea amtakaye, na akamwongoa amtakaye. Naye ni Mtukufu Mwenye nguvu na Mwenye hikima."}, {"id": 525414, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1755, "text": "Na tulimtuma Musa pamoja na miujiza yetu, tukamwambia: Watoe watu wako kwenye giza uwapeleke kwenye mwangaza. Na uwakumbushe siku za Mwenyezi Mungu. Hakika katika hayo zipo ishara kwa kila mwenye kusubiri akashukuru."}, {"id": 525415, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1756, "text": "Na Musa alipo waambia watu wake: Kumbukeni neema ya Mwenyezi Mungu aliyo kujaalieni, pale alipo kuokoeni kutokana na watu wa Firauni walio kupeni adhabu mbaya kabisa, wakiwachinja wenenu wanaume, na wakiwawacha hai wanawake. Na katika hayo ulikuwa mtihani mkubwa unao toka kwa Mola wenu Mlezi."}, {"id": 525416, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1757, "text": "Na alipo tangaza Mola wenu Mlezi: Mkishukuru nitakuzidishieni; na mkikufuru, basi adhabu yangu ni kali."}, {"id": 525417, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1758, "text": "Na Musa alisema: Mkikufuru nyinyi na wote waliomo duniani, hakika Mwenyezi Mungu ni Mkwasi, Anajitosha, Msifiwa."}, {"id": 525418, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1759, "text": "Je! Hazikukufikieni khabari za walio kabla yenu? Kaumu ya Nuhu, na A'adi, na Thamud, na walio baada yao, ambao hapana awajuaye ila Mwenyezi Mungu? Mitume wao waliwajia kwa hoja zilizo wazi, na wao wakarudisha mikono yao kwenye vinywa vyao, wakasema: Hakika sisi tumeyakataa hayo mliyo tumwa, na hakika sisi tuna shaka kubwa na mnayo tuitia."}, {"id": 525419, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1760, "text": "Mitume wao wakasema: Ati pana shaka na Mwenyezi Mungu, Muumba mbingu na ardhi? Yeye anakuiteni apate kukufutieni madhambi yenu, na akupeni muhula mpaka muda ulio wekwa. Wakasema: Nyinyi si chochote ila ni wanaadamu kama sisi. Mnataka kutuzuilia na waliyo kuwa wakiabudu baba zetu. Basi tuleteeni hoja ilio wazi."}, {"id": 525420, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 212, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1761, "text": "Mitume wao wakawaambia: Sisi kweli si chochote ila ni wanaadamu kama nyinyi. Lakini Mwenyezi Mungu humfanyia hisani amtakaye katika waja wake. Wala sisi hatuwezi kuleta uthibitisho ila kwa idhini ya Mwenyezi Mungu. Na juu ya Mwenyezi Mungu ndio wategemee Waumini."}, {"id": 525421, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 212, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1762, "text": "Na tuna nini hata tusimtegemee Mwenyezi Mungu, na hali Yeye ametuongoa kwenye Njia zetu. Na hapana shaka sisi tutayavumilia hayo maudhi mnayo tuudhi. Na juu ya Mwenyezi Mungu wategemee wanao tegemea."}, {"id": 525422, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1763, "text": "Na walio kufuru wakawaambia Mitume wao: Tutakutoeni katika nchi yetu, au mrudi katika mila yetu. Basi Mola wao Mlezi aliwaletea wahyi: Hakika tutawaangamiza walio dhulumu!"}, {"id": 525423, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1764, "text": "Na tutakuwekeni katika ardhi baada yao. Haya ni kwa anaye ogopa kusimamishwa mbele yangu, na akaogopa maonyo yangu."}, {"id": 525424, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1765, "text": "Na wakaomba ushindi, na akashindwa kila jabari mkaidi,"}, {"id": 525425, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1766, "text": "Ambaye nyuma yake ipo Jahannamu, na atanywishwa maji ya usaha."}, {"id": 525426, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1767, "text": "Awe anayagugumia, wala hawezi kuyameza. Na mauti yawe yanamjia kutoka kila upande, naye wala hafi. Na zaidi ya hayo ipo adhabu nyengine kali vile vile."}, {"id": 525428, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1768, "text": "Mfano wa walio mkufuru Mola wao Mlezi - vitendo vyao ni kama jivu linalo peperushwa kwa nguvu za upepo katika siku ya mdharba. Hawawezi kupata chochote katika waliyo yafanya. Huko ndiko kupotelea mbali!"}, {"id": 525429, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1769, "text": "Huoni kuwa Mwenyezi Mungu ameziumba mbingu na ardhi kwa Haki? Akitaka atakuondoeni na alete viumbe wapya!"}, {"id": 525430, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1770, "text": "Na hilo kwa Mwenyezi Mungu si jambo gumu."}, {"id": 525431, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1771, "text": "Na wote watahudhuria mbele ya Mwenyezi Mungu. Wanyonge watawaambia walio takabari: Sisi tulikuwa wafwasi wenu; basi hebu hamtuondolei kidogo hivi katika adhabu ya Mwenyezi Mungu? Watasema: Lau Mwenyezi Mungu angeli tuongoa basi hapana shaka nasi tungeli kuongoeni. Ni mamoja kwetu tukipapatika au tukisubiri; hatuna pa kukimbilia."}, {"id": 525432, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1772, "text": "Na Shet'ani atasema itapo katwa hukumu: Hakika Mwenyezi Mungu alikuahidini ahadi ya kweli. Nami nalikuahidini; lakini sikukutimizieni. Na sikuwa na mamlaka juu yenu, isipo kuwa nilikuiteni, nanyi mkaniitikia. Basi msinilaumu mimi, bali jilaumuni wenyewe. Mimi siwezi kuwa mtetezi wenu, wala nyinyi hamwezi kuwa watetezi wangu. Hakika mimi nilikataa tangu zamani kunishirikisha na Mwenyezi Mungu. Hakika madhaalimu watakuwa na adhabu chungu."}, {"id": 525433, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1773, "text": "Na walio amini na wakatenda mema wataingizwa katika Mabustani yapitayo mito kati yake, wadumu humo kwa idhini ya Mola wao Mlezi. Maamkio yao humo yatakuwa: Salaam!"}, {"id": 525434, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1774, "text": "Hebu hukuona vipi Mwenyezi Mungu alivyo piga mfano wa neno zuri? Ni kama mti mzuri, mizizi yake ni imara, na matawi yake yako mbinguni."}, {"id": 525435, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1775, "text": "Hutoa matunda yake kila wakati kwa idhini ya Mola wake Mlezi. Na Mwenyezi Mungu huwapigia watu mifano ili wapate kukumbuka."}, {"id": 525436, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1776, "text": "Na mfano wa neno ovu ni kama mti muovu, ulio ng'olewa juu ya ardhi. Hauna imara."}, {"id": 525437, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1777, "text": "Mwenyezi Mungu huwatia imara wenye kuamini kwa kauli ya imara katika maisha ya dunia na katika Akhera. Na Mwenyezi Mungu huwaacha kupotea hao wenye kudhulumu. Na Mwenyezi Mungu hufanya apendavyo."}, {"id": 525438, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1778, "text": "Hebu hukuwaona wale walio badilisha neema ya Mwenyezi Mungu kwa kufuru, na wakawafikisha watu wao kwenye nyumba ya maangamizo?"}, {"id": 525439, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1779, "text": "Nayo ni Jahannamu! Maovu yaliyoje makaazi hayo!"}, {"id": 525440, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1780, "text": "Na walimfanyia Mwenyezi Mungu washirika ili wapoteze watu kwenye Njia yake. Sema: Stareheni! Kwani marejeo yenu ni Motoni!"}, {"id": 525441, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1781, "text": "Waambie waja wangu walio amini, washike Sala, na watoe katika tulivyo waruzuku, kwa siri na dhaahiri, kabla haijafika Siku isiyo kuwa na biashara wala urafiki."}, {"id": 525442, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1782, "text": "Mwenyezi Mungu ndiye aliye ziumba mbingu na ardhi, na akateremsha maji kutoka mbinguni. Na kwa hayo akatoa matunda kuwa ni riziki yenu. Na akafanya yakutumikieni majahazi yanayo pita baharini kwa amri yake, na akaifanya mito ikutumikieni."}, {"id": 525443, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1783, "text": "Na akalifanya jua na mwezi kwa manufaa yenu daima dawamu, na akaufanya usiku na mchana kwa manufaa yenu."}, {"id": 525444, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1784, "text": "Na akakupeni kila mlicho muomba. Na mkihisabu neema za Mwenyezi Mungu hamwezi kuzidhibiti. Hakika mwanaadamu ni dhaalimu mkubwa, mwenye kuzikufuru neema."}, {"id": 525446, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1785, "text": "Na Ibrahim alipo sema: Ewe Mola wangu Mlezi! Ujaalie mji huu uwe wa amani, na uniepushe mimi na wanangu na kuabudu masanamu."}, {"id": 525447, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1786, "text": "Ewe Mola wangu Mlezi! Hakika hayo yamewapoteza watu wengi. Basi aliye nifuata mimi huyo ni wangu, na aliye niasi, hakika Wewe ni Mwenye kusamehe Mwenye kurehemu."}, {"id": 525448, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1787, "text": "Mola wetu Mlezi! Hakika mimi nimewaweka baadhi ya dhuriya zangu katika bonde lisilo kuwa na mimea, kwenye Nyumba yako Takatifu, ewe Mola wetu Mlezi, ili washike Sala. Basi zijaalie nyoyo za watu zielekee kwao, na waruzuku matunda, ili wapata kushukuru."}, {"id": 525449, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1788, "text": "Ewe Mola wetu Mlezi! Hakika Wewe unajua tunayo yaficha na tunayo yatangaza. Na hapana kitu kinacho fichikana kwa Mwenyezi Mungu katika ardhi wala katika mbingu."}, {"id": 525450, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1789, "text": "Alhamdulillahi! Himdi zote ni za Mwenyezi Mungu aliye nipa juu ya uzee wangu Ismail na Is-haq. Hakika Mola wangu Mlezi ni Mwenye kusikia maombi."}, {"id": 525451, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1790, "text": "Mola wangu Mlezi! Nijaalie niwe mwenye kushika Sala, na katika dhuriya zangu pia. Ewe Mola wetu Mlezi, na ipokee dua yangu."}, {"id": 525452, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1791, "text": "Mola wetu! Unighufirie mimi na wazazi wangu wote wawili, na Waumini, siku ya kusimama hisabu."}, {"id": 525453, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1792, "text": "Wala usidhani Mwenyezi Mungu ameghafilika na wanayo yafanya madhaalimu. Hakika Yeye anawaakhirisha tu mpaka siku yatapo kodoka macho yao."}, {"id": 525454, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1793, "text": "Nao wako mbioni, vichwa juu, na macho hayapepesi, na nyoyo zao tupu."}, {"id": 525455, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1794, "text": "Na waonye watu siku itapo wajia adhabu, na walio dhulumu waseme: Ewe Mola wetu Mlezi! Tuakhirishe muda kidogo tuitikie wito wako na tuwafuate Mitume. Kwani nyinyi si mliapa zamani kwamba hamtaondokewa?"}, {"id": 525456, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1795, "text": "Na mkakaa katika maskani zile zile za walio zidhulumu nafsi zao. Na ikakudhihirikieni jinsi tulivyo watendea. Nasi tukakupigieni mifano mingi."}, {"id": 525457, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1796, "text": "Na kwa hakika walifanya vitimbi vyao, na vitimbi vyao anavijua Mwenyezi Mungu. Wala vitimbi vyao si vya kuondosha milima."}, {"id": 525458, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1797, "text": "Basi usimdhanie Mwenyezi Mungu kuwa ni mwenye kuwavunjia ahadi yake Mitume wake. Hakika Mwenyezi Mungu ni Mwenye kushinda, na ni Mwenye kulipiza."}, {"id": 525459, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1798, "text": "Siku itapo geuzwa ardhi iwe ardhi nyengine, na mbingu pia. Nao watahudhuria mbele ya Mwenyezi Mungu Mmoja, Mwenye nguvu."}, {"id": 525460, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1799, "text": "Na utaona wakhalifu siku hiyo wamefungwa katika minyororo;"}, {"id": 525461, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1800, "text": "Nguo zao zitakuwa za lami, na Moto utazigubika nyuso zao."}, {"id": 525462, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1801, "text": "Ili Mwenyezi Mungu ailipe kila nafsi kwa yale iliyo yachuma. Hakika Mwenyezi Mungu ni Mwepesi wa kuhisabu."}, {"id": 525463, "identifier": "sw.barwani", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 1802, "text": "Hili ni Tangazo liwafikie watu, liwaonye, na wapate kujua kuwa hakika Yeye ni Mungu Mmoja, na wapate kukumbuka wenye akili."}]