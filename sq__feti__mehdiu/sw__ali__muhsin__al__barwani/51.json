[{"id": 528332, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Naapa kwa pepo zinazo tawanya,"}, {"id": 523825, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4677, "text": "Na zinazo beba mizigo,"}, {"id": 528258, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4678, "text": "Na zinazo kwenda kwa wepesi."}, {"id": 528333, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4679, "text": "Na zinazo gawanya kwa amri,"}, {"id": 528334, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Hakika mnayo ahidiwa bila ya shaka ni kweli,"}, {"id": 528335, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4681, "text": "Na kwa hakika malipo bila ya shaka yatatokea."}, {"id": 528336, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4682, "text": "Naapa kwa mbingu zenye njia,"}, {"id": 528337, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Hakika nyinyi bila ya shaka mmo katika kauli inayo khitalifiana."}, {"id": 528338, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4684, "text": "Anageuzwa kutokana na haki mwenye kugeuzwa."}, {"id": 528339, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Wazushi wameangamizwa."}, {"id": 528340, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4686, "text": "Ambao wameghafilika katika ujinga."}, {"id": 528341, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Wanauliza: Ni lini hiyo siku ya malipo?"}, {"id": 528342, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4688, "text": "Hiyo ni siku watakayo adhibiwa Motoni."}, {"id": 528343, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4689, "text": "Onjeni adhabu yenu! Haya ndiyo mliyo kuwa mkiyahimiza."}, {"id": 528344, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Hakika wachamngu watakuwa katika Mabustani na chemchem."}, {"id": 528345, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4691, "text": "Wanapokea aliyo wapa Mola wao Mlezi. Kwa hakika hao walikuwa kabla ya haya wakifanya mema."}, {"id": 528346, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4692, "text": "Walikuwa wakilala kidogo tu usiku."}, {"id": 528347, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4693, "text": "Na kabla ya alfajiri wakiomba maghfira."}, {"id": 528348, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Na katika mali yao ipo haki ya mwenye kuomba na asiye omba."}, {"id": 528349, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4695, "text": "Na katika ardhi zipo Ishara kwa wenye yakini."}, {"id": 528350, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4696, "text": "Na pia katika nafsi zenu - Je! Hamwoni?"}, {"id": 528351, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4697, "text": "Na katika mbingu ziko riziki zenu na mliyo ahidiwa."}, {"id": 528352, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4698, "text": "Basi naapa kwa Mola Mlezi wa mbingu na ardhi, hakika haya ni kweli kama ilivyo kweli kuwa nyinyi mnasema."}, {"id": 528353, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Je! Imekufikia hadithi ya wageni wa Ibrahim wanao hishimiwa?"}, {"id": 528354, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Walipo ingia kwake wakasema: Salama! Na yeye akasema: Salama! Nyinyi ni watu nisio kujueni."}, {"id": 528355, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4701, "text": "Akenda kwa ahali yake na akaja na ndama aliye nona."}, {"id": 528356, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4702, "text": "Akawakaribisha, akasema: Mbona hamli?"}, {"id": 528357, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4703, "text": "Akahisi kuwaogopa katika nafsi yake. Wakasema: Usiwe na khofu, nao wakambashiria kupata kijana mwenye ilimu."}, {"id": 528358, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Ndipo mkewe akawaelekea, na huku anasema na akijipiga usoni kwa mastaajabu, na kusema: Mimi ni kikongwe na tasa!"}, {"id": 528359, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Wakasema: Ndivyo vivyo hivyo alivyo sema Mola wako Mlezi. Hakika Yeye ni Mwenye hikima na Mwenye kujua."}, {"id": 528360, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4706, "text": "AKASEMA: Basi ujumbe wenu ni nini, enyi mlio tumwa?"}, {"id": 528361, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Wakasema: Hakika sisi tumetumwa kwa watu wakosefu,"}, {"id": 528362, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4708, "text": "Tuwatupie mawe ya udongo,"}, {"id": 528364, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4709, "text": "Yaliyo tiwa alama kutoka kwa Mola wako Mlezi kwa ajili ya wanao pindukia mipaka."}, {"id": 528365, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Kwa hivyo tutawatoa katika hao wale walio amini."}, {"id": 528366, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4711, "text": "Lakini hatukupata humo ila nyumba moja tu yenye Waislamu!"}, {"id": 528367, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Na tukaacha humo Ishara kwa ajili ya wanao iogopa adhabu chungu."}, {"id": 528368, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4713, "text": "Na katika khabari za Musa, tulipo mtuma kwa Firauni na hoja wazi."}, {"id": 528369, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Lakini alipuuza kwa sababu ya nguvu zake na akasema: Huyu ni mchawi au mwendawazimu!"}, {"id": 528370, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Basi tukamkamata yeye na majeshi yake na tukawatupa baharini, na yeye ndiye wa kulaumiwa."}, {"id": 528371, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4716, "text": "Na katika khabari za A'di tulipo wapelekea upepo wa kukata uzazi."}, {"id": 528372, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4717, "text": "Haukuacha chochote ulicho kifikia ila ulikifanya kama kilio nyambuka."}, {"id": 528373, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4718, "text": "Na katika khabari za Thamudi walipo ambiwa: Jifurahisheni kwa muda mdogo tu."}, {"id": 528374, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Wakaasi amri ya Mola wao Mlezi. Basi uliwanyakua moto wa radi nao wanaona."}, {"id": 528375, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4720, "text": "Basi hawakuweza kusimama wala hawakuwa wenye kujitetea."}, {"id": 528377, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4721, "text": "Na kaumu ya Nuhu hapo mbele. Hakika hao walikuwa watu wapotovu."}, {"id": 528378, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4722, "text": "Na mbingu tumezifanya kwa nguvu na uwezo na hakika Sisi bila ya shaka ndio twenye uwezo wa kuzitanua."}, {"id": 528379, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Na ardhi tumeitandaza; basi watandazaji wazuri namna gani Sisi!"}, {"id": 528380, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Na katika kila kitu tumeumba kwa jozi ili mzingatie."}, {"id": 528381, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4725, "text": "Basi kimbilieni kwa Mwenyezi Mungu, hakika mimi ni mwonyaji kwenu wa kubainisha nitokae kwake."}, {"id": 528382, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Wala msifanye kuwa kuna mungu mwengine pamoja na Mwenyezi Mungu. Hakika mimi ni mwonyaji kwenu wa kubainisha nitokae kwake."}, {"id": 528383, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4727, "text": "Basi ndio hivyo hivyo, hakuwajia kabla yao Mtume ila walisema: Huyu ni mchawi au mwendawazimu."}, {"id": 528384, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Je! Wameambizana kwa haya? Bali hawa ni watu waasi."}, {"id": 528385, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4729, "text": "Basi waachilie mbali, nawe hulaumiwi."}, {"id": 528386, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Na kumbusha, kwani ukumbusho huwafaa Waumini."}, {"id": 528387, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Nami sikuwaumba majini na watu ila waniabudu Mimi."}, {"id": 528388, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4732, "text": "Na sitaki kwao riziki, wala sitaki wanilishe."}, {"id": 528389, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Hakika Mwenyezi Mungu ndiye Mwenye kuruzuku, Mwenye nguvu, Madhubuti."}, {"id": 528390, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Hakika walio dhulumu wana fungu lao la adhabu kama fungu la wenzao. Basi wasinihimize."}, {"id": 528391, "identifier": "sw.barwani", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Ole wao walio ikanusha siku yao waliyo ahidiwa."}]