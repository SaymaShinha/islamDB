[{"id": 527160, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3504, "text": "Alif Lam Mim (A. L. M.)"}, {"id": 527161, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3505, "text": "Huu ni mteremsho wa Kitabu kisicho kuwa na shaka yoyote kinacho toka kwa Mola Mlezi wa walimwengu wote."}, {"id": 527162, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3506, "text": "Au wanasema: Amekizua? Bali hichi ni Kweli iliyo toka kwa Mola wako Mlezi ili uwaonye watu wasio fikiwa na mwonyaji kabla yako; huenda wakaongoka."}, {"id": 527163, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3507, "text": "Mwenyezi Mungu ndiye aliye umba mbingu na ardhi na viliomo baina yao kwa siku sita, na akatawala kwenye Kiti cha Enzi. Nyinyi hamna mlinzi wala mwombezi isipo kuwa Yeye tu. Basi, je, hamkumbuki?"}, {"id": 527164, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3508, "text": "Anapitisha mambo yote yalio baina mbingu na ardhi, kisha yanapanda kwake kwa siku ambayo kipimo chake ni miaka elfu kwa mnavyo hisabu nyinyi."}, {"id": 527165, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3509, "text": "Huyu ndiye Mwenye kuyajua yasiyo onekana na yanayo onekana. Mwenye nguvu, Mwenye kurehemu."}, {"id": 527166, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3510, "text": "Ambaye ametengeneza vizuri umbo la kila kitu; na akaanzisha kumuumba mtu kwa udongo."}, {"id": 527167, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3511, "text": "Na kisha akajaalia uzao wake utokane na kizazi cha maji madhaifu."}, {"id": 527168, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3512, "text": "Kisha akamtengeneza, na akampulizia roho yake, na akakujaalieni kusikia na kuona, na nyoyo za kufahamu. Ni uchache mnavyo shukuru."}, {"id": 527169, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 165, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3513, "text": "Nao husema: Tutapo kwisha potea chini ya ardhi, ni kweli tutarudishwa katika umbo jipya? Bali wao wanakanusha kwamba watakutana na Mola wao Mlezi."}, {"id": 527170, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 415, "ruku": 358, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3514, "text": "Sema: Atakufisheni Malaika wa mauti aliye wakilishwa juu yenu; kisha mtarejeshwa kwa Mola wenu Mlezi."}, {"id": 527171, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3515, "text": "Na ungeli waona wakosefu wanvyo inamisha vichwa vyao mbele ya Mola wao Mlezi, (wakisema): Mola wetu Mlezi! Tumesha ona, na tumesha sikia. Turejeshe tukatende mema, kwani hakika sisi tumekwisha kuwa na yakini sasa."}, {"id": 527172, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3516, "text": "Na lau tungeli taka tunge mpa kila mtu uwongofu wake. Lakini imekwisha kuwa kauli iliyo toka kwangu: Kwa yakini nitaijaza Jahannamu kwa wote hawa, majini na watu."}, {"id": 527173, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3517, "text": "Basi onjeni kwa vile mlivyo usahau mkutano wa Siku yenu hii. Na Sisi hakika tunakusahauni. Basi onjeni adhabu ya milele kwa yale mliyo kuwa mkiyatenda."}, {"id": 527174, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": true, "sajda_id": 10, "sajda_obligatory": true, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3518, "text": "Hakika wanao ziamini Aya zetu ni hao tu ambao wakikumbushwa hizo, basi wao huanguka kusujudu, na humsabihi Mola wao Mlezi kwa kumhimidi, nao hawajivuni."}, {"id": 527175, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3519, "text": "Mbavu zao zinaachana na vitanda kwa kumwomba Mola wao Mlezi kwa khofu na kutumaini, na hutoa kutokana na tulivyo waruzuku."}, {"id": 527176, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3520, "text": "Nafsi yoyote haijui waliyo fichiwa katika hayo yanayo furahisha macho - ni malipo ya yale waliyo kuwa wakiyatenda."}, {"id": 527177, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3521, "text": "Ati aliye Muumini atakuwa sawa na aliye mpotovu? Hawawi sawa."}, {"id": 527178, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3522, "text": "Ama walio amini na wakatenda mema, watakuwa nazo Bustani za makaazi mazuri. Ndio pa kufikia kwa waliyo kuwa wakiyatenda."}, {"id": 527179, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 416, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3523, "text": "Na ama wale walio tenda uovu, basi makaazi yao ni Motoni. Kila wakitaka kutoka humo watarudishwa humo humo. Na wataambiwa: Onjeni adhabu ya Moto ambayo mliyo kuwa mkiikanusha."}, {"id": 527180, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3524, "text": "Na tutawaonjesha adhabu khafifu kabla ya adhabu kubwa, huenda labda watarejea."}, {"id": 527181, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 359, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3525, "text": "Na ni nani dhaalimu mkubwa kuliko yule anaye kumbushwa kwa Ishara za Mola wake Mlezi, kisha akazikataa? Hakika Sisi tutajilipiza kwa wakosefu."}, {"id": 527182, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3526, "text": "Hakika Sisi tulikwisha mpa Musa Kitabu. Basi usiwe na shaka kuwa kakipokea. Na tukakifanya kuwa ni uwongofu kwa Wana wa Israili."}, {"id": 527184, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3527, "text": "Na tukawafanya miongoni mwao waongozi wanao ongoa watu kwa amri yetu, walipo subiri na wakawa na yakini na Ishara zetu."}, {"id": 527185, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3528, "text": "Hakika Mola wako Mlezi ndiye atakaye fafanua baina yao Siku ya Kiyama katika waliyo kuwa wakikhitalifiana."}, {"id": 527186, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3529, "text": "Je! Haijawabainikia kwamba kaumu ngapi tuliziangamiza kabla yao, nao wanapita katika maskani zao hizo? Hakika katika hayo zipo Ishara. Basi, je, hawasikii?"}, {"id": 527187, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3530, "text": "Je! Hawaoni ya kwamba tunayapeleka maji kwenye ardhi kavu, kisha kwayo tunaitoa mimea wanayo kula wanyama wao na wao wenyewe? Je! Hawaoni?"}, {"id": 527188, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3531, "text": "Wanasema: Ushindi huu ni lini, kama mnasema kweli?"}, {"id": 527189, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3532, "text": "Sema: Siku ya Ushindi, wale walio kufuru imani yao haitawafaa kitu, wala wao hawatapewa muhula."}, {"id": 527190, "identifier": "sw.barwani", "surah_number": 32, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0633\u062c\u062f\u0629", "surah_en_name": "As-Sajda", "surah_en_name_translation": "The Prostration", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 417, "ruku": 360, "hizb_quarter": 166, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 3533, "text": "Basi wapuuze, nawe ngonja; hakika wao wanangoja."}]