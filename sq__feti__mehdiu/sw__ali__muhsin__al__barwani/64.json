[{"id": 528840, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5200, "text": "Kinamtakasa Mwenyezi Mungu kila kilichomo katika mbingu na katika ardhi. Ufalme ni wake, na sifa njema ni zake. Na Yeye ni Mwenye uweza juu ya kila kitu."}, {"id": 528841, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5201, "text": "Yeye ndiye aliye kuumbeni. Miongoni mwenu yupo aliye kafiri, na yupo aliye Muumini. Na Mwenyezi Mungu anayaona mnayo yatenda."}, {"id": 528842, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5202, "text": "Ameziumba mbingu na ardhi kwa haki, na akakupeni sura, na akazifanya nzuri sura zenu. Na marudio ni kwake."}, {"id": 528844, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5203, "text": "Anajua viliomo katika mbingu na ardhi, na anajua mnayo yaficha na mnayo yatangaza. Na Mwenyezi Mungu ni Mjuzi wa yaliomo vifuani."}, {"id": 528845, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5204, "text": "Kwani haikukujieni khabari ya walio kufuru kabla, wakaonja matokeo mabaya ya mambo yao? Na wao wana adhabu iliyo chungu."}, {"id": 528846, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5205, "text": "Hayo ni kwa kuwa walikuwa wakiwajia Mitume wao kwa hoja zilizo wazi, nao wakasema: Hivyo binaadamu ndio atuongoe? Basi wakakufuru, na wakageuka upande. Mwenyezi Mungu akawa hana haja nao. Na Mwenyezi Mungu ni Mkwasi, si mhitaji, naye ni Msifiwa."}, {"id": 528847, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5206, "text": "Walio kufuru wanadai kuwa hawatafufuliwa. Sema: Kwani? Naapa kwa Mola wangu Mlezi! Hapana shaka mtafufuliwa, na kisha hapana shaka mtaambiwa mliyo yatenda. Na hayo ni mepesi kwa Mwenyezi Mungu."}, {"id": 528848, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5207, "text": "Basi muaminini Mwenyezi Mungu na Mtume wake, na Nuru tuliyo iteremsha. Na Mwenyezi Mungu anazo khabari za mnayo yatenda."}, {"id": 528849, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5208, "text": "Siku atakayo kukusanyeni kwa ajili ya Siku ya Mkusanyiko; hiyo ni siku ya kupunjana. Na anaye muamini Mwenyezi Mungu na akatenda mema, atamfutia maovu yake, na atamuingiza katika Mabustani yenye kupitiwa kati yake na mito, watadumu humo milele. Huko ndiko kufuzu kukubwa."}, {"id": 528850, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5209, "text": "Na walio kufuru na wakakadhibisha Ishara zetu, hao ni watu wa Motoni, watadumu humo milele. Na hayo ndiyo marudio mabaya."}, {"id": 528851, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5210, "text": "Haufiki msiba wowote ila kwa idhini ya Mwenyezi Mungu. Na mwenye kumuamini Mwenyezi Mungu huuongoa moyo wake. Na Mwenyezi Mungu ni Mjuzi wa kila kitu."}, {"id": 528852, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5211, "text": "Na mt'iini Mwenyezi Mungu, na mt'iini Mtume. Mkigeuka, basi hakika juu ya Mtume wetu ni kufikisha (Ujumbe wake) kwa uwazi tu."}, {"id": 528788, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5212, "text": "Mwenyezi Mungu, hapana mungu isipo kuwa Yeye. Na juu ya Mwenyezi Mungu nawategemee Waumini."}, {"id": 528853, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5213, "text": "Enyi mlio amini! Hakika miongoni mwa wake zenu na watoto wenu wamo maadui zenu. Basi tahadharini nao. Na mkisamehe, na mkapuuza, na mkaghufiria basi hakika Mwenyezi Mungu ni Mwingi wa maghfira, Mwenye kurehemu."}, {"id": 528854, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5214, "text": "Hakika mali yenu na watoto wenu ni jaribio. Na kwa Mwenyezi Mungu upo ujira mkubwa."}, {"id": 528855, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5215, "text": "Basi mcheni Mwenyezi Mungu kama mwezavyo, na sikieni, na t'iini, na toeni, itakuwa kheri kwa nafsi zenu. Na mwenye kuepushwa na uchoyo wa nafsi yake, basi hao ndio walio fanikiwa."}, {"id": 528856, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5216, "text": "Mkimkopesha Mwenyezi Mungu mkopo mzuri, atakuzidishieni mardufu, na atakusameheni. Na Mwenyezi Mungu ni Mwingi wa shukra, Mpole."}, {"id": 528857, "identifier": "sw.barwani", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sw", "name": "Al-Barwani", "transelator_en_name": "Ali Muhsin Al-Barwani", "format": "text", "type": "translation", "ayah_number": 5217, "text": "Mwenye kujua siri na dhaahiri, Mwenye nguvu, Mwenye hikima."}]