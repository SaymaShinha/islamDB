[{"id": 504267, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5552, "text": "Betohem, seriozisht, n\u00eb Dit\u00ebn e Kijametit,"}, {"id": 504268, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5553, "text": "Dhe betohem, seriozisht, n\u00eb shpirtin q\u00eb veten e qorton."}, {"id": 504269, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5554, "text": "A mendon njeriu se nuk do t\u2019i tubojm\u00eb eshtrat e tij?"}, {"id": 504270, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5555, "text": "Po, si jo! Ne kemi mund\u00ebsi t\u00eb rikrijojm\u00eb eedhe majet e thonjve t\u00eb gisht\u00ebrinjve t\u00eb tij."}, {"id": 504271, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5556, "text": "Por njeriu d\u00ebshiron t\u00eb gaboj\u00eb n\u00eb jet\u00eb."}, {"id": 504272, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5557, "text": "Ai pyet: \u201cKur do t\u00eb b\u00ebhet kijameti\u201d?"}, {"id": 504273, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5558, "text": "Por kur t\u2019i shtanget shikimi,"}, {"id": 504274, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5559, "text": "Edhe h\u00ebna t\u00eb err\u00ebsohet,"}, {"id": 504275, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5560, "text": "Dhe Dielli me h\u00ebn\u00eb t\u00eb bashkohet,"}, {"id": 504276, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5561, "text": "At\u00eb dit\u00eb njeriu do t\u00eb thot\u00eb: \u201cKah t\u00eb iket?!\u201d"}, {"id": 504277, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5562, "text": "Askund! Nuk ka strehim."}, {"id": 504278, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5563, "text": "At\u00eb dit\u00eb cakur \u00ebsht\u00eb te Zoti yt,"}, {"id": 504279, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5564, "text": "At\u00eb dit\u00eb njeriu do t\u00eb informohet p\u00ebr at\u00eb q\u00eb e ka shpejtuar ose e ka vonuar,"}, {"id": 504280, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5565, "text": "Por njeriu \u00ebsht\u00eb d\u00ebshmitar\u00eb kund\u00ebr vetes,"}, {"id": 504281, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5566, "text": "Edhe pse p\u00ebrpiqet t\u00eb sjell\u00eb arsyetimet e tij."}, {"id": 504282, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5567, "text": "At\u00eb (Kur\u2019anin) mos e shqipto me gjuh\u00eb p\u00ebr ta shpejtuar,"}, {"id": 504283, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5568, "text": "Ne kemi detyr\u00eb tubimin dhe leximin e tij,"}, {"id": 504284, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5569, "text": "E kur e lexojm\u00eb, ti p\u00ebrcille leximin e tij,"}, {"id": 504285, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5570, "text": "E pastaj, \u00ebsht\u00eb detyr\u00eb e jona ta shkoqisim."}, {"id": 504286, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5571, "text": "Por ani! Ju e dashuroni k\u00ebt\u00eb jet\u00eb t\u00eb kalueshme,"}, {"id": 504287, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5572, "text": "Dhe e leni pas shpine at\u00eb tjetr\u00ebn."}, {"id": 504288, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5573, "text": "At\u00eb dit\u00eb disa fytyra do t\u00eb shkelqejn\u00eb"}, {"id": 504289, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5574, "text": "E disa fytyra at\u00eb dit\u00eb do t\u00eb jen\u00eb t\u00eb ngrysura."}, {"id": 504290, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5575, "text": "Zotin e vet do ta shikojn\u00eb,"}, {"id": 504291, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5576, "text": "Duke menduar se do t\u2019i godit fatkeq\u00ebsi."}, {"id": 504292, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5577, "text": "Po, si jo! Kur (shpirti) dep\u00ebrton n\u00eb eshtrat kyqe,"}, {"id": 504293, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5578, "text": "dhe t\u00eb thuhet, \u201cKush \u00ebsht\u00eb sh\u00ebrues\u201d?"}, {"id": 504294, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5579, "text": "Dhe mendoi se \u00ebsht\u00eb momenti i ndarjes,"}, {"id": 504295, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5580, "text": "Dhe k\u00ebmba me k\u00ebmb\u00eb nd\u00ebrlidhet..."}, {"id": 504296, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5581, "text": "Te Zoti yt do t\u00eb sillen at\u00eb dit\u00eb,"}, {"id": 504297, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5582, "text": "\u201cDhe nuk ka besuar as e ka kryer faljen,"}, {"id": 504299, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5583, "text": "por ka kthyer shpin\u00ebn dhe ka p\u00ebrg\u00ebnjeshtruar."}, {"id": 504300, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5584, "text": "E pastaj shkon tek t\u00eb vet\u00ebt, mendjemadh\u201d."}, {"id": 504301, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5585, "text": "I mjeri ti! I mjeri ti!"}, {"id": 504302, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5586, "text": "Dhe prap\u00eb, i mjeri ti! I mjeri ti!"}, {"id": 504303, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5587, "text": "A mendon njeriu se do t\u00eb lihet n\u00eb vullnet t\u00eb vetin, pa p\u00ebrgjegj\u00ebsi?"}, {"id": 504304, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5588, "text": "A nuk ka qen\u00eb nj\u00eb pik\u00eb fare q\u00eb hudhet,"}, {"id": 504305, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Pastaj ka qen\u00eb gjak i trashur dhe e ka krijuar n\u00eb form\u00ebn e p\u00ebrsosur, t\u00eb fundit,"}, {"id": 504306, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5590, "text": "Dhe prej atij b\u00ebri dy qifte, mashkull dhe fem\u00ebr."}, {"id": 504307, "identifier": "sq.mehdiu", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5591, "text": "Po a nuk ka Ai mund\u00ebsi t\u00eb ngjall\u00eb t\u00eb vdekurin?"}]