, "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5804, "text": "Dhe kur devet mbarsa lihen,"}, {"id": 504511, "identifier": "sq.mehdiu", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5805, "text": "Dhe kur mblidhen eg\u00ebrsirat,"}, {"id": 504512, "identifier": "sq.mehdiu", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5806, "text": "Dhe kur detrat si n\u00eb zjarr vlojn\u00eb,"}, {"id": 504513, "identifier": "sq.mehdiu", "surah_number": 81, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u0643\u0648\u064a\u0631", "surah_en_name": "At-Takwir", "surah_en_name_translation": "The Overthrowing", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 586, "ruku": 523, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_o