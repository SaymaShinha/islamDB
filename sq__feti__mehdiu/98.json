[{"id": 504821, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6131, "text": "Ata t\u00eb cil\u00ebt mohuan, nga pasues t\u00eb librit dhe politeist\u00ebt, nuk largoheshin deri kur u erdh argumenti i qart\u00eb."}, {"id": 504822, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6132, "text": "Nj\u00eb Profet nga All-llahu i cili lexon broshura t\u00eb past\u00ebrta,"}, {"id": 504823, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6133, "text": "Ku ka norma t\u00eb drejta e stabile,"}, {"id": 504824, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6134, "text": "Dhe ata t\u00eb cil\u00ebve u qe dh\u00ebn\u00eb Libri nuk u ndan\u00eb dei at\u00ebher\u00eb kur u erdh argumenti i qart\u00eb,"}, {"id": 504825, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6135, "text": "Porse u qe urdh\u00ebruar t\u00eb adhurojn\u00eb vet\u00ebm All-llahun, q\u00eb si besimtar\u00eb t\u00eb sinqert\u00eb, me sinqeritet t\u2019i rr\u00ebfejn\u00eb fen\u00eb, edhe t\u00eb kryejn\u00eb t\u00eb falurit, edh t\u00eb japin zeqatin, e ajo \u00ebsht\u00eb fe stabile."}, {"id": 504826, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6136, "text": "Ata t\u00eb cil\u00ebt nuk besojn\u00eb nga ithtar\u00ebt e librit edhe politeist\u00ebt, do t\u00eb jen\u00eb, me siguri n\u00eb zjarrin e xhehennemit, ku do t\u00eb mbesin p\u00ebrgjithmon\u00eb. Ata jan\u00eb krijesa m\u00eb t\u00eb k\u00ebqia,"}, {"id": 504828, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6137, "text": "Kurse ata q\u00eb besojn\u00eb dhe b\u00ebjn\u00eb vepra t\u00eb mira ata jan\u00eb, nj\u00ebmend, krijesat m\u00eb t\u00eb mira."}, {"id": 504829, "identifier": "sq.mehdiu", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 6138, "text": "Shp\u00ebrblimi i tyre, te Zoti i tyre, jan\u00eb xhennete t\u00eb Ad\u00ebnit, n\u00ebp\u00ebr t\u00eb cil\u00ebt do t\u00eb rrjedhin lumenj, ku do t\u00eb mbesin t\u00eb banojn\u00eb p\u00ebrgjithmon\u00eb. All-llahu do t\u00eb jet\u00eb i k\u00ebnaqur me ata, por edhe ata do t\u00eb jen\u00eb t\u00eb k\u00ebnaqur me Te. Ajo \u00ebsht\u00eb p\u00ebr ata i cili ka pasur droje nga Zoti i vet."}]