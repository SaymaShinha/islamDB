[{"id": 504613, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5910, "text": "Pasha qiellin me pyrgje p\u00ebr yje,"}, {"id": 504614, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5911, "text": "Pasha Dit\u00ebn e premtuar,"}, {"id": 504615, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5912, "text": "Pasha d\u00ebshmitarin dhe \u00e7ka \u00ebsht\u00eb d\u00ebshmuar,"}, {"id": 504616, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5913, "text": "Qofshin mallkuar hap\u00ebsit e hendeqeve,"}, {"id": 504617, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Nxit\u00ebsit e zjarrit me plot l\u00ebnd\u00eb djeg\u00ebse,"}, {"id": 504618, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5915, "text": "Dhe q\u00eb rrijn\u00eb ulur rreth tij,"}, {"id": 504619, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5916, "text": "Dhe q\u00eb jan\u00eb d\u00ebshmitar\u00eb se \u00e7\u2019kan\u00eb b\u00ebr\u00eb me besimtar\u00ebt!"}, {"id": 504620, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5917, "text": "Dhe nuk i urrejn\u00eb p\u00ebr tjet\u00ebr p\u00ebrve\u00e7 p\u00ebrse i besojn\u00eb All-llahu, t\u00eb fortit, t\u00eb merituarit p\u00ebr respekt,"}, {"id": 504621, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Pushtet i t\u00eb cilit sht\u00eb n\u00eb qiej dhe n\u00eb tok\u00eb dhe All-llahu \u00ebsht\u00eb d\u00ebshmitar p\u00ebr \u00e7do send."}, {"id": 504622, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Ata t\u00eb cil\u00ebt maltretojn\u00eb besimtar\u00ebt dhe besimtaret dhe nuk pendohen, i pret d\u00ebnim i xhehennemit dhe d\u00ebnim me djegije,"}, {"id": 504623, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5920, "text": "Kurse ata q\u00eb besojn\u00eb dhe b\u00ebjn\u00eb vepra t\u00eb mira, i presin kopshte t\u00eb xhennetit ku do t\u00eb rrjedhin lumenj \u2013 ai \u00ebsht\u00eb sukses i madh."}, {"id": 504624, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Nj\u00ebmend hakmarrja e Zotit t\u00ebnd \u00ebsht\u00eb shum\u00eb e fort\u00eb."}, {"id": 504625, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Pa dyshim vet\u00ebm Ai \u00ebsht\u00eb q\u00eb e fillon dhe e rifillon,"}, {"id": 504626, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Dhe fal shum\u00eb dhe ka plot dashuri."}, {"id": 504627, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5924, "text": "Zot i gjith\u00ebsis\u00eb i Lart\u00ebmadh\u00ebrish\u00ebm,"}, {"id": 504628, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Q\u00eb punon \u00e7ka d\u00ebshiron vet."}, {"id": 504629, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5926, "text": "A t\u00eb ka ardhur ty lajmi p\u00ebr ushtrit\u00eb"}, {"id": 504630, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5927, "text": "E Faraonit dhe Themudit?"}, {"id": 504631, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Por ata t\u00eb cil\u00ebt mohojn\u00eb \u2013 p\u00ebrg\u00ebnjeshtrojn\u00eb."}, {"id": 504632, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5929, "text": "Por All-llahu do t\u2019i p\u00ebrthekoj\u00eb nga t\u00eb gjitha an\u00ebt,"}, {"id": 504633, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5930, "text": "Po, ai \u00ebsht\u00eb Kur\u2019an madh\u00ebshtor,"}, {"id": 498914, "identifier": "sq.mehdiu", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Feti Mehdiu", "transelator_en_name": "Feti Mehdiu", "format": "text", "type": "translation", "ayah_number": 5931, "text": "N\u00eb nj\u00eb pllakat\u00eb t\u00eb ruajtur mir\u00eb."}]