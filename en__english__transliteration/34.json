[{"id": 196754, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3607, "text": "Alhamdu lillaahil lazee lahoo maa fis samaawaati wa maa fil ardi wa lahul hamdu fil aakhirah; wa Huwal Hakeemul Khabeer"}, {"id": 196755, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3608, "text": "Ya'lamu maa yaliju fil ardi wa maa yakhruju minhaa wa maa yanzilu minas samaaa'i wa maa ya'ruju feehaa; wa Huwar Raheemul Ghafoor"}, {"id": 196756, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3609, "text": "Wa qaalal lazeena kafaroo laa taateenas Saa'ah; qul balaa wa Rabbee lataatiyannakum 'Aalimul Ghaib; laa ya'zubu 'anhu misqaalu zarratin fis samaawaati wa laa fil ardi wa laaa asgharu min zaalika wa laaa akbaru illaa fee kitaabim mubeen"}, {"id": 196757, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3610, "text": "Liyajziyal lazeena aamanoo wa 'amilus saalihaat; ulaaa'ika lahum maghfiratunw wa rizqun kareem"}, {"id": 196758, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3611, "text": "Wallazeena sa'aw feee aayaatinaa mu'aajizeena ulaaa 'ika lahum 'azaabum mir irjzin aleem"}, {"id": 196759, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3612, "text": "Wa yaral lazeena utul 'Ilmal lazeee unzila ilaika mir Rabbika huwal haqqa wa yahdeee ilaaa siraatil 'Azeezil Hameed"}, {"id": 196760, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 428, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3613, "text": "Wa qaalal lazeena kafaroo hal nadullukum 'alaa rajuliny yanabbi 'ukum izaa muzziqtum kulla mumazzaqin innakum lafee khalqin jadeed"}, {"id": 196761, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3614, "text": "Aftaraa 'alal laahi kaziban am bihee jinnah; balil lazeena laa yu'minoona bil Aakhirati fil'azaabi waddad laalil ba'eed"}, {"id": 196762, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 370, "hizb_quarter": 171, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3615, "text": "Afalam yaraw ilaa maa baina aydeehim wa maa khalfahum minas samaaa'i wal ard; in nashad nakhsif bihimul arda aw nusqit 'alaihim kisafam minas samaaa'; inna fee zaalika la Aayatal likulli 'abdim muneeb"}, {"id": 196763, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3616, "text": "Wa laqad aatainaa Daawooda minnaa fadlany yaa jibaalu awwibee ma'ahoo wattaira wa alannaa lahul hadeed"}, {"id": 196764, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3617, "text": "Ani'mal saabighaatinw wa qaddir fis sardi wa'maloo saalihan innee bimaa ta'maloona Baseer"}, {"id": 196765, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3618, "text": "Wa li-Sulaimaanar reeha ghuduwwuhaa shahrunw wa ra-waahuhaa shahrunw wa asalnaa lahoo 'ainal qitr; wa minal jinni mai ya'malu baina yadaihi bi izni Rabbih; wa mai yazigh minhum 'an amrinaa nuziqhu min 'azaabis sa'eer"}, {"id": 196766, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3619, "text": "Ya'maloona lahoo ma yashaaa'u mim mahaareeba wa tamaaseela wa jifaanin kaljawaabi wa qudoorir raasiyaat; i'maloo aala Daawooda shukraa; wa qaleelum min 'ibaadiyash shakoor"}, {"id": 196767, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 429, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3620, "text": "Falammaa qadainaa 'alaihil mawta ma dallahum 'alaa mawtiheee illaa daaabbatul ardi taakulu minsa atahoo falammaa kharra tabaiyanatil jinnu al law kaanoo ya'lamoonal ghaiba maa labisoo fil 'azaabil muheen"}, {"id": 196768, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3621, "text": "Laqad kaana li Saba-in fee maskanihim Aayatun jannataani 'ai yameeninw wa shimaalin kuloo mir rizq Rabbikum washkuroolah; baldatun taiyibatunw wa Rabbun Ghafoor"}, {"id": 196769, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3622, "text": "Fa-a''radoo fa-arsalnaa 'alaihim Sailal 'Arimi wa baddalnaahum bijannataihim jannataini azwaatai ukulin khamtinw wa aslinw wa shai'im min sidrin qaleel"}, {"id": 196770, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3623, "text": "Zaalika jazainaahum bimaa kafaroo wa hal nujaazeee illal kafoor"}, {"id": 196771, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3624, "text": "Wa ja'alnaa bainahum wa bainal qural latee baaraknaa feehaa quran zaahiratanw wa qaddamaa feehas sayr; seeroo feehaa la yaalirya wa aiyaaman aamineen"}, {"id": 196772, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3625, "text": "Faqaaloo Rabbanaa baa'id baina asfaarinaa wa zalamooo anfusahum faja'alnaahum ahaadeesa wa mazzaq naahum kulla mumazzaq; inna fee zaalika la Aayaatil likulli sabbaarin shakoor"}, {"id": 196774, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3626, "text": "Wa laqad saddaq 'alaihim Ibleesu zannnabhoo fattaba'oohu illaa fareeqam minal mu'mineen"}, {"id": 196775, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 371, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3627, "text": "Wa maa kaana lahoo 'alaihim min sultaanin illaa lina'lama mai yu minu bil Aakhirati mimman huwa minhaa fee shakk; wa Rabbuka 'alaa kulli shai'in Hafeez"}, {"id": 196776, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 430, "ruku": 372, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3628, "text": "Qulid 'ul lazeena za'amtum min doonil laahi laa yamlikoona misqaala zarratin fissamaawaati wa laa fil ardi wa maa lahum feehimaa min shirkinw wa maa lahoo minhum min zaheer"}, {"id": 196777, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 172, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3629, "text": "Wa laa tanfa'ush shafaa'atu 'indahooo illaa liman azina lah; hattaaa izaa fuzzi'a 'an quloobihim qaaloo maazaa qaala Rabbukum; qaalul haqq, wa Huwal 'Aliyul Kabeer"}, {"id": 196778, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3630, "text": "Qul mai yarzuqukum minas samaawaati wal ardi qulil laahu wa innaaa aw iyyaakum la'alaa hudan aw fee dalaalim mubeen"}, {"id": 196779, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3631, "text": "Qul laa tus'aloona 'ammaaa ajramnaa wa laa nus'alu 'ammaa ta'maloon"}, {"id": 196780, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3632, "text": "Qul yajma'u bainanaa Rabbunaa summa yaftahu bainanaa bilhaqq; wa Huwal Fattaahul 'Aleem"}, {"id": 196781, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3633, "text": "Qul arooniyal lazeena alhaqtum bihee shurakaaa'a kallaa; bal Huwal Laahul 'Azeezul Hakeem"}, {"id": 196782, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3634, "text": "Wa maaa arsalnaaka illaa kaaffatal linnaasi basheeranw wa nazeeranw wa laakinna aksaran naasi laa ya'lamoon"}, {"id": 196783, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3635, "text": "Wa yaqooloona mataa haazal wa'du in kuntum saadiqeen"}, {"id": 196784, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 372, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3636, "text": "Qul lakum mee'aadu Yawmil laa tastaakhiroona 'anhu saa'atanw wa la tastaqdimoon"}, {"id": 196785, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 431, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3637, "text": "Wa qaalal lazeena kafaroo lan nu'mina bihaazal Quraani wa laa billazee baina yadayh; wa law taraaa iziz zaalimoona mawqoofoona 'inda Rabbihim yarji'u ba'duhum ilaa ba'dinil qawla yaqoolul lazeenas tud'ifoo lillazeenas takbaroo law laaa antum lakunnaa mu'mineen"}, {"id": 196786, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3638, "text": "Qaalal lazeenas takbaroo lillazeenas tud'ifooo anahnu sadadnaakum 'anil hudaa ba'da iz jaaa'akum bal kuntum mujrimeen"}, {"id": 196787, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3639, "text": "Wa qaalal lazeenastud'ifoo lillazeenas takbaroo bal makrul laili wannahaari iz taamuroonanaaa an nakfura billaahi wa naj'ala lahooo andaadaa; wa asarrun nadaamata lammaa ra awul 'azaab; wa ja'alnal aghlaala feee a'naaqil lazeena kafaroo; hal yujzawna illaa maa kanoo ya'maloon"}, {"id": 196788, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3640, "text": "Wa maaa arsalnaa' fee qaryatim min nazeerin illaa qaala mutrafooaa innaa bimaaa ursiltum bihee kaafiroon"}, {"id": 196789, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3641, "text": "Wa qaaloo nahnu aksaru amwaalanw wa awlaadanw wa maa nahnu bimu 'azzabeen"}, {"id": 196790, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 373, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3642, "text": "Qul inna Rabbee yabsutur rizqa limai yashaaa'u wa yaqdiru wa laakinna aksaran naasi laa ya'lamoon"}, {"id": 196791, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3643, "text": "Wa maaa amwaalukum wa laaa awlaadukum billatee tuqarribukum 'indanaa zulfaaa illaa man aamana wa 'amila saalihan fa ulaaa'ika lahum jazaaa'ud di'fi bimaa 'amiloo wa hum fil ghurufaati aaminoon"}, {"id": 196792, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3644, "text": "Wallazeena yas'awna feee Aayaatinaa mu'aajizeena ulaaa'ika fil'azaabi muhdaroon"}, {"id": 196793, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 432, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3645, "text": "Qul inna Rabbee yabsutur rizqa limai yashaaa'u min 'ibaadihee wa yaqdiru lah; wa maaa anfaqtum min shai'in fahuwa yukhlifuhoo wa Huwa khairur raaziqeen"}, {"id": 196794, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3646, "text": "Wa yawma yahshuruhum jamee'an summa yaqoolu lilmalaaa'ikati a-haaa'ulaaa'i iyyaakum kaanoo ya'budoon"}, {"id": 196795, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3647, "text": "Qaaloo Subhaanaka Anta waliyyunaa min doonihim bal kaanoo ya'budoonal jinna aksaruhum bihim mu'minoon"}, {"id": 196796, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3648, "text": "Fal Yawma laa yamliku ba'dukum liba'din naf'anw wa laa darraa; wa naqoolu lilzeena zalamoo zooqoo 'azaaban Naaril latee kuntum bihaa tukazziboon"}, {"id": 196797, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3649, "text": "Wa izaa tutlaa 'alaihim Aayaatunaa baiyinaatin qaaloo maa haazaa illaa rajuluny yureedu ai-yasuddakum 'ammaa kaana ya'budu aabaaa'ukum wa qaaloo maa haazaaa illaaa ifkum muftaraa; wa qaalal lazeena kafaroo lilhaqqi lammaa jaaa'ahum in haazaaa illaa sihrum mubeen"}, {"id": 196798, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3650, "text": "Wa maaa aatainaahum min Kutubiny yadrusoonahaa wa maaa arsalnaaa ilaihim qablaka min nazeer"}, {"id": 196799, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 374, "hizb_quarter": 173, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3651, "text": "Wa kazzabal lazeena min qablihim wa maa balaghoo mi'shaara maaa aatainaahum fakazzaboo Rusulee; fakaifa kaana nakeer"}, {"id": 196800, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3652, "text": "Qul innamaaa a'izukum biwaahidatin an taqoomoo lillaahi masnaa wa furaadaa summa tatafakkaroo; maa bisaahibikum min jinnah; in huwa illaa nazeerul lakum baina yadai 'azaabin shadeed"}, {"id": 196801, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3653, "text": "Qul maa sa-altukum min ajrin fahuwa lakum in ajriya illaa 'alal laahi wa Huwa 'alaa kullin shai-in Shaheed"}, {"id": 196802, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 433, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3654, "text": "Qul inna Rabbee yaqzifu bilhaqq 'Allaamul Ghuyoob"}, {"id": 196803, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3655, "text": "Qul jaaa'al haqqu wa maa yubdi'ul baatilu wa maa yu'eed"}, {"id": 196804, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3656, "text": "Qul in dalaltu fainnamaaa adillu 'alaa nafsee wa inih-tadaitu fabimaa yoohee ilaiya Rabbee; innahoo Samee'un Qareeb"}, {"id": 196805, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3657, "text": "Wa law taraaa iz fazi'oo falaa fawta wa ukhizoo mim makaanin qareeb"}, {"id": 196806, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3658, "text": "Wa qaloo aamannaa bihee wa annaa lahumut tanaawushu mim makaanim ba'eed"}, {"id": 196807, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3659, "text": "Wa qad kafaroo bihee min qablu wa yaqzifoona bilghaibi mim makaanim ba'eed"}, {"id": 196808, "identifier": "en.transliteration", "surah_number": 34, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0633\u064e\u0628\u064e\u0623", "surah_en_name": "Saba", "surah_en_name_translation": "Sheba", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 375, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3660, "text": "Wa heela bainahum wa baina maa yashtahoona kamaa fu'ila bi-ashyaa'ihim min qabl; innahum kaanoo fee shakkim mureeb"}]