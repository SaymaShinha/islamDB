[{"id": 196486, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3341, "text": "Alif-Laaam-Meeem"}, {"id": 196487, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3342, "text": "Ahasiban naasu anyu yutrakooo any yaqoolooo aamannaa wa hum la yuftanoon"}, {"id": 196488, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3343, "text": "Wa laqad fatannal lazeena min qablihim fala ya'lamannal laahul lazeena sadaqoo wa la ya'lamannal kaazibeen"}, {"id": 196489, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3344, "text": "Am hasibal lazeena ya'maloonas sayyiaati any yasbiqoonaa; saaa'a maa yahkumoon"}, {"id": 196490, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3345, "text": "Man kaana yarjoo liqaaa 'allaahi fa inna ajalal laahi laaat; wa Huwass Sameeul 'Aleem"}, {"id": 196491, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 396, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3346, "text": "Wa man jaahada fainnamaa yujaahidu linafsih; innal laaha laghaniyyun 'anil 'aalameen"}, {"id": 196492, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3347, "text": "Wallazeena aamanoo wa 'amilus saalihaati lanukaf firanna 'anhum saiyiaatihim wa lanajziyannahum ahsanal lazee kaanoo ya'maloon"}, {"id": 196493, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3348, "text": "Wa wassainal insaana biwaalidaihi husnanw wa in jaahadaaka litushrika bee maa laisa laka bihee 'ilmun falaa tuti'humaa; ilaiya marji'ukum fa unabbi'ukum bimaa kuntum ta'maloon"}, {"id": 196494, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3349, "text": "Wallazeena aamanoo w a'amilus saalihaati lanudkhilan nahum fis saaliheen"}, {"id": 196495, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3350, "text": "Wa minan naasi many yaqoolu aamannaa billaahi faizaaa ooziya fil laahi ja'ala fitnatan naasi ka'azaabil laahi wa la'in jaaa'a nasrum mir Rabbika la yaqoolunna innaa kunnaa ma'akum; awa laisal laahu bi a'lama bimaa fee surdooril 'aalameen"}, {"id": 196496, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3351, "text": "Wa la ya'lamannal laahul lazeena aamanoo wa la ya'lamannal munaafiqeen"}, {"id": 196497, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3352, "text": "Wa qaalal lazaeena kafaroo lillazeena aamanut tabi'oo sabeelanaa walnahmil khataayaakum wa maa hum bihaamileena min khataa yaahum min shai'in innahum lakaaziboon"}, {"id": 196498, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 342, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3353, "text": "Wa la yahmilunna asqaa lahum wa asqaalam ma'a asqaalihim wa la yus'alunna Yawmal Qiyaamati 'ammaa kaanoo yaftaroon"}, {"id": 196499, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 397, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3354, "text": "Wa laqad arsalnaa Noohan ilaa qawmihee falabisa feehim alfa sanatin illaa khamseena 'aaman fa akhazahumut toofaanu wa hum zaalimoon"}, {"id": 196500, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3355, "text": "Fa anjainaahu wa as haabas safeenati wa ja'alnaahaaa Aayatal lil'aalameen"}, {"id": 196501, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3356, "text": "Wa Ibraheema iz qaala liqawmihi' budul laaha wattaqoohu zaalikum khayrul lakum in kuntum ta'lamoon"}, {"id": 196502, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3357, "text": "Innamaa ta'ubdoona min doonil laahi awsaananw-wa takhluqoona ifkaa; innal lazeena ta'budoona min doonil laahi laa yamlikoona lakum rizqan fabtaghoo 'indal laahir rizqa fabtaghoo 'indal laahir rizqa wa'budoohu washkuroo lahooo ilaihi turja'oon"}, {"id": 196503, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3358, "text": "Wa in tukazziboo faqad kazzaba umamum min qablikum wa maa'alar Rasooli illal balaaghul mubeen"}, {"id": 196504, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3359, "text": "Awa lam yaraw kaifa yubdi'ul laahul khalqa summa yu'eeduh; inna zaalika 'alal laahi yaseer"}, {"id": 196505, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3360, "text": "Qul seeroo fil ardi fanzuroo kaifa baa al khalqa summal laahu yunshi''un nash atal Aakhirah; innal laaha 'alaa kulli shai'in Qadeer"}, {"id": 196506, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3361, "text": "Yu'azzibu many yashaaa'u wa yarhamu many yashaaa'; wa ilaihi tuqlaboon"}, {"id": 196507, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 343, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3362, "text": "Wa maaa antum bimu'jizeena fil ardi wa laa fissamaaa'i wa maa lakum min doonil laahi minw waliyyinw wa laa naseer"}, {"id": 196508, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 398, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3363, "text": "Wallazeena kafaroo bi Aayaatil laahi wa liqaaa'iheee ulaaa'ika ya'isoo mir rahmatee wa ulaaa'ika lahum 'azaabun aleem"}, {"id": 196510, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3364, "text": "Famaa kaana jawaaba qawmiheee illaaa an qaaluqtuloohu aw harriqoohu faanjaahul laahu minan naar; inna fee zaalika la Aayaatil laqawminy yu'minoon"}, {"id": 196511, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 159, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3365, "text": "Wa qaala innamat takhaz tum min doonil laahi awsaanam mawaddata bainikum fil hayaatid dunyaa summa yawmal qiyaamati yakfuru ba'dukum biba 'dinw wa yal'anu ba'dukum ba'danw-wa yal'anu ba'dukum ba'danw wa maa waakumun Naaru wa maa lakum min naasireen"}, {"id": 196512, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3366, "text": "Fa aamana lahoo Loot; wa qaala innee mauhajirun ilaa Rabbee innahoo Huwal 'Azeezul Hakeem"}, {"id": 196513, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3367, "text": "Wa wahabnaa lahoo Ishaaqa wa Ya'Qooba wa ja'alnaa fee zurriyyatihin Nubuwwata wal Kitaaba wa aatainaahu ajrahoo fid dunyaa wa innahoo fil aakhirati laminas saaliheen"}, {"id": 196514, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3368, "text": "Wa Lootan iz qaala liqawmiheee innakum laatoonal faahishata maa sabaqakum bihaa min ahadim minal 'aalameen"}, {"id": 196515, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3369, "text": "A'innakum lataatoonar rijaala wa taqta'oonas sabeela wa taatoona fee naadekumul munkara famaa kaana jawaaba qawmiheee illaaa an qaalu' tinaaa bi'azaabil laahi in kunta minas saadiqeen"}, {"id": 196516, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 399, "ruku": 344, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3370, "text": "Qaala Rabbin surnee 'alal qawmil mufsideen"}, {"id": 196517, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3371, "text": "Wa lammaa jaaa'at Rusulunaaa Ibraaheema bil bushraa qaalooo innaa muhlikoo ahli haazihil qaryati inna ahlahaa kaanoo zaalimeen"}, {"id": 196518, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3372, "text": "Qaala inna feeha Lootaa; qaaloo nahnu a'lamu biman feehaa lanunajjjiyannahoo wa ahlahooo illam ra atahoo kaanat minal ghaabireen"}, {"id": 196519, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3373, "text": "Wa lammaaa an jaaa'at Rusulunaa Lootan seee'a bihim wa daaqa bihim zar'anw wa qaaloo laa takhaf wa laa tahzan innaa munajjooka wa ahlaka illam ra ataka kaanat minal ghaabireen"}, {"id": 196520, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3374, "text": "Innaa munziloona 'alaaa ahli haazihil qaryati rijzam minas samaaa'i bimaa kaanoo yafsuqoon"}, {"id": 196521, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3375, "text": "Wa laqat taraknaa min haaa aayatam baiyinatal liqawminy ya'qiloon"}, {"id": 196522, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3376, "text": "Wa ilaa Madyana akhaahum Shu'ayban faqaala yaa qawmi'-budul laaha warjul yawmal aakhira wa laa ta'saw fil ardi mufsideen"}, {"id": 196523, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3377, "text": "Fakazzaboohu fa akhazat humur rajfatu fa asbahoo fee daarihim jaasimeen"}, {"id": 196524, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 400, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3378, "text": "Wa 'Aadanw wa Samooda wa qat tabaiyana lakum mim masaakinihim wa zaiyana lahumush Shaitaanu a'maalahum fasaddahum 'anis sabeeli wa kaanoo mustabsireen"}, {"id": 196525, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3379, "text": "Wa Qaaroona wa Fir'awna wa haamaana wa laqad jaaa'ahum Moosa bilbaiyinaati fastakbaroo fil ardi wa maa kaanoo saabiqeen"}, {"id": 196526, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3380, "text": "Fakullan akhaznaa bizam bihee faminhum man arsalnaa 'alaihi haasibaa; wa minhum man akhazat hus saihatu wa minhum man khasafnaa bihil arda wa minhum man aghraqnaa; wa maa kaanal laahu li yazlimahum wa laakin kaanoo anfusahum yazlimoon"}, {"id": 196527, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3381, "text": "Masalul lazeenat takhazoo min doonil laahi awliyaaa'a kamasalil 'ankaboot, ittakhazat baitaa; wa inna awhanal buyooti la baitul 'ankaboot; law kaanoo ya'lamoon"}, {"id": 196528, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3382, "text": "Innal laaha ya'lamu maa yad'oona min doonihee min shai'; wa Huwal 'Azeezul Hakeem"}, {"id": 196529, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3383, "text": "Wa tilkal amsaalu nadribuhaa linnaasi wa maa ya'qiluhaaa illal 'aalimoon"}, {"id": 196530, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 345, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3384, "text": "Khalaqal laahus samaawaati wal arda bilhaqq; inna fee zaalika la aayatal lilmu mineen"}, {"id": 196531, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 20, "manzil": 5, "page": 401, "ruku": 346, "hizb_quarter": 160, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3385, "text": "Utlu maaa oohiya ilaika mional Kitaabi wa aqimis Salaata innas Salaata tanhaa 'anil fahshaaa'i wal munkar; wa lazikrul laahi akbar; wal laahu ya'lamu maa tasna'oon"}, {"id": 196532, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3386, "text": "Wa laa tujaadilooo Ahlal Kitaabi illaa billatee hiya ahsanu illal lazeena zalamoo minhum wa qoolooo aamannaa billazeee unzila ilainaa wa unzila ilaikum wa illaahunna wa illahukum waahidunw-wa nahnu lahoo muslimoon"}, {"id": 196533, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3387, "text": "Wa kazaalika anzalnaaa ilaikal Kitaab; fallazeena aatainaahumul kitaaba yu'minoona bihee wa min haaa'ulaaa'i many yu'minu bih; wa maa yajhadu bi'Aayaatinaa illal kaafiroon"}, {"id": 196534, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3388, "text": "Wa maa kunta tatloo min qablihee min kitaabinw wa laa takhuttubhoo bi yameenika izal lartaabal mubtiloon"}, {"id": 196535, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3389, "text": "Bal huwa aayaatum baiyinaatun fee sudooril lazeena ootul 'ilm; wa maa yajhadu bi aayaatinaa illaz zaalimoon"}, {"id": 196536, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3390, "text": "Wa qaaloo law laaa unzila 'alaihi aayaatum mir Rabbihee qul innamal aayaatu 'indal laahi wa innamaaa ana nazeerum mubeen"}, {"id": 196537, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 346, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3391, "text": "Awa lam yakfihim annaaa anzalnaa 'alaikal kitaaba yutlaa 'alaikhim; inna fee zaalika larahmatanw wa zikraa liqawminy yu'minoon"}, {"id": 196538, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 402, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3392, "text": "Qul kafaa billaahi bainee wa bainakum shaheedaa; ya'lamu maa fis samaawaati wal ard; wallazeena aamanoo bil baatili wa kafaroo billaahi ulaaa'ika humul khaasiroon"}, {"id": 196539, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3393, "text": "Wa yasta'jiloonaka bil'azaab; wa law laaa ajalum musammal lajaaa'ahumul 'zaab; wa la yaatiyannahum baghta tanw wa hum laa yash'uroon"}, {"id": 196540, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3394, "text": "Yasta'jiloonak bil'azaab; wa inna Jahannama lamuhee tatum bilkaafireen"}, {"id": 196541, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3395, "text": "Yawma yaghshaahumul 'azaabu min fawqihim wa min tahti arjulim wa yaqoolu zooqoo maa kuntum ta'maloon"}, {"id": 196542, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3396, "text": "Yaa 'ibaadiyal lazeena aamanooo inna ardee waasi 'atun fa iyyaaya fa'budoon"}, {"id": 196543, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3397, "text": "Kullu nafsin zaaa'iqatul mawti summa ilainaa turja'oon"}, {"id": 196544, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3398, "text": "Wallazeena aamanoo wa 'amilus saalihaati la nubawwi 'annahum minal Jannati ghurafan tajree min tahtihal anhaaru khaalideena feehaa; ni'ma ajrul 'aamileen"}, {"id": 196545, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3399, "text": "Allazeena sabaroo wa 'alaa Rabbihim yatawakkaloon"}, {"id": 196546, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3400, "text": "Wa ka ayyim min daaabbatil laa tahmilu riqqahaa; al laahu yarzuquhaa wa iyyaakum; wa Huwas Samee'ul Aleem"}, {"id": 196547, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3401, "text": "Wa la'in sa altahum man khalaqas samaawaati wal arda wa sakhkharash shamsa wal qamara la yaqoolunnal laahu fa ann yu'fakoon"}, {"id": 196548, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3402, "text": "Allaahu yabsutur rizqa limany yashaaa'u min 'ibaadihee wa yaqdiru lah; innal laaha bikulli shai'in Aleem"}, {"id": 196549, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 403, "ruku": 347, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3403, "text": "Wa la'in sa altahum man nazzala minas samaaa'e maaa'an fa ahyaa bihil arda mim ba'di mawtihaa la yaqoolunnal laah; qulil hamdu lillah; bal aksaruhum laa ya'qiloon"}, {"id": 196550, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3404, "text": "Wa maa haazihil hayaa tud dunyaaa illaa lahwunw-wa la'ib; wa innad Daaral Aakhirata la hiyal ha yawaan; law kaano ya'lamoon"}, {"id": 196551, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3405, "text": "Fa-izaa rakiboo fil fulki da'awul laaha mukhilseena lahud deena falammaa najjaa hum ilal baari izaa hum yushrikoon"}, {"id": 196552, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3406, "text": "Li yakfuroo bimaaa aatainaahum wa li yatamatta'oo fasaw fa ya'lamoon"}, {"id": 196553, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3407, "text": "Awalam yaraw annaa ja'alnaa haraman aaminanw wa yutakhattafun naasu min haw lihim; afabil baatili yu'minoona wa bini'matil laahi yakfuroon"}, {"id": 196554, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3408, "text": "Wa man azlamu mimma nif taraa 'alal laahi kaziban aw kazzaba bilhaqqi lammaa jaaa'ah; alaisa fee jahannama maswal lil kaafireen"}, {"id": 196555, "identifier": "en.transliteration", "surah_number": 29, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0646\u0643\u0628\u0648\u062a", "surah_en_name": "Al-Ankaboot", "surah_en_name_translation": "The Spider", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 348, "hizb_quarter": 161, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3409, "text": "Wallazeena jaahadoo feenaa lanahdiyannahum subulana; wa innal laaha lama'al muhsineen"}]