[{"id": 198248, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5105, "text": "Qad sami'al laahu qawlal latee tujaadiluka fee zawjihaa wa tashtakeee ilal laahi wallaahu yasma'u tahaawurakumaa; innal laaha samee'um baseer"}, {"id": 198249, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5106, "text": "Allazeena yuzaahiroona minkum min nisaaa'ihim maa hunnaa ummahaatihim in ummahaatuhum illal laaa'ee waladnahum; wa innaahum la yaqooloona munkaram minal qawli wa zooraa; wa innal laaha la'afuwwun ghafoor"}, {"id": 198250, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5107, "text": "Wallazeena yuzaahiroona min nisaaa'ihim summa ya'oodoona limaa qaaloo fatabreeru raqabatim min qabli any-yatamaaassaa; zaalikum too'azoona bih; wallaahu bimaa ta'maloona khabeer"}, {"id": 198251, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5108, "text": "Famal lam yajid fa siyaamu shahraini mutataabi'ayni min qabli any-yatamaaassaa famal lam yastati' fa-it'aamu sitteena miskeena; zaalika litu'minoo billaahi wa rasoolih'wa tilka hudoodul laah; wa lilkaafireena 'azaabun aleem"}, {"id": 198252, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5109, "text": "Innal lazeena yuhaaaddoonal laaha wa Rasoolahoo kubitoo kamaa kubital lazeena min qablihim; wa qad anzalnaaa aayaatim baiyinaat; wa lilkaa fireena 'azaabum muheen"}, {"id": 198253, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 542, "ruku": 476, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5110, "text": "Yawma yab'asuhumul laahu jamee'an fayunabbi'uhum bimaa 'amiloo; ahsaahul laahu wa nasooh; wallaahu 'alaa kulli shai'in shaheed"}, {"id": 198254, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5111, "text": "Alam tara annal laaha ya'lamu maa fis samaawaati wa maa fil ardi maa yakoonu min najwaa salaasatin illaa Huwa raabi'uhum wa laa khamsatin illaa huwa saadisuhum wa laaa adnaa min zaalika wa laaa aksara illaa huwa ma'ahum ayna, maa kaanoo summa yunabbi'uhum bimaa 'amiloo yawmal qiyaamah; innal laaha bikulli shai'in aleem"}, {"id": 198255, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5112, "text": "Alam tara ilal lazeena nuhoo 'anin najwaa summa ya'oodoona limaa nuhoo 'anhu wa yatanaajawna bil ismi wal'udwaani wa ma'siyatir rasooli wa izaa jaaa'ooka haiyawka bimaa lam yuhai yika bihil laahu wa yaqooloona fee anfusihim law laa yu'azzibunal laahu bimaa naqool; hasbuhum jahannnamu yaslawnahaa fabi'sal maseer"}, {"id": 198256, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5113, "text": "Yaaa ayyuhal lazeena aamanoo izaa tanaajaytum falaa tatanaajaw bil ismi wal 'udwaani wa ma'siyatir rasooli wa tanaajaw bil birri wattaqwaa wattaqul laahal lazeee ilaihi tuhsharoon"}, {"id": 198257, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5114, "text": "Innaman najwaa minash shaitaani liyahzunal lazeena aamanoo wa laisa bidaaarrihim shai'an illaa bi-iznil laah; wa 'alal laahi falyatawakkalil mu'minoon"}, {"id": 198258, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 543, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5115, "text": "Yaaa ayyuhal lazeena aamanoo izaa qeela lakum tafassahoo fil majaalisi fafsahoo yafsahil laahu lakum wa izaa qeelan shuzoo fanshuzoo yarfa'il laahul lazeena aamanoo minkum wallazeena ootul 'ilma darajaat; wallaahu bimaa ta'maloona khabeer"}, {"id": 198259, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5116, "text": "Yaaa ayyuhal lazeena aamanooo izaa naajitumur Rasoola faqaddimoo baina yadai najwaakum sadaqah; zaalika khairul lakum wa athar; fa il lam tajidoo fa innal laaha ghafoorur Raheem"}, {"id": 198260, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 477, "hizb_quarter": 217, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5117, "text": "'A-ashfaqtum an tuqaddimoo baina yadai najwaakum sadaqaat; fa-iz lam taf'aloo wa taabal laahu 'alaikum fa aqeemus Salaata wa aatuz Zakaata wa atee'ul laaha wa rasoolah; wallaahu khabeerum bimaa ta'maloon"}, {"id": 198261, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5118, "text": "Alam tara ilal lazeena tawallaw qawman ghadibal laahu 'alaihim maa hum minkum wa laa minhum wa yahlifoona 'alal kazibi wa hum ya'lamoon"}, {"id": 198262, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5119, "text": "A'addal laahu lahum 'azaaban shadeedan innahum saaa'a maa kaanoo ya'maloon"}, {"id": 198263, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5120, "text": "Ittakhazooo aymaanahum junnatan fasaddoo 'an sabeelil laahi falahum 'azaabum muheen"}, {"id": 198264, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5121, "text": "Lan tughniya 'anhum amwaaluhum wa laaa awladuhum minal laahi shai'aa; ulaaa 'ika As haabun Naari hum feehaa khaalidoon"}, {"id": 198265, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5122, "text": "Yawma yab'asuhumul laahujamee'an fa yahlifoona lahoo kamaa yahlifoona lakum wa yahsaboona annahum 'alaa shai'; alaaa innahum humul kaaziboon"}, {"id": 198266, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5123, "text": "Istahwaza 'alaihimush shaitaanu fa ansaahum zikral laah; ulaaa'ika hizbush shaitaaan; alaaa innaa hizbash shaitaani humul khaasiroon"}, {"id": 198267, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5124, "text": "Innal lazeena yuhaaaddoonal laaha wa Rasoolahooo ulaaa'ika fil azalleen"}, {"id": 198268, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 544, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5125, "text": "Katabal laahu la aghlibanna ana wa Rusulee; innal laaha qawiyyun 'Azeez"}, {"id": 198269, "identifier": "en.transliteration", "surah_number": 58, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u062c\u0627\u062f\u0644\u0629", "surah_en_name": "Al-Mujaadila", "surah_en_name_translation": "The Pleading Woman", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 478, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5126, "text": "Laa tajidu qawmany yu'minoona billaahi wal yawmil aakhiri yuwaaaddoona man haaaddal laaha wa Rasoolahoo wa law kaanooo aabaaa'ahum aw abnaaa'ahum aw ikhwaa nahum aw 'asheeratahum; ulaaa'ika kataba fee quloobihi mul eemaana wa ayyadahum biroohimminhu wa yudkhilu hum jannatin tajree min tahtihal anhaaru khaalideena feehaa; radiyal laahu 'anhum wa radoo 'anh; ulaaa 'ika hizbul laah; alaaa inna hizbal laahi humul muflihoon"}]