[{"id": 197657, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4511, "text": "Haa-Meeeem"}, {"id": 197658, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4512, "text": "Tanzeelul Kitaabi minal laahil-'Azeezil Hakeem"}, {"id": 197659, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4513, "text": "Maa khalaqnas samaawaati wal arda wa maa bainahumaaa illaa bilhaqqi wa ajalim musammaa; wallazeena kafaroo 'ammaaa unziroo mu'ridoon"}, {"id": 197660, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4514, "text": "Qul ara'aytum maa tad'oona min doonil laahi aroonee maazaa khalaqoo minal ardi am lahum shirkun fis samaawaati eetoonee bi kitaabim min qabli haazaaa aw asaaratim min 'ilmin in kuntum saadiqeen"}, {"id": 197661, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 502, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4515, "text": "Wa man adallu mimmany yad'oo min doonil laahi mallaa yastajeebu lahooo ilaa Yawmil Qiyaamati wa hum'an du'aaa'ihim ghaafiloon"}, {"id": 197662, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4516, "text": "Wa izaa hushiran naasu kaanoo lahum a'daaa'anw wa kaanoo bi'ibaadatihim kaafireen"}, {"id": 197663, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4517, "text": "Wa izaa tutlaa 'alaihim Aayaatunaa baiyinaatin qaalal lazeena kafaroo lilhaqqi lammaa jaaa'ahum haazaa sihrum mubeen"}, {"id": 197664, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4518, "text": "Am yaqooloonaf taraahu qul inif taraituhoo falaa tamlikoona lee minal laahi shai'an Huwa a'lamu bimaa tufeedoona feehi kafaa bihee shaheedam bainee wa bainakum wa Huwal Ghafoorur Raheem"}, {"id": 197665, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4519, "text": "Qul maa kuntu bid'am minal Rusuli wa maaa adreee ma yuf'alu bee wa laa bikum in attabi'u illaa maa yoohaaa ilaiya ya maaa ana illaa nazeerum mubeen"}, {"id": 197666, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 438, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4520, "text": "Qul ara'aytum in kaana min 'indil laahi wa kafartum bihee wa shahida shaahidum mim Banee Israaa'eela 'alaa mislihee fa aamana wastak bartum innal laaha laa yahdil qawmaz zaalimeen"}, {"id": 197667, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4521, "text": "Wa qaalal lazeena kafaroo lillazeena aamanoo law kaana khairam maa sabaqoonaaa ilyh; wa iz lam yahtadoo bihee fasa yaqooloona haazaaa ifkun qadeem"}, {"id": 197668, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4522, "text": "Wa min qablihee kitaabu Moosaaa imaamanw-wa rahmah; wa haazaa Kitaabum musad diqul lisaanan 'Arabiyyal liyunziral lazeena zalamoo wa bushraa lilmuhsineen"}, {"id": 197669, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4523, "text": "Innal lazeena qaaloo Rabbunal laahu summas taqaamoo falaa khawfun 'alaihim wa laahum yahzanoon"}, {"id": 197670, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 503, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4524, "text": "Ulaaa'ika Ashabul Jannati khaalideena feehaa jazaaa'am bimaa kaano ya'maloon"}, {"id": 197671, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4525, "text": "Wa wassainal insaana biwaalidaihi ihsaanan hamalathu ummuhoo kurhanw-wa wada'athu kurhanw wa hamluhoo wa fisaaluhoo salaasoona shahraa; hattaaa izaa balagha ashuddahoo wa balagho arba'eena sanatan qaala Rabbi aqzi'neee an ashkura ni'matakal lateee an'amta 'alaiya wa 'alaa waalidaiya wa an a'mala saalihan tardaahu wa aslih lee fee zurriyyatee innee tubtu ilaika wa innee minal muslimeen"}, {"id": 197672, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4526, "text": "Ulaaa'ikal lazeena nata qabbalu 'anhum ahsana maa 'amiloo wa natajaawazu 'an saiyiaatihim feee Ashaabil jannati Wa'das sidqil lazee kaanoo yoo'adoon"}, {"id": 197673, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4527, "text": "Wallazee qaala liwaali daihi uffil lakumaaa ata'idanineee an ukhraja wa qad khalatil quroonu min qablee wa humaa yastagheesaanil laaha wailaka aamin inna wa'dal laahi haqq, fa yaqoolu maa haazaaa illaaa asaateerul awwaleen"}, {"id": 197674, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4528, "text": "Ulaaa'ikal lazeena haqqa 'alaihimul qawlu feee umamin qad khalat min qablihim minal jinni wal insi innahum kaanoo khaasireen"}, {"id": 197675, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4529, "text": "Wa likullin darajaatum mimmaa 'amiloo wa liyuwaf fiyahum a'maalahum wa hum laa yuzlamoon"}, {"id": 197676, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 504, "ruku": 439, "hizb_quarter": 201, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4530, "text": "Wa Yawma yu'radul lazeena kafaroo 'alan Naai azhabtum taiyibaatikum fee hayaatikumud dunyaa wastam ta'tum bihaa fal Yawma tujzawna 'azaabal hooni bimaa kuntum tastakbiroona fil ardi bighairil haqqi wa bimaa kuntum tafsuqoon"}, {"id": 197677, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4531, "text": "Wazkur akhaa 'Aad, iz anzara qawmahoo bil Ahqaafi wa qad khalatin nuzuru mim baini yadaihi wa min khalfiheee allaa ta'budooo illal laaha inneee akhaafu 'alaikum 'azaaba Yawmin 'azeem"}, {"id": 197678, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4532, "text": "Qaaloo aji'tanaa litaa fikanaa 'an aalihatinaa faatinaa bimaa ta'idunaaa in kunta minas saadiqeen"}, {"id": 197679, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4533, "text": "Qaala innamal 'ilmu indal laahi wa uballighukum maaa uriltu bihee wa laakinneee araakum qawman tajhaloon"}, {"id": 197680, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4534, "text": "Falammaa ra awhu 'aaridam mustaqbila awdiyatihim qaaloo haazaa 'aaridum mumtirunaa; bal huwa masta'jaltum bihee reehun feehaa 'azaabun aleem"}, {"id": 197681, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4535, "text": "Tudammiru kulla shai'im bi-amri Rabbihaa fa asbahoo laa yuraaa illaa masaakinuhum; kazaalika najzil qawmal mujrimeen"}, {"id": 197682, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 440, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4536, "text": "Wa laqad makkannaahum feemaaa im makkannaakum feehi waj'alnaa lahum sam'anw wa absaaranw wa af'idatan famaaa aghnaa 'anhum samu'uhum wa laaa absaaruhum wa laaa af'idatuhum min shai'in iz kaanoo yajhadoona bi Aayaatil laahi wa haaqa bihim maa kaanoo bihee yastahzi'oon"}, {"id": 197683, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4537, "text": "Wa laqad ahlaknaa ma hawlakum minal quraa wa sarrafnal Aayaati la'allahum yarji'oon"}, {"id": 197684, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 505, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4538, "text": "Falaw laa nasarahumul lazeenat takhazoo min doonil laahi qurbaanan aalihatam bal dalloo 'anhum' wa zaalika ifkuhum wa maa kaanoo yaftaroon"}, {"id": 197685, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4539, "text": "Wa iz sarafinaaa ilaika nafaram minal jinni yastami'oonal Quraana falammaa hadaroohu qaalooo ansitoo falammaa qudiya wallaw ilaa qawmihim munzireen"}, {"id": 197686, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4540, "text": "Qaaloo yaa qawmanaaa innaa sami'naa Kitaaban unzila mim ba'di Moosa musaddiqal limaa baina yadihi yahdeee ilal haqqi wa ilaa Tareeqim Mustaqeem"}, {"id": 197687, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4541, "text": "Yaa qawmanaaa ajeeboo daa'iyal laahi wa aaminoo bihee yaghfir lakum min zunoobikum wa yujirkum min 'azaabin aleem"}, {"id": 197688, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4542, "text": "Wa mal laa yujib daa'iyal laahi falaisa bimu'jizin fil ardi wa laisa lahoo min dooniheee awliyaaa'; ulaaa ika fee dalaalim mubeen"}, {"id": 197689, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4543, "text": "Awalam yaraw annal laahal lazee khalaqas samaawaati wal larda wa lam ya'ya bikhal qihinna biqaadirin 'alaaa anyyuhiyal mawtaa; balaaa innahoo 'alaa kulli shai'in Qadeer"}, {"id": 197690, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4544, "text": "Wa Yawma yu'radul lazeena kafaroo 'alan naari alaisa haaza bil haqq; qaaloo balaa wa Rabbinaa; qaala fazooqul 'azaaba bimaa kuntum takfuroon"}, {"id": 197691, "identifier": "en.transliteration", "surah_number": 46, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u062d\u0642\u0627\u0641", "surah_en_name": "Al-Ahqaf", "surah_en_name_translation": "The Dunes", "revelation_type": "Meccan", "juz": 26, "manzil": 6, "page": 506, "ruku": 441, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4545, "text": "Fasbir kamaa sabara ulul 'azmi minar Rusuli wa laa tasta'jil lahum; ka annahum Yawma yarawna maa yoo'adoona lam yalbasooo illaa saa'atam min nahaar; balaagh; fahal yuhlaku illal qawmul faasiqoon"}]