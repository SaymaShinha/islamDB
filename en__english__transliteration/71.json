recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5429, "text": "Faqultus taghfiroo Rabakam innahoo kaana Ghaffaaraa"}, {"id": 198574, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5430, "text": "Yursilis samaaa'a 'alaikum midraaraa"}, {"id": 198575, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5431, "text": "Wa yumdidkum bi am waalinw wa baneena wa yaj'al lakum Jannaatinw wa yaj'al lakum anhaaraa"}, {"id": 198576, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5432, "text": "Maa lakum laa tarjoona lillaahi waqaaraa"}, {"id": 198577, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5433, "text": "Wa qad khalaqakum at waaraa"}, {"id": 198578, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5434, "text": "Alam taraw kaifa khalaqal laahu sab'a samaawaatin tibaaqaa"}, {"id": 198579, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0646\u0648\u062d", "surah_en_name": "Nooh", "surah_en_name_translation": "Noah", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 571, "ruku": 504, "hizb_quarter": 228, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5435, "text": "Wa ja'alal qamara feehinna nooranw wa ja'alash shamsa siraajaa"}, {"id": 198580, "identifier": "en.transliteration", "surah_number": 71, "surah_ar_name": "\u0633\u064