[{"id": 197417, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4273, "text": "Haa Meeem"}, {"id": 197418, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4274, "text": "'Ayyyn Seeen Qaaaf"}, {"id": 197419, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4275, "text": "Kazaalika yooheee ilaika wa ilal lazeena min qablikal laahul 'Azeezul Hakeem"}, {"id": 197420, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4276, "text": "Lahoo maa fis samaa waati wa maa fil ardi wa Huwal 'Aliyul 'Azeem"}, {"id": 197421, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4277, "text": "Takaadus samaawaatu yatafattarna min fawqihinn; walmalaaa'ikatu yusabbihoona bihamdi Rabbihim wa yastaghfiroona liman fil ard; alaaa innal laaha huwal Ghafoorur Raheem"}, {"id": 197422, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4278, "text": "Wallazeenat takhazoo min dooniheee awliyaaa'al laahu hafeezun 'alaihim wa maaa anta 'alaihim biwakeel"}, {"id": 197423, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4279, "text": "Wa kazaalika awhainaaa llaika Qur-aanan 'Arabiyyal litunzir aUmmal Quraa wa man hawlahaa wa tunzira Yawmal Jam'ilaa raiba feeh; fareequn fil jannati wa fareequn fissa'eer"}, {"id": 197424, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4280, "text": "Wa law shaaa'al laahu laja'alahum ummatanw waahi datanw walaakiny yudkhilumany yashaaa'u fee rahmatih; waz zaalimoona maa lahum minw waliyyinw wa laa naseer"}, {"id": 197425, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 419, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4281, "text": "Amit takhazoo min dooniheee awliyaaa'a fallaahu Huwal Waliyyu wa Huwa yuhyil mawtaa wa Huwa 'alaa kulli shai'in Qadeer"}, {"id": 197426, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 483, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4282, "text": "Wa makh-talaftum feehi min shai'in fahukmuhooo ilallaah; zaalikumul laahu Rabbee 'alaihi tawakkaltu wa ilaihi uneeb"}, {"id": 197427, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4283, "text": "Faatirus samaawaati wal ard; ja'ala lakum min anfusikum azwaajanw wa minal an'aami azwaajany yazra'ukum feeh; laisa kamislihee shai'unw wa Huwas Samee'ul Baseer"}, {"id": 197428, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4284, "text": "Lahoo maqaaleedus samaawaati wal ardi yabsutur rizqa limany yashaaa'u wa yaqdir; innahoo bikulli shai'unw wa Huwas Samee'ul Baseer"}, {"id": 197429, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4285, "text": "Shara'a lakum minad deeni maa wassaa bihee Noohanw wallazeee awhainaaa ilaika wa maa wassainaa biheee Ibraaheema wa Moosa wa 'Eesaaa an aqeemud adeena wa laa tatafarraqoo feeh; kabura 'alal mushrikeena maa tad'oohum ilaih; Allaahu yajtabee ilaihi many yashaaa'u wa yahdeee ilaihi mai yuneeb"}, {"id": 197430, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4286, "text": "Wa maa tafarraqooo illaa mim ba'di maa jaaa'ahumul 'ilmu baghyam bainahum; wa law laa Kalimatun sabaqat mir Rabbika ilaaa ajalim musammal laqudiya bainahum; wa innal lazeena oorisul Kitaaba mim ba'dihim lafee shakkim minhu mureeb"}, {"id": 197431, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 484, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4287, "text": "Falizaalika fad'u wastaqim kamaaa umirta wa laa tattabi' ahwaaa'ahum wa qul aamantu bimaaa anzalal laahu min Kitaab, wa umirtu li a'dila bainakum Allaahu Rabbunaa wa Rabbukum lanaaa a'maa lunaa wa lakum a'maalukim laa hujjata bainanaa wa baina kumul laahu yajma'u bainanaa wa ilaihil maseer"}, {"id": 197432, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4288, "text": "Wallazeena yuhaaajjoona fil laahi mim ba'di mastujeeba lahoo hujjatuhum daahidatun 'inda Rabbihim wa 'alaihim ghadabunw wa lahum 'azaabun shadeed"}, {"id": 197433, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4289, "text": "Allahul lazeee anzalal Kitaaba bilhaqqi wal Meezaan; wa ma yudreeka la'allas Saa'ata qareeb"}, {"id": 197434, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4290, "text": "Yasta'jilu bihal lazeena laa yu'minoona bihaa wallazeena aamanoo mushfiqoona minhaa wa ya'lamoona annahal haqq; alaaa innal lazeena yumaaroona fis Saa'ati lafee dalaalim ba'eed"}, {"id": 197435, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 420, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4291, "text": "Allahu lateefum bi'ibaadihee yarzuqu mai yashaaa'u wa Huwal Qawiyyul 'Azeez"}, {"id": 197436, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4292, "text": "Man kaana yureedu harsal Aakhirati nazid lahoo fee harsihee wa man kaana yureedu harsad dunyaa nu'tihee mnhaa wa maa lahoo fil Aakhirati min naseeb"}, {"id": 197437, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4293, "text": "Am lahum shurakaaa'u shara'oo lahum minad deeni maa lam yaazam bihil laah; wa law laa kalimatul fasli laqudiya bainahum; wa innaz zaalimeena lahum 'azaabun aleem"}, {"id": 197438, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 485, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4294, "text": "Taraz zaalimeena mushfiqeena mimmaa kasaboo wa huwa waaqi'um bihim; wallazeena aamanoo wa 'amilus saalihaati fee rawdaatil jannaati lahum maa yashaaa'oona 'inda Rabbihim; zaalika huwal fadlul kabeer"}, {"id": 197439, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4295, "text": "Zaalikal lazee yubash shirul laahu 'ibaadahul lazeena aamanoo wa 'amilus saalihaat; qul laaa as'alukum 'alaihi ajran illal mawaddata fil qurbaa; wa mai yaqtarif hasanatan nazid lahoo feehaa husnaa; innal laaha Ghafoorun Shakoor"}, {"id": 197440, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4296, "text": "Am yaqooloonaf tara 'alal laahi kaziban fa-iny yasha il laahu yakhtim 'alaa qalbik; wa yamhul laahul baatila wa yuhiqqul haqqa bi Kalimaatih; innahoo 'Aleemum bizaatis sudoor"}, {"id": 197441, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4297, "text": "Wa Huwal lazee yaqbalut tawbata 'an 'ibaadihee wa ya'foo 'anis saiyiaati wa ya'lamu maa taf'aloon"}, {"id": 197442, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 194, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4298, "text": "Wa yastajeebul lazeena aamanoo wa 'amilu saalihaati wa yazeeduhum min fadlih; wal kaafiroona lahum 'azaabun shadeed"}, {"id": 197443, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4299, "text": "Wa law basatal laahur rizqa li'ibaadihee labaghaw fil ardi wa laakiny yunazzilu biqadarim maa yashaaa'; innahoo bi'ibaadihee Khabeerum Baseer"}, {"id": 197444, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4300, "text": "Wa Huwal lazee yunazzilul ghaisa mim ba'di maa qanatoo wa yanshuru rahmatah; wa Huwal Waliyyul Hameed"}, {"id": 197446, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 421, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4301, "text": "Wa min Aayaatihee khalqus samaawaati wal ardi wa maa bassa feehimaa min daaabbah; wa Huwa 'alaa jam'ihim izaa yashaaa'u Qadeer"}, {"id": 197447, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4302, "text": "Wa maaa asaabakum mim museebatin fabimaa kasabat aydeekum wa ya'foo 'an kaseer"}, {"id": 197448, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 486, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4303, "text": "Wa maaa antum bimu'jizeena fil ardi wa maa lakum min doonil laahi minw wa liyyinw wa laa naseer"}, {"id": 197449, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4304, "text": "Wa min Aayaatihil ja waarifil bahri kal a'lam"}, {"id": 197450, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4305, "text": "Iny yashaaa yuskinir reeha fa yazlalna rawaakida 'alaa zahirh; inna fee zaalika la Aayaatil likulli sabbaarin shakoor"}, {"id": 197451, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4306, "text": "Aw yoobiqhunna bimaa kasaboo wa ya'fu 'an kaseer"}, {"id": 197452, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4307, "text": "Wa ya'lamal lazeena yujaadiloona feee Aayaatinaa maa lahum mim mahees"}, {"id": 197453, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4308, "text": "Famaa ooteetum min shai'in famataa'ul hayaatid dunyaa wa maa 'indal laahi khairunw wa abqaa lillazeena aamanoo wa 'alaa Rabbihim yatawakkaloon"}, {"id": 197454, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4309, "text": "Wallazeena yajtaniboona kabaaa'iral ismi wal fawaa hisha wa izaa maa ghadiboo hum yaghfiroon"}, {"id": 197455, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4310, "text": "Wallazeenas tajaaboo li Rabbhim wa aqaamus Salaata wa amruhum shooraa bainahum wa mimmaa razaqnaahum yunfiqoon"}, {"id": 197456, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4311, "text": "Wallazeena izaa asaabahumul baghyu hum yantasiroon"}, {"id": 197457, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4312, "text": "Wa jazaaa'u saiyi'atin saiyi'tum misluha faman 'afaa wa aslaha fa ajruhoo 'alal laah; innahoo laa yuhibbuz zaalimeen"}, {"id": 197458, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4313, "text": "Wa lamanin tasara ba'da zulmihee fa ulaaa'ika maa 'alaihim min sabeel"}, {"id": 197459, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4314, "text": "Innamas sabeelu 'alal lazeena yazlimoonan naasa wa yabghoona fil ardi bighairil haqq; ulaaa'ika lahum 'azaabun aleem"}, {"id": 197460, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 422, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4315, "text": "Wa laman sabara wa ghafara inna zaalika lamin 'azmil umoor"}, {"id": 197461, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 487, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4316, "text": "Wa mai yudlilil laahu famaa lahoo minw waliyyim mim ba'dih; wa taraz zaalimeena lammaa ra awul 'azaaba yaqooloona hal ilaa maraddim min sabeel"}, {"id": 197462, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4317, "text": "Wa taraahum yu'radoona 'alaihaa khaashi'eena minazzulli yanzuroona min tarfin khaifiyy; wa qaalal lazeena aamanooo innal khaasireenal lazeena khasiroon anfusahum wa ahleehim Yawmal Qiyaamah; alaaa innaz zaalimeena fee'azaabim muqeem"}, {"id": 197463, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4318, "text": "Wa maa kaana lahum min awliyaaa'a yansuroonahum min doonil laah; wa mai yudlilil laahu famaa lahoo min sabeel"}, {"id": 197464, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4319, "text": "Istajeeboo li Rabbikum min qabli any yaatiya Yawmul laa maradda lahoo minal laah; maa lakum mim malja iny yawma'izinw wa maa lakum min nakeer"}, {"id": 197465, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4320, "text": "Fa-in a'radoo famaaa arsalnaaka 'alaihim hafeezan in 'alaika illal balaagh; wa innaaa izaaa azaqnal insaana minnaa rahmatan fariha bihaa wa in tusibhum saiyi'atum bimaa qaddamat aydeehim fa innal insaana kafoor"}, {"id": 197466, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4321, "text": "Lillaahi mulkus samaawaati wal ard; yakhluqu maa yashaaa'; yahabu limai yashaaa'u inaasanw wa yahabu limai yashaaa'uz zukoor"}, {"id": 197467, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 195, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4322, "text": "Aw yuzawwijuhum zukraananw wa inaasanw wa yaj'alu mai yashaaa'u 'aqeemaa; innahoo 'Aleemun Qadeer"}, {"id": 197468, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 488, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4323, "text": "Wa maa kaana libasharin any yukallimahul laahu illaa wahyan aw minw waraaa'i hijaabin aw yursila Rasoolan fa yoohiya bi iznuhee maa yashaaa'; innahoo 'Aliyyun Hakeem"}, {"id": 197469, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 489, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4324, "text": "Wa kazaalika awhainaaa ilaika rooham min amrinaa; maa kunta tadree mal Kitaabu wa lal eemaanu wa laakin ja'alnaahu nooran nahdee bihee man nashaaa'u min 'ibaadinaa; wa innaka latahdeee ilaaa Siraatim Mustaqeem"}, {"id": 197445, "identifier": "en.transliteration", "surah_number": 42, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0648\u0631\u0649", "surah_en_name": "Ash-Shura", "surah_en_name_translation": "Consultation", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 489, "ruku": 423, "hizb_quarter": 196, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4325, "text": "Siraatil laahil lazee lahoo maa fis samaawaati wa maa fil ard; alaaa ilal laahi taseerul umoor"}]