[{"id": 197692, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4546, "text": "Allazeena kafaroo wa saddoo'an sabeelil laahi adalla a'maalahum"}, {"id": 197693, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4547, "text": "Wallazeena aamanoo wa 'amilus saalihaati wa aamanoo bimaa nuzzila 'alaa Muhammadinw-wa huwal haqqu mir Rabbihim kaffara 'anhum saiyiaatihim wa aslaha baalahum"}, {"id": 197694, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4548, "text": "Zaalika bi annal lazeena kafarut taba'ul baatila wa annal lazeena aamanut taba'ul haqqa mir Rabbihim; kazaalika yadribul laahu linnaasi amsaalahum"}, {"id": 197695, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4549, "text": "Fa-izaa laqeetumul lazeena kafaroo fadarbar riqaab, hattaaa izaa askhan tumoohum fashuddul wasaaq, fa immaa mannnam ba'du wa immaa fidaaa'an hattaa tada'al harbu awzaarahaa; zaalika wa law yashaaa'ul laahu lantasara minhum wa laakil laiyabluwa ba'dakum biba'd; wallazeena qutiloo fee sabeelil laahi falany yudilla a'maalahum"}, {"id": 197696, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4550, "text": "Sa-yahdeehim wa yusihu baalahum"}, {"id": 197697, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4551, "text": "Wa yudkhiluhumul jannata 'arrafahaa lahum"}, {"id": 197698, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4552, "text": "Yaaa ayyuhal lazeena aamanooo in tansurul laaha yansurkum wa yusabbit aqdaamakum"}, {"id": 197699, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4553, "text": "Wallazeena kafaroo fata's al lahum wa adalla a'maalahum"}, {"id": 197700, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 202, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4554, "text": "Zaalika bi annahum karihoo maaa anzalal laahu faahbata a'maalahum"}, {"id": 197701, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4555, "text": "Afalam yaseeroo fil ardi fayanzuroo kaifa kaana 'aaqibatul lazeena min qablihim; dammaral laahu 'alaihim wa lilkaafireena amsaaluhaa"}, {"id": 197702, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 507, "ruku": 442, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4556, "text": "Zaalika bi annal laaha mawlal lazeena aamanoo wa annal kaafireena laa mawlaa lahum"}, {"id": 197703, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4557, "text": "Innal-laaha yudkhilul lazeena aamanoo wa 'amilus saalihaati Jannaatin tajree min tahtihal anhaaru wallazeena kafaroo yatamatta'oona wa yaakuloona kamaa taakuluian'aamu wan Naaru maswallahum"}, {"id": 197704, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4558, "text": "Wa ka ayyim min qaryatin hiya ashaddu quwwatam min qaryatikal lateee akhrajatka ahlaknaahum falaa naasira lahum"}, {"id": 197705, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4559, "text": "Afaman kaana 'alaa baiyinatim mir Rabbihee kaman zuyyina lahoo sooo'u 'amalihee wattaba'ooo ahwaaa'ahum"}, {"id": 197706, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4560, "text": "Masalul jannatil latee wu'idal muttaqoona feehaaa anhaarum mim maaa'in ghayri aasininw wa anhaarum mil labanil lam yataghaiyar ta'muhoo wa anhaarum min khamril lazzatil lishshaaribeena wa anhaarum min 'asalim musaffanw wa lahum feeha min kullis samaraati wa maghfiratum mir Rabbihim kaman huwa khaalidun fin naari wa suqoo maaa'an hameeman faqatta'a am'aaa'ahum"}, {"id": 197707, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4561, "text": "Wa minhum mai yastami' ilaika hattaaa izaa kharajoo min 'indika qaaloo lillazeena ootul 'ilma maazaa qaala aanifaa; ulaaa'ikal lazeena taba'al laahu 'alaa quloobihim wattaba'ooo ahwaaa'ahum"}, {"id": 197708, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4562, "text": "Wallazeenah tadaw zaadahum hudanw wa aataahum taqwaahum"}, {"id": 197709, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4563, "text": "Fahal yanzuroona illas Saa'ata an taatiyahum baghtatan faqad jaaa'a ashraatuhaa; fa-annnaa lahum izaa jaaa'at hum zikraahum"}, {"id": 197710, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 508, "ruku": 443, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4564, "text": "Fa'lam annahoo laaa ilaaha illal laahu wastaghfir lizambika wa lilmu'mineena walmu'minaat; wallaahu ya'lamu mutaqallabakum wa maswaakum"}, {"id": 197712, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4565, "text": "Wa yaqoolul lazeena aamanoo law laa nuzzilat Sooratun fa izaaa unzilat Sooratum Muhkamatunw wa zukira feehal qitaalu ra aytal lazeena fee quloobihim maraduny yanzuroona ilaika nazaral maghshiyyi 'alaihi minal mawti fa'awlaa lahum"}, {"id": 197713, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4566, "text": "Taa'atunw wa qawlum ma'roof; fa izaa 'azamal amru falaw sadaqul laaha lakaana khairal lahum"}, {"id": 197714, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4567, "text": "Fahal 'asaitum in tawallaitum an tufsidoo fil ardi wa tuqatti'ooo arhaamakum"}, {"id": 197715, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4568, "text": "Ulaaa'ikal lazeena la'anahumul laahu fa asammahum wa a'maaa absaarahum"}, {"id": 197716, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4569, "text": "Afalaa yatadabbaroonal Qur-aana am 'alaa quloobin aqfaaluhaa"}, {"id": 197717, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4570, "text": "Innal lazeenar taddoo 'alaaa adbaarihim mim ba'di maa tabaiyana lahumul hudash Shaitaanu sawwala lahum wa amlaa lahum"}, {"id": 197718, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4571, "text": "Zaalika bi annahum qaaloo lillazeena karihoo maa nazzalal laahu sanutee'ukum fee ba'dil amri wallaahu ya'lamu israarahum"}, {"id": 197719, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4572, "text": "Fakaifa izaa tawaffat humul malaaa'ikatu yadriboona wujoohahum wa adbaa rahum"}, {"id": 197720, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 444, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4573, "text": "Zaalika bi annahumut taba'oo maaa askhatal laaha wa karihoo ridwaanahoo fa ahbata a'maalahum"}, {"id": 197721, "identifier": "en.transliteration", "surah_number": 47, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0645\u062d\u0645\u062f", "surah_en_name": "Muhammad", "surah_en_name_translation": "Muhammad", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 509, "ruku": 445, "hizb_quarter": 203, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4574, "text": "Am hasibal lazeena fee quloobihim maradu