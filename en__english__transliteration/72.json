[{"id": 198592, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5448, "text": "Qul oohiya ilaiya annna hustama'a nafarum minal jinnni faqaalooo innaa sami'naa quraanan 'ajabaa"}, {"id": 198593, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5449, "text": "Yahdeee ilar rushdi fa aamannaa bihee wa lan nushrika bi rabbinaaa ahadaa"}, {"id": 198594, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5450, "text": "Wa annahoo Ta'aalaa jaddu Rabbinaa mat takhaza saahibatanw wa la waladaa"}, {"id": 198595, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5451, "text": "Wa annahoo kaana yaqoolu safeehunaa 'alal laahi shatataa"}, {"id": 198596, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5452, "text": "Wa annaa zanannaaa al lan taqoolal insu wal jinnu 'alal laahi kazibaa"}, {"id": 198597, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5453, "text": "Wa annahoo kaana rijaa lum minal insi ya'oozoona birijaalim minal jinni fazaa doohum rahaqaa"}, {"id": 198598, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5454, "text": "Wa annahum zannoo kamaa zanantum al lany yab'asal laahu ahadaa"}, {"id": 198599, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5455, "text": "Wa annaa lamasnas sa maaa'a fa wajadnaahaa muli'at harasan shadeedanw wa shuhubaa"}, {"id": 198600, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5456, "text": "Wa annaa kunnaa naq'udu minhaa maqaa'ida lis'sam'i famany yastami'il aana yajid lahoo shihaabar rasada"}, {"id": 198601, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5457, "text": "Wa annaa laa nadreee asharrun ureeda biman fil ardi am araada bihim rabbuhum rashadaa"}, {"id": 198602, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5458, "text": "Wa annaa minnas saalihoona wa minnaa doona zaalika kunnaa taraaa'ilqa qidadaa"}, {"id": 198603, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 572, "ruku": 506, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5459, "text": "Wa annaa zanannaaa al lan nu'jizal laaha fil ardi wa lan nu'jizahoo harabaa"}, {"id": 198604, "identifier": "en.transliteration", "surah_number": 72, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0646", "surah_en_name": "Al-Jinn", "surah_en_name_translation": "The Jinn", "revelati