[{"id": 198361, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 558, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5218, "text": "Yaaa ayyuhan nabiyyu izaa tallaqtummun nisaaa'a fatalliqoohunna li'iddatihinna wa ahsul'iddata; wattaqul laaha rabbakum laa tukhri joohunna mim bu-yootihinna wa laa yakhrujna illaaa any yaateema bifaahishatim mubaiyinah; wa tilka hudoodul laah; wa many yata'adda hudoodal laahi faqad zalama nafsha; laa tadree la'allal laaha yuhdisu ba'dazaalika amraa"}, {"id": 198362, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 558, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5219, "text": "Fa izaa balaghna ajalahunna fa amsikoohunna bima'roofin aw faariqoohunna bima'roofinw wa ashhidoo zawai 'adlim minkum wa aqeemush shahaadata lillaah; zaalikum yoo'azu bihee man kaana yu'minu billaahi wal yawmil aakhir; wa many yattaqil laaha yaj'al lahoo makhrajaa"}, {"id": 198363, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 558, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5220, "text": "Wa yarzuqhu min haisu laa yahtasib; wa many yatawakkal 'alal laahi fahuwa husbuh; innal laaha baalighu amrih; qad ja'alal laahu likulli shai'in qadraa"}, {"id": 198366, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 558, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5221, "text": "Wallaaa'ee ya'isna minal maheedi min nisaaa 'ikum inir tabtum fa'iddatuhunna salaasatu ashhurinw wallaaa'ee lam yahidn; wa ulaatul ahmaali ajaluhunna any yada'na hamlahun; wa many yattaqil laaha yaj'al lahoo min armrihee yusraa"}, {"id": 198367, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 558, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5222, "text": "Zaalika amrul laahi anzalahoo ilaikum; wa many yattaqil laaha yukaffir 'anhu saiyi aatihee wa yu'zim lahoo ajraa"}, {"id": 198368, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5223, "text": "Askinoohunna min haisu sakantum minw wujdikum wa laa tudaaarroohunna litudaiyiqoo 'alaihinn; wa in kunna ulaati hamlin fa anfiqoo 'alihinna hattaa yada'na hamlahunn; fain arda'na lakum fa aatoo hunna ujoorahunna waatamiroo bainakum bima'roofinw wa in ta'aasartum fasaturdi'u lahooo ukhraa"}, {"id": 198369, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 492, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5224, "text": "Liyuntiq zoo sa'atim min sa'atihee wa man qudira 'alaihi riquhoo falyunfiq mimmaaa aataahul laah; laa yukalliful laahu nafsan illaa maaa aataahaa; sa yaj'alul laahu ba'da'usriny yusraa"}, {"id": 198370, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 493, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5225, "text": "Wa ka ayyim min qaryatin 'atat 'an amri Rabbihaa wa Rusulihee fahaasabnaahaa hisaaban shadeedanw wa 'azzabnaahaa 'azaaban nukraa"}, {"id": 198371, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 493, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5226, "text": "Fazaaqat wabbala amrihaa wa kaana 'aaqibatu amrihaa khusraa"}, {"id": 198372, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 493, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5227, "text": "A'addal laahu lahum 'azaaban shadeedan fattaqul laaha yaaa ulil albaab, allazeena aammanoo; qad anzalal laahu ilaikum zikraa"}, {"id": 198373, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 493, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5228, "text": "Rasoolany yatloo 'alaikum aayaatil laahi mubaiyinaatil liyukhrijal lazeena aamanoo wa 'amilus saalihaati minaz zulumaati ilan noor; wa many yu'mim billaahi wa ya;mal saalihany yudkhilhu jannaatin tajree min tahtihal anhaaru khaalideena feehaa abadan qad ahsanal laahu lahoo rizqqq"}, {"id": 198374, "identifier": "en.transliteration", "surah_number": 65, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0644\u0627\u0642", "surah_en_name": "At-Talaaq", "surah_en_name_translation": "Divorce", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 559, "ruku": 493, "hizb_quarter": 223, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5229, "text": "Allaahul lazee khalaq Sab'a Samaawaatinw wa minal ardi mislahunna yatanazzalul amru bainahunna lita'lamooo annal laaha 'alaa kulli shai'in Qadeerunw wa annal laaha qad ahaata bikulli shai'in ilmaa"}]