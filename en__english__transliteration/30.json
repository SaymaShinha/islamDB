[{"id": 196556, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3410, "text": "Alif-Laaam-Meeem"}, {"id": 196557, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3411, "text": "Ghulibatir Room"}, {"id": 196484, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3412, "text": "Feee adnal ardi wa hummim ba'di ghalabihim sa ya ghliboon"}, {"id": 196558, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3413, "text": "Fee bid'i sineen; lillaahil amru min qablu wa mim ba'd; wa yawma'iziny yafrahul mu'minoon"}, {"id": 196560, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3414, "text": "Binasril laa; yansuru mai yashaaa'u wa Huwal 'Azeezur Raheem"}, {"id": 196561, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3415, "text": "Wa'dal laahi laa yukhliful laahu wa'dahoo wa laakin na aksaran naasi laa ya'lamoon"}, {"id": 196562, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3416, "text": "Ya'lamoona zaahiram minal hayaatid dunya wa hum 'anil Aakhirati hum ghaafiloon"}, {"id": 196563, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3417, "text": "Awalam yatafakkaroo feee anfusihim; maa khalaqal laahus samaawaati wal arda wa maa bainahumaaa illaa bil haqqi wa ajalim musammaa; wa inna kaseeram minan naasi biliqaaa'i Rabbihim lakaafiroon"}, {"id": 196564, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3418, "text": "Awalm yaseeroo fil ardi fa-yanzuroo kaifa kaana 'aaqibatul lazeena min qablihim; kaanooo ashadda minhhum quwwatanw wa asaarul arda wa 'amaroohaaa aksara mimmaa 'amaroohaa wa jaaa'athum Rusuluhum bil baiyinaati famaa kaanal laahu liyazli mahum wa laakin kaanooo anfusahum yazlimoon"}, {"id": 196565, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3419, "text": "Summa kaana'aaqibatal lazeena asaaa'us sooo aaa an kazzaboo bi aayaatil laahi wa kaanoo bihaa yastahzi'oon"}, {"id": 196566, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3420, "text": "Allaahu yabda'ul khalqa summa yu'eeduhoo summa ilaihi turja'oon"}, {"id": 196567, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3421, "text": "Wa yawma taqoomus Saa'atu yublisul mujrimoon"}, {"id": 196568, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3422, "text": "Wa lam yakul lahum min shurakaaa'ihim shufa'aaa'u wa kaanoo bishurakaaa'ihim kaafireen"}, {"id": 196569, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3423, "text": "Wa Yawma taqoomus Saa'atu Yawma'iziny yatafarraqoon"}, {"id": 196570, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3424, "text": "Fa ammal lazeena aamanoo wa 'amilus saalihaati fahum fee rawdatiny yuhbaroon"}, {"id": 196571, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3425, "text": "Wa ammal lazeena kafaroo wa kazzaboo bi-Aayaatinaa wa liqaaa'il Aakhirati faulaaa'ika fil'azaabi muhdaroon"}, {"id": 196572, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3426, "text": "Fa Subhaanal laahi heena tumsoona wa heena tusbihoon"}, {"id": 196573, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3427, "text": "Wa lahul hamdu fis samaawaati wal ardi wa 'ashiyyanw wa heena tuzhiroon"}, {"id": 196574, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3428, "text": "Yukhrijul haiya minal maiyiti wa yukhrijul maiyita minal haiyi wa yuhyil arda ba'da mawtihaa; wa kazaalika tukhrajoon"}, {"id": 196575, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3429, "text": "Wa min Aayaatiheee an khalaqakum min turaabin summa izaaa antum basharun tantashiroon"}, {"id": 196576, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3430, "text": "Wa min Aayaatiheee an khalaqa lakum min anfusikum azwaajal litaskunooo ilaihaa wa ja'ala bainakum mawad datanw wa rahmah; inna fee zaalika la Aayaatil liqawminy yatafakkaroon"}, {"id": 196577, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3431, "text": "Wa min Aayaatihee khalqus samaawaati wal aardi wakhtilaafu alsinatikum wa alwaanikum; inna fee zaalika la Aayaatil lil'aalimeen"}, {"id": 196578, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3432, "text": "Wa min Aayaatihee manaamukum bil laili wannahaari wabtighaaa'ukum min fadlih; inna fee zaalika la Aayaatil liqawminy yasma'oon"}, {"id": 196579, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3433, "text": "Wa min Aayaatihee yureekumul barqa khawfanw wa tama'anw wa yunazzilu minas samaaa'i maaa'an fa yuhyee bihil arda ba'da mawtihaaa inna fee zaalika la Aayaatil liqawminy ya'qiloon"}, {"id": 196580, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3434, "text": "Wa min Aayaatihee an taqoomas samaaa'u wal ardu bi-amrih; summa izaa da'aakum da'watam minal ardi izaaa antum takhrujoon"}, {"id": 196581, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3435, "text": "Wa lahoo man fissamaawaati wal ardi kullul lahoo qaanitoon"}, {"id": 196582, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3436, "text": "Wa Huwal lazee yabda'ul khalqa summa yu'eeduhoo wa huwa ahwanu 'alaih; wa lahul masalul la'laa fissamaawaati wal-ard; wa Huwal 'Azeezul Hakeem"}, {"id": 196583, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3437, "text": "Daraba lakum masalam min anfusikum hal lakum mimmaa malakat aymaanukum min shurakaaa'a fee maa razaqnaakum fa antum feehi sawaaa'un takhaafoonahum kakheefa tikum anfusakum; kazaalika nufassilul Aayaati liqawminy ya'qiloon"}, {"id": 196584, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3438, "text": "Balit taba'al lazeena zalamooo ahwaaa'ahum bighairi 'ilmin famai yahdee man adallal laahu wa maa lahum min naasireen"}, {"id": 196585, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3439, "text": "Fa aqim wajhaka liddeeni Haneefaa; fitratal laahil latee fataran naasa 'alaihaa; laa taabdeela likhalqil laah; zaalikad deenul qaiyimu wa laakinna aksaran naasi laa ya'lamoon"}, {"id": 196586, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3440, "text": "Muneebeena ilaihi wattaqoohu wa aqeemus Salaata wa laa takoonoo minal mushrikeen"}, {"id": 196587, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3441, "text": "Minal lazeena farraqoo deenahum wa kaanoo shiya'an kullu hizbim bimaa ladaihim farihoon"}, {"id": 196588, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3442, "text": "Wa izaa massan naasa durrun da'aw Rabbahum muneebeena ilaihi summa izaaa azaqahum minhu rahmatan izaa fareequm minhum be Rabbihim yushrikoon"}, {"id": 196589, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3443, "text": "Li yakfuroo bimaaa aatainaahum; fatamatta'oo fasawfa ta'lamoon"}, {"id": 196590, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3444, "text": "Am anzalnaa 'alaihim sultaanan fahuwa yatakallamu bimaa kaanoo bihee yushrikoon"}, {"id": 196591, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3445, "text": "Wa izaaa azaqnan naasa rahmatan farihoo bihaa wa in tusibhum sayyi'atum bimaa qaddamat aydeehim izaa hum yaqnatoon"}, {"id": 196592, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3446, "text": "Awalam yaraw annal laaha yabsutur rizqa limai yashaaa'u wa yaqdir; inna fee zaalika la Aayaatil liqawminy yu'minoon"}, {"id": 196593, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3447, "text": "Fa aati zal qurbaa haqqahoo walmiskeena wabnassabeel; zaalika khairul lil lazeena yureedoona Wajhal laahi wa ulaaa'ika humul muflihoon"}, {"id": 196594, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3448, "text": "Wa maaa aataitum mir ribal li yarbuwa feee amwaalin naasi falaa yarboo 'indal laahi wa maaa aataitum min zaakaatin tureedoona wajhal laahi fa ulaaa'ika humul mud'ifoon"}, {"id": 196595, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3449, "text": "Allaahul lazee khalaqa kum summa razaqakm summa yumeetukum summa yuhyeekum hal min shurakaaa'ikum mai yaf'alu min zaalikum min shai'; Sub haanahoo wa Ta'aalaa 'ammaa yushrikoon"}, {"id": 196596, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3450, "text": "Zaharal fasaadu fil barri wal bahri bimaa kasabat aydinnaasi li yuzeeqahum ba'dal lazee 'amiloo la'allahum yarji'oon"}, {"id": 196597, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3451, "text": "Qul seeroo fil ardi fanzuroo kaifa kaana 'aaqibatul lazeena min qabl; kaana aksaruhum mushrikeen"}, {"id": 196598, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3452, "text": "Fa aqim wajhaka lid deenil qaiyimi min qabli any yaatiya Yawmul laa maradda lahoo minal laahi Yawma'iziny yassadda'oon"}, {"id": 196599, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3453, "text": "Man kafara fa'alaihi kufruhoo wa man 'amila saalihan fali anfusihim yamhadoon"}, {"id": 196600, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3454, "text": "Li yajziyal lazeena aamanoo wa 'amilus saalihaati min fadlih; innahoo laa yuhibbul kaafireen"}, {"id": 196601, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3455, "text": "Wa min Aayaatiheee anyyursilar riyaaha mubashshi raatinw wa li yuzeeqakum mir rahmatihee wa litajriyal fulku bi amrihee wa litabtaghoo min fadlihee wa la'allakum tashkuroon"}, {"id": 196602, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3456, "text": "Wa laqad arsalnaa min qablika Rusulan ilaa qawmihim fajaaa'oohum bil baiyinaati fantaqamnaa minal lazeena ajramoo wa kaana haqqan 'alainaa nasrul mu'mineen"}, {"id": 196603, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3457, "text": "Allaahul lazee yursilur riyaaha fatuseeru sahaaban fa yabsutuhoo fis samaaa'i kaifa yashaaa'u wa yaj'aluhoo kisafan fataral wadqa yakhruju min khilaalihee fa izaaa asaaba bihee mai yashaaa'u min 'ibaadiheee izaa hum yastabshiroon"}, {"id": 196604, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3458, "text": "Wa in kaanoo min qabli any yunazzala 'alaihim min qablihee lamubliseen"}, {"id": 196605, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3459, "text": "Fanzur ilaaa aasaari rahmatil laahi kaifa yuhyil arda ba'da mawtihaa; inna zaalika lamuhyil mawtaa wa Huwa 'alaa kulli shai'in Qadeer"}, {"id": 196606, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3460, "text": "Wa la'in arsalnaa reehan fara awhu musfarral lazalloo mim ba'dihee yakfuroon"}, {"id": 196607, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3461, "text": "Fa innaka laa tusmi'ul mawtaa wa laa tusmi'us summad du'aaa'a izaa wallaw mudbireen"}, {"id": 196609, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3462, "text": "Wa maa anta bihaadil 'umyi 'an dalaalatihim in tusmi'u illaa mai yuminu bi aayaatinaa fahum muslimoon"}, {"id": 196610, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3463, "text": "Allahul lazee khalaqa kum min du'fin summa ja'ala mim ba'di du'fin quwwatan summa ja'ala mim ba'di quwwatin du'fanw wa shaibah; yakhluqu maa yashaaa'u wa Huwal 'Aleemul Qadeer"}, {"id": 196611, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3464, "text": "Wa Yawma taqoomus Saa'atu yuqsimul mujrimoona maa labisoo ghaira saa'ah; kazaalika kaanoo yu'fakoon"}, {"id": 196612, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3465, "text": "Wa qaalal lazeena ootul 'ilma wal eemaana laqad labistum fee kitaabil laahi ilaa yawmil ba'si fahaazaa yawmul ba'si wa laakinnakum kuntum laa ta'lamoon"}, {"id": 196613, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3466, "text": "Fa Yawma'izil laa yanfa'ul lazeena zalamoo ma'ziratu hum wa laa hum yusta'taboon"}, {"id": 196614, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3467, "text": "Wa laqad darabnaa linnaasi fee haazal Quraani min kulli masal; wa la'in ji'tahum bi aayatil la yaqoolannal lazeena kafaroo in antum illaa mubtiloon"}, {"id": 196615, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3468, "text": "Kazaalika yatba'ul laahu 'alaa quloobil lazeena laa ya'lamoon"}, {"id": 196616, "identifier": "en.transliteration", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 3469, "text": "Fasbir inna wa'dal laahi haqqunw wa laa yastakhif fannakal lazeena laa yooqinoon"}]