[{"id": 197731, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4584, "text": "Innaa fatahnaa laka Fatham Mubeenaa"}, {"id": 197732, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4585, "text": "Liyaghfira lakal laahu maa taqaddama min zambika wa maa ta akhkhara wa yutimma ni'matahoo 'alaika wa yahdiyaka siraatam mustaqeema"}, {"id": 197733, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4586, "text": "Wa yansurakal laahu nasran 'azeezaa"}, {"id": 197734, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4587, "text": "Huwal lazeee anzalas sakeenata fee quloobil mu'mineena liyazdaadooo eemaanamma'a eemaanihim; wa lillaahi junoodus samawaati wal ard; wa kaanal laahu 'Aleeman Hakeemaa"}, {"id": 197735, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4588, "text": "Liyudkhilal mu'mineena walmu'minaati jannaatin tajree min tahtihal anhaaru khaalideena feehaa wa yukaffira 'anhum saiyi aatihim; wa kaana zaalika 'indal laahi fawzan 'azeemaa"}, {"id": 197736, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4589, "text": "Wa yu'azzibal munaafiqeena walmunaafiqaati wal mushrikeena walmushrikaatiz zaaanneena billaahi zannas saw'; 'alaihim daaa'iratus saw'i wa ghadibal laahu 'alaihim wa la'anahum wa a'adda lahum jahannama wa saaa' at maseeraa"}, {"id": 197711, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4590, "text": "Wa lillaahi junoodus samaawaati wal ard; wa kaanal laahu 'azeezan hakeema"}, {"id": 197737, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4591, "text": "Innaaa arsalnaaka shaahi danw wa mubashshiranw wa nazeera"}, {"id": 197738, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4592, "text": "Litu minoo billaahi wa Rasoolihee wa tu'azziroohu watuwaqqiroohu watusabbi hoohu bukratanw wa aseelaa"}, {"id": 197739, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 512, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4593, "text": "Innal lazeena yubaayi'oonaka innamaa yubaayi'oonal laaha yadul laahi fawqa aydehim; faman nakasa fainnamaa yuankusu 'alaa nafsihee wa man awfaa bimaa 'aahada 'alihul laaha fasa yu'teehi ajran 'azeemaa"}, {"id": 197740, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 512, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4594, "text": "Sa yaqoolu lakal mukhal lafoona minal-A'raabi shaighalatnaaa amwaalunaa wa ahloonaa fastaghfir lanaa; yaqooloona bi alsinatihim maa laisa fee quloobihim; qul famany yamliku lakum minal laahi shai'an in araada bikum darran aw araada bikum naf'aa; bal kaanal laahu bimaa ta'maloona Khabeeraa"}, {"id": 197741, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 512, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4595, "text": "Bal zanantum al lany yanqalibar Rasoolu walmu'minoona ilaaa ahleehim abadanw wa zuyyina zaalika fee quloobikum wa zanantum zannnas saw'i wa kuntum qawmam booraa"}, {"id": 197742, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 512, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4596, "text": "Wa mal lam yu'mim billaahi wa Rasoolihee fainnaaa a'tadnaa lilkaafireena sa'eeraa"}, {"id": 197743, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 512, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4597, "text": "Wa lillaahii mulkus samaawaati wal ard; yaghfiru limany yashaaa'u wa yu'azzibu many yashaaa'; wa kaanal laahu Ghafoorar Raheemaa"}, {"id": 197744, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 512, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4598, "text": "Sa yaqoolul mukhalla foona izan talaqtum ilaa maghaanima litaakhuzoohaa zaroonaa nattabi'kum yureedoona any yubaddiloo Kalaamallaah; qul lan tattabi'oonaa kazaalikum qaalal laahu min qablu fasa yaqooloona bal tahsudoonanna; bal kaanoo laa yafqahoona illaa qaleela"}, {"id": 197745, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4599, "text": "Qul lilmukhallafeena minal A'raabi satud'awna ilaa qawmin ulee baasin shadeedin tuqaati loonahum aw yuslimoona fa in tutee'oo yu'tikumul laahu ajran hasananw wa in tatawallaw kamaa tawallaitum min qablu yu'azzibkum 'azaaban aleemaa"}, {"id": 197746, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 447, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4600, "text": "Laisa 'alal a'maa harajunw wa laa 'alal a'raji harajunw wa laa 'alal mareedi haraj' wa many yutil'il laaha wa Rasoolahoo yudkhilhu jannaatin tajree min tahtihal anhaaru wa many yatawalla yu'azzibhu 'azaaban aleemaa"}, {"id": 197747, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4601, "text": "Laqad radiyal laahu 'anil mu'mineena iz yubaayi 'oonaka tahtash shajarati fa'alima maa fee quloobihim fa anzalas sakeenata 'alaihim wa asaa bahum fat han qareebaa"}, {"id": 197748, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4602, "text": "Wa maghaanima kaseera tany yaakhuzoonahaa; wa kaanal laahu 'Azeezan Hakeemaa"}, {"id": 197749, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4603, "text": "Wa'adakumul laahu ma ghaanima kaseeratan taakhuzoo nahaa fa'ajjala lakum haazihee wa kaffa aydiyan naasi 'ankum wa litakoona aayatal lilmu'mineena wa yahdiyakum siraatam mustaqeema"}, {"id": 197750, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4604, "text": "Wa ukhraa lam taqdiroo 'alaihaa qad ahaatal laahu bihaa; wa kaanal laahu 'alaa kulli shai'in qadeera"}, {"id": 197751, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4605, "text": "Wa law qaatalakumul lazeena kafaroo la wallawul adbaara summa laa yajidoona waliyanw-wa laa naseeraa"}, {"id": 197752, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 513, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4606, "text": "Sunnatal laahil latee qad khalat min qablu wa lan tajida lisunnatil laahi tabdeelaa"}, {"id": 197753, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 514, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4607, "text": "Wa Huwal lazee kaffa aydiyahum 'ankum wa aydiyakum 'anhum bibatni Makkata mim ba'di an azfarakum 'alaihim; wa kaanal laahu bimaa ta'maloona Baseera"}, {"id": 197754, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 514, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4608, "text": "Humul lazeena kafaroo wa saddookum 'anil-Masjidil-Haraami walhadya ma'koofan any yablugha mahillah; wa law laa rijaalum mu'minoona wa nisaaa'um mu'minaatul lam ta'lamoohum an tata'oohum fatuseebakum minhum ma'arratum bighairi 'ilmin liyud khilal laahu fee rahmatihee many yashaaa'; law tazayyaloo la'azzabnal lazeena kafaroo minhum 'azaaban aleema"}, {"id": 197755, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 514, "ruku": 448, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4609, "text": "Iz ja'alal lazeena kafaroo fee quloobihimul hamiyyata hamiyyatal jaahiliyyati fa anzalal laahu sakeenatahoo 'alaa Rasoolihee wa 'alal mu mineena wa alzamahum kalimatat taqwaa wa kaanooo ahaqqa bihaa wa ahlahaa; wa kaanal laahu bikulli shai'in Aleema"}, {"id": 197756, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 514, "ruku": 449, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4610, "text": "Laqad sadaqal laahu Rasoolahur ru'yaa bilhaqq, latadkhulunnal Masjidal-Haraama in shaaa'al laahu aamineena muhalliqeena ru'oosakum wa muqassireena laa takhaafoona fa'alima maa lam ta'lamoo faja'ala min dooni zaalika fathan qareebaa"}, {"id": 197759, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 514, "ruku": 449, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4611, "text": "Huwal lazeee arsala Rasoolahoo bilhudaa wa deenil haqqi liyuzhirahoo 'alad deeni kullih; wa kafaa billaahi Shaheeda"}, {"id": 197760, "identifier": "en.transliteration", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 449, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4612, "text": "Muhammadur Rasoolul laah; wallazeena ma'ahooo ashiddaaa'u 'alal kuffaaari ruhamaaa'u bainahum taraahum rukka'an sujjadany yabtaghoona fadlam minal laahi wa ridwaanan seemaahum fee wujoohihim min asaris sujood; zaalika masaluhum fit tawraah; wa masaluhum fil Injeeli kazar'in akhraja shat 'ahoo fa 'aazarahoo fastaghlaza fastawaa 'alaa sooqihee yu'jibuz zurraa'a liyagheeza bihimul kuffaar; wa'adal laahul lazeena aamanoo wa 'amilus saalihaati minhum maghfiratanw wa ajran 'azeemaa"}]