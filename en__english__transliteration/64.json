[{"id": 198343, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5200, "text": "Yusabbihu lillaahi maa fis samaawaati wa maa fil ardi lahul mulku wa lahul hamd, wa Huwa 'alaa kulli shai 'in Qadeer"}, {"id": 198344, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5201, "text": "Huwal lazee khalaqakum faminkum kaafirunw wa min kum mu'min ; wallaahu bimaa ta'maloona Baseer"}, {"id": 198345, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5202, "text": "Khalaqas samaawaati wal arda bilhaqqi wa sawwarakum fa ahsana suwarakum wa ilaihil maseer"}, {"id": 198346, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5203, "text": "Ya'lamu maa fis samaawaati wal ardi wa ya'lamu maa tusirroona wa maa tu'linoon; wallaahu 'Aleemum bizaatis sudoor"}, {"id": 198347, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5204, "text": "Alam yaatikum naba'ul lazeena kafaroo min qablu fazaaqoo wabaala amrihim wa lahum 'azaabun aleem"}, {"id": 198348, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5205, "text": "Zaalika bi annahoo kaanat taateehim Rusuluhum bilbaiyinaati faqaaloo a basharuny yahdoonanaa fakafaroo wa tawallaw; wastaghnal laah; wallaahu ghaniyyun hameed"}, {"id": 198349, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5206, "text": "Za'amal lazeena kafarooo al-lany yub'asoo; qul balaa wa rabbee latub'asunna summa latunabba'unna bimaa 'amiltum; wa zaalika 'alal laahi yaseer"}, {"id": 198350, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5207, "text": "Fa-aaminoo billaahi wa rasoolihee wannooril lazeee anzalnaa; wallaahu bima ta'maloona khabeer"}, {"id": 198351, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5208, "text": "Ywma yajma'ukum li yawmil jam'i zaalika yawmut taghaabun; wa many-yumim billaahi wa ya'mal saalihany yukaffir 'anhu sayyi aatihee wa yudkhilhu jannaatin tajree min tahtihal anhaaru khaalideena feehaaa abadaa; zaalikal fawzul 'azeem"}, {"id": 198352, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5209, "text": "Wallazeena kafaroo wa kazzaboo bi aayaaatinaaa ulaaa'ika ashaabun naari khaalideena feehaa bi'sal maseer"}, {"id": 198353, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5210, "text": "Maaa asaaba mim musee batin illaa bi-iznil laah; wa many yu'mim billaahi yahdi qalbah; wallaahu bikulli shai;in Aleem"}, {"id": 198354, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5211, "text": "Wa atee'ul laaha wa atee'ur Rasool; fa in tawallaitum fa innamaa 'alaa Rasoolinal balaaghul mubeen"}, {"id": 198355, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5212, "text": "Allaahu laaa ilaaha illaa Hoo; wa 'alal laahi falyata wakkalil mu'minoon"}, {"id": 198356, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5213, "text": "Yaaa ayyuhal lazeena aamanooo inna min azwaaji kum wa awlaadikum 'aduwwal lakum fahzaroohum; wa in ta'foo wa tasfahoo wa taghfiroo fa innal laaha ghafoorur Raheem"}, {"id": 198357, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5214, "text": "Innamaa amwaalukum wa awlaadukum fitnah; wallaahu 'indahooo ajrun 'azeem"}, {"id": 198358, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5215, "text": "Fattaqul laaha mastat'tum wasma'oo wa atee'oo waanfiqoo khairal li anfusikum; wa many-yooqa shuha nafsihee fa-ulaaa'ika humul muflihoon"}, {"id": 198359, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5216, "text": "In tuqridul laaha qardan hasanany yudd'ifhu lakum wa yaghfir lakum; wallaahu Shakoorun Haleem"}, {"id": 198360, "identifier": "en.transliteration", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5217, "text": "'Aalimul-Ghaibi wash-shahaadatil 'Azeezul Hakeem"}]