[{"id": 195931, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2792, "text": "Sooratun anzalnaahaa wa faradnaahaa wa anzalnaa feehaaa Aayaatim baiyinaatil la'allakum tazakkaroon"}, {"id": 195932, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2793, "text": "Azzaaniyatu wazzaanee fajlidoo kulla waahidim minhumaa mi'ata jaldatinw wa laa taakhuzkum bihimaa raafatun fee deenil laahi in kuntum tu'minoona billaahi wal Yawmil Aakhiri wal yashhad 'azaabahumaa taaa'ifatum minal mu'mineen"}, {"id": 195933, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2794, "text": "Azzaanee laa yankihu illaa zaaniyatan aw mushrikatanw wazzaaniyatu laa yankihuhaaa illaa zaanin aw mushrik; wa hurrima zaalika 'alal mu'mineen"}, {"id": 195934, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2795, "text": "Wallazeena yarmoonal muhsanaati summa lam yaatoo bi-arba'ati shuhadaaa'a fajlidoohum samaaneena jaldatanw wa laa taqbaloo lahum shahaadatan abadaa; wa ulaaa'ika humul faasiqoon"}, {"id": 195935, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2796, "text": "Illal lazeena taaboo mim ba'di zaalika wa aslahoo fa innal laaha Ghafoorur Raheem"}, {"id": 195936, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2797, "text": "Wallazeena yarmoona azwaajahum wa lam yakul lahum shuhadaaa'u illaaa anfusuhum fashahaadatu ahadihim arb'u shahaadaatim billaahi innahoo laminas saadiqeen"}, {"id": 195937, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2798, "text": "Wal khaamisatu anna la'natal laahi 'alaihi in kaana minal kaazibeen"}, {"id": 195938, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2799, "text": "Wa yadra'u anhal 'azaaba an tashhada arba'a shahaa daatim billaahi innahoo laminal kaazibeen"}, {"id": 195939, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2800, "text": "Wal khaamisata anna ghadabal laahi 'alaihaaa in kaana minas saadiqeen"}, {"id": 195940, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 350, "ruku": 301, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2801, "text": "Wa law laa fadlul laahi 'alaikum wa rahmatuhoo wa annal laaha Tawwaabun Hakeem"}, {"id": 195941, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2802, "text": "Innal lazeena jaaa'oo bilifki 'usbatum minkum; laa tahsaboohu sharral lakum bal huwa khairul lakum; likul limri'im minhum mak tasaba minal-ism; wallazee tawallaa kibrahoo minhum lahoo 'azaabun 'azeem"}, {"id": 195942, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2803, "text": "Law laaa iz sami'tumoohu zannal mu'minoona walmu'minaatu bi anfusihim khairanw wa qaaloo haazaaa ifkum mmubeen"}, {"id": 195943, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2804, "text": "Law laa jaaa'oo 'alaihi bi-arba'ati shuhadaaa'; fa iz lam yaatoo bishshuhadaaa'i fa ulaaa 'ika 'indal laahi humul kaaziboon"}, {"id": 195944, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2805, "text": "Wa law laa fadlul laahi 'alaikum wa rahmatuhoo fiddunyaa wal aakhirati lamassakum fee maaa afadtum feehi 'azaabun 'azeem"}, {"id": 195945, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2806, "text": "iz talaqqawnahoo bi alsinatikum wa taqooloona bi afwaahikum maa laisa lakum bihee 'ilmunw wa tahsaboo nahoo haiyinanw wa huwa 'indl laahi 'azeem"}, {"id": 195946, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2807, "text": "Wa law laaa iz sami'tu moohu qultum maa yakoonu lanaaa an natakallama bihaazaa Subhaanaka haaza buhtaanun 'azeem"}, {"id": 195947, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2808, "text": "Ya'izukumul laahu an ta'oodoo limisliheee abadan in kuntum mu'mineen"}, {"id": 195948, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2809, "text": "Wa yubaiyinul laahu lakumul Aayaat; wallaahu 'Aleemun Hakeem"}, {"id": 195949, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2810, "text": "Innal lazeena yuhibboona an tashee'al faahishatu fil lazeena aamanoo lahum 'azaabun aleemun fid dunyaa wal Aakhirah; wallaahu ya'lamu wa antum laa ta'lamoon"}, {"id": 195950, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 351, "ruku": 302, "hizb_quarter": 140, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2811, "text": "Wa law laa fadlul laahi 'alaikum wa rahmatuhoo wa annal laaha Ra'oofur Raheem"}, {"id": 195951, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 303, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2812, "text": "Yaaa aiyuhal lazeena aamanoo laa tattabi'oo khutuwaatish Shaitaan; wa many-yattabi' khutuwaatish Shaitaani fa innahoo yaamuru bilfahshaaa'i walmunkar; wa law laa fadlul laahi 'alaikum wa rahmatuhoo maa zakaa minkum min ahadin abadanw wa laakinnal laaha yuzakkee many-yashaaa'; wallaahu Samee'un 'Aleem"}, {"id": 195952, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 303, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2813, "text": "Wa laa yaatali ulul fadli minkum wassa'ati ai yu'tooo ulil qurbaa walmasaakeena walmuhaajireena fee sabeelillaahi walya'foo walyasfahoo; alaa tuhibboona ai yaghfiral laahu lakum; wal laahu Ghafoorur Raheem"}, {"id": 195953, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 303, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2814, "text": "Innal lazeena yarmoonal muhsanaatil ghaafilaatil mu'minaati lu'inoo fid dunyaa wal Aakhirati wa lahum 'azaabun 'azeem"}, {"id": 195954, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 303, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2815, "text": "Yawma tashhhadu 'alaihim alsinatuhum wa aideehim wa arjuluhum bimaa kaanoo ya'maloon"}, {"id": 195955, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 303, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2816, "text": "Yawma'iziny yuwaf feehimul laahu deenahumul haqqa wa ya'lamoona annal laaha Huwal Haqqul Mubeen"}, {"id": 195956, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 303, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2817, "text": "Alkhabeesaatu lilkha beeseena walkhabeesoona lilkhabeesaati wattaiyibaatu littaiyibeena wattaiyiboona littaiyibaat; ulaaa'ika mubar ra'oona maimma yaqooloona lahum maghfiratunw wa rizqun kareem"}, {"id": 195957, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 352, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2818, "text": "Yaaa aiyuhal lazeena aamanoo laa tadkhuloo buyootan ghaira buyootikum hatta tastaanisoo wa tusallimoo 'allaa ahlihaa; zaalikum khairul lakum la'allakum tazakkaroon"}, {"id": 195958, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 353, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2819, "text": "Fa il lam tajidoo feehaaa ahadan falaa tadkhuloohaa hattaa yu'zana lakum wa in qeela lakumurji'oo farji'oo huwa azkaa lakum; wallaahu bimaa ta'maloona 'Aleem"}, {"id": 195959, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 353, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2820, "text": "Laisa 'alaikum junaahun ann tadkhuloo buyootan ghaira maskoonatin feeha mataa'ul lakum; wallaahu ya'lamu maa tubdoona wa maa taktumoon"}, {"id": 195960, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 353, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2821, "text": "Qul lilmu' mineena yaghuuddoo min absaarihim wa yahfazoo furoojahum; zaalika azkaa lahum; innallaaha khabeerum bimaa yasna'oon"}, {"id": 195961, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 353, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2822, "text": "Wa qul lilmu'minaati yaghdudna min absaarihinna wa yahfazna furoojahunna wa laa yubdeena zeenatahunna illaa maa zahara minhaa walyadribna bikhumurihinna 'alaa juyoobihinna wa laa yubdeena zeenatahunna illaa libu'oolatihinna aw aabaaa'i hinna aw aabaaa'i bu'oolati hinna aw abnaaa'ihinaa aw abnaaa'i bu'oolatihinnna aw ikhwaanihinnna aw baneee ikhwaanihinna aw banee akhawaatihinna aw nisaaa'i hinna aw maa malakat aimaanuhunna awit taabi'eena ghairi ilil irbati minar rijaali awit tiflillazeena lam yazharoo 'alaa 'awraatin nisaaa'i wala yadribnna bi arjulihinna min zeenatihinn; wa toobooo ilallaahi jammee'an aiyuhal mu'minoona la'allakum tuflihoon"}, {"id": 195962, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 354, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2823, "text": "Wa ankihul ayaamaa minkum was saaliheena min 'ibaadikum wa imaa'kum; iny-yakoonoo fuqaraaa'a yughni himul laahu min fadlih; wal laahu Waasi'un 'Aleem"}, {"id": 195963, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 354, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2824, "text": "Wal yasta'fifil lazeena laa yajidoona nikaahan hata yughniyahumul laahu mi fadlih; wallazeena yabtaghoonal kitaaba mimmaa malakat aimaanukum fakaatiboohum in 'alimtum feehim khairanw wa aatoohum mimmaalil laahil lazeee aataakum; wa laa tukrihoo fatayaatikum 'alal bighaaa'i in aradna tahassunal litabtaghoo 'aradal hayaatid dunyaa; wa mai yukrihhunna fa innal laaha mim ba'di ikraahihinna Ghafoor Raheem"}, {"id": 195964, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 354, "ruku": 304, "hizb_quarter": 141, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2825, "text": "Wa laqad anzalnaaa ilaikum Aayaatim mubaiyinaatinw wa masalam minnal lazeena khalaw min qablikum wa maw'izatal lilmuttaqeen"}, {"id": 195966, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 354, "ruku": 305, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2826, "text": "Allaahu noorus samaawaati wal ard; masalu noorihee kamishkaatin feehaa misbaah; almisbaahu fee zujaajatin azzujaajatu ka annahaa kawkabun durriyyuny yooqadu min shajaratim mubaarakatin zaitoonatil laa shariqiyyatinw wa laa gharbiyyatiny yakaadu zaituhaa yudeee'u wa law alm tamsashu naar; noorun 'alaa noor; yahdil laahu linoorihee mai yashaaa'; wa yadribul laahul amsaala linnaas; wallaahu bikulli shai'in Aleem"}, {"id": 195967, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 354, "ruku": 305, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2827, "text": "Fee buyootin azinal laahu an turfa'a wa yuzkara feehasmuhoo yusabbihu lahoo feehaa bilghuduwwi wal aasaal"}, {"id": 195968, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 305, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2828, "text": "Rinjaalul laa tulheehim tijaaratunw wa laa bai'un 'an zikril laahi wa iqaamis Salaati wa eetaaa'iz Zakaati yakkhaafoona Yawman tataqallabu feehil quloobu wal absaar"}, {"id": 195969, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 305, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2829, "text": "Liyajziyahumul laahu ahsana maa 'amiloo wa yazeedahum min fadlih; wal laahu yarzuqu mai yashaaa'u bighairi hisaab"}, {"id": 195971, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 305, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2830, "text": "Wallazeena kafarooo a'maaluhum kasaraabim biqee'atiny yahsabuhuz zamaanu maaa'an hattaaa izaa jaaa'ahoo lam yajid hu shai'anw wa wajadal laaha 'indahoo fa waffaahu hisaabah; wallaahu saree'ul hisaab"}, {"id": 195972, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 305, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2831, "text": "Aw kazulumaatin fee bahril lujjiyyiny yaghshaahu mawjum min fawqihee mawjum min fawqihee mawjum min fawqihee sahaab; zulumatum ba'duhaa fawqa ba'din izaaa akhraja yadahoo lam yakad yaraahaa wa mal lam yaj'alil laahu lahoo noora famaa lahoo min noor"}, {"id": 195973, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2832, "text": "Alam tara annal laaha yusabbihu lahoo man fissamaawaati wal ardi wat tairu saaaffaatim kullun qad 'alima Salaatahoo wa tasbeehah; wallaahu 'aleemum bimaa yaf'aloon"}, {"id": 195974, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2833, "text": "Wa lillaahi mulkus samaawaati wal ardi wa ilal laahil maseer"}, {"id": 195975, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 355, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2834, "text": "Alam tara annal laaha yuzjee sahaaban summa yu'allifu bainahoo summa yaj'aluhoo rukaaman fataral wadqa yakhruju min khilaalihee wa yunazzilu minas samaaa'i min jibaalin feehaa mim barain fa yuseebu bihee mai yashaaa'u wa yasrifuhoo 'am mai yashaaa'u yakkaadu sanaa barqihee yazhabu bil absaar"}, {"id": 195976, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2835, "text": "Yuqallibul laahul laila wannahaar; inna fee zaalika la'ibratal li ulil absaar"}, {"id": 195977, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2836, "text": "Wallaahu khalaqa kulla daaabbatim mim maaa'in faminhum mai yamshee 'alaa batnihee wa minhum mai yamshee 'alaa rijlaine wa minhum mai yamshee 'alaaa arba'; yakhluqul laahu maa yashaaa'; innal laaha 'alaa kulli shai'in Qadeer"}, {"id": 195978, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2837, "text": "Laqad anzalnaaa Aayaatim mubaiyinaat; wallaahu yahdee mai yashaaa'u ilaa Siraatim Mustaqeem"}, {"id": 195979, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2838, "text": "Wa yaqooloona aamannaa billaahi wa bir Rasooli wa ata'naa summa yatawallaa fareequm minhum mim ba'di zaalik; wa maaa ulaaa'ika bilmu'mineen"}, {"id": 195980, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2839, "text": "Wa izaa du'ooo ilal laahi wa Rasoolihee li yahkuma bainahum izaa fareequm minhum mu'ridoon"}, {"id": 195981, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2840, "text": "Wa iny-yakul lahumul haqqu yaatooo ilaihi muz'ineen"}, {"id": 195982, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 306, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2841, "text": "Afee quloobihim ma radun amirtaabooo am yakhaafoona ani yaheefallaahu 'alaihim wa Rasooluh; bal ulaaa'ika humuz zaalimoon"}, {"id": 195983, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 307, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2842, "text": "Innamaa kaana qawlal mu'mineena izaa du'ooo ilal laahi wa Rasoolihee li yahkuma bainahum ai yaqooloo sami'naa wa ata'naa; wa ulaaa'ika humul muflihoon"}, {"id": 195984, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 307, "hizb_quarter": 142, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2843, "text": "Wa mai yuti'il laaha wa Rasoolahoo wa yakhshal laaha wa yattaqhi fa ulaaa'ika humul faaa'izoon"}, {"id": 195985, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 356, "ruku": 307, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2844, "text": "Wa aqsamoo billaahi jahda aimaanihim la'in amartahum la yakhrujunna qul laa tuqsimoo taa'atum ma'roofah innal laaha khabeerum bimaa ta'maloon"}, {"id": 195986, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 357, "ruku": 307, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2845, "text": "Qul atee'ul laaha wa atee'ur Rasoola fa in tawallaw fa innamaa 'alaihi maa hummila wa 'alaikum maa hummiltum wa in tutee'oohu tahtadoo; wa maa'alar Rasooli illal balaaghul mubeen"}, {"id": 195987, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 357, "ruku": 307, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2846, "text": "Wa'adal laahul lazeena aamanoo minkum wa 'amilus saalihaati la yastakhlifan nahum fil ardi kamastakh lafal lazeena min qablihim wa la yumakkinanna lahum deenahumul lazir tadaa lahum wa la yubaddilannahum mim ba'di khawfihim amnaa; ya'budoonanee laayushrikoona bee shai'aa; wa man kafara ba'da zaalika fa ulaaa'ika humul faasiqoon"}, {"id": 195988, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 357, "ruku": 307, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2847, "text": "Wa aqeemus Salaata wa aatuz Zakaata wa atee'ur Rasoola la'allakum turhamoon"}, {"id": 195965, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 357, "ruku": 307, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2848, "text": "Laa tahsabannal lazeena kafaroo mu'jizeena fil ard; wa maawaahumun Naaru wa labi'sal maseer"}, {"id": 195989, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 357, "ruku": 308, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2849, "text": "Yaaa aiyuhal lazeena aamanoo li yastaazinkumul lazeena malakat aimaanukum wallazeena lam yablughul huluma minkum salaasa marraat; min qabli Salaatil Fajri wa heena tada'oona siyaa bakum minaz zaheerati wa mim ba'di Salaatil Ishaaa'; salaasu 'awraatil lakum; laisa 'alaikum wa laa 'alaihim junaahum ba'dahunn; tawwaafoona 'alaikum ba'dukum 'alaa ba'd; kazaalika yubaiyinul laahu lakumul aayaat wallaahu 'Aleemun Hakeem"}, {"id": 195990, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 358, "ruku": 308, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2850, "text": "Wa izaa balaghal atfaalu minkumul huluma fal yastaazinoo kamas taazanal lazeena min qablihim; kazaalika yubaiyinul laahu lakum Aayaatih; wallaahu 'Aleemun Hakeem"}, {"id": 195991, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 358, "ruku": 308, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2851, "text": "Walqawaa'idu minan nisaaa'il laatee laa yarjoona nikaahan falisa 'alaihinna junaahun ai yada'na siyaabahunna ghaira mutabar rijaatim bizeenah; wa ai yasta'fifna khairul lahunn; wallaahu Samee'un 'Aleem"}, {"id": 195993, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 358, "ruku": 308, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2852, "text": "Laisa 'alal a'maa barajunw wa laa 'alal a'raji barajunw wa laa 'alal mareedi barajun wa laa 'alaa anfusikum an taakuloo mim buyootikum aw buyooti aabaaa'ikum aw buyooti ummahaatikum aw buyooti ikhwaanikum aw buyooti akhawaatikum aw buyooti a'maamikum aw buyooti 'ammaatikum aw buyooti akhwaalikum aw buyooti khaalaatikum aw maa malaktum mafaatihahooo aw sadeeqikum; laisa 'alaikum junaahun an taakuloo jamee'an aw ashtaata; fa izaa dakhaltum buyootan fasallimoo 'alaaa anfusikum tahiyyatam min 'indil laahi mubaarakatan taiyibah; kazaalika yubai yinul laahu lakumul Aayaati la'allakum ta'qiloon"}, {"id": 195994, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 359, "ruku": 309, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2853, "text": "Innamal mu'minoonal lazeena aamanoo billaahi wa Rasoolihee wa izaa kaanoo ma'ahoo 'alaaa amrin jaami'il lam yazhaboo hataa yastaazinooh; innal lazeena yastaa zinookana ulaaa'ikal lazeena yu'minoona billaahi wa Rasoolih; fa izas taazanooka liba'di shaanihim faazal liman shi'ta minhum wastaghfir lahumul laah; innal laaha Gahfoor Raheem"}, {"id": 195995, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 359, "ruku": 309, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2854, "text": "La taj'aloo du'aaa'ar Rasooli bainakum kadu'aaa'i badikum ba'daa; qad ya'lamul laahul lazeena yatasallaloona minkum liwaazaa; fal yahzaril lazeena yukhaalifoona 'an amriheee an tuseebahum fitnatun aw yuseebahum 'azaabun aleem"}, {"id": 195996, "identifier": "en.transliteration", "surah_number": 24, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0648\u0631", "surah_en_name": "An-Noor", "surah_en_name_translation": "The Light", "revelation_type": "Medinan", "juz": 18, "manzil": 4, "page": 359, "ruku": 309, "hizb_quarter": 143, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 2855, "text": "Alaaa inna lillaahi maa fis samaawaati wal ardi qad ya'lamu maaa antum 'alaihi wa Yawma yurja'oona ilaihi fa yunaabi 'uhum bimaa 'amiloo; wallaahu bikulli shai'in 'Aleem"}]