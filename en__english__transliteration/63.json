[{"id": 198333, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 488, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5189, "text": "Izaa jaaa'akal munaafiqoona qaaloo nashhadu innaka la rasoolul laah; wallaahu ya'lamu innaka la rasooluhoo wallaahu yashhadu innal munaafiqeena lakaaziboon"}, {"id": 198334, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 488, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5190, "text": "Ittakhazoo aymaanahum junnatan fasaddoo 'an sabeelil laah; innahum saaa'a maa kaanoo ya'maloon"}, {"id": 198335, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 488, "hizb_quarter": 221, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5191, "text": "Zaalika bi annahum aamanoo summa kafaroo fatubi'a 'alaa quloobihim fahum laa yafqahoon"}, {"id": 198336, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 554, "ruku": 488, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5192, "text": "Wa izaa ra aytahum tu'jibuka ajsaamuhum wa iny yaqooloo tasma' liqawlihim kaannahum khushubum musannadah; yahsaboona kulla saihatin 'alaihim; humul 'aduwwu fahzarhum; qaatalahumul laahu annaa yu'fakoon"}, {"id": 198337, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 488, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5193, "text": "Wa izaa qeela lahum ta'aalaw yastaghfir lakum rasoolul laahi lawwaw ru'oo sahum wa ra aytahum yasuddoona wa hum mustakbiroon"}, {"id": 198338, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 488, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5194, "text": "Sawaaa'un 'alaihim as taghfarta lahum am lam tastaghfir lahum lany yaghfiral laahu lahum; innal laaha laa yahdil qawmal faasiqeen"}, {"id": 198339, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 488, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5195, "text": "Humul lazeena yaqooloona laa tunfiqoo 'alaa man inda Rasoolil laahi hatta yanfaddoo; wa lillaahi khazaaa' inus samaawaati wal ardi wa laakinnal munaafiqeena la yafqahoon"}, {"id": 198340, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 488, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5196, "text": "Yaqooloona la'ir raja'naaa ilal madeenati la yukhrijanal a'azzu minhal azall; wa lillaahil 'izzatu wa li Rasoolihee wa lilmu'mineena wa laakinnal munaafiqeena laa ya'lamoon"}, {"id": 198341, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 489, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5197, "text": "Yaaa ayyuhal lazeena aamanoo la tulhikum amwaalukum wa laa awlaadukum 'anzikril laah; wa mai-yaf'al zaalika fa-ulaaa'ika humul khaasiroon"}, {"id": 198342, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 489, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5198, "text": "Wa anifqoo mim maa razaqnaakum min qabli any-yaatiya ahadakumul mawtu fa yaqoola rabbi law laaa akhkhartaneee ilaaa ajalin qareebin fa assaddaqa wa akum minassaaliheen"}, {"id": 198320, "identifier": "en.transliteration", "surah_number": 63, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0646\u0627\u0641\u0642\u0648\u0646", "surah_en_name": "Al-Munaafiqoon", "surah_en_name_translation": "The Hypocrites", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 555, "ruku": 489, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5199, "text": "Wa lany yu 'akhkhiral laahu nafsan izaa jaaa'a ajaluhaa; wallaahu khabeerum bimaa ta'maloon"}]