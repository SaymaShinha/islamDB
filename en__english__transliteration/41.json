[{"id": 197363, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4219, "text": "Haa Meeem"}, {"id": 197364, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4220, "text": "Tanzeelum Minar-Rahmaanir-Raheem"}, {"id": 197365, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4221, "text": "Kitaabun fussilat Aayaatuhoo Qur-aanan 'Arabiyyal liqawminy ya'lamoon"}, {"id": 197366, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4222, "text": "Basheeranw wa nazeeran fa-a'rada aksaruhum fahum laa yasma'oon"}, {"id": 197367, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4223, "text": "Wa qaaloo quloobunaa feee akinnatim mimmaa tad'oonaaa ilaihi wa feee aazaaninaa waqrunw wa mim baininaa wa bainika bijaabun fa'mal innanaa 'aamiloon"}, {"id": 197368, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4224, "text": "Qul innamaaa ana basharum mislukum yoohaaa ilaiya annamaaa ilaahukum Ilaahunw Waahidun fastaqeemooo ilaihi wastagfirooh; wa wailul lil mushrikeen"}, {"id": 197369, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4225, "text": "Allazeena laa yu'toonaz Zakaata wa hum bil-Aakhiratihum kaafiroon"}, {"id": 197370, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 413, "hizb_quarter": 190, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4226, "text": "Innal lazeena aamanoo wa 'amilus saalihaati lahum ajrun ghairu mamnoon"}, {"id": 197371, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4227, "text": "Qul a'innakum latakfuroona billazee khalaqal arda fee yawmaini wa taj'aloona lahooo andaadaa; zaalika Rabbul 'aalameen"}, {"id": 197372, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4228, "text": "Wa ja'ala feehaa rawaa siya min fawqihaa wa baaraka feehaa wa qaddara feehaaaa aqwaatahaa feee arba'ati ayyaamin sawaaa'al lissaaa'ileen"}, {"id": 197373, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 477, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4229, "text": "Summas tawaaa ilas-samaaa'i wa hiya dukhaanun faqaala lahaa wa lil ardi'tiyaaa taw'an aw karhan qaalataaa atainaa taaa'i'een"}, {"id": 197374, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4230, "text": "Faqadaahunna sab'a samaawaatin fee yawmaini wa awhaa fee kulli samaaa'in amarahaa; wa zaiyannassa maaa'ad dunyaa bimasaabeeha wa hifzaa; zaalika taqdeerul 'Azeezil 'Aleem"}, {"id": 197375, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4231, "text": "Fa-in a'radoo faqul anzartukum saa'iqatam misla saa'iqati 'Aadinw wa Samood"}, {"id": 197376, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4232, "text": "Iz jaaa'at humur Rusulu mim baini aydeehim wa min khalfihim allaa ta'budooo illal laaha qaaloo law shaaa'a Rabunaa la anzala malaaa 'ikatan fa innaa bimaaa ursiltum bihee kaafiroon"}, {"id": 197377, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4233, "text": "Fa ammaa 'Aadun fastak baroo fil ardi bighairul haqqi wa qaaloo man ashaddu minnaa quwwatan awalam yaraw annal laahal lazee khalaqahum Huwa ashaddu minhum quwwatanw wa kaanoo bi Aayaatinaa yajhadoon"}, {"id": 197378, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4234, "text": "Fa arsalnaa 'alaihim reehan sarsaran feee ayyaamin nahisaatil linuzeeqahum 'azaabal khizyi fil hayaatid dunyaa wa la'azaabul Aakhirati akhzaa wa hum laa yunsaroon"}, {"id": 197379, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4235, "text": "Wa ammaa Samoodu fahadinaahum fastahabbul 'ama 'alal huda fa akhazathum saa'iqatul 'azaabil hooni bimaa kaanoo yaksiboon"}, {"id": 197380, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 414, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4236, "text": "Wa najjainal lazeena aamanoo wa kaanoo yattaqoon"}, {"id": 197381, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4237, "text": "Wa yawma yuhsharu a'daaa'ul laahi ilan Naari fahum yooza'oon"}, {"id": 197382, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 478, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4238, "text": "Hattaaa izaa maa jaaa'oohaa shahida 'alaihim samu'uhum wa absaaruhum wa julooduhum bimaa kaanoo ya'maloon"}, {"id": 197383, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4239, "text": "Wa qaaloo lijuloodihim lima shahittum 'alainaa qaaloo antaqanal laahul lazeee antaqa kulla shai'inw wa Huwa khalaqakum awwala marratinw wa ilaihi turja'oon"}, {"id": 197384, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4240, "text": "Wa maa kuntum tastatiroona ai-yashhada 'alaikum sam'ukum wa laaa absaarukum wa laa juloodukum wa laakin zanantum annal laaha laa ya'lamu kaseeram mimmaa ta'maloon"}, {"id": 197385, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4241, "text": "Wa zaalikum zannukumul lazee zanantum bi-Rabbikum ardaakum fa asbahtum minal khaasireen"}, {"id": 197386, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 191, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4242, "text": "Fa-iny yasbiroo fan Naaru maswal lahum wa iny-yasta'tiboo famaa hum minal mu'tabeen"}, {"id": 197387, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 415, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4243, "text": "Wa qaiyadnaa lahum quranaaa'a fazaiyanoo lahum maa baina aideehim wa maa khalfahum wa haqqa 'alaihimul qawlu feee umamin qad khalat min qablihim minal jinni wal insi innahum kaanoo khaasireen"}, {"id": 197388, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4244, "text": "Wa qaalal lazeena kafaroo laa tasma'oo lihaazal Quraani walghaw feehi la'allakum taghlihoon"}, {"id": 197389, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4245, "text": "Falanuzeeqannal lazeena kafaroo 'azaaban shadeedanw wa lanajziyannahum aswallazee kaanoo ya'maloon"}, {"id": 197390, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4246, "text": "Zaalika jazaaa'u a'daaa'il laahin Naaru lahum feehaa daarul khuld, jazaaa'am bimaa kaanoo bi aayaatinaa yajhdoon"}, {"id": 197391, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 479, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4247, "text": "Wa qaalal lazeena kafaroo Rabbanaaa arinal lazaini adal laanaa minal jinni wal insi naj'alhumaa tahta aqdaaminaa liyakoonaa minal asfaleen"}, {"id": 197392, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4248, "text": "Innal lazeena qaaloo Rabbunal laahu summas taqaamoo tatanazzalu 'alaihimul malaaa 'ikatu allaa takhaafoo wa laa tahzanoo wa abshiroo bil Jannnatil latee kuntum too'adoon"}, {"id": 197393, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4249, "text": "Nahnu awliyaaa'ukum fil hayaatid dunyaa wa fil Aakhirati wa lakum feehaa maa tashtaheee anfusukum wa lakum feehaa ma tadda'oon"}, {"id": 197394, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 416, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4250, "text": "Nuzulam min Ghafoorir Raheem"}, {"id": 197395, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4251, "text": "Wa man ahsanu qawlam mimman da'aaa ilal laahi wa 'amila saalihanw wa qaala innanee minal muslimeen"}, {"id": 197396, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4252, "text": "Wa laa tastawil hasanatu wa las saiyi'ah; idfa' billatee hiya ahsanu fa'izal lazee bainaka wa bainahoo 'adaawatun ka'annahoo waliyun hameem"}, {"id": 197397, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4253, "text": "Wa maa yulaqqaahaaa illal lazeena sabaroo wa maa yulaqqaahaaa illaa zoo hazzin 'azeem"}, {"id": 197398, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4254, "text": "Wa immaa yanzaghannaka minash Shaitaani nazghun fasta'iz billaahi innahoo Huwas Samee'ul 'Aleem"}, {"id": 197399, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4255, "text": "Wa min Aayaatihil lailu wannahaaru washshamsu walqamar; laa tasjudoo lishshamsi wa laa lilqamari wasjudoo lillaahil lazee khala qahunna in kuntum iyyaahu ta'budoon"}, {"id": 197400, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 480, "ruku": 417, "hizb_quarter": 192, "sajda": true, "sajda_id": 12, "sajda_obligatory": true, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4256, "text": "Fa inis-takbaroo fallazee na 'inda Rabbika yusabbihoona lahoo billaili wannnahaari wa hum laa yas'amoon"}, {"id": 197401, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4257, "text": "Wa min Aayaatiheee annaka taral arda khaashi'atan fa izaaa anzalna 'alaihal maaa'ah tazzat wa rabat; innal lazeee ahyaahaa lamuhiyil mawtaa; innahoo 'alaa kulli shai-in Qadeer"}, {"id": 197402, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4258, "text": "Innal lazeena yulhidoona feee Aayaatina laa yakhfawna 'alainaa' afamai yulqaa fin Naari khayrun am mai yaateee aaminai Yawmal Qiyaamah; i'maloo ma shi'tum innahoo bimaa ta'maloona Baseer"}, {"id": 197403, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4259, "text": "Inna Lazeena kafaroo biz Zikri lammaa jaa'ahum wa innahoo la Kitaabun 'Azeez"}, {"id": 197404, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4260, "text": "Laa yaateehil baatilu mim baini yadaihi wa laa min khalfihee tanzeelum min Hakeemin Hameed"}, {"id": 197405, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4261, "text": "Maa yuqaalu laka illaa maa qad qeela lir Rusuli min qablik; inna Rabbaka lazoo maghfiratinw wa zoo 'iqaabin aleem"}, {"id": 197406, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 417, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4262, "text": "Wa law ja'alnaahu Qur-aanan A'jamiyyal laqaaloo law laa fussilat Aayaatuhoo 'a A'jamiyyunw wa 'Arabiyy; qul huwa lillazeena aamanoo hudanw wa shifaaa'unw wallazeena la yu'minoona feee aazaanihim waqrunw wa huwa 'alaihim 'amaa; ulaaa'ika yunaadawna mim maakaanim ba'eed"}, {"id": 197407, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4263, "text": "Wa laqad aatainaa Moosal Kitaaba fakhtulifa fee; wa lawlaa Kalimatun sabaqat mir Rabbika laqudiya bainahum; wa innahum lafee shakkim minhu mureeb"}, {"id": 197408, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 24, "manzil": 6, "page": 481, "ruku": 418, "hizb_quarter": 192, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4264, "text": "Man 'amila salihan falinafshihee wa man asaaa'a fa'alaihaa; wamaa rabbuka bizallaamil lil 'abeed"}, {"id": 197409, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4265, "text": "Ilaihi yuraddu 'ilmus Saaa'ah; wa maa takhruju min samaraatim min akmaamihaa wa maa tahmilu min unsaa wa laa tada'u illaa bi'ilmih; wa Yawma yunaadeehim aina shurakaaa'ee qaalooo aazannaaka maa minnaa min shaheed"}, {"id": 197410, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4266, "text": "Wa dalla 'anhum maa kaanoo yad'oona min qablu wa zannoo maa lahum mim mahees"}, {"id": 197411, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4267, "text": "Laa yas'amul insaanu min du'aaa'il khairi wa im massa hush sharru fa ya'oosun qanoot"}, {"id": 197412, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4268, "text": "Wa la in azaqnaahu rahmatam minnaa mim ba'di dar raaa'a massat hu la yaqoolanna haazaa lee wa maaa azunnus Saa'ata qaaa'imatanw wa la'in ruji'tu ilaa Rabbeee inna lee 'indahoo lalhusnaa; falanu nabbi'annal lazeena kafaroo bimaa 'amiloo wa lanuzeeqan nahum min 'azaabin ghaleez"}, {"id": 197413, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4269, "text": "Wa izaaa an'amnaa 'alal insaani a'rada wa na-aa bijaani bihee wa izaa massahush sharru fazoo du'aaa'in 'areed"}, {"id": 197414, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4270, "text": "Qul araiaitum in kaana min 'indil laahi summa kafar tum bihee man adallu mimman huwa fee shiqaqim ba'eed"}, {"id": 197415, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4271, "text": "Sanureehim Aayaatinaa fil aafaaqi wa feee anfusihim hattaa yatabaiyana lahum annahul haqq; awa lam yakfi bi Rabbika annahoo 'alaa kulli shai-in Shaheed"}, {"id": 197416, "identifier": "en.transliteration", "surah_number": 41, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0635\u0644\u062a", "surah_en_name": "Fussilat", "surah_en_name_translation": "Explained in detail", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 482, "ruku": 418, "hizb_quarter": 193, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 4272, "text": "Alaaa innahum fee miryatim mil liqaaa'i Rabbihim; alaaa innahoo bikulli shai'im muheet"}]