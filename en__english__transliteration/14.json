[{"id": 194898, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1751, "text": "Alif-Laaam-Raa; Kitaabun anzalnaahu ilaika litukhrijan-naasa minaz zulumaati ilan noori bi-izni Rabbihim ilaa siraatil 'Azeezil Hameed"}, {"id": 194899, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1752, "text": "Allaahil lazee lahoo maa fis samaawaati wa maa fill ard; wa wailul lilkaafireena min 'azaabin shadeed"}, {"id": 194900, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1753, "text": "Allazeena yastahibboo nal hayaatad dunyaa 'alal aakhirati wa yasuddoona 'ansabeelil laahi wa yabghoonahaa 'iwajaa; ulaaa 'ika fee dalaalim ba'eed"}, {"id": 194901, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1754, "text": "Wa maaa arsalnaa mir Rasoolin illaa bilisaani qawmihee liyubaiyina lahum faiudillul laahu mai yashaaa'u wa yahde mai yashaaa'; wa Huwal 'Azeezul Hakeem"}, {"id": 194902, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 255, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1755, "text": "Wa laqad arsalnaa Moosaa bi Aayaatinaa an akhrij qawmaka minaz zulumaati ilan noori wa zak kirhum bi ayyaamil laah; inna fee zaalika la aayaatil likulli sabbaarin shakoor"}, {"id": 194903, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 211, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1756, "text": "Wa iz qaala Moosaa liqawmihiz kuroo ni'matal laahi 'alaikum iz anjaakum min Aali Fir'awna yasoomoo nakum sooo'al 'azaabi wa yuzabbihoona abnaaa'akum wa yastahyoona nisaaa'akum; wa fee zaalikum balaaa'um mir Rabbikum 'azeem"}, {"id": 194904, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1757, "text": "Wa iz ta azzana Rabbukum la'in shakartum la azeedannakum wa la'in kafartum inn'azaabee lashadeed"}, {"id": 194905, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1758, "text": "Wa qaala Moosaaa in takfurooo antum wa man fil ardi jamee'an fa innal laaha la Ghaniyyun Hameed"}, {"id": 194906, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 102, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1759, "text": "Alam yaatikum naba'ul lazeena min qablikum qawmi Noohinw wa 'Aadinw wa Samood, wallazeena mim ba'dihim; laa ya'lamuhum illallaah; jaaa'at hum Rusuluhum bilbaiyinaati faraddooo aydiyahum feee afwaahihim wa qaalooo innaa kafarnaa bimaaa ursiltum bihee wa innaa lafee shakkim mimmaa tad'oonanaaa ilaihi mureeb"}, {"id": 194907, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 256, "ruku": 212, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1760, "text": "Qaalat Rusuluhum afillaahi shakkun faatiris samaawaati wal ardi yad'ookum liyaghfira lakum min zunoobikum wa yu'akhkhirakum ilaaa ajalim musam maa; qaaloo in antum illaa basharum mislunaa tureedoona an tasuddoonaa 'ammaa kaana ya'budu aabaaa'unaa faatoonaa bisul taanim mubeen"}, {"id": 194908, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 212, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1761, "text": "Qaalat lahum Rusuluhum in nahnu illaa basharum mislukum wa laakinnal laaha yamunnu 'alaa mai yashaaa'u min 'ibaadihee wa maa kaana lanaaa an naatiyakum bisul taanin illaa bi iznil laah; wa 'alal laahi falyatawakkalil mu'minonn"}, {"id": 194909, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 212, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1762, "text": "Wa maa lanaa allaa natawakkala 'alal laahi wa qad hadaanaa subulanaa; wa lanasbiranna 'alaa maaa aazaitumoonaa; wa 'alal laahi falyatawakkalil mutawakkiloon"}, {"id": 194910, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1763, "text": "Wa qaalal lazeena kafaroo li Rusulihim lanukhrijanna kum min aardinaaa aw lata'oo dunna fee millatinaa fa awhaaa ilaihim Rabbuhum lanuhlikannaz zalimeen"}, {"id": 194911, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1764, "text": "Wa lanuskinan nakumul arda mim ba'dihim; zaalika liman khaafa maqaamee wa khaafa wa'eed"}, {"id": 194912, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1765, "text": "Wastaftahoo wa khaaba kullu jabbaarin 'aneed"}, {"id": 194913, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1766, "text": "Minw waraaa'ihee jahannamu wa yusqaa mim maaa'in sadeed"}, {"id": 194914, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1767, "text": "Yatajarra'uhoo wa laa yakaadu yuseeghuhoo wa yaateehil mawtu min kulli makaaninw wa maa huwa bimaiyitinw wa minw waraaa'ihee 'azaabun ghaleez"}, {"id": 194915, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 257, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1768, "text": "Masalul lazeena kafaroo bi Rabbihim a'maaluhum karamaadinish taddat bihir reehu fee yawmin 'aasif; laa yaqdiroona mimmaa kasaboo 'alaa shai'; zaalika huwad dalaalul ba'eed"}, {"id": 194916, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1769, "text": "Alam tara annal laaha khalaqas samaawaati wal arda bilhaqq; iny yashaa yuzhibkum wa yaati bikhalqin jadeed"}, {"id": 194917, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1770, "text": "Wa maa zaalika 'alal laahi bi 'azeez"}, {"id": 194919, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 213, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1771, "text": "Wa barazoo lillaahi jamee'an faqaalad du'afaaa'u lillazeenas takbarooo innaa kunnaa lakum taba'an fahal antum mughnoona 'annaa min 'azaabil laahi min shai'; qaaloo law hadaanal laahu lahadai naakum sawaaa'un 'alainaaa ajazi'naa am sabarnaa maa lanaa mim mahees"}, {"id": 194920, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1772, "text": "Wa qaalash Shaitaanu lammaa qudiyal amru innal laaha wa'adakum wa'dal haqqi wa wa'attukum faakhlaftukum wa maa kaana liya 'alaikum min sultaanin illaaa an da'awtukum fastajabtum lee falaa taloomoonee wa loomooo anfusakum maaa ana bimusrikhikum wa maaa antum bimusrikhiyya innee kafartu bimaaa ashraktumooni min qabl; innaz zaalimeena lahum azaabun aleem"}, {"id": 194921, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1773, "text": "Wa udkhilal lazeena aamanoo wa 'amilus saalihaati Jannaatin tajree min tahtihal anhaaru khaalideena feehaa bi izni Rabbihim tahiyyatuhum feeha salaam"}, {"id": 194922, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 258, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1774, "text": "Alam tara kaifa darabal laahu masalan kalimatan taiyibatan kashajaratin taiyibatin asluhaa saabitunw wa far'uhaa fis samaaa'"}, {"id": 194923, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1775, "text": "Tu'teee ukulahaa kulla heenim bi izni Rabbihaa; wa yadribul laahul amsaala linnaasi la'allahum yatazak karoon"}, {"id": 194924, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1776, "text": "Wa masalu kalimatin khabeesatin kashajaratin khabee satinij tussat min fawqil ardi maa lahaa min qaraar"}, {"id": 194925, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 214, "hizb_quarter": 103, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1777, "text": "Yusabbitul laahul lazeena aamanoo bilqawlis saabiti fil hayaatid dunyaa wa fil Aakhirati wa yudillul laahuz zaalimeen; wa yaf'alul laahu maa yashaaa'"}, {"id": 194926, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1778, "text": "Alam tara ilal lazeena baddaloo ni'matal laahi kufranw wa ahalloo qawmahum daaral bawaar"}, {"id": 194927, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1779, "text": "Jahannama yaslawnahaa wa bi'sal qaraar"}, {"id": 194928, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1780, "text": "Wa ja'aloo lillaahi andaadal liyudilloo 'an sabeelih; qul tamatta'oo fa innaa maseerakum ilan Naar"}, {"id": 194929, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1781, "text": "Qul li'ibaadiyal lazeena aamanoo yuqeemus Salaata wa yunfiqoo mimmaa razaqnaahum sirranw wa 'alaaniyatam min qabli any yaatiya Yawmul laa bai'un feehi wa laa khilaal"}, {"id": 194930, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1782, "text": "Allaahul lazee khalaqas samaawaati wal arda wa anzala minas samaaa'i maaa'an faakhraja bihee minas samaraati rizqal lakum wa sakhkhara lakumul fulka litajriya fil bahri bi amrihee wa sakhkhara lakumul anhaar"}, {"id": 194931, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 259, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1783, "text": "Wa sakhkhara lakumush shamsa walqamara daaa'ibaini wa sakhkhara lakumul laila wannahaar"}, {"id": 194932, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 215, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1784, "text": "Wa aataakum min kulli maa sa altumooh; wa in ta'uddoo ni'matal laahi laa tuhsoohaa; innal insaana lazaloo mun kaffaar"}, {"id": 194933, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1785, "text": "Wa iz qaala Ibraaheemu Rabbij 'al haazal balada aaminanw wajnubnee wa baniyya an na'budal asnaam"}, {"id": 194934, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1786, "text": "Rabbi innahunna adlalna kaseeram minan naasi faman tabi'anee fa innahoo minnee wa man 'asaanee fa innaka Ghafoorur Raheem"}, {"id": 194935, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1787, "text": "Rabbanaaa inneee askantu min zurriyyatee biwaadin ghairi zee zar'in 'inda Baitikal Muharrami Rabbanaa liyuqeemus Salaata faj'al af'idatam minan naasi tahweee ilaihim warzuqhum minas samaraati la'allahum yashkuroon"}, {"id": 194936, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1788, "text": "Rabbanaaa innaka ta'lamu maa nukhfee wa maa nu'lin; wa maa yakhfaa 'alal laahi min shai'in fil ardi wa laa fis samaaa'"}, {"id": 194937, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1789, "text": "Alhamdu lillaahil lazee wahaba lee 'alal kibari Ismaa'eela wa Ishaaq; inna Rabbee lasamee'ud du'aaa"}, {"id": 194938, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1790, "text": "Rabbij 'alnee muqeemas Salaati wa min zurriyyatee Rabbanaa wa taqabbal du'aaa'"}, {"id": 194890, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 216, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1791, "text": "Rabbanagh fir lee wa liwaalidaiya wa lilmu'mineena Yawma yaqoomul hisaab"}, {"id": 194940, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 260, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1792, "text": "Wa laa tahsabannal laaha ghaafilan 'ammaa ya'maluz zaalimoon; innamaa yu'akh khiruhum li Yawmin tashkhasu feehil absaar"}, {"id": 194941, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1793, "text": "Muhti'eena muqni'ee ru'oosihim laa yartaddu ilaihim tarfuhum wa af'idatuhum hawaaa'"}, {"id": 194918, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1794, "text": "Wa anzirin naasa Yawma yaateehimul 'azaabu fa yaqoolul lazeena zalamoo Rabbanaaa akhkhirnaaa ilaaa ajalin qareebin nujib da'wataka wa nattabi 'ir Rusul; awalam takoonooo aqsamtum min qablu maa lakum min zawaal"}, {"id": 194943, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1795, "text": "Wa sakantum fee masaakinil lazeena zalamooo anfusahum wa tabaiyana lakum kaifa fa'alnaa bihim wa darabnaa lakumul amsaal"}, {"id": 194944, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1796, "text": "Wa qad makaroo makrahum wa 'indal laahi makruhum wa in kaana makruhum litazoola minhul jibaal"}, {"id": 194945, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1797, "text": "Falaa tahsabannal laaha mukhlifa wa'dihee Rusulah; innal laaha 'azeezun zuntiqaam"}, {"id": 194946, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1798, "text": "Yawma tubaddalul ardu ghairal ardi wassamaawaatu wa barazoo lillaahil Waahidil Qahhaar"}, {"id": 194947, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1799, "text": "Wa taral mujrimeena Yawma 'izim muqarraneena filasfaad"}, {"id": 194948, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1800, "text": "Saraabeeluhum min qatiraaninw wa taghshaa wujoohahumun Naar"}, {"id": 194949, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1801, "text": "Liyajziyal laahu kulla nafsim maa kasabat; innal laaha saree'ul hisaab"}, {"id": 194950, "identifier": "en.transliteration", "surah_number": 14, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0628\u0631\u0627\u0647\u064a\u0645", "surah_en_name": "Ibrahim", "surah_en_name_translation": "Abraham", "revelation_type": "Meccan", "juz": 13, "manzil": 3, "page": 261, "ruku": 217, "hizb_quarter": 104, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 1802, "text": "Haaza balaaghul linnaasi wa liyunzaroo bihee wa liya'lamooo annamaa Huwa Illaahunw Waahidunw wa liyaz zakkara ulul albaab"}]