[{"id": 198307, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5164, "text": "Sabbaha lillaahi maa fisamaawaati wa maa fil ardi wa huwal 'Azeezul Hakeem"}, {"id": 198308, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5165, "text": "Yaa ayyuhal lazeena aamanoo lima taqooloona maa laa taf'aloon"}, {"id": 198309, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5166, "text": "Kabura maqtan 'indal laahi an taqooloo maa laa taf'aloon"}, {"id": 198310, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5167, "text": "Innal laaha yuhibbul lazeena yuqaatiloona fee sabeelihee saffan kaannahum bunyaanum marsoos"}, {"id": 198311, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 551, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5168, "text": "Wa iz qaala Moosa liqawmihee yaa qawmi lima tu'zoonanee wa qat ta'lamoona annee Rasoolul laahi ilaikum falammaa zaaghooo azaaghal laahu quloobahum; wallaahu laa yahdil qawmal faasiqeen"}, {"id": 198312, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5169, "text": "Wa iz qaala 'Eesab-nu-Mayama yaa Banee Israaa'eela innee Rasoolul laahi ilaikum musaddiqal limaa baina yadayya minat Tawraati wa mubashshiram bi Rasooliny yaatee mim ba'dis muhooo Ahmad; falammaa jaaa'ahum bil baiyinaati qaaloo haazaa sihrum mubeen"}, {"id": 198313, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5170, "text": "Wa man azlamu mimma nif taraa 'alal laahil kaziba wa huwa yad'aaa ilal Islaam; wallaahu laa yahdil qawmaz zaalimeen"}, {"id": 198314, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5171, "text": "Yureedoona liyutfi'oo nooral laahi bi afwaahimim wallaahu mutimmu noorihee wa law karihal kaafiroon"}, {"id": 198315, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 484, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5172, "text": "Huwal lazee arsala Rasoolahoo bilhudaa wa deenil haqqi liyuzhirahoo 'alad deeni kullihee wa law karihal mushrikoon"}, {"id": 198316, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5173, "text": "Yaaa ayyuhal lazeena aammano hal adullukum 'alaa tijaaratin tunjeekum min 'azaabin aleem"}, {"id": 198317, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5174, "text": "Tu'minoona billaahi wa Rasoolihee wa tujaahidoona fee sabeelil laahi bi amwaalikum wa anfusikum; zaalikum khairul lakum in kuntum ta'lamoon"}, {"id": 198318, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5175, "text": "Yaghfir lakum zunoobakum wa yudkhilkum Jannaatin tajree min tahtihal anhaaru wa masaakina taiyibatan fee Jannaati 'Ad; zaalikal fawzul 'Azeem"}, {"id": 198319, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5176, "text": "Wa ukhraa tuhibboonahaa nasrum minal laahi wa fat hun qaree; wa bashshiril mu 'mineen"}, {"id": 198321, "identifier": "en.transliteration", "surah_number": 61, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0635\u0641", "surah_en_name": "As-Saff", "surah_en_name_translation": "The Ranks", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 552, "ruku": 485, "hizb_quarter": 220, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Transliteration", "transelator_en_name": "English Transliteration", "format": "text", "type": "transliteration", "ayah_number": 5177, "text": "Yaaa ayyuhal lazeena aamaanoo koonooo ansaaral laahi kamaa qaala 'Eesab-nu-Maryama lil Hawaariyyeena man ansaareee ilal laah; qaalal Hawaariyyoona nahnu ansaa rul laahi fa aamanat taaa'ifatum mim Bannee Israaa'eela wa kafarat taaa'ifatun fa ayyadnal lazeena aammanoo 'alaa 'aduwwihim fa asbahoo zaahireen"}]