[{"id": 517362, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6180, "text": "Mjer\u00eb p\u00ebr secilin q\u00eb e ofendon e p\u00ebrqesh (njer\u00ebzit),"}, {"id": 517363, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6181, "text": "Q\u00eb ka tubuar pasuri dhe q\u00eb at\u00eb e ruan t\u00eb mos i pak\u00ebsohet."}, {"id": 517364, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6182, "text": "E mendon se pasuria e tij do t\u00eb b\u00ebj\u00eb t\u00eb p\u00ebrjetsh\u00ebm."}, {"id": 517365, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Jo, t\u00eb mos medoj\u00eb ashtu! Se ai pa tjet\u00ebr do t\u00eb hidhet n\u00eb Hutame."}, {"id": 517366, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6184, "text": "E \u00e7ka dini ti se \u00e7\u2019\u00ebsht\u00eb Hutame?"}, {"id": 517367, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6185, "text": "\u00cbsht\u00eb zjarri i All-llahut i ndezur fort."}, {"id": 517368, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6186, "text": "Q\u00eb dep\u00ebrton deri n\u00eb lo\u00e7k\u00eb t\u00eb zemr\u00ebs."}, {"id": 517369, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6187, "text": "Ai i mbyll ata, ua z\u00eb frym\u00ebn."}, {"id": 517370, "identifier": "sq.ahmeti", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6188, "text": "Ata jan\u00eb t\u00eb lidhur n\u00eb pranga"}]