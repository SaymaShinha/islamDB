[{"id": 516961, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5759, "text": "Ai vrenjti (fytyr\u00ebn) dhe u kthye."}, {"id": 516962, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5760, "text": "Ngase atij i erdh i verb\u00ebri."}, {"id": 516963, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5761, "text": "E ku mund ta dish ti, ndoshta ai do t\u00eb pastrohet."}, {"id": 516964, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5762, "text": "Apo do t\u00eb k\u00ebshillohet dhe k\u00ebshilla do t\u2019i b\u00ebj\u00eb dobi!"}, {"id": 516965, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5763, "text": "E ai q\u00eb nuk ndien nevoj\u00eb (p\u00ebr Zotin) pse ka pasuri,"}, {"id": 516966, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5764, "text": "Ti atij i v\u00eb veshin (i drejtohesh)!"}, {"id": 516967, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5765, "text": "Po ti nuk ke p\u00ebrgjegj\u00ebsi, pse ai nuk pastrohet."}, {"id": 516968, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5766, "text": "Nd\u00ebrsa ai q\u00eb nxiton dhe vjen te ti,"}, {"id": 516969, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5767, "text": "Dhe meq\u00eb ai frik\u00ebsohet,"}, {"id": 516970, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5768, "text": "E ti nuk e z\u00eb asgj\u00eb (dhe nuk ia v\u00eb veshin fjal\u00ebs s\u00eb tij),"}, {"id": 516971, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5769, "text": "Jo ashtu! N\u00eb t\u00eb v\u00ebrtet\u00eb k\u00ebto jan\u00eb k\u00ebshilla."}, {"id": 516972, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5770, "text": "Kush do merr mesim nga kjo (nga Kur\u2019ani)."}, {"id": 516973, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5771, "text": "\u00cbsht\u00eb k\u00ebshill\u00eb n\u00eb flet t\u00eb \u00e7mueshme."}, {"id": 516974, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5772, "text": "Me vler\u00eb t\u00eb lart\u00eb e t\u00eb ruajtur."}, {"id": 516975, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5773, "text": "N\u00eb duar t\u00eb udh\u00ebtuesve (engj\u00ebjve udh\u00ebtues mes All-llahut e njer\u00ebzve)."}, {"id": 516976, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5774, "text": "T\u00eb nderuar e t\u00eb ruajtur."}, {"id": 516977, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5775, "text": "Qoft\u00eb mallkuar njeriu, sa mohues i fort\u00eb \u00ebsht\u00eb ai!"}, {"id": 516978, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5776, "text": "Po prej \u00e7kafi e krijoi Ai at\u00eb?"}, {"id": 516979, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5777, "text": "At\u00eb e krijoi prej nj\u00eb pike uji dhe e p\u00ebrgatiti."}, {"id": 516980, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5778, "text": "Pastaj atij ia leht\u00ebsoi rrug\u00ebn."}, {"id": 516981, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5779, "text": "Mandej at\u00eb e b\u00ebri t\u00eb vdes\u00eb dhe atij i b\u00ebri varr (t\u00eb varros\u00ebt)."}, {"id": 516982, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5780, "text": "Pastaj kur t\u00eb doj\u00eb Ai e ringjall\u00eb at\u00eb."}, {"id": 516983, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5781, "text": "Jo! Ai nuk zbatoi at\u00eb q\u00eb urdh\u00ebroi Ai."}, {"id": 516984, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5782, "text": "Njeriu le t\u00eb shikoje ushqimin e vet."}, {"id": 516985, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5783, "text": "Ne l\u00ebshuam shi t\u00eb mjaftuesh\u00ebm."}, {"id": 516986, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5784, "text": "Pastaj e \u00e7am\u00eb tok\u00ebn sipas nevoj\u00ebs s\u00eb bim\u00ebs,"}, {"id": 516987, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5785, "text": "Dhe b\u00ebm\u00eb q\u00eb n\u00eb t\u00eb t\u00eb mbijn\u00eb drith\u00ebra"}, {"id": 516988, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5786, "text": "Edhe rrush e perime,"}, {"id": 516795, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5787, "text": "Edhe ullinj e hurma,"}, {"id": 516953, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5788, "text": "Edhe kopshte t\u00eb dendura,"}, {"id": 511353, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5789, "text": "Dhe pem\u00eb e kullosa,"}, {"id": 511371, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5790, "text": "Si mjete gjall\u00ebrimi p\u00ebr ju dhe p\u00ebr bag\u00ebtin\u00eb tuaj."}, {"id": 511372, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5791, "text": "E kur t\u00eb vij\u00eb ushtima (krisma e kijametit);"}, {"id": 511374, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5792, "text": "At\u00eb dit\u00eb njeriu ik\u00ebn prej v\u00ebllait t\u00eb vet,"}, {"id": 511375, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5793, "text": "Prej n\u00ebn\u00ebs dhe prej babait t\u00eb vet,"}, {"id": 511379, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5794, "text": "Prej gruas dhe prej f\u00ebmj\u00ebve t\u00eb vet."}, {"id": 511376, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5795, "text": "At\u00eb dit\u00eb secilit njeri i mjafton \u00e7\u00ebshtja e vet."}, {"id": 511377, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5796, "text": "At\u00eb dit\u00eb do t\u00eb ket\u00eb fytyra t\u00eb shndritura,"}, {"id": 511378, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5797, "text": "T\u00eb buz\u00ebqeshura e t\u00eb g\u00ebzuara."}, {"id": 511380, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 585, "ruku": 522, "hizb_quarter": 234, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5798, "text": "At\u00eb dit\u00eb do t\u00eb ket\u00eb edhe fytyra t\u00eb pluh\u00ebruara,"}, {"id": 511373, "identifier": "sq.ahmeti", "surah_number": 80, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0639\u0628\u0633", "surah_en_name": "Abasa", "surah_en_name_translation": "He frowned", "revelation_type": "Meccan", "juz": 30, 