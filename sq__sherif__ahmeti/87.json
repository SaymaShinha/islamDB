[{"id": 517137, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5949, "text": "Madh\u00ebroje emrin e Zotin t\u00ebnd t\u00eb Lart\u00eb!"}, {"id": 517138, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5950, "text": "I cili krijoi dhe p\u00ebrsosi,"}, {"id": 517139, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5951, "text": "Dhe i cili p\u00ebrcaktoi e orientoi,"}, {"id": 517140, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5952, "text": "Dhe i cili mund\u00ebsoi mbirjen e kullos\u00ebs;"}, {"id": 517141, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5953, "text": "E pastaj at\u00eb e b\u00ebri mbeturin\u00eb t\u00eb zez\u00eb."}, {"id": 517142, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5954, "text": "Ne do t\u00eb lexojm\u00eb ty e nuk do t\u00eb harrosh (Kur\u2019anin)."}, {"id": 517143, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5955, "text": "P\u00ebrve\u00e7 asaj q\u00eb All-llahu d\u00ebshiron. Ai e di t\u00eb hapt\u00ebn e t\u00eb fsheht\u00ebn."}, {"id": 517144, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5956, "text": "Dhe Ne do ta leht\u00ebsojm\u00eb ty m\u00eb t\u00eb leht\u00ebn."}, {"id": 517145, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5957, "text": "Pra k\u00ebshillo aty ku k\u00ebshilla b\u00ebn dobi."}, {"id": 517146, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Do t\u00eb k\u00ebshillohet ai q\u00eb ka frik\u00eb,"}, {"id": 517147, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5959, "text": "Nd\u00ebrsa do t\u00eb shmanget m\u00eb i keqi,"}, {"id": 517148, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5960, "text": "I cili do t\u00eb hyj\u00eb n\u00eb zjarrin e madh."}, {"id": 517149, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5961, "text": "Pastaj nuk do t\u00eb vdes\u00eb n\u00eb t\u00eb, por as nuk do t\u00eb jetoj\u00eb."}, {"id": 517150, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5962, "text": "Ka shp\u00ebtuar ai q\u00eb \u00ebsht\u00eb patruar."}, {"id": 517151, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5963, "text": "Q\u00eb e p\u00ebrkujton emrin e Zotit t\u00eb vet dhe falet."}, {"id": 517152, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5964, "text": "Po ju i jepni p\u00ebrpar\u00ebsi jet\u00ebs s\u00eb dunjas\u00eb,"}, {"id": 517153, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5965, "text": "Nd\u00ebrsa ahireti (amshimi) \u00ebsht\u00eb m\u00eb i mir\u00eb dhe e p\u00ebrjetsh\u00ebm."}, {"id": 517154, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5966, "text": "V\u00ebrtet kjo ishte n\u00eb broshurat e lashta:"}, {"id": 517155, "identifier": "sq.ahmeti", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 5967, "text": "n\u00eb broshurat e Ibrahimit dhe t\u00eb Musait."}]