[{"id": 517226, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6044, "text": "Pasha diellin dhe drit\u00ebn e tij!"}, {"id": 517227, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6045, "text": "Pasha h\u00ebn\u00ebn kur vjen pas tij!"}, {"id": 517228, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6046, "text": "Pasha dit\u00ebn kur ai (dielli) e shndrit!"}, {"id": 517229, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6047, "text": "Pasha nat\u00ebn kur ajo e mbulon at\u00eb (drit\u00ebn e diellit)!"}, {"id": 517230, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6048, "text": "Pasha qiellin dhe At\u00eb q\u00eb e nd\u00ebrtoi at\u00eb!"}, {"id": 517231, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6049, "text": "Pasha tok\u00ebn dhe At\u00eb q\u00eb e shtriu at\u00eb (e sheshoi)!"}, {"id": 517232, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6050, "text": "Pasha njeriun dhe At\u00eb q\u00eb e krijoi at\u00eb!"}, {"id": 517233, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6051, "text": "Dhe ia m\u00ebsoi se cilat jan\u00eb t\u00eb k\u00ebqijat dhe t\u00eb mirat e tij."}, {"id": 517234, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6052, "text": "Pra, ka shp\u00ebtuar ai q\u00eb e pastroi vetveten."}, {"id": 517235, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6053, "text": "E ka d\u00ebshtuar ai q\u00eb e posht\u00ebroi vetveten."}, {"id": 517236, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6054, "text": "Themudi p\u00ebrg\u00ebnjeshtroi p\u00ebr shkak t\u00eb arroganc\u00ebs s\u00eb vet."}, {"id": 517237, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6055, "text": "Kur m\u00eb i keqi i tyre u ngrit me t\u00eb shpejt\u00eb."}, {"id": 517238, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6056, "text": "e Pejgamberi atyre u pat th\u00ebn\u00eb: \u201cKinie kujdes deven\u00eb e All-llahut dhe ndaj pengimit t\u00eb saj q\u00eb t\u00eb pij\u00eb uj\u00eb."}, {"id": 517239, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6057, "text": "Ata nuk i besuan atij dhe at\u00eb (deven\u00eb) e therr\u00ebn dhe Zoti i tyre p\u00ebr shkak t\u00eb m\u00ebkatit t\u00eb tyre ua rreshtoi nd\u00ebshkimet dhe i rrafshoi,"}, {"id": 517240, "identifier": "sq.ahmeti", "surah_number": 91, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0645\u0633", "surah_en_name": "Ash-Shams", "surah_en_name_translation": "The Sun", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 533, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6058, "text": "Duke mos iu frik\u00ebsuar prapavis\u00eb s\u00eb atij (nd\u00ebshkimi)."}]