[{"id": 517273, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6091, "text": "A nuk ta hap\u00ebm ne gjoksin t\u00ebnd?"}, {"id": 517274, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6092, "text": "Dhe Ne hoq\u00ebm prej teje barr\u00ebn t\u00ebnde,"}, {"id": 517275, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6093, "text": "E cila shtypte shpin\u00ebn t\u00ebnde."}, {"id": 517276, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6094, "text": "Dhe Ne, ta ngrit\u00ebm lart\u00eb fam\u00ebn t\u00ebnde?"}, {"id": 517277, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6095, "text": "E, pa dyshim se pas v\u00ebshtir\u00ebsis\u00eb \u00ebsht\u00eb leht\u00ebsimi."}, {"id": 517278, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6096, "text": "V\u00ebrtet, pas v\u00ebshtir\u00ebsis\u00eb vjen leht\u00ebsimi."}, {"id": 517279, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6097, "text": "E kur ta kryesh (obligimin), at\u00ebbot\u00eb mundohu me adhurim (All-llahut)."}, {"id": 517280, "identifier": "sq.ahmeti", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6098, "text": "Dhe, vet\u00ebm te Zoti yt p\u00ebrq\u00ebndro synimin!"}]