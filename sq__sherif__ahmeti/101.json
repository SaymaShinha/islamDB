[{"id": 517340, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6158, "text": "Krisma e kijametit."}, {"id": 517341, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6159, "text": "\u00c7'ka \u00ebsht\u00eb krisma?"}, {"id": 517342, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6160, "text": "Dhe \u00e7\u2019t\u00eb m\u00ebsoi ty se \u00e7\u2019\u00ebsht\u00eb krisma?"}, {"id": 517343, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6161, "text": "Ajo \u00ebsht\u00eb dit\u00eb kur njer\u00ebzit b\u00ebhen si insekte t\u00eb shp\u00ebrndara."}, {"id": 517344, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6162, "text": "Dhe kodrat b\u00ebhen si leshi i shprishur."}, {"id": 517345, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6163, "text": "E sa i p\u00ebrket atij q\u00eb i r\u00ebndohet peshoja e veprave t\u00eb tij,"}, {"id": 517346, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6164, "text": "Ai \u00ebsht\u00eb n\u00eb nj\u00eb jet\u00eb t\u00eb k\u00ebndshme."}, {"id": 517347, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6165, "text": "Nd\u00ebrsa, atij q\u00eb ka peshoj\u00ebn e leht\u00eb t\u00eb veprave t\u00eb tij,"}, {"id": 517348, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6166, "text": "Vendi i tij do t\u00eb jet\u00eb n\u00eb Havije."}, {"id": 517349, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6167, "text": "E \u00e7\u2019\u00ebsht\u00eb ajo q\u00eb t\u00eb njoftoi \u00e7\u2019\u00ebsht\u00eb ai (Havije)?"}, {"id": 517350, "identifier": "sq.ahmeti", "surah_number": 101, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0627\u0631\u0639\u0629", "surah_en_name": "Al-Qaari'a", "surah_en_name_translation": "The Calamity", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 600, "ruku": 543, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Sherif Ahmeti", "transelator_en_name": "Sherif Ahmeti", "format": "text", "type": "translation", "ayah_number": 6168, "text": "\u00cbsht\u00eb zjarri me nxeht\u00ebsi shum\u00eb t\u00eb lart\u00eb."}]