mber": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 515, "ruku": 449, "hizb_quarter": 205, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "zh", "name": "Ma Jian", "transelator_en_name": "Ma Jian", "format": "text", "type": "translation", "ayah_number": 4612, "text": "\u7a46\u7f55\u9ed8\u5fb7\u662f\u771f\u4e3b\u7684\u4f7f\u8005\uff0c\u5728\u4ed6\u5de6\u53f3\u7684\u4eba\uff0c\u5bf9\u5916\u9053\u662f\u5e84\u4e25\u7684\uff0c\u5bf9\u6559\u80de\u662f\u6148\u7965\u7684\u3002\u4f60\u770b\u4ed6\u4eec\u97a0\u8eac\u53e9\u5934\uff0c\u8981\u6c42\u771f\u4e3b\u7684\u6069\u60e0\u548c\u559c\u60a6\uff0c\u4ed6\u4eec\u7684\u6807\u8bb0\u5c31\u5728\u4ed6\u4eec\u7684\u8138\u4e0a\uff0c\u90a3\u662f\u53e9\u5934\u7684\u6548\u679c\u3002\u90a3\u662f\u4ed6\u4eec\u5728\u300a\u8ba8\u62c9\u7279\u300b\u4e2d\u7684\u8b6c\u55bb\u3002\u4ed6\u4eec\u5728\u300a\u5f15\u652f\u52d2\u300b\u4e2d\u7684\u8b6c\u55bb\uff0c\u662f\u4ed6\u4eec\u8c61\u4e00\u68f5\u5e84\u7a3c\uff0c\u53d1\u51fa\u652f\u6761\uff0c\u800c\u4ed6\u52a9\u5b83\u957f\u5927\uff0c\u800c\u90a3\u652f\u6761\u6e10\u6e10\u8301\u58ee\uff0c\u7ec8\u4e8e\u56fa\u5b9a\u5728\u82d7\u672c\u4e0a\uff0c\u4f7f\u519c\u592b\u6b23\u8d4f\u3002\u4ed6\u5c06\u4ed6\u4eec\u9020\u6210\u90a3\u6837\uff0c\u4ee5\u4fbf\u4ed6\u501f\u4ed6\u4eec\u6fc0\u6012\u5916\u9053\u3002\u771f\u4e3b\u5e94\u8bb8\u4ed6\u4eec\u8fd9\u7b49\u4fe1\u9053\u800c\u4e14\u884c\u5584\u8005\uff0c\u5c06\u8499\u8d66\u5ba5\u548c\u91cd\u5927\u7684\u62a5\u916c\u3002"}]