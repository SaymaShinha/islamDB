[{"id": 697392, "identifier": "zh.jian", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "zh", "name": "Ma Jian", "transelator_en_name": "Ma Jian", "format": "text", "type": "translation", "ayah_number": 5127, "text": "\u51e1\u5728\u5929\u5730\u95f4\u7684\uff0c\u90fd\u8d5e\u9882\u771f\u4e3b\u8d85\u7edd\u4e07\u7269\u3002\u4ed6\u786e\u662f\u4e07\u80fd\u7684\uff0c\u786e\u662f\u81f3\u777f\u7684\u3002"}, {"id": 697393, "identifier": "zh.jian", "surah_number": 59, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0634\u0631", "surah_en_name": "Al-Hashr", "surah_en_name_translation": "The Exile", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 545, "ruku": 479, "hizb_quarter": 218, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "zh", "name": "Ma Jian", "transelator_en_name": "Ma Jian", "format": "text", "type": "translation", "ayah_number": 5128, "text": "\u5728\u521d\u6b21\u653e\u9010\u7684\u65f6\u5019\uff0c\u4ed6\u66fe\u5c06\u4fe1\u5949\u300a\u5723\u7ecf\u300b\u800c\u4e0d\u4fe1\u300a\u53e4\u5170\u7ecf\u300b\u7684\u4eba\u4ece\u4ed6\u4eec\u7684\u5bb6\u4e2d\u9a71\u9010\u51fa\u5883\u3002\u4f60\u4eec\u6ca1\u6709\u731c\u60f3\u5230\u4ed6\u4eec\u4f1a\u9000\u51fa\u53bb\uff0c\u4ed6\u4eec\u731c\u60f3\u81ea\u5df1\u7684\u5821\u5792\u80fd\u9632\u5fa1\u771f\u4e3b\uff1b\u4f46\u771f\u4e3b\u7684\u5211\u7f5a\uff0c\u7adf\u4ece\u4ed6\u4eec\u6599\u4e0d\u5230\u7684\u5730\u65b9\u964d\u4e34\u4ed6\u4eec\u3002\u4ed6\u5c06\u6050\u6016\u6295\u5728\u4ed6\u4eec\u7684\u5fc3\u4e2d\uff0c\u4ed6\u4eec\u7528\u81ea\u5df1\u7684\u624b\u548c\u4fe1\u58eb\u4eec\u7684\u624b\u62c6\u6bc1\u81ea\u5df1\u7684\u623f\u5c4b\u3002\u670