[{"id": 552022, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3410, "text": "Elif lam mim."}, {"id": 552023, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3411, "text": "Rum ma\u011flub edildi."}, {"id": 552024, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3412, "text": "En yak\u0131n bir yerde, fakat onlar bu ma\u011flubiyetten sonra galip olacaklar."}, {"id": 552026, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3413, "text": "Birka\u00e7 y\u0131l i\u00e7inde; emir, \u00f6nde de Allah'\u0131n, sonda da ve o g\u00fcn inananlar, ferahlayacak, sevinecek."}, {"id": 552027, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 404, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3414, "text": "Allah'\u0131n yard\u0131m\u0131yla; o, diledi\u011fine yard\u0131m eder ve odur \u00fcst\u00fcn ve rahim."}, {"id": 552028, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3415, "text": "Allah'\u0131n vaadidir; Allah vaadinden caymaz ve fakat insanlar\u0131n \u00e7o\u011fu bilmez."}, {"id": 552029, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3416, "text": "D\u00fcnya ya\u015fay\u0131\u015f\u0131n\u0131n yaln\u0131z d\u0131\u015f y\u00fcz\u00fcn\u00fc bilirler ve onlar, ahiretten gafil olanlard\u0131r."}, {"id": 552030, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3417, "text": "Hi\u00e7 olmazsa kendi kendilerine bir d\u00fc\u015f\u00fcnmezler mi ki Allah, g\u00f6kleri ve yery\u00fcz\u00fcn\u00fc ve ikisinin aras\u0131ndakileri ger\u00e7ek olarak ve mukadder bir zaman i\u00e7in yaratm\u0131\u015ft\u0131r ve \u015f\u00fcphe yok ki insanlar\u0131n \u00e7o\u011fu, Rablerine kavu\u015facaklar\u0131n\u0131 inkar ederler elbet."}, {"id": 552031, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3418, "text": "Yery\u00fcz\u00fcn\u00fc gezip de g\u00f6rmezler mi kendilerinden \u00f6ncekilerin sonlar\u0131 ne olmu\u015f; onlar, kuvvet bak\u0131m\u0131ndan daha \u00fcst\u00fcnd\u00fc bunlardan ve yery\u00fcz\u00fcn\u00fcn alt\u0131n\u0131 \u00fcst\u00fcne getirerek ekmi\u015fler ve oras\u0131n\u0131, bunlar\u0131n imar etti\u011finden daha da fazla imar etmi\u015flerdi ve onlara da apa\u00e7\u0131k delillerle gelmi\u015fti peygamberleri; derken Allah zulmetmemi\u015fti onlara ve fakat onlar, kendilerine zulmetmi\u015flerdi."}, {"id": 552032, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 349, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3419, "text": "Sonra da Allah'\u0131n delillerini yalanlad\u0131klar\u0131 ve onlarla alay ettikleri i\u00e7in o k\u00f6t\u00fcl\u00fck edenlerin sonu k\u00f6t\u00fc oldu gitti."}, {"id": 552033, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3420, "text": "Allah, \u00f6nce yarat\u0131r da sonra \u00f6ld\u00fcrerek tekrar halkeder ve yarat\u0131l\u0131\u015f\u0131 yeniler, sonra da hepiniz d\u00f6nd\u00fcr\u00fcl\u00fcr, onun tap\u0131s\u0131na g\u00f6t\u00fcr\u00fcl\u00fcrs\u00fcn\u00fcz."}, {"id": 552034, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3421, "text": "Ve k\u0131yametin koptu\u011fu g\u00fcn, su\u00e7lular, rahmetten meyus olurlar."}, {"id": 552035, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3422, "text": "Ve onlara, Tanr\u0131'ya ortak sand\u0131klar\u0131 \u015feylerden \u015fefaat eden de olmaz ve onlar da Tanr\u0131'ya \u015ferik sand\u0131klar\u0131 \u015feylere kafir olurlar."}, {"id": 552036, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3423, "text": "Ve k\u0131yametin koptu\u011fu g\u00fcn yok mu, i\u015fte o g\u00fcn tamam\u0131yla ayr\u0131l\u0131rlar da."}, {"id": 552037, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 405, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3424, "text": "\u0130nanan ve iyi i\u015flerde bulunanlar, cennet bah\u00e7esinde sevinip nimetlere nail olur onlar."}, {"id": 552038, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3425, "text": "Ve fakat kafir olanlara ve delillerimizi ve ahirete kavu\u015facaklar\u0131n\u0131 yalanlayanlara gelince: Art\u0131k onlard\u0131r azap i\u00e7in haz\u0131rlananlar."}, {"id": 552039, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3426, "text": "Art\u0131k tenzih edin Allah'\u0131 ak\u015fama girince ve sabaha erince."}, {"id": 552040, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3427, "text": "Ve onad\u0131r hamd g\u00f6klerde ve yery\u00fcz\u00fcnde; ve tenzih edin onu g\u00fcnd\u00fcz\u00fcn sonlar\u0131nda ve \u00f6\u011fle vaktinde."}, {"id": 552041, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 350, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3428, "text": "\u00d6l\u00fcden diri izhar eder, diriden \u00f6l\u00fc izhar eder ve yery\u00fcz\u00fcn\u00fc diriltir \u00f6l\u00fcm\u00fcnden sonra ve b\u00f6ylece \u00e7\u0131kar\u0131r mezarlar\u0131n\u0131zdan sizi de."}, {"id": 552042, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3429, "text": "Ve delillerindendir ki sizi topraktan yaratm\u0131\u015ft\u0131r da sonra insan haline gelir, yery\u00fcz\u00fcn\u00fcn her yan\u0131na da\u011f\u0131l\u0131rs\u0131n\u0131z."}, {"id": 552043, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3430, "text": "Ve delillerindendir ki sizin cinsinizden e\u015fler yaratm\u0131\u015ft\u0131r size, onlarla uzla\u015f\u0131p ge\u00e7inesiniz diye ve aran\u0131za da sevgi ve merhamet ihsan etmi\u015ftir; \u015f\u00fcphe yok ki bunda, d\u00fc\u015f\u00fcnen toplulu\u011fa deliller var."}, {"id": 552044, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3431, "text": "Ve delillerindendir g\u00f6klerin ve yery\u00fcz\u00fcn\u00fcn yarat\u0131l\u0131\u015f\u0131 ve dillerinizin ve renklerinizin ayr\u0131l\u0131\u011f\u0131; \u015f\u00fcphe yok ki bunda, bilenlere deliller var."}, {"id": 552045, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3432, "text": "Ve delillerindendir uykunuz geceleyin ve g\u00fcnd\u00fcz\u00fcn ve lutfundan r\u0131zk\u0131n\u0131z\u0131 aray\u0131p bulu\u015funuz. \u015e\u00fcphe yok ki bunda duyan toplulu\u011fa deliller var."}, {"id": 552046, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 406, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3433, "text": "Ve delillerindendir ki sizi hem korkutan, hem umduran \u015fim\u015fe\u011fi g\u00f6stermede ve g\u00f6kten ya\u011fmur ya\u011fd\u0131rmada da o suretle \u00f6l\u00fcm\u00fcnden sonra yery\u00fcz\u00fcn\u00fc diriltmede. \u015e\u00fcphe yok ki bunda, ak\u0131l eden toplulu\u011fa deliller var."}, {"id": 552047, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3434, "text": "Ve delillerindendir ki g\u00f6kle yer, \u00f6ylece durmada; sonra sizi bir \u00e7a\u011f\u0131rd\u0131 m\u0131 hemen yeralt\u0131ndan \u00e7\u0131kacaks\u0131n\u0131z."}, {"id": 552048, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3435, "text": "Ve onundur g\u00f6klerde ve yery\u00fcz\u00fcnde ne varsa; hepsi de ona itaat eder."}, {"id": 552050, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 351, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3436, "text": "\u00d6yle bir mabuttur ki her \u015feyi \u00f6nce yarat\u0131r, sonra \u00f6ld\u00fcr\u00fcr de tekrar diriltir ve bu, pek kolayd\u0131r ona ve onundur g\u00f6klerde ve yery\u00fcz\u00fcnde y\u00fcce s\u0131fatlar ve odur \u00fcst\u00fcn, h\u00fck\u00fcm ve hikmet sahibi."}, {"id": 552051, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3437, "text": "Size, kendinize ait bir\u015feyle \u00f6rnek getirmede: K\u00f6lelerinizden, cariyelerinizden, sizi r\u0131z\u0131kland\u0131rd\u0131\u011f\u0131m\u0131z \u015feylerde size ortak olanlar var m\u0131 ve siz, o mallarda, onlarla bir olur musunuz, onlar\u0131 mallar\u0131n\u0131za ortak eder de onlar da, sizin korkup titredi\u011finiz gibi o mallar\u0131n \u00fcst\u00fcne korkup titrerler mi? \u0130\u015fte, ak\u0131l eden toplulu\u011fa delilleri b\u00f6ylece tekrarlay\u0131p a\u00e7\u0131klar\u0131z."}, {"id": 552052, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3438, "text": "Hay\u0131r, o zulmedenler, bilgisizce kendi havalar\u0131na uydular; Allah'\u0131n sapt\u0131rd\u0131\u011f\u0131 ki\u015fiyi kim do\u011fru yola sevkedebilir? Ve onlara bir yard\u0131mc\u0131 da yoktur."}, {"id": 552053, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 162, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3439, "text": "Art\u0131k, y\u00fcz\u00fcn\u00fc tam do\u011fru dine d\u00f6nd\u00fcr, Allah'\u0131n ilk yaratt\u0131\u011f\u0131 selamet haline ki insanlar\u0131, o tabii halde, selamet halinde yaratm\u0131\u015ft\u0131r; Allah'\u0131n yarat\u0131\u015f\u0131, din, de\u011fi\u015ftirilemez; budur en do\u011fru din ve fakat insanlar\u0131n \u00e7o\u011fu bilmez."}, {"id": 552054, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3440, "text": "Ne emrettiyse ona uyarak hepiniz, y\u00fcz\u00fcn\u00fcz\u00fc o dine d\u00f6nd\u00fcr\u00fcn ve namaz k\u0131l\u0131n ve \u015firk ko\u015fanlardan olmay\u0131n."}, {"id": 552055, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 407, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3441, "text": "O \u015firk ko\u015fanlardan ki dinlerinde ayk\u0131r\u0131l\u0131\u011fa d\u00fc\u015fm\u00fc\u015fler de b\u00f6l\u00fckb\u00f6l\u00fck olmu\u015flard\u0131r ve her z\u00fcmre, kendisinde bulunana raz\u0131 olup gitmi\u015ftir."}, {"id": 552056, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3442, "text": "Ve insanlara bir zarar eri\u015fti mi d\u00f6n\u00fcp Rablerini \u00e7a\u011f\u0131r\u0131rlar, sonra onlara, kendi kat\u0131ndan bir rahmet tatt\u0131r\u0131nca da onlar\u0131n bir b\u00f6l\u00fc\u011f\u00fc, Rablerine \u015firk ko\u015farlar."}, {"id": 552057, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3443, "text": "\u015eirk ko\u015farlar, onlara verdi\u011fimiz nimetlere nank\u00f6rl\u00fck etmek i\u00e7in; \u015fimdilik ge\u00e7inin bakal\u0131m, yak\u0131nda bilip anlars\u0131n\u0131z."}, {"id": 552058, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3444, "text": "Yoksa biz onlara kesin bir delil mi indirdik de \u015firk ko\u015ftuklar\u0131 \u015feyler hakk\u0131nda onlara s\u00f6z s\u00f6yledi."}, {"id": 552059, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3445, "text": "\u0130nsanlara bir rahmet tatt\u0131rd\u0131k m\u0131 onunla sevinir, \u00f6v\u00fcn\u00fcrler ve onlara, elleriyle yap\u0131p haz\u0131rlad\u0131klar\u0131 bir k\u00f6t\u00fcl\u00fck gelip \u00e7at\u0131nca da hemen \u00fcmitlerini keserler."}, {"id": 552060, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3446, "text": "G\u00f6rmezler mi ki \u015f\u00fcphe yok Allah, diledi\u011finin r\u0131zk\u0131n\u0131 bolla\u015ft\u0131r\u0131r, diledi\u011finin de daralt\u0131r. \u015e\u00fcphe yok ki bunda, inanan toplulu\u011fa deliller var elbet."}, {"id": 552061, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3447, "text": "Art\u0131k yak\u0131nlara, yoksula ve yolda kalana hakk\u0131n\u0131 ver, Allah'\u0131n r\u0131zas\u0131n\u0131 dileyenlere bu, daha hay\u0131rl\u0131d\u0131r ve onlard\u0131r kurtulanlar\u0131n, muratlar\u0131na erenlerin ta kendileri."}, {"id": 552062, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3448, "text": "Halk\u0131n mal\u0131 arts\u0131n diye faize ait verdi\u011finiz \u015feyler, Allah kat\u0131nda artmaz; Allah'\u0131n r\u0131zas\u0131n\u0131 dileyerek verdi\u011finiz zekat artar ve sevaplar\u0131n\u0131 katkat artt\u0131ranlar, onlard\u0131r."}, {"id": 552063, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 352, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3449, "text": "\u00d6yle bir Allah't\u0131r ki sizi yaratm\u0131\u015ft\u0131r, sonra r\u0131z\u0131k vermi\u015ftir size, sonra \u00f6ld\u00fcr\u00fcr, sonra da diriltir sizi. Ona e\u015f sand\u0131klar\u0131n\u0131z\u0131n i\u00e7inde bunlardan bir \u015fey yapabilen var m\u0131? M\u00fcnezzehtir ve y\u00fccedir o \u015firk ko\u015fanlar\u0131n \u015firk ko\u015ftuklar\u0131 \u015feylerden."}, {"id": 552064, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 408, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3450, "text": "Bozgun belirdi karada ve denizde, insanlar\u0131n elleriyle kazand\u0131klar\u0131 su\u00e7lar y\u00fcz\u00fcnden; bu da, belki d\u00f6nerler, vazge\u00e7erler diye yapt\u0131klar\u0131na kar\u015f\u0131l\u0131k \u00e7ekecekleri cezan\u0131n az bir k\u0131sm\u0131n\u0131 onlara tatt\u0131rmak i\u00e7in."}, {"id": 552065, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3451, "text": "De ki: Gezin yery\u00fcz\u00fcnde de bak\u0131n, g\u00f6r\u00fcn \u00f6nce gelip ge\u00e7enlerin sonlar\u0131 neye varm\u0131\u015f; onlar\u0131n \u00e7o\u011fu m\u00fc\u015frikti."}, {"id": 552066, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3452, "text": "Reddine imkan bulunm\u0131yan o g\u00fcn, Allah taraf\u0131ndan gelmezden \u00f6nce y\u00fcz\u00fcn\u00fc tam do\u011fru olan dine \u00e7evir, o g\u00fcn onlar, b\u00f6l\u00fckb\u00f6l\u00fck olacaklard\u0131r."}, {"id": 552067, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3453, "text": "Kim kafir olursa k\u00fcfr\u00fcn\u00fcn su\u00e7u, ona aittir ve kim, iyi i\u015flerde bulunursa bu \u00e7e\u015fit adamlar da o iyili\u011fi kendileri i\u00e7in haz\u0131rlam\u0131\u015flard\u0131r."}, {"id": 552068, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3454, "text": "Bu da, inanan ve iyi i\u015flerde bulunanlar\u0131, lutfundan m\u00fckafatland\u0131rmak i\u00e7indir, \u015f\u00fcphe yok ki o, kafirleri sevmez."}, {"id": 552069, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3455, "text": "Ve delillerindendir \u015f\u00fckretmeniz i\u00e7in m\u00fcjdeci r\u00fczgarlar\u0131 g\u00f6ndermesi ve rahmetini size tatt\u0131rmas\u0131 ve emriyle gemileri y\u00fcr\u00fctmesi ve lutfundan r\u0131zk\u0131n\u0131z\u0131 arat\u0131p buldurmas\u0131."}, {"id": 552070, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3456, "text": "Ve andolsun ki senden \u00f6nce de kavimlerine peygamberler g\u00f6nderdik de apa\u00e7\u0131k delillerle geldiler onlara; derken c\u00fcrmettiklerinden dolay\u0131 \u00f6\u00e7 ald\u0131k onlardan ve inananlara yard\u0131m, bir hakt\u0131r bize."}, {"id": 552072, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3457, "text": "\u00d6yle bir Allah't\u0131r ki r\u00fczgarlar\u0131 yollar da bulutlar\u0131 s\u00fcrer onlar, g\u00f6ky\u00fcz\u00fcnde bulutu yayar diledi\u011fi gibi ve da\u011f\u0131n\u0131k, par\u00e7apar\u00e7a bir hale de koyar onlar\u0131, derken bakars\u0131n ki bulutlardan ya\u011fmur ya\u011fmaya ba\u015flar da kullar\u0131ndan diledi\u011fine nasip eder o ya\u011fmuru ve onlar da m\u00fcjdelerler birbirlerini, sevinirler."}, {"id": 552073, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3458, "text": "Halbuki onlara ya\u011fmur ya\u011fd\u0131r\u0131lmadan \u00f6nce hepsi de \u00fcmitlerini kesmi\u015flerdi."}, {"id": 552074, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 409, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3459, "text": "Art\u0131k Allah'\u0131n rahmet eserlerine bak da g\u00f6r, \u00f6l\u00fcm\u00fcnden sonra nas\u0131l diriltir yery\u00fcz\u00fcn\u00fc; \u015f\u00fcphe yok ki o, elbette \u00f6l\u00fcy\u00fc de diriltir ve onun, her \u015feye g\u00fcc\u00fc yeter."}, {"id": 552075, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3460, "text": "Ve andolsun ki bir r\u00fczgar yollad\u0131k da nebatlar\u0131 sararm\u0131\u015f g\u00f6rd\u00fcler mi ard\u0131ndan hemen nank\u00f6rl\u00fc\u011fe ba\u015flarlar."}, {"id": 552076, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3461, "text": "Hi\u00e7 \u015f\u00fcphe yok ki sen, sesini duyuramazs\u0131n \u00f6l\u00fcye ve ard\u0131na d\u00f6n\u00fcp giderlerken davetini duyuramazs\u0131n sa\u011f\u0131rlara."}, {"id": 552077, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 353, "hizb_quarter": 163, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3462, "text": "Ve sen, k\u00f6rleri sap\u0131kl\u0131klar\u0131ndan d\u00f6nd\u00fcr\u00fcp do\u011fru yola sevkedemezsin. Sen, ancak delillerimize inananlara duyurursun; ger\u00e7ekten de onlard\u0131r M\u00fcsl\u00fcman olanlar."}, {"id": 552078, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3463, "text": "\u00d6yle bir Allah't\u0131r ki sizi zay\u0131f bir sudan yaratm\u0131\u015ft\u0131r, sonra bir zay\u0131fl\u0131k olan \u00e7ocukluk \u00e7a\u011f\u0131ndan \u00e7\u0131kar\u0131p g\u00fc\u00e7kuvvet vermi\u015ftir size, sonra kuvvetli \u00e7a\u011fdan gene bir zay\u0131fl\u0131k \u00e7a\u011f\u0131na ve ihtiyarl\u0131k ya\u015f\u0131na getirmi\u015ftir sizi; yarat\u0131r ne dilerse ve odur bilen, g\u00fcc\u00fc yeten."}, {"id": 552079, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3464, "text": "Ve k\u0131yametin koptu\u011fu g\u00fcn su\u00e7lular, ancak bir an yat\u0131p e\u011flendiklerine and i\u00e7erler; i\u015fte b\u00f6yle as\u0131ls\u0131z \u015feylere kap\u0131l\u0131yordu onlar."}, {"id": 552080, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3465, "text": "Kendilerine bilgi ve inan\u00e7 verilenlerse derler ki: Andolsun ki siz, Allah'\u0131n takdiri ne kadarsa, ta tekrar dirilece\u011finiz g\u00fcnedek yat\u0131p e\u011flendiniz, ger\u00e7ekten de budur tekrar dirilece\u011finiz g\u00fcn ve fakat siz bilmiyorsunuz."}, {"id": 552081, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3466, "text": "Bir g\u00fcnd\u00fcr o g\u00fcn ki kendilerine zulmedenlerin \u00f6z\u00fcrleri de kabul edilmeyecek o g\u00fcn, t\u00f6vbe edip yapt\u0131klar\u0131ndan vazge\u00e7meleri de istenmeyecek art\u0131k."}, {"id": 552082, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3467, "text": "Ve biz, bu Kur'an'da, insanlara her \u00e7e\u015fit \u00f6rne\u011fi getirdik ve sen, onlara bir delil g\u00f6stersen: Siz derler, ancak asl\u0131 olmayan \u015feyleri \u00f6ne s\u00fcrenlersiniz."}, {"id": 552083, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3468, "text": "\u0130\u015fte, Allah, bilmeyenlerin g\u00f6n\u00fcllerini, bu \u00e7e\u015fit m\u00fch\u00fcrler."}, {"id": 552084, "identifier": "tr.golpinarli", "surah_number": 30, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0631\u0648\u0645", "surah_en_name": "Ar-Room", "surah_en_name_translation": "The Romans", "revelation_type": "Meccan", "juz": 21, "manzil": 5, "page": 410, "ruku": 354, "hizb_quarter": 164, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 3469, "text": "Dayan, \u015f\u00fcphe yok ki Allah'\u0131n vaadi ger\u00e7ektir ve adamak\u0131ll\u0131 inanmayanlar, sak\u0131n senin gayretini hafifletip gev\u015fetmesin."}]