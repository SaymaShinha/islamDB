[{"id": 554813, "identifier": "tr.golpinarli", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6214, "text": "Allah'\u0131n yard\u0131m\u0131 ve fetih, gelip \u00e7att\u0131 m\u0131."}, {"id": 554814, "identifier": "tr.golpinarli", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6215, "text": "Ve insanlar\u0131n, b\u00f6l\u00fckb\u00f6l\u00fck, Allah dinine girdi\u011fini g\u00f6rd\u00fcn m\u00fc."}, {"id": 554815, "identifier": "tr.golpinarli", "surah_number": 110, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0635\u0631", "surah_en_name": "An-Nasr", "surah_en_name_translation": "Divine Support", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 603, "ruku": 552, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6216, "text": "Art\u0131k, Rabbine hamd ederek ten