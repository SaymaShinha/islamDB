, "identifier": "tr.golpinarli", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6094, "text": "Ve ad\u0131n\u0131 y\u00fccelttik."}, {"id": 554693, "identifier": "tr.golpinarli", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6095, "text": "Art\u0131k \u015f\u00fcphe yok ki her g\u00fc\u00e7l\u00fckle beraber bir de kolayl\u0131k var."}, {"id": 554694, "identifier": "tr.golpinarli", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6096, "text": "\u015e\u00fcphe yok ki her g\u00fc\u00e7l\u00fckle beraber bir de kolayl\u0131k var."}, {"id": 554695, "identifier": "tr.golpinarli", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6097, "text": "Art\u0131k sen de ibadeti bitirince yorul."}, {"id": 554696, "identifier": "tr.golpinarli", "surah_number": 94, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0634\u0631\u062d", "surah_en_name": "Ash-Sharh", "surah_en_name_translation": "The Consolation", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 536, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6098, "text": "Ve ancak Rabbinden iste, ona do\u011frul."}]