[{"id": 554657, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6059, "text": "Andolsun bas\u0131nca, geceye."}, {"id": 554658, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6060, "text": "Ve \u0131\u015f\u0131y\u0131nca, g\u00fcne."}, {"id": 554659, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6061, "text": "Ve erke\u011fi ve di\u015fiyi yaratana."}, {"id": 554660, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6062, "text": "\u015e\u00fcphe yok ki \u00e7al\u0131\u015fman\u0131z, elbette \u00e7e\u015fitlidir, ba\u015fkaba\u015fka."}, {"id": 554661, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6063, "text": "Ve kim verdi ve \u00e7ekindiyse."}, {"id": 554662, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6064, "text": "Ve en g\u00fczel s\u00f6z\u00fc ger\u00e7eklediyse."}, {"id": 554663, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6065, "text": "Art\u0131k ona en kolay yolu kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 554664, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6066, "text": "Ve ama kim nekeslik etti ve zenginle\u015fmeyi dilediyse."}, {"id": 554665, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6067, "text": "Ve en g\u00fczel s\u00f6z\u00fc yalanlad\u0131ysa."}, {"id": 554666, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6068, "text": "Art\u0131k ona da en g\u00fc\u00e7 yolu kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 554667, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6069, "text": "Ve helak oldu\u011fu zaman mal\u0131, ona bir fayda vermez."}, {"id": 554668, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6070, "text": "\u015e\u00fcphe yok ki do\u011fru yolu g\u00f6stermek, bize d\u00fc\u015fer."}, {"id": 554669, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6071, "text": "Ve \u015f\u00fcphe yok ki bizimdir son ya\u015fay\u0131\u015f da ve \u00f6nceki de."}, {"id": 554670, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6072, "text": "Art\u0131k sizi korkuttum alevalev parlayan ate\u015fle."}, {"id": 554671, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6073, "text": "Oraya da ancak pek bahts\u0131z ki\u015fi at\u0131l\u0131r, yanar."}, {"id": 554672, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6074, "text": "\u00d6yle ki yalanlam\u0131\u015ft\u0131r o ve y\u00fcz\u00fcn\u00fc d\u00f6nd\u00fcrm\u00fc\u015ft\u00fcr."}, {"id": 554673, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6075, "text": "Ve ondan, ancak, pek ziyade \u00e7ekinen uzak kal\u0131r."}, {"id": 554674, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6076, "text": "\u00d6ylesine ki mal\u0131n\u0131 verir de \u00f6z\u00fcn\u00fc tertemiz bir hale kor."}, {"id": 554675, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6077, "text": "Ve hi\u00e7bir kimseden, bir nimetle m\u00fckafatlanmay\u0131 dilemez."}, {"id": 554676, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6078, "text": "Yapt\u0131\u011f\u0131n\u0131, ancak y\u00fccelerden y\u00fcce Rabbinin r\u0131zas\u0131 i\u00e7in yapar."}, {"id": 554677, "identifier": "tr.golpinarli", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6079, "text": "Ve o da, raz\u0131 olacakt\u0131r ondan."}]