[{"id": 553877, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5272, "text": "Nun, andolsun kaleme ve yazd\u0131klar\u0131na."}, {"id": 553878, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5273, "text": "Sen, Rabbinin nimeti sayesinde deli de\u011filsin."}, {"id": 553879, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5274, "text": "Ve sana, t\u00fckenmez, minnetsiz bir m\u00fckafat var."}, {"id": 553880, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5275, "text": "Ve \u015f\u00fcphe yok ki sen, pek b\u00fcy\u00fck bir ahlaka sahipsin elbette."}, {"id": 553881, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5276, "text": "Yak\u0131nda sen de g\u00f6r\u00fcrs\u00fcn ve onlar da g\u00f6r\u00fcrler."}, {"id": 553882, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5277, "text": "Delili\u011fe u\u011fram\u0131\u015f hanginiz?"}, {"id": 553883, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5278, "text": "\u015e\u00fcphe yok ki Rabbin, kendi yolundan sapan\u0131 da daha iyi bilir ve o, do\u011fru yolu bulanlar\u0131 da daha iyi bilir."}, {"id": 553884, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5279, "text": "Art\u0131k yalanlayanlara itaat etme."}, {"id": 553885, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5280, "text": "Onlara yumu\u015fakl\u0131k g\u00f6stermeni arzularlar, \u00f6yle hareket etsen onlar da yumu\u015fakl\u0131k g\u00f6sterirler."}, {"id": 553886, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5281, "text": "Ve itaat etme \u00e7ok yemin edenlerin, reyinde isabet bulunmayanlar\u0131n hi\u00e7birine."}, {"id": 553887, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5282, "text": "Ay\u0131p arayan, kovucu ve s\u00f6z getirip g\u00f6t\u00fcr\u00fcc\u00fcyle."}, {"id": 553888, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5283, "text": "Hayr\u0131 tamam\u0131yla meneden haddini a\u015fm\u0131\u015f su\u00e7luya."}, {"id": 553889, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5284, "text": "Ayr\u0131ca da \u00e7irkin ve k\u00f6t\u00fc huylu soysuza."}, {"id": 553890, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5285, "text": "Malm\u00fclk ve evlat sahibi bile olsa."}, {"id": 553891, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5286, "text": "Ona ayetlerimizi okuyunca eskilere ait masallar dedi."}, {"id": 553892, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5287, "text": "B\u00fcy\u00fcy\u00fcp bir hortuma d\u00f6nen burnuna, yak\u0131nda bir damga vururuz."}, {"id": 553893, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5288, "text": "Ve biz, onlar\u0131 a\u00e7l\u0131kla, k\u0131tl\u0131kla s\u0131nar\u0131z, nitekim o bah\u00e7e sahiplerini de s\u0131nam\u0131\u015ft\u0131k; hani, sabahleyin erkenden, bah\u00e7elerindeki mahsul\u00fc kesmeye ant i\u00e7mi\u015flerdi."}, {"id": 553894, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5289, "text": "Ve Tanr\u0131 dilerse de dememi\u015flerdi."}, {"id": 553895, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5290, "text": "Halbuki bah\u00e7enin \u00fcst\u00fcnde, Rabbinden gelen bir felaket dola\u015fmadayd\u0131 ki onlar uyuyorlard\u0131."}, {"id": 553896, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5291, "text": "Derken bah\u00e7e, b\u00fct\u00fcn mahsul\u00fc kesilip bi\u00e7ilmi\u015f, kupkuru \u00e7orak bir yere, bir \u00e7\u00f6le d\u00f6nm\u00fc\u015ft\u00fc."}, {"id": 553897, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5292, "text": "Sabahleyin birbirlerine sesleniyorlard\u0131."}, {"id": 553845, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5293, "text": "Mahsul\u00fcn\u00fcz\u00fc kesip dev\u015firecekseniz erkence ko\u015fun, gidin."}, {"id": 553765, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5294, "text": "Derken yola d\u00fc\u015ft\u00fcler ve birbirlerine de gizlice diyorlard\u0131 ki."}, {"id": 553898, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5295, "text": "Bug\u00fcn hi\u00e7bir yoksula yol vermeyin, yan\u0131n\u0131za gelmesin sak\u0131n."}, {"id": 553899, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5296, "text": "Ve kendilerini, yoksullar\u0131 men etmeye g\u00fc\u00e7leri yeter sanarak erkenden gittiler."}, {"id": 553900, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5297, "text": "Bah\u00e7eyi g\u00f6r\u00fcnce ger\u00e7ekten de dediler, elbette yolumuzu \u015fa\u015f\u0131rd\u0131k."}, {"id": 553901, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5298, "text": "Hay\u0131r dediler, biz mahrum olup gitmi\u015fiz."}, {"id": 553902, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5299, "text": "\u0130\u00e7lerinden en iyileri, ben demedim miydi size dedi, mabudunuzu tenzih etseniz ne olurdu."}, {"id": 553812, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5300, "text": "Dediler ki: \u015ean\u0131 y\u00fccedir Rabbimizin, ger\u00e7ekten de zalimlerden olduk biz."}, {"id": 553903, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5301, "text": "Bir birlerine d\u00f6nerek birbirlerini k\u0131namaya ba\u015flad\u0131lar."}, {"id": 553792, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5302, "text": "Yaz\u0131klar olsun bize dediler, ger\u00e7ekten de azm\u0131\u015f\u0131z biz."}, {"id": 553904, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5303, "text": "Umulur ki Rabbimiz, onun yerine bize daha da hay\u0131rl\u0131s\u0131n\u0131 verir, ger\u00e7ekten de biz, Rabbimizi dilemede, ondan istemedeyiz."}, {"id": 553905, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5304, "text": "\u0130\u015fte bunun gibidir azap ve elbette ahiret azab\u0131, daha da b\u00fcy\u00fckt\u00fcr bilirseniz."}, {"id": 553906, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5305, "text": "\u015e\u00fcphe yok ki \u00e7ekinenlere, Rableri kat\u0131nda Naim cennetleri var."}, {"id": 553907, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5306, "text": "Art\u0131k M\u00fcsl\u00fcmanlar\u0131 da su\u00e7lularla bir mi tutaca\u011f\u0131z?"}, {"id": 553908, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5307, "text": "Ne oldu size ki? Nas\u0131l h\u00fckmediyorsunuz?"}, {"id": 553909, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5308, "text": "Yoksa size mahsus bir kitap var da oradan m\u0131 okuyorsunuz."}, {"id": 553910, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5309, "text": "Orada, neyi be\u011fenir, isterseniz sizindir diye mi yaz\u0131l\u0131?"}, {"id": 553911, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5310, "text": "Yoksa h\u00fckm\u00fc k\u0131yametedek s\u00fcrecek antlar m\u0131 ettik size, \u015f\u00fcphe yok ki ne buyurursan\u0131z o olacak sizin i\u00e7in diye?"}, {"id": 553912, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5311, "text": "Onlara sor, bunlara kefil olan kimmi\u015f i\u00e7lerinden?"}, {"id": 553913, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5312, "text": "Yoksa ortaklar\u0131 m\u0131 var? Do\u011fru s\u00f6yl\u00fcyorlarsa gelsinler bakal\u0131m ortaklar\u0131yla."}, {"id": 553914, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5313, "text": "O g\u00fcn, i\u015fler g\u00fc\u00e7le\u015fir ve secdeye davet edilirler, derken g\u00fc\u00e7leri yetmez."}, {"id": 553915, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5314, "text": "G\u00f6zleri yere dikilir, \u00fcstlerine a\u015fa\u011f\u0131l\u0131k \u00e7\u00f6ker ve ger\u00e7ekten de sa\u011f esenken de secdeye davet edilmi\u015flerdir de secde etmemi\u015flerdi."}, {"id": 553916, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5315, "text": "Art\u0131k sen, bu s\u00f6z\u00fc yalanlayan\u0131 b\u0131rak bana, biz onlar\u0131 yava\u015fyava\u015f, hi\u00e7 bilmedikleri yerden cehenneme \u00e7ekerdururuz."}, {"id": 553917, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5316, "text": "Ve onlara m\u00fchlet vermedeyim, fakat \u015f\u00fcphe yok ki azab\u0131m, pek kuvvetlidir."}, {"id": 553918, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5317, "text": "Yoksa onlardan \u00fccret istiyorsun da derken onlar da a\u011f\u0131r bir bor\u00e7 alt\u0131nda m\u0131 kald\u0131lar?"}, {"id": 553919, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5318, "text": "Yoksa gizli alem, onlar\u0131n yan\u0131nda da onu mu yaz\u0131yorlar?"}, {"id": 553920, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5319, "text": "Art\u0131k sabret Rabbinin h\u00fckm\u00fcne ve bal\u0131kla arkada\u015f olana benzeme; hani o, dertten bo\u011fulmu\u015f bir halde Rabbine nida etmi\u015fti."}, {"id": 553921, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5320, "text": "Rabbinden bir nimet eri\u015fmeseydi ona elbette bir yere, fena bir halde b\u0131rak\u0131l\u0131r giderdi."}, {"id": 553922, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5321, "text": "Derken Rabbi, onu se\u00e7ti de temiz ki\u015filerden k\u0131ld\u0131."}, {"id": 553923, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5322, "text": "Ve az kalm\u0131\u015ft\u0131 ki kafirler, Kur'an'\u0131 duyduklar\u0131 zaman seni g\u00f6zleriyle yiyip helak etsinler ve derlerdi ki: \u015e\u00fcphe yok, bu, bir deli elbette."}, {"id": 553924, "identifier": "tr.golpinarli", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5323, "text": "Halbuki o, ancak alemlere bir \u00f6\u011f\u00fctt\u00fcr."}]