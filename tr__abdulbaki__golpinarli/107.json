entifier": "tr.golpinarli", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6204, "text": "Ve zekat vermeyi menederler."}]