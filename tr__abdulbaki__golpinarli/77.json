[{"id": 554222, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5623, "text": "Andolsun, ard\u0131 ard\u0131nca, iyilikle g\u00f6nderilenlere."}, {"id": 554223, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5624, "text": "\u015eiddetle esip yelenlere."}, {"id": 554224, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5625, "text": "Bulutlar\u0131 yay\u0131p s\u00fcrenlere."}, {"id": 554225, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5626, "text": "Ger\u00e7ekle asl\u0131 olmayan\u0131 ay\u0131rt edenlere."}, {"id": 554226, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5627, "text": "\u00d6\u011f\u00fctleri telkin edenlere."}, {"id": 554227, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5628, "text": "\u00d6z\u00fcrle su\u00e7u yok etmek hususunda olsun, yahut korkutma hususuna ait bulunsun."}, {"id": 554228, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5629, "text": "\u015e\u00fcphe yok ki size vaat edilen, mutlaka olacak."}, {"id": 554229, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5630, "text": "Y\u0131ld\u0131zlar\u0131n \u0131\u015f\u0131klar\u0131 s\u00f6n\u00fcnce."}, {"id": 554230, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5631, "text": "Ve g\u00f6k yar\u0131l\u0131nca."}, {"id": 554231, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5632, "text": "Ve da\u011flar, yerlerinden kopup d\u00fcmd\u00fcz olunca."}, {"id": 554232, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5633, "text": "Ve peygamberler toplan\u0131nca."}, {"id": 554233, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5634, "text": "Hangi g\u00fcn i\u00e7in geciktirildi bunlar?"}, {"id": 554234, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5635, "text": "Ay\u0131rma g\u00fcn\u00fc i\u00e7in."}, {"id": 554235, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5636, "text": "Ve nedir, bilir misin ay\u0131rma g\u00fcn\u00fc?"}, {"id": 554236, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5637, "text": "Vay hallerine o g\u00fcn yalanlayanlar\u0131n."}, {"id": 554237, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5638, "text": "\u00d6nce gelenleri helak etmedik mi?"}, {"id": 554238, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5639, "text": "Sonra da son gelenleri tutar, katar\u0131z onlara."}, {"id": 554239, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5640, "text": "B\u00f6yle yapar\u0131z g\u00fcnahkarlara."}, {"id": 554240, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 580, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5641, "text": "Vay hallerine o g\u00fcn yalanlayanlar\u0131n."}, {"id": 554241, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5642, "text": "Sizi, baya\u011f\u0131 ve az\u0131c\u0131k bir sudan yaratmad\u0131k m\u0131?"}, {"id": 554242, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5643, "text": "Derken onu, karar edilecek kuvvetli bir yerde tutmad\u0131k m\u0131?"}, {"id": 554243, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5644, "text": "Bilinen bir m\u00fcddete dek."}, {"id": 554244, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5645, "text": "Derken taktir ettik yarat\u0131l\u0131\u015f\u0131n\u0131, ne g\u00fczel de takdir ederiz biz."}, {"id": 554245, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5646, "text": "Vay hallerine o g\u00fcn yalanlayanlar\u0131n."}, {"id": 554246, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5647, "text": "Yery\u00fcz\u00fcn\u00fc, bir toplant\u0131 yeri olarak halk etmedik mi?"}, {"id": 554247, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5648, "text": "Dirilere ve \u00f6l\u00fclere."}, {"id": 554248, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5649, "text": "Ve orada, sabit ve metin da\u011flar yaratt\u0131k ve sizi, tatl\u0131 suyla suvard\u0131k."}, {"id": 554249, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5650, "text": "Vay hallerine o g\u00fcn yalanlayanlar\u0131n."}, {"id": 554250, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 5651, "text": "Haydi y\u00fcr\u00fcy\u00fcn yalanlad\u0131\u011f\u0131n\u0131za do\u011fru."}, {"id": 554251, "identifier": "tr.golpinarli", "surah_number": 77, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0631\u0633\u0644\u0627\u062a", "surah_en_name": "Al-Mursalaat", "surah_en_name_translation": "The Emissaries", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 581, "ruku": 516, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Ab