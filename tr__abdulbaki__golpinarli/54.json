[{"id": 553447, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4847, "text": "Yakla\u015ft\u0131 k\u0131yamet ve yar\u0131ld\u0131 ay."}, {"id": 553448, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Ve onlar, bir delil g\u00f6rd\u00fcler mi y\u00fcz \u00e7evirirler de s\u00fcr\u00fcp giden bir b\u00fcy\u00fc derler."}, {"id": 553449, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4849, "text": "Ve yalanlarlar ve dileklerine uyarlar ve her i\u015f, kararla\u015ft\u0131r\u0131lm\u0131\u015ft\u0131r."}, {"id": 553450, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4850, "text": "Ve andolsun, \u00f6yle haberler geldi onlara ki o haberlerde onlar\u0131 vazge\u00e7irecek, onlara \u00f6\u011f\u00fct verecek \u015feyler vard\u0131."}, {"id": 553451, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4851, "text": "Y\u00fcksek hikmet vard\u0131, derken korkutu\u015flar fayda vermedi gitti."}, {"id": 553452, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Art\u0131k y\u00fcz \u00e7evir onlardan; o g\u00fcn \u00e7a\u011f\u0131ran, ho\u015flan\u0131lmayan bir\u015feye \u00e7a\u011f\u0131r\u0131r."}, {"id": 553453, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4853, "text": "G\u00f6zleri yerde, kabirlerden \u00e7\u0131karlar, sanki onlar, da\u011f\u0131lm\u0131\u015f \u00e7ekirgelerdir."}, {"id": 553454, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4854, "text": "Y\u00f6nelirler \u00e7a\u011f\u0131rana; kafirler, bug\u00fcn derler, ne de zorlu g\u00fcn."}, {"id": 553455, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4855, "text": "Onlardan \u00f6nce Nuh kavmi de kulumuzu yalanlam\u0131\u015ft\u0131 ve delil dediler ona, pek fena incittiler onu."}, {"id": 553456, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4856, "text": "Derken Rabbine dua etti: \u015e\u00fcphe yok ki altoldum ben, art\u0131k sen yard\u0131m et bana."}, {"id": 553457, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4857, "text": "Derken a\u00e7t\u0131k g\u00f6klerin kap\u0131lar\u0131n\u0131 da \u015far\u0131l \u015far\u0131l ard\u0131 gelmez ya\u011fmurlar ya\u011fd\u0131rd\u0131k."}, {"id": 553458, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4858, "text": "Ve yerden de sular f\u0131\u015fk\u0131rtt\u0131k, derken sular, mukadder bir emre g\u00f6re birle\u015fti."}, {"id": 553459, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4859, "text": "Ve onu, tahtalardan yap\u0131lm\u0131\u015f ve m\u0131hlarla kenetlenmi\u015f bir gemide ta\u015f\u0131d\u0131k."}, {"id": 553460, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4860, "text": "G\u00f6z\u00fcm\u00fcz\u00fcn \u00f6n\u00fcnde ak\u0131p giderdi; bir m\u00fckafatt\u0131 nank\u00f6rl\u00fck g\u00f6rene."}, {"id": 553461, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4861, "text": "Ve andolsun ki bir delil olarak b\u0131rakt\u0131k onu, fakat bir ibret alan m\u0131 var?"}, {"id": 553462, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4862, "text": "Derken nas\u0131ld\u0131 azab\u0131m benim ve korkutu\u015flar\u0131m?"}, {"id": 553463, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4863, "text": "Ve andolsun \u00f6\u011f\u00fct ve ibret i\u00e7in Kur'an'\u0131 kolayla\u015ft\u0131rd\u0131k, fakat bir ibret alan m\u0131 var?"}, {"id": 553464, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4864, "text": "\u00c2d da yalanlam\u0131\u015ft\u0131, derken nas\u0131ld\u0131 azab\u0131m benim ve korkutu\u015flar\u0131m?"}, {"id": 553465, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4865, "text": "\u015e\u00fcphe yok ki s\u00fcr\u00fcp giden u\u011fursuz bir g\u00fcnde onlara bir kas\u0131rgad\u0131r yollad\u0131k."}, {"id": 553466, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4866, "text": "Onlar\u0131 k\u00f6k\u00fcnden koparmadayd\u0131, sanki k\u00f6klerinden kopup ba\u015f a\u015fa\u011f\u0131 devrilen hurma k\u00fct\u00fckleriydi onlar."}, {"id": 553467, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4867, "text": "Derken nas\u0131ld\u0131 azab\u0131m benim ve korkutu\u015flar\u0131m?"}, {"id": 553468, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4868, "text": "Ve andolsun ki \u00f6\u011f\u00fct ve ibret i\u00e7in Kur'an'\u0131 kolayla\u015ft\u0131rd\u0131k, fakat bir ibret alan m\u0131 var?"}, {"id": 553469, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4869, "text": "Semud da korkutucular\u0131 yalanlad\u0131."}, {"id": 553470, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4870, "text": "Derken bizden bir adama m\u0131 uyaca\u011f\u0131z dediler, ger\u00e7ekten de o zaman elbette sap\u0131kl\u0131\u011fa d\u00fc\u015feriz, ate\u015flere yanarkavruluruz."}, {"id": 553471, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4871, "text": "Vahiy, i\u00e7imizden gelegele ona m\u0131 geliyor? Hay\u0131r, o, yalanc\u0131 kendini be\u011fenmi\u015f birisi."}, {"id": 553472, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4872, "text": "Yar\u0131n bilirler kimmi\u015f yalanc\u0131 kendini be\u011fenmi\u015f."}, {"id": 553473, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4873, "text": "\u015e\u00fcphe yok ki onlar\u0131 s\u0131namak i\u00e7in di\u015fi deveyi g\u00f6nderiyoruz, art\u0131k g\u00f6zetle onlar\u0131 ve dayan."}, {"id": 553474, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4874, "text": "Ve haber ver onlara, su, aralar\u0131nda payla\u015ft\u0131r\u0131lm\u0131\u015ft\u0131r, her b\u00f6l\u00fck, n\u00f6betinde haz\u0131r olur, su al\u0131r."}, {"id": 553475, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4875, "text": "Derken arkada\u015flar\u0131na seslendiler, derken k\u0131l\u0131c\u0131n\u0131 \u00e7ekti de devenin ayaklar\u0131n\u0131 kesti, \u00f6ld\u00fcrd\u00fc."}, {"id": 553476, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4876, "text": "Derken nas\u0131ld\u0131 azab\u0131m benim ve korkutu\u015flar\u0131m?"}, {"id": 553477, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4877, "text": "Ger\u00e7ekten de bir ba\u011f\u0131r\u0131\u015f g\u00f6nderdik onlara, derken hayvan a\u011f\u0131l\u0131na konan \u00e7al\u0131ya \u00e7\u0131rp\u0131ya d\u00f6nd\u00fcler."}, {"id": 553478, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4878, "text": "Ve andolsun ki \u00f6\u011f\u00fct ve ibret i\u00e7in Kur'an'\u0131 kolayla\u015ft\u0131rd\u0131k, fakat bir ibret alan m\u0131 var?"}, {"id": 553479, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Lut kavmi de korkutucular\u0131 yalanlad\u0131lar."}, {"id": 553480, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4880, "text": "Ger\u00e7ekten de, Lut'un ailesi m\u00fcstesna, onlara ta\u015f ya\u011fd\u0131ran bir yel g\u00f6nderdik, Lut'un ailesini de bir seher \u00e7a\u011f\u0131 kurtard\u0131k."}, {"id": 553481, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4881, "text": "Kat\u0131m\u0131zdan bir nimet olarak; i\u015fte b\u00f6yle m\u00fckafatland\u0131r\u0131r\u0131z \u015f\u00fckredeni."}, {"id": 553482, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4882, "text": "Ve andolsun ki o, bizim helakimizle korkutmu\u015ftu onlar\u0131 da onlar, bu korkutu\u015flardan \u015f\u00fcpheye d\u00fc\u015fm\u00fc\u015flerdi."}, {"id": 553483, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4883, "text": "Ve ger\u00e7ekten de onun konuklar\u0131n\u0131 istemi\u015flerdi de biz, k\u00f6r edivermi\u015ftik g\u00f6zlerini, art\u0131k tad\u0131n azab\u0131m\u0131 ve korkutu\u015flar\u0131m\u0131n sonucunu."}, {"id": 553484, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4884, "text": "Ve andolsun ki bir sabah \u00e7a\u011f\u0131 \u00fcstlerine bir azap \u00e7\u00f6k\u00fcvermi\u015fti onlar\u0131n."}, {"id": 553485, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4885, "text": "Art\u0131k tad\u0131n azab\u0131m\u0131 ve korkutu\u015flar\u0131m\u0131."}, {"id": 553486, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4886, "text": "Ve andolsun ki \u00f6\u011f\u00fct ve ibret i\u00e7in Kur'an'\u0131 kolayla\u015ft\u0131rd\u0131k, fakat bir ibret alan m\u0131 var?"}, {"id": 553487, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4887, "text": "Ve andolsun ki Firavun soyuna da korkutucular gelmi\u015fti."}, {"id": 553488, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4888, "text": "B\u00fct\u00fcn delillerimizi yalanlad\u0131lar, derken onlar\u0131 \u00fcst\u00fcn ve mutlak kudretli bir helak edi\u015fle helak ediverdik."}, {"id": 553489, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4889, "text": "Sizin kafirleriniz, onlardan hay\u0131rl\u0131 m\u0131, yoksa kitaplarda bir kurtulu\u015f mu var size?"}, {"id": 553490, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4890, "text": "Yoksa biz, birbirine yard\u0131m eden bir toplulu\u011fuz mu derler?"}, {"id": 553491, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4891, "text": "O topluluk, yak\u0131nda bozguna u\u011frayacak ve ard\u0131n\u0131 d\u00f6n\u00fcp ka\u00e7acak."}, {"id": 553492, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4892, "text": "Onlara vaadedilen azab\u0131n mukadder zaman\u0131 k\u0131yamettir ve k\u0131yametin azab\u0131, daha da zararl\u0131d\u0131r ve daha da ac\u0131."}, {"id": 553493, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4893, "text": "\u015e\u00fcphe yok ki su\u00e7lular, sap\u0131kl\u0131k i\u00e7inde ve yak\u0131p kavuran ate\u015flerdedir."}, {"id": 553494, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4894, "text": "O g\u00fcn, y\u00fcz\u00fcst\u00fc ate\u015fe s\u00fcr\u00fcklenip at\u0131l\u0131rlar; tad\u0131n bakal\u0131m, cehennemin yak\u0131\u015f\u0131n\u0131."}, {"id": 553495, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4895, "text": "\u015e\u00fcphe yok ki biz; her \u015feyi, bilgimizde mukadder oldu\u011fu gibi ve zaman\u0131nda yaratt\u0131k."}, {"id": 553496, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4896, "text": "Ve bizim emrimiz, birdir, ancak bir g\u00f6z k\u0131rp\u0131\u015f, bir g\u00f6z yumup a\u00e7\u0131\u015f gibi tezdir."}, {"id": 553497, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4897, "text": "Ve andolsun ki taraftarlar\u0131n\u0131z\u0131 da helak ettik, fakat bir ibret alan m\u0131 var?"}, {"id": 553498, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4898, "text": "Ve i\u015fledikleri her \u015fey, kitaplardad\u0131r."}, {"id": 553499, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4899, "text": "Ve k\u00fc\u00e7\u00fck, b\u00fcy\u00fck, hepsi de yaz\u0131l\u0131d\u0131r."}, {"id": 553500, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4900, "text": "\u015e\u00fcphe yok ki \u00e7ekinenler, cennetlerdedir, \u0131rmaklar\u0131n ba\u015flar\u0131nda."}, {"id": 553501, "identifier": "tr.golpinarli", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 4901, "text": "Ger\u00e7eklik makam\u0131nda, \u00e7ok kudretli bir b\u00fcy\u00fck padi\u015fah kat\u0131nda."}]