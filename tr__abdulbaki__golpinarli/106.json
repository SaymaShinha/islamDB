[{"id": 554793, "identifier": "tr.golpinarli", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6194, "text": "Bunu da Kuray\u015f'\u0131n uzla\u015fmas\u0131, hallerinin d\u00fczene girmesi i\u00e7in yapt\u0131."}, {"id": 554794, "identifier": "tr.golpinarli", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6195, "text": "Yaz ve k\u0131\u015f, al\u0131\u015fveri\u015f i\u00e7in g\u00f6\u00e7\u00fcp konarak yolculuk ederlerken uzla\u015f\u0131p d\u00fczene girmeleri i\u00e7in."}, {"id": 554795, "identifier": "tr.golpinarli", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6196, "text": "Art\u0131k kulluk edin bu evin Rabbine."}, {"id": 554796, "identifier": "tr.golpinarli", "surah_number": 106, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642\u0631\u064a\u0634", "surah_en_name": "Quraish", "surah_en_name_translation": "Quraysh", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 548, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Abdulbak\u00ee G\u00f6lp\u0131narl\u0131", "transelator_en_name": "Abdulbaki Golpinarli", "format": "text", "type": "translation", "ayah_number": 6197, "text": "\u00d6yle Rab ki doyurdu da kurtard\u0131 sizi a\u00e7l\u0131ktan ve emin etti sizi korkudan."}]