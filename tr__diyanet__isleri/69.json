[{"id": 566376, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5324, "text": "Ger\u00e7ekle\u015fecek olan!"}, {"id": 566377, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5325, "text": "Nedir o ger\u00e7ekle\u015fecek olan g\u00fcn?"}, {"id": 566378, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5326, "text": "Ger\u00e7ekle\u015fecek olan\u0131n ne oldu\u011funu sana ne bildirir?"}, {"id": 566379, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5327, "text": "Semud ve Ad milletleri tepelerine inecek bu ger\u00e7e\u011fi yalanlad\u0131lar."}, {"id": 566380, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5328, "text": "Bu y\u00fczden Semud milleti zorlu bir sars\u0131nt\u0131 ile yok edildi."}, {"id": 566381, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5329, "text": "Ad milleti de bu y\u00fczden \u00f6n\u00fcnde durulmaz, dondurucu bir r\u00fczgarla yok edildi."}, {"id": 566382, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5330, "text": "Allah onlar\u0131n k\u00f6k\u00fcn\u00fc kesmek \u00fczere, \u00fczerlerine o r\u00fczgar\u0131 yedi gece sekiz g\u00fcn, estirdi. Halk\u0131n, k\u00f6k\u00fcnden \u00e7\u0131kar\u0131lm\u0131\u015f hurma k\u00fct\u00fckleri gibi yere y\u0131k\u0131ld\u0131klar\u0131n\u0131 g\u00f6r\u00fcrs\u00fcn."}, {"id": 566383, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5331, "text": "Onlardan arda kalm\u0131\u015f bir \u015fey g\u00f6r\u00fcr m\u00fcs\u00fcn?"}, {"id": 566384, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5332, "text": "Firavun, ondan \u00f6ncekiler ve alt \u00fcst olmu\u015f kasabalarda oturanlar da su\u00e7 i\u015flemi\u015flerdi."}, {"id": 566385, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5333, "text": "Rabbinin peygamberine ba\u015f kald\u0131rm\u0131\u015flard\u0131. Bunun \u00fczerine Rableri onlar\u0131 \u015fiddeti artt\u0131k\u00e7a artan bir \u015fekilde yakalad\u0131."}, {"id": 566386, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5334, "text": "Su ta\u015ft\u0131\u011f\u0131 vakit, size bir ibret olmak \u00fczere, anlay\u0131\u015fl\u0131 kulaklar anlas\u0131n diye s\u00fcz\u00fclen gemide, sizi Biz ta\u015f\u0131m\u0131\u015f\u0131zd\u0131r."}, {"id": 566387, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5335, "text": "Su ta\u015ft\u0131\u011f\u0131 vakit, size bir ibret olmak \u00fczere, anlay\u0131\u015fl\u0131 kulaklar anlas\u0131n diye s\u00fcz\u00fclen gemide, sizi Biz ta\u015f\u0131m\u0131\u015f\u0131zd\u0131r."}, {"id": 566388, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5336, "text": "Sura bir \u00fcf\u00fcr\u00fc\u015f \u00fcf\u00fcr\u00fcld\u00fc\u011f\u00fc, yer ve da\u011flar kald\u0131r\u0131l\u0131p bir vuru\u015fla birbirine \u00e7arp\u0131ld\u0131\u011f\u0131 zaman, i\u015fte o g\u00fcn olacak olur, k\u0131yamet kopar."}, {"id": 566389, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5337, "text": "Sura bir \u00fcf\u00fcr\u00fc\u015f \u00fcf\u00fcr\u00fcld\u00fc\u011f\u00fc, yer ve da\u011flar kald\u0131r\u0131l\u0131p bir vuru\u015fla birbirine \u00e7arp\u0131ld\u0131\u011f\u0131 zaman, i\u015fte o g\u00fcn olacak olur, k\u0131yamet kopar."}, {"id": 566390, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5338, "text": "Sura bir \u00fcf\u00fcr\u00fc\u015f \u00fcf\u00fcr\u00fcld\u00fc\u011f\u00fc, yer ve da\u011flar kald\u0131r\u0131l\u0131p bir vuru\u015fla birbirine \u00e7arp\u0131ld\u0131\u011f\u0131 zaman, i\u015fte o g\u00fcn olacak olur, k\u0131yamet kopar."}, {"id": 566391, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5339, "text": "G\u00f6k yar\u0131l\u0131r; o g\u00fcn d\u00fczeni bozulur."}, {"id": 566392, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5340, "text": "Melekler onun \u00e7evresindedirler; o g\u00fcn Rabbinin ar\u015f\u0131n\u0131 onlardan ba\u015fka sekiz tanesi y\u00fcklenir."}, {"id": 566393, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5341, "text": "O g\u00fcn siz huzura al\u0131n\u0131rs\u0131n\u0131z, hi\u00e7bir \u015feyiniz gizli kalmaz."}, {"id": 566394, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5342, "text": "Kitab\u0131 sa\u011f\u0131ndan verilen; \"Al\u0131n, kitab\u0131m\u0131 okuyun, do\u011frusu bir hesapla\u015fma ile kar\u015f\u0131la\u015faca\u011f\u0131m\u0131 umuyordum\" der."}, {"id": 566395, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5343, "text": "Kitab\u0131 sa\u011f\u0131ndan verilen; \"Al\u0131n, kitab\u0131m\u0131 okuyun, do\u011frusu bir hesapla\u015fma ile kar\u015f\u0131la\u015faca\u011f\u0131m\u0131 umuyordum\" der."}, {"id": 566396, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5344, "text": "Art\u0131k o, meyveleri sarkm\u0131\u015f, y\u00fcksek bir bah\u00e7ede, ho\u015f bir ya\u015fay\u0131\u015f i\u00e7indedir."}, {"id": 566397, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5345, "text": "Art\u0131k o, meyveleri sarkm\u0131\u015f, y\u00fcksek bir bah\u00e7ede, ho\u015f bir ya\u015fay\u0131\u015f i\u00e7indedir."}, {"id": 566398, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5346, "text": "Art\u0131k o, meyveleri sarkm\u0131\u015f, y\u00fcksek bir bah\u00e7ede, ho\u015f bir ya\u015fay\u0131\u015f i\u00e7indedir."}, {"id": 566399, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5347, "text": "Onlara \u015f\u00f6yle denir: \"Ge\u00e7mi\u015f g\u00fcnlerde, pe\u015finen i\u015flediklerinize kar\u015f\u0131l\u0131k afiyetle yiyiniz i\u00e7iniz.\""}, {"id": 566400, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5348, "text": "Fakat kitab\u0131 kendisine solundan verilen kimse: \"Kitab\u0131m ke\u015fke bana verilmeseydi; ke\u015fke hesab\u0131m\u0131n ne oldu\u011funu bilmeseydim; bu i\u015f ke\u015fke son bulmu\u015f olsayd\u0131; mal\u0131m bana fayda vermedi; g\u00fcc\u00fcm de kalmad\u0131\" der."}, {"id": 566401, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5349, "text": "Fakat kitab\u0131 kendisine solundan verilen kimse: \"Kitab\u0131m ke\u015fke bana verilmeseydi; ke\u015fke hesab\u0131m\u0131n ne oldu\u011funu bilmeseydim; bu i\u015f ke\u015fke son bulmu\u015f olsayd\u0131; mal\u0131m bana fayda vermedi; g\u00fcc\u00fcm de kalmad\u0131\" der."}, {"id": 566402, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5350, "text": "Fakat kitab\u0131 kendisine solundan verilen kimse: \"Kitab\u0131m ke\u015fke bana verilmeseydi; ke\u015fke hesab\u0131m\u0131n ne oldu\u011funu bilmeseydim; bu i\u015f ke\u015fke son bulmu\u015f olsayd\u0131; mal\u0131m bana fayda vermedi; g\u00fcc\u00fcm de kalmad\u0131\" der."}, {"id": 566403, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5351, "text": "Fakat kitab\u0131 kendisine solundan verilen kimse: \"Kitab\u0131m ke\u015fke bana verilmeseydi; ke\u015fke hesab\u0131m\u0131n ne oldu\u011funu bilmeseydim; bu i\u015f ke\u015fke son bulmu\u015f olsayd\u0131; mal\u0131m bana fayda vermedi; g\u00fcc\u00fcm de kalmad\u0131\" der."}, {"id": 566404, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5352, "text": "Fakat kitab\u0131 kendisine solundan verilen kimse: \"Kitab\u0131m ke\u015fke bana verilmeseydi; ke\u015fke hesab\u0131m\u0131n ne oldu\u011funu bilmeseydim; bu i\u015f ke\u015fke son bulmu\u015f olsayd\u0131; mal\u0131m bana fayda vermedi; g\u00fcc\u00fcm de kalmad\u0131\" der."}, {"id": 566405, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5353, "text": "\u0130lgililere \u015f\u00f6yle buyurulur: \"O'nu al\u0131n, ba\u011flay\u0131n.\""}, {"id": 566406, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5354, "text": "\"Sonra cehenneme yaslay\u0131n\""}, {"id": 566407, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5355, "text": "\"Sonra onu boyu yetmi\u015f ar\u015f\u0131n olan zincire vurun\";"}, {"id": 566408, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5356, "text": "\"\u00c7\u00fcnk\u00fc, o, y\u00fcce Allah'a inanmazd\u0131.\""}, {"id": 566409, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5357, "text": "\"Yoksulun yiyece\u011fi ile ilgilenmezdi.\""}, {"id": 566410, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5358, "text": "\"Bu sebeple burada bug\u00fcn onun bir ac\u0131yan\u0131 yoktur.\""}, {"id": 566411, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5359, "text": "\"G\u00fcnahkarlar\u0131n yiyece\u011fi olan kanl\u0131 irinden ba\u015fka bir yiyece\u011fi de yoktur.\""}, {"id": 566412, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5360, "text": "\"G\u00fcnahkarlar\u0131n yiyece\u011fi olan kanl\u0131 irinden ba\u015fka bir yiyece\u011fi de yoktur.\""}, {"id": 566413, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5361, "text": "G\u00f6rebildikleriniz ve g\u00f6remedikleriniz \u00fczerine yemin ederim ki, Kuran \u015ferefli bir el\u00e7inin getirdi\u011fi s\u00f6zd\u00fcr."}, {"id": 566414, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5362, "text": "G\u00f6rebildikleriniz ve g\u00f6remedikleriniz \u00fczerine yemin ederim ki, Kuran \u015ferefli bir el\u00e7inin getirdi\u011fi s\u00f6zd\u00fcr."}, {"id": 566415, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5363, "text": "G\u00f6rebildikleriniz ve g\u00f6remedikleriniz \u00fczerine yemin ederim ki, Kuran \u015ferefli bir el\u00e7inin getirdi\u011fi s\u00f6zd\u00fcr."}, {"id": 566416, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5364, "text": "O, \u015fair s\u00f6z\u00fc de\u011fildir; ne az inan\u0131yorsunuz!"}, {"id": 566417, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5365, "text": "Kahin s\u00f6z\u00fc de de\u011fildir; ne az d\u00fc\u015f\u00fcn\u00fcyorsunuz!"}, {"id": 566418, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5366, "text": "Kuran, Alemlerin Rabbinden indirilmedir."}, {"id": 566419, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5367, "text": "E\u011fer o (Muhammed), Bize kar\u015f\u0131, ona baz\u0131 s\u00f6zler katm\u0131\u015f olsayd\u0131, Biz onu kuvvetle yakalard\u0131k, sonra onun \u015fah damar\u0131n\u0131 kopar\u0131rd\u0131k."}, {"id": 566420, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5368, "text": "E\u011fer o (Muhammed), Bize kar\u015f\u0131, ona baz\u0131 s\u00f6zler katm\u0131\u015f olsayd\u0131, Biz onu kuvvetle yakalard\u0131k, sonra onun \u015fah damar\u0131n\u0131 kopar\u0131rd\u0131k."}, {"id": 566421, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5369, "text": "E\u011fer o (Muhammed), Bize kar\u015f\u0131, ona baz\u0131 s\u00f6zler katm\u0131\u015f olsayd\u0131, Biz onu kuvvetle yakalard\u0131k, sonra onun \u015fah damar\u0131n\u0131 kopar\u0131rd\u0131k."}, {"id": 566422, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5370, "text": "Hi\u00e7biriniz de onu koruyamazd\u0131n\u0131z."}, {"id": 566423, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5371, "text": "Do\u011frusu Kuran Allah'a kar\u015f\u0131 gelmekten sak\u0131nanlara bir \u00f6\u011f\u00fctt\u00fcr."}, {"id": 566424, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5372, "text": "\u0130\u00e7inizde yalanlayanlar bulundu\u011funu \u015f\u00fcphesiz bilmekteyiz."}, {"id": 566425, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5373, "text": "Do\u011frusu Kuran, inkarc\u0131lar i\u00e7in bir \u00fcz\u00fcnt\u00fcd\u00fcr."}, {"id": 566154, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5374, "text": "O, \u015f\u00fcphesiz kesin ger\u00e7ektir."}, {"id": 566426, "identifier": "tr.diyanet", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5375, "text": "\u00d6yleyse \u00e7ok b\u00fcy\u00fck olan Rabbinin ad\u0131n\u0131 tesbih et."}]