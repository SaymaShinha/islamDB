[{"id": 567068, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6024, "text": "Bu \u015fehre (Mekke'ye) yemin ederim; ki sen bu \u015fehirde oturmu\u015fsun."}, {"id": 567069, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6025, "text": "Bu \u015fehre (Mekke'ye) yemin ederim; ki sen bu \u015fehirde oturmu\u015fsun."}, {"id": 567070, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6026, "text": "Do\u011furana ve do\u011furdu\u011funa and olsun ki;"}, {"id": 567071, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6027, "text": "\u0130nsano\u011flunu, zorluklara katlanacak \u015fekilde yaratt\u0131k."}, {"id": 567072, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6028, "text": "\u0130nsano\u011flu, kendisine kimsenin g\u00fc\u00e7 yetiremeyece\u011fini mi san\u0131yor?"}, {"id": 567073, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6029, "text": "\"Y\u0131\u011f\u0131n y\u0131\u011f\u0131n mal t\u00fcketmi\u015fimdir\" diyor."}, {"id": 567074, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6030, "text": "O, kimsenin kendisini g\u00f6rmedi\u011fini mi zannediyor?"}, {"id": 567075, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6031, "text": "Biz onun i\u00e7in iki g\u00f6z, bir dil ve iki dudak var etmedik mi?"}, {"id": 567076, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6032, "text": "Biz onun i\u00e7in iki g\u00f6z, bir dil ve iki dudak var etmedik mi?"}, {"id": 567077, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6033, "text": "Biz ona e\u011fri ve do\u011fru iki yolu da g\u00f6stermedik mi?"}, {"id": 567078, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6034, "text": "Ama o, zor ge\u00e7idi a\u015fmaya giri\u015femedi."}, {"id": 567079, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6035, "text": "O zor ge\u00e7idin ne oldu\u011funu sen bilir misin?"}, {"id": 563209, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6036, "text": "O ge\u00e7it, bir k\u00f6le ve esir azadetmek,"}, {"id": 567080, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6037, "text": "Yahut, a\u00e7l\u0131k g\u00fcn\u00fcnde, yak\u0131n\u0131 olan bir \u00f6ks\u00fcz\u00fc, yahut topra\u011fa serilmi\u015f bir yoksulu doyurmakt\u0131r."}, {"id": 567081, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6038, "text": "Yahut, a\u00e7l\u0131k g\u00fcn\u00fcnde, yak\u0131n\u0131 olan bir \u00f6ks\u00fcz\u00fc, yahut topra\u011fa serilmi\u015f bir yoksulu doyurmakt\u0131r."}, {"id": 567082, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6039, "text": "Yahut, a\u00e7l\u0131k g\u00fcn\u00fcnde, yak\u0131n\u0131 olan bir \u00f6ks\u00fcz\u00fc, yahut topra\u011fa serilmi\u015f bir yoksulu doyurmakt\u0131r."}, {"id": 567083, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6040, "text": "Sonra, inan\u0131p birbirlerine sab\u0131r tavsiye edenlerden, merhametlilerden olmay\u0131 tavsiye edenlerden olmakt\u0131r."}, {"id": 567084, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6041, "text": "\u0130\u015fte bunlar amel defterleri sa\u011fdan verilenlerdir."}, {"id": 567085, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6042, "text": "Ayetlerimizi inkar edenler, i\u015fte onlar amel defterleri sollar\u0131ndan verilenlerdir."}, {"id": 567086, "identifier": "tr.diyanet", "surah_number": 90, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0644\u062f", "surah_en_name": "Al-Balad", "surah_en_name_translation": "The City", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 532, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6043, "text": "Onlar her y\u00f6nden ate\u015fle kapat\u0131lacaklard\u0131r."}]