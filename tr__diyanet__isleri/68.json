[{"id": 566326, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5272, "text": "Nun; kalem ve onunla yaz\u0131lanlara and olsun ki, sen Rabbinin nimetine u\u011fram\u0131\u015f bir kimsesin, deli (cinlenmi\u015f) de\u011filsin."}, {"id": 566327, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5273, "text": "Nun; kalem ve onunla yaz\u0131lanlara and olsun ki, sen Rabbinin nimetine u\u011fram\u0131\u015f bir kimsesin, deli (cinlenmi\u015f) de\u011filsin."}, {"id": 566328, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5274, "text": "Do\u011frusu sana kesintisiz bir ecir vard\u0131r."}, {"id": 566329, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5275, "text": "\u015e\u00fcphesiz sen b\u00fcy\u00fck bir ahlaka sahipsindir."}, {"id": 566330, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5276, "text": "Hanginizin akl\u0131ndan zoru oldu\u011funu yak\u0131nda sen de g\u00f6receksin, onlar da g\u00f6recekler."}, {"id": 566331, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5277, "text": "Hanginizin akl\u0131ndan zoru oldu\u011funu yak\u0131nda sen de g\u00f6receksin, onlar da g\u00f6recekler."}, {"id": 566332, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5278, "text": "Do\u011frusu senin Rabbin, yolundan sap\u0131tanlar\u0131 \u00e7ok iyi bilir; O, do\u011fru yolda olanlar\u0131 da \u00e7ok iyi bilir."}, {"id": 566333, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5279, "text": "Bundan b\u00f6yle, yalanlayanlara itaat etme;"}, {"id": 566334, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5280, "text": "(Onlar sana indirilen ayetlerden be\u011fenmediklerini b\u0131rakman suretiyle senin) kendilerine yumu\u015fak davranman\u0131 isterler; b\u00f6yle yapsan, onlar da seni \u00f6ver, yumu\u015fak davran\u0131rlar."}, {"id": 566335, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5281, "text": "Diliyle i\u011fneleyen, kovuculuk eden, iyili\u011fi daima \u00f6nleyen, a\u015f\u0131r\u0131 giden, su\u00e7 i\u015fleyen, \u00e7ok yemin eden al\u00e7ak zorbaya, b\u00fct\u00fcn bunlar d\u0131\u015f\u0131nda bir de soysuzlukla damgalanm\u0131\u015f kimseye, mal ve o\u011fullar\u0131 vard\u0131r diye ald\u0131r\u0131\u015f etmeyesin."}, {"id": 566336, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5282, "text": "Diliyle i\u011fneleyen, kovuculuk eden, iyili\u011fi daima \u00f6nleyen, a\u015f\u0131r\u0131 giden, su\u00e7 i\u015fleyen, \u00e7ok yemin eden al\u00e7ak zorbaya, b\u00fct\u00fcn bunlar d\u0131\u015f\u0131nda bir de soysuzlukla damgalanm\u0131\u015f kimseye, mal ve o\u011fullar\u0131 vard\u0131r diye ald\u0131r\u0131\u015f etmeyesin."}, {"id": 566337, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5283, "text": "Diliyle i\u011fneleyen, kovuculuk eden, iyili\u011fi daima \u00f6nleyen, a\u015f\u0131r\u0131 giden, su\u00e7 i\u015fleyen, \u00e7ok yemin eden al\u00e7ak zorbaya, b\u00fct\u00fcn bunlar d\u0131\u015f\u0131nda bir de soysuzlukla damgalanm\u0131\u015f kimseye, mal ve o\u011fullar\u0131 vard\u0131r diye ald\u0131r\u0131\u015f etmeyesin."}, {"id": 566338, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5284, "text": "Diliyle i\u011fneleyen, kovuculuk eden, iyili\u011fi daima \u00f6nleyen, a\u015f\u0131r\u0131 giden, su\u00e7 i\u015fleyen, \u00e7ok yemin eden al\u00e7ak zorbaya, b\u00fct\u00fcn bunlar d\u0131\u015f\u0131nda bir de soysuzlukla damgalanm\u0131\u015f kimseye, mal ve o\u011fullar\u0131 vard\u0131r diye ald\u0131r\u0131\u015f etmeyesin."}, {"id": 566339, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5285, "text": "Diliyle i\u011fneleyen, kovuculuk eden, iyili\u011fi daima \u00f6nleyen, a\u015f\u0131r\u0131 giden, su\u00e7 i\u015fleyen, \u00e7ok yemin eden al\u00e7ak zorbaya, b\u00fct\u00fcn bunlar d\u0131\u015f\u0131nda bir de soysuzlukla damgalanm\u0131\u015f kimseye, mal ve o\u011fullar\u0131 vard\u0131r diye ald\u0131r\u0131\u015f etmeyesin."}, {"id": 566340, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5286, "text": "Ayetlerimiz ona okundu\u011fu zaman: \"\u00d6ncekilerin masallar\u0131\" der."}, {"id": 566341, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5287, "text": "Onun havada olan burnunu yak\u0131nda yere s\u00fcrtece\u011fiz."}, {"id": 566343, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5288, "text": "Biz bunlar\u0131, vaktiyle bah\u00e7e sahiplerini denedi\u011fimiz gibi denedik. Sahipleri daha sabah olmadan, bah\u00e7eyi dev\u015fireceklerine bir istisna pay\u0131 b\u0131rakmaks\u0131z\u0131n yemin etmi\u015flerdi."}, {"id": 566344, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5289, "text": "Biz bunlar\u0131, vaktiyle bah\u00e7e sahiplerini denedi\u011fimiz gibi denedik. Sahipleri daha sabah olmadan, bah\u00e7eyi dev\u015fireceklerine bir istisna pay\u0131 b\u0131rakmaks\u0131z\u0131n yemin etmi\u015flerdi."}, {"id": 566345, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5290, "text": "Ama onlar daha uykudayken Rabbinin kat\u0131ndan g\u00f6nderilen bir salg\u0131n o bah\u00e7eyi sar\u0131vermi\u015fti de bah\u00e7e kapkara kesilmi\u015fti."}, {"id": 566346, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5291, "text": "Ama onlar daha uykudayken Rabbinin kat\u0131ndan g\u00f6nderilen bir salg\u0131n o bah\u00e7eyi sar\u0131vermi\u015fti de bah\u00e7e kapkara kesilmi\u015fti."}, {"id": 566347, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5292, "text": "Sabah erken: \"\u00dcr\u00fcnlerinizi dev\u015firecekseniz erken \u00e7\u0131k\u0131n\" diye birbirlerine seslendiler."}, {"id": 566348, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5293, "text": "Sabah erken: \"\u00dcr\u00fcnlerinizi dev\u015firecekseniz erken \u00e7\u0131k\u0131n\" diye birbirlerine seslendiler."}, {"id": 566349, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5294, "text": "\"Bug\u00fcn orada, hi\u00e7bir d\u00fc\u015fk\u00fcn kimse yan\u0131m\u0131za sokulmas\u0131n\" diye gizli gizli konu\u015farak y\u00fcr\u00fcyorlard\u0131."}, {"id": 566350, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5295, "text": "\"Bug\u00fcn orada, hi\u00e7bir d\u00fc\u015fk\u00fcn kimse yan\u0131m\u0131za sokulmas\u0131n\" diye gizli gizli konu\u015farak y\u00fcr\u00fcyorlard\u0131."}, {"id": 566351, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5296, "text": "Yoksullara yard\u0131m etmeye g\u00fc\u00e7leri yeterken b\u00f6yle konu\u015farak erkenden gittiler."}, {"id": 566352, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5297, "text": "Bah\u00e7eyi g\u00f6rd\u00fcklerinde: \"Herhalde yolumuzu \u015fa\u015f\u0131rm\u0131\u015f olaca\u011f\u0131z; belki de biz yoksun b\u0131rak\u0131ld\u0131k\" dediler."}, {"id": 566353, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5298, "text": "Bah\u00e7eyi g\u00f6rd\u00fcklerinde: \"Herhalde yolumuzu \u015fa\u015f\u0131rm\u0131\u015f olaca\u011f\u0131z; belki de biz yoksun b\u0131rak\u0131ld\u0131k\" dediler."}, {"id": 566354, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5299, "text": "Ortancalar\u0131: \"Ben size Allah'\u0131 anman\u0131z gerekmez mi, dememi\u015f miydim?\" dedi."}, {"id": 566355, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5300, "text": "\"Rabbimizi tenzih ederiz; do\u011frusu biz yaz\u0131k etmi\u015ftik\" dediler."}, {"id": 566356, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5301, "text": "Birbirlerini yermeye ba\u015flad\u0131lar."}, {"id": 566357, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5302, "text": "Sonra \u015f\u00f6yle dediler: \"Yaz\u0131klar olsun bize; do\u011frusu azg\u0131nl\u0131k edenlerdendik.\""}, {"id": 566358, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5303, "text": "\"Belki Rabbimiz bize bundan daha iyisini verir; do\u011frusu art\u0131k, Rabbimizden dilemekteyiz.\""}, {"id": 566359, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5304, "text": "\u0130\u015fte azap b\u00f6yledir; ama ahiret azab\u0131 daha b\u00fcy\u00fckt\u00fcr; ke\u015fke bilseler!"}, {"id": 566360, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5305, "text": "Allah'a kar\u015f\u0131 gelmekten sak\u0131nanlara, Rableri kat\u0131nda nimet cennetleri vard\u0131r."}, {"id": 566361, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5306, "text": "Kendilerini Allah'a vermi\u015f olanlar\u0131 hi\u00e7 su\u00e7lular gibi tutar m\u0131y\u0131z?"}, {"id": 566362, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5307, "text": "Ne oluyorsunuz? Ne bi\u00e7im h\u00fckmediyorsunuz?"}, {"id": 566363, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5308, "text": "Yoksa okudu\u011funuz bir kitab\u0131n\u0131z m\u0131 var?"}, {"id": 566364, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5309, "text": "Se\u00e7tikleriniz herhalde orada olacakt\u0131r."}, {"id": 566365, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5310, "text": "Yoksa aleyhimizde, k\u0131yamet g\u00fcn\u00fcne kadar s\u00fcregidecek ahidleriniz mi var ki, kendinize h\u00fckmettikleriniz sizin olacakt\u0131r?"}, {"id": 566366, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5311, "text": "Sor onlara: \"Bunu kim \u00fczerine al\u0131r?\""}, {"id": 566367, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5312, "text": "Yoksa onlar\u0131n ortaklar\u0131 m\u0131 vard\u0131r? Do\u011fru s\u00f6zl\u00fc iseler ortaklar\u0131n\u0131 getirsinler."}, {"id": 566368, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5313, "text": "O g\u00fcn i\u015fin deh\u015fetinden bald\u0131rlar a\u00e7\u0131l\u0131r; g\u00f6zleri d\u00f6nm\u00fc\u015f olarak y\u00fczlerini zillet b\u00fcr\u00fcr; secdeye \u00e7a\u011f\u0131r\u0131l\u0131rlar ama buna g\u00fc\u00e7leri yetmez. Oysa, kendileri sapasa\u011flam olduklar\u0131 zaman secdeye \u00e7a\u011f\u0131r\u0131lm\u0131\u015flard\u0131."}, {"id": 566369, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5314, "text": "O g\u00fcn i\u015fin deh\u015fetinden bald\u0131rlar a\u00e7\u0131l\u0131r; g\u00f6zleri d\u00f6nm\u00fc\u015f olarak y\u00fczlerini zillet b\u00fcr\u00fcr; secdeye \u00e7a\u011f\u0131r\u0131l\u0131rlar ama buna g\u00fc\u00e7leri yetmez. Oysa, kendileri sapasa\u011flam olduklar\u0131 zaman secdeye \u00e7a\u011f\u0131r\u0131lm\u0131\u015flard\u0131."}, {"id": 566370, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5315, "text": "Kuran'\u0131 yalanlayanlar\u0131 Bana b\u0131rak; Biz onlar\u0131 bilmedikleri yerden yava\u015f yava\u015f azaba yakla\u015ft\u0131raca\u011f\u0131z."}, {"id": 566342, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5316, "text": "Onlara mehil veriyorum; do\u011frusu Benim tuza\u011f\u0131m sa\u011flamd\u0131r."}, {"id": 566231, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5317, "text": "Yoksa, sen onlardan \u00fccret istiyorsun da, a\u011f\u0131r bir bor\u00e7 alt\u0131nda m\u0131 kal\u0131yorlar? Elbette hay\u0131r."}, {"id": 566107, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5318, "text": "Yoksa, gayb\u0131n bilgisi kendilerinin kat\u0131nda da onlar m\u0131 yaz\u0131yorlar?"}, {"id": 566371, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5319, "text": "Sen Rabbinin h\u00fckm\u00fcne kadar sabret; bal\u0131k sahibi (Yunus) gibi olma, o, pek \u00fczg\u00fcn olarak Rabbine seslenmi\u015fti."}, {"id": 566372, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5320, "text": "Rabbinin kat\u0131ndan ona bir nimet ula\u015fmasayd\u0131, k\u0131nanm\u0131\u015f olarak sahile at\u0131lacakt\u0131."}, {"id": 566373, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5321, "text": "Rabbi onu se\u00e7ip iyilerden k\u0131ld\u0131. Do\u011frusu inkar edenler, Kuran'\u0131 dinlediklerinde nerdeyse seni g\u00f6zleriyle y\u0131k\u0131p devireceklerdi. \"O delidir\" diyorlard\u0131."}, {"id": 566374, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5322, "text": "Rabbi onu se\u00e7ip iyilerden k\u0131ld\u0131. Do\u011frusu inkar edenler, Kuran'\u0131 dinlediklerinde nerdeyse seni g\u00f6zleriyle y\u0131k\u0131p devireceklerdi. \"O delidir\" diyorlard\u0131."}, {"id": 566375, "identifier": "tr.diyanet", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5323, "text": "Oysa Kuran, alemler i\u00e7in bir \u00f6\u011f\u00fctten ba\u015fka bir \u015fey de\u011fildir."}]