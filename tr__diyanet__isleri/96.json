[{"id": 561901, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6107, "text": "Yaratan Rabbinin ad\u0131yla oku!"}, {"id": 567146, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6108, "text": "O, insan\u0131 p\u0131ht\u0131la\u015fm\u0131\u015f kandan (alak'tan) yaratt\u0131."}, {"id": 567147, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6109, "text": "Oku! Kalemle \u00f6\u011freten, insana bilmedi\u011fini bildiren Rabbin, en b\u00fcy\u00fck kerem sahibidir."}, {"id": 567148, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6110, "text": "Oku! Kalemle \u00f6\u011freten, insana bilmedi\u011fini bildiren Rabbin, en b\u00fcy\u00fck kerem sahibidir."}, {"id": 567149, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6111, "text": "Oku! Kalemle \u00f6\u011freten, insana bilmedi\u011fini bildiren Rabbin, en b\u00fcy\u00fck kerem sahibidir."}, {"id": 567150, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6112, "text": "Ama, insano\u011flu kendini m\u00fcsta\u011fni sayarak azg\u0131nl\u0131k eder."}, {"id": 567151, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6113, "text": "Ama, insano\u011flu kendini m\u00fcsta\u011fni sayarak azg\u0131nl\u0131k eder."}, {"id": 567152, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6114, "text": "D\u00f6n\u00fc\u015f \u015f\u00fcphesiz Rabbinedir."}, {"id": 567153, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6115, "text": "Sen, namaz k\u0131lan kulu bundan menedeni g\u00f6rd\u00fcn m\u00fc?"}, {"id": 567154, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6116, "text": "Sen, namaz k\u0131lan kulu bundan menedeni g\u00f6rd\u00fcn m\u00fc?"}, {"id": 567155, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6117, "text": "S\u00f6yle bakal\u0131m, o kul do\u011fru yolda giden veya Allah'a kar\u015f\u0131 gelmekten sak\u0131nmay\u0131 buyuran bir kimse olsun; veya s\u00f6yle, yalanlay\u0131p y\u00fcz \u00e7eviren birisi olsun"}, {"id": 567156, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6118, "text": "S\u00f6yle bakal\u0131m, o kul do\u011fru yolda giden veya Allah'a kar\u015f\u0131 gelmekten sak\u0131nmay\u0131 buyuran bir kimse olsun; veya s\u00f6yle, yalanlay\u0131p y\u00fcz \u00e7eviren birisi olsun"}, {"id": 567157, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6119, "text": "S\u00f6yle bakal\u0131m, o kul do\u011fru yolda giden veya Allah'a kar\u015f\u0131 gelmekten sak\u0131nmay\u0131 buyuran bir kimse olsun; veya s\u00f6yle, yalanlay\u0131p y\u00fcz \u00e7eviren birisi olsun"}, {"id": 567158, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6120, "text": "Allah'\u0131n her \u015feyi g\u00f6rmekte oldu\u011funu bilmez mi?"}, {"id": 567159, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6121, "text": "Ama bundan vazge\u00e7mezse, and olsun ki, onu per\u00e7eminden,"}, {"id": 567160, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6122, "text": "Yalanc\u0131 ve g\u00fcnahkar per\u00e7eminden cehenneme s\u00fcr\u00fckleriz."}, {"id": 567161, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6123, "text": "O zaman, kafadarlar\u0131n\u0131 \u00e7a\u011f\u0131rs\u0131n,"}, {"id": 567162, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6124, "text": "Biz de zebanileri \u00e7a\u011f\u0131raca\u011f\u0131z."}, {"id": 567163, "identifier": "tr.diyanet", "surah_number": 96, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0639\u0644\u0642", "surah_en_name": "Al-Alaq", "surah_en_name_translation": "The Clot", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 597, "ruku": 538, "hizb_quarter": 239, "sajda": true, "sajda_id": 15, "sajda_obligatory": true, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6125, "text": "Sak\u0131n ona uyma; sen secde et, Rabbine yakla\u015f."}]