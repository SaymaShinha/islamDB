[{"id": 566957, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5910, "text": "\u0130\u00e7inde bur\u00e7lar\u0131 bulunan g\u00f6\u011fe and olsun;"}, {"id": 566958, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5911, "text": "S\u00f6z verilen k\u0131yamet g\u00fcn\u00fcne and olsun;"}, {"id": 566959, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5912, "text": "\u015eahitlik edene ve edilene and olsun ki, insanlar \u00f6ld\u00fckten sonra diriltileceklerdir."}, {"id": 566960, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5913, "text": "Haz\u0131rlad\u0131klar\u0131 hendekleri, tutu\u015fturulmu\u015f ate\u015fle doldurarak onun \u00e7evresinde oturup, inanm\u0131\u015f kimselere dinlerinden d\u00f6nmeleri i\u00e7in yapt\u0131klar\u0131 i\u015fkenceleri seyredenler kahrolmu\u015ftur!"}, {"id": 566961, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Haz\u0131rlad\u0131klar\u0131 hendekleri, tutu\u015fturulmu\u015f ate\u015fle doldurarak onun \u00e7evresinde oturup, inanm\u0131\u015f kimselere dinlerinden d\u00f6nmeleri i\u00e7in yapt\u0131klar\u0131 i\u015fkenceleri seyredenler kahrolmu\u015ftur!"}, {"id": 566962, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5915, "text": "Haz\u0131rlad\u0131klar\u0131 hendekleri, tutu\u015fturulmu\u015f ate\u015fle doldurarak onun \u00e7evresinde oturup, inanm\u0131\u015f kimselere dinlerinden d\u00f6nmeleri i\u00e7in yapt\u0131klar\u0131 i\u015fkenceleri seyredenler kahrolmu\u015ftur!"}, {"id": 566963, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5916, "text": "Haz\u0131rlad\u0131klar\u0131 hendekleri, tutu\u015fturulmu\u015f ate\u015fle doldurarak onun \u00e7evresinde oturup, inanm\u0131\u015f kimselere dinlerinden d\u00f6nmeleri i\u00e7in yapt\u0131klar\u0131 i\u015fkenceleri seyredenler kahrolmu\u015ftur!"}, {"id": 566965, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5917, "text": "Bu inkarc\u0131lar\u0131n, inananlara k\u0131zmalar\u0131; onlar\u0131n sadece, g\u00f6klerin ve yerin h\u00fck\u00fcmranl\u0131\u011f\u0131 kendisinin bulunan ve \u00f6v\u00fclme\u011fe lay\u0131k ve g\u00fc\u00e7l\u00fc olan Allah'a inanm\u0131\u015f olmalar\u0131ndand\u0131. Allah her \u015feye \u015fahiddir."}, {"id": 566966, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Bu inkarc\u0131lar\u0131n, inananlara k\u0131zmalar\u0131; onlar\u0131n sadece, g\u00f6klerin ve yerin h\u00fck\u00fcmranl\u0131\u011f\u0131 kendisinin bulunan ve \u00f6v\u00fclme\u011fe lay\u0131k ve g\u00fc\u00e7l\u00fc olan Allah'a inanm\u0131\u015f olmalar\u0131ndand\u0131. Allah her \u015feye \u015fahiddir."}, {"id": 566967, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Ama inanm\u0131\u015f erkek ve kad\u0131nlara i\u015fkence ederek onlar\u0131 dinlerinden \u00e7evirme\u011fe u\u011fra\u015fanlar, e\u011fer tevbe etmezlerse, onlara cehennem azab\u0131 vard\u0131r. Yak\u0131c\u0131 azap da onlarad\u0131r."}, {"id": 566968, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5920, "text": "\u015e\u00fcphesiz inan\u0131p yararl\u0131 i\u015fler i\u015fleyenlere, onlara, i\u00e7lerinden \u0131rmaklar akan cennetler vard\u0131r. Bu, b\u00fcy\u00fck kurtulu\u015ftur."}, {"id": 566969, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Do\u011frusu Rabbinin yakalamas\u0131 amans\u0131zd\u0131r."}, {"id": 566970, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5922, "text": "\u00d6nce yarat\u0131p sonra bunu tekrar eden O'dur."}, {"id": 566971, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Y\u00fcce ar\u015f\u0131n sahibi, \u00e7ok seven, ba\u011f\u0131\u015flayan O'dur."}, {"id": 566972, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5924, "text": "Y\u00fcce ar\u015f\u0131n sahibi, \u00e7ok seven, ba\u011f\u0131\u015flayan O'dur."}, {"id": 566973, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Her diledi\u011fini mutlaka yapand\u0131r."}, {"id": 566974, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Firavun ve Semud ordular\u0131n\u0131n haberi sana geldi mi?"}, {"id": 566975, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5927, "text": "Firavun ve Semud ordular\u0131n\u0131n haberi sana geldi mi?"}, {"id": 566976, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Do\u011frusu inkar edenler, hep yalanlayagelmi\u015flerdir."}, {"id": 566977, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5929, "text": "Oysa Allah onlar\u0131 ardlar\u0131ndan \u00e7evirmi\u015ftir."}, {"id": 566978, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5930, "text": "Do\u011frusu sana vahyedilen bu Kitap, Levhi Mahfuz'da bulunan \u015fanl\u0131 bir Kuran'd\u0131r."}, {"id": 566979, "identifier": "tr.diyanet", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5931, "text": "Do\u011frusu sana vahyedilen bu Kitap, Levhi Mahfuz'da bulunan \u015fanl\u0131 bir Kuran'd\u0131r."}]