[{"id": 567215, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6180, "text": "Mal toplayarak onu tekrar tekrar sayan, diliyle \u00e7eki\u015ftirip alay eden kimsenin vay haline!"}, {"id": 567216, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6181, "text": "Mal toplayarak onu tekrar tekrar sayan, diliyle \u00e7eki\u015ftirip alay eden kimsenin vay haline!"}, {"id": 567217, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6182, "text": "Mal\u0131n\u0131n kendisini \u00f6l\u00fcms\u00fcz k\u0131laca\u011f\u0131n\u0131 san\u0131r."}, {"id": 567218, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6183, "text": "Hay\u0131r; o, and olsun ki, Hutame'ye at\u0131lacakt\u0131r."}, {"id": 567219, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6184, "text": "Hutame'nin ne oldu\u011funu sen bilir misin?"}, {"id": 567220, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6185, "text": "O, y\u00fcreklere \u00e7\u00f6kecek olan, Allah'\u0131n tutu\u015fturulmu\u015f ate\u015fidir."}, {"id": 567221, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6186, "text": "O, y\u00fcreklere \u00e7\u00f6kecek olan, Allah'\u0131n tutu\u015fturulmu\u015f ate\u015fidir."}, {"id": 567222, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6187, "text": "Onlar, uzun s\u00fctunlar aras\u0131nda, her y\u00f6nden o ate\u015fle kapat\u0131lm\u0131\u015flard\u0131r."}, {"id": 567223, "identifier": "tr.diyanet", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6188, "text": "Onlar, uzun s\u00fctunlar aras\u0131nda, her y\u00f6nden o ate\u015fle kapat\u0131lm\u0131\u015flard\u0131r."}]