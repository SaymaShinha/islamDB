[{"id": 567102, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6059, "text": "Karar\u0131p ortal\u0131\u011f\u0131 b\u00fcr\u00fcd\u00fc\u011f\u00fc zaman geceye and olsun."}, {"id": 567103, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6060, "text": "A\u00e7\u0131l\u0131p ayd\u0131nlatt\u0131\u011f\u0131 zaman g\u00fcnd\u00fcze and olsun."}, {"id": 567104, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6061, "text": "Erke\u011fi ve di\u015fiyi yaratana and olsun ki:"}, {"id": 567105, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Do\u011frusu sizin \u00e7al\u0131\u015fmalar\u0131n\u0131z \u00e7e\u015fitlidir."}, {"id": 567106, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6063, "text": "Elinde bulunandan verenin, Allah'a kar\u015f\u0131 gelmekten sak\u0131nan\u0131n, en g\u00fczel s\u00f6z olan Allah'\u0131n birli\u011fini do\u011frulayan\u0131n i\u015flerini kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 567107, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6064, "text": "Elinde bulunandan verenin, Allah'a kar\u015f\u0131 gelmekten sak\u0131nan\u0131n, en g\u00fczel s\u00f6z olan Allah'\u0131n birli\u011fini do\u011frulayan\u0131n i\u015flerini kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 567108, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6065, "text": "Elinde bulunandan verenin, Allah'a kar\u015f\u0131 gelmekten sak\u0131nan\u0131n, en g\u00fczel s\u00f6z olan Allah'\u0131n birli\u011fini do\u011frulayan\u0131n i\u015flerini kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 567109, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 6066, "text": "Ama, cimrilik eden, kendini Allah'tan m\u00fcsta\u011fni sayan, en g\u00fczel s\u00f6z\u00fc yalanlayan kimsenin g\u00fc\u00e7l\u00fc\u011fe u\u011framas\u0131n\u0131 kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 567110, "identifier": "tr.diyanet", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sa