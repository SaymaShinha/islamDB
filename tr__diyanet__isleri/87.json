[{"id": 561771, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5949, "text": "Y\u00fcce Rabbinin ad\u0131n\u0131 tesbih et."}, {"id": 561855, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5950, "text": "O, yarat\u0131p \u015fekil vermi\u015ftir."}, {"id": 566995, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5951, "text": "O, her \u015feyi \u00f6l\u00e7\u00fcyle yap\u0131p do\u011fru yolu g\u00f6stermi\u015ftir."}, {"id": 566996, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5952, "text": "O, ye\u015fillikler bitirmi\u015ftir."}, {"id": 566997, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5953, "text": "Sonra da onlar\u0131 siyah \u00e7er\u00e7\u00f6pe \u00e7evirmi\u015ftir."}, {"id": 566998, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5954, "text": "Sana Kuran'\u0131 Biz okutaca\u011f\u0131z ve asla unutmayacaks\u0131n;"}, {"id": 566999, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5955, "text": "Allah'\u0131n diledi\u011fi bundan m\u00fcstesnad\u0131r. Do\u011frusu a\u00e7\u0131\u011f\u0131 da, gizliyi de bilen O'dur."}, {"id": 567000, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5956, "text": "Kolay olan\u0131 yapmay\u0131 sana kolayla\u015ft\u0131r\u0131r\u0131z."}, {"id": 567001, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5957, "text": "Faydal\u0131 olacaksa insanlara \u00f6\u011f\u00fct ver."}, {"id": 567002, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Allah'tan korkan \u00f6\u011f\u00fct alacakt\u0131r."}, {"id": 567003, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5959, "text": "Bedbaht olan ondan ka\u00e7\u0131nacakt\u0131r."}, {"id": 567004, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5960, "text": "O, en b\u00fcy\u00fck ate\u015fe yaslanacakt\u0131r."}, {"id": 567005, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5961, "text": "O, orada ne \u00f6lecektir ne de dirilecektir."}, {"id": 567006, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5962, "text": "Ar\u0131nm\u0131\u015f olan, Rabbinin ad\u0131n\u0131 an\u0131p namaz k\u0131lan, saadete eri\u015fecektir."}, {"id": 567007, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5963, "text": "Ar\u0131nm\u0131\u015f olan, Rabbinin ad\u0131n\u0131 an\u0131p namaz k\u0131lan, saadete eri\u015fecektir."}, {"id": 567008, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5964, "text": "Ama sizler d\u00fcnya hayat\u0131n\u0131 tercih ediyorsunuz."}, {"id": 567009, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5965, "text": "Oysa ahiret daha iyi ve daha bakidir."}, {"id": 567010, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5966, "text": "Do\u011frusu bu h\u00fck\u00fcmler ilk sahifelerde, \u0130brahim ve Musa'n\u0131n sahifelerinde de vard\u0131r."}, {"id": 567011, "identifier": "tr.diyanet", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5967, "text": "Do\u011frusu bu h\u00fck\u00fcmler ilk sahifelerde, \u0130brahim ve Musa'n\u0131n sahifelerinde de vard\u0131r."}]