[{"id": 566896, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5849, "text": "\u0130nsanlardan, kendileri bir \u015feyi \u00f6l\u00e7erek ald\u0131klar\u0131 zaman tam alan; ama onlara bir \u015feyi \u00f6l\u00e7\u00fcp tartarak verdiklerinde eksik tutan kimselerin, vay haline!"}, {"id": 566897, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5850, "text": "\u0130nsanlardan, kendileri bir \u015feyi \u00f6l\u00e7erek ald\u0131klar\u0131 zaman tam alan; ama onlara bir \u015feyi \u00f6l\u00e7\u00fcp tartarak verdiklerinde eksik tutan kimselerin, vay haline!"}, {"id": 566898, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5851, "text": "\u0130nsanlardan, kendileri bir \u015feyi \u00f6l\u00e7erek ald\u0131klar\u0131 zaman tam alan; ama onlara bir \u015feyi \u00f6l\u00e7\u00fcp tartarak verdiklerinde eksik tutan kimselerin, vay haline!"}, {"id": 566899, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Bunlar, b\u00fcy\u00fck bir g\u00fcnde tekrar dirileceklerini sanm\u0131yorlar m\u0131?"}, {"id": 566900, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5853, "text": "Bunlar, b\u00fcy\u00fck bir g\u00fcnde tekrar dirileceklerini sanm\u0131yorlar m\u0131?"}, {"id": 566901, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5854, "text": "O g\u00fcn insanlar Alemlerin Rabbinin huzurunda dururlar."}, {"id": 566902, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5855, "text": "Sak\u0131n\u0131n; Allah'\u0131n buyru\u011fundan d\u0131\u015far\u0131 \u00e7\u0131kanlar, muhakkak \"Siccin\" adl\u0131 defterde yaz\u0131l\u0131d\u0131r."}, {"id": 566903, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5856, "text": "Siccin'in ne oldu\u011funu sen nerden bilirsin?"}, {"id": 566904, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5857, "text": "O, yaz\u0131lm\u0131\u015f bir kitapt\u0131r."}, {"id": 566905, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5858, "text": "Yalanlayanlar\u0131n o g\u00fcn vay haline!"}, {"id": 566906, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5859, "text": "Onlar, k\u0131yamet g\u00fcn\u00fcn\u00fc yalanlam\u0131\u015f olanlard\u0131r."}, {"id": 566907, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5860, "text": "Oysa onu m\u00fctecaviz g\u00fcnahkardan ba\u015fka kimse yalanlamaz."}, {"id": 566908, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5861, "text": "Ona ayetlerimiz okundu\u011fu zaman \"\u00d6ncekilerin masallar\u0131\" der."}, {"id": 566909, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5862, "text": "Hay\u0131r, hay\u0131r; onlar\u0131n kazand\u0131klar\u0131 kalblerini pasland\u0131r\u0131p k\u00f6rletmi\u015ftir."}, {"id": 566910, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5863, "text": "Hay\u0131r; do\u011frusu onlar o g\u00fcn, Rablerinden yoksun kalacaklard\u0131r."}, {"id": 566911, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5864, "text": "Sonra onlar, \u015f\u00fcphesiz, cehenneme gireceklerdir."}, {"id": 566912, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5865, "text": "Sonra da: \"yalanlay\u0131p durdu\u011funuz i\u015fte budur\" denecektir."}, {"id": 566913, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5866, "text": "Ama iyilerin defteri y\u00fcksek katlardad\u0131r."}, {"id": 566914, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5867, "text": "O y\u00fcksek katlar\u0131n ne oldu\u011funu sen bilir misin?"}, {"id": 566915, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5868, "text": "O, g\u00f6zde meleklerin g\u00f6rd\u00fc\u011f\u00fc, yaz\u0131l\u0131 bir kitapd\u0131r."}, {"id": 566916, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5869, "text": "O, g\u00f6zde meleklerin g\u00f6rd\u00fc\u011f\u00fc, yaz\u0131l\u0131 bir kitapd\u0131r."}, {"id": 566917, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5870, "text": "\u0130yiler, \u015f\u00fcphesiz, nimet i\u00e7inde ve tahtlar \u00fczerinde etraf\u0131 seyrederler."}, {"id": 566918, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5871, "text": "\u0130yiler, \u015f\u00fcphesiz, nimet i\u00e7inde ve tahtlar \u00fczerinde etraf\u0131 seyrederler."}, {"id": 566919, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5872, "text": "Onlar\u0131, y\u00fczlerindeki nimet p\u0131r\u0131lt\u0131s\u0131ndan tan\u0131rs\u0131n."}, {"id": 566920, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5873, "text": "Sonunda misk kokusu b\u0131rakan, a\u011fz\u0131 kapal\u0131 saf bir i\u00e7ecekten i\u00e7erler. \u0130yi \u015feyler i\u00e7in yar\u0131\u015fanlar, bunun i\u00e7in yar\u0131\u015fs\u0131nlar."}, {"id": 566921, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5874, "text": "Sonunda misk kokusu b\u0131rakan, a\u011fz\u0131 kapal\u0131 saf bir i\u00e7ecekten i\u00e7erler. \u0130yi \u015feyler i\u00e7in yar\u0131\u015fanlar, bunun i\u00e7in yar\u0131\u015fs\u0131nlar."}, {"id": 566922, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5875, "text": "Onun katk\u0131s\u0131 g\u00f6zdelerin i\u00e7ti\u011fi y\u00fcce kaynaktand\u0131r."}, {"id": 566923, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5876, "text": "Onun katk\u0131s\u0131 g\u00f6zdelerin i\u00e7ti\u011fi y\u00fcce kaynaktand\u0131r."}, {"id": 566924, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5877, "text": "Su\u00e7lular, \u015f\u00fcphesiz, inanm\u0131\u015f olanlara g\u00fclerlerdi."}, {"id": 566925, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5878, "text": "Yanlar\u0131ndan ge\u00e7tikleri zaman da birbirlerine g\u00f6z k\u0131rparlard\u0131."}, {"id": 566926, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5879, "text": "Taraftarlar\u0131na vard\u0131klar\u0131nda bununla e\u011flenirlerdi."}, {"id": 566927, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5880, "text": "\u0130nananlar\u0131 g\u00f6rd\u00fckleri zaman: \"Do\u011frusu bunlar sap\u0131k olanlard\u0131r\" derlerdi."}, {"id": 566928, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5881, "text": "Oysa kendileri, inananlara g\u00f6zc\u00fc olarak g\u00f6nderilmemi\u015flerdi."}, {"id": 566929, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5882, "text": "Bug\u00fcn de, inananlar inkarc\u0131lara g\u00fclerler."}, {"id": 566930, "identifier": "tr.diyanet", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Diyanet \u0130\u015fleri", "transelator_en_name": "Diyanet Isleri", "format": "text", "type": "translation", "ayah_number": 5883, "text": "Tahtlar \u00fczerinde, inkarc\u0131lar\u0131n yapt\u0131klar\u0131