[{"id": 242919, "identifier": "en.sahih", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6126, "text": "Indeed, We sent the Qur'an down during the Night of Decree."}, {"id": 242920, "identifier": "en.sahih", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6127, "text": "And what can make you know what is the Night of Decree?"}, {"id": 242921, "identifier": "en.sahih", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u062f\u0631", "surah_en_name": "Al-Qadr", "surah_en_name_translation": "The Power, Fate", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 598, "ruku": 539, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6128, "text": "The Night of Decree is better than a thousand months."}, {"id": 242922, "identifier": "en.sahih", "surah_number": 97, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0