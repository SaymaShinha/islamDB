[{"id": 241290, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4474, "text": "Ha, Meem."}, {"id": 241291, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4475, "text": "The revelation of the Book is from Allah, the Exalted in Might, the Wise."}, {"id": 241292, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4476, "text": "Indeed, within the heavens and earth are signs for the believers."}, {"id": 241294, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4477, "text": "And in the creation of yourselves and what He disperses of moving creatures are signs for people who are certain [in faith]."}, {"id": 241295, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4478, "text": "And [in] the alternation of night and day and [in] what Allah sends down from the sky of provision and gives life thereby to the earth after its lifelessness and [in His] directing of the winds are signs for a people who reason."}, {"id": 241296, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4479, "text": "These are the verses of Allah which We recite to you in truth. Then in what statement after Allah and His verses will they believe?"}, {"id": 241297, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4480, "text": "Woe to every sinful liar"}, {"id": 241298, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4481, "text": "Who hears the verses of Allah recited to him, then persists arrogantly as if he had not heard them. So give him tidings of a painful punishment."}, {"id": 241299, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4482, "text": "And when he knows anything of Our verses, he takes them in ridicule. Those will have a humiliating punishment."}, {"id": 241300, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4483, "text": "Before them is Hell, and what they had earned will not avail them at all nor what they had taken besides Allah as allies. And they will have a great punishment."}, {"id": 241301, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4484, "text": "This [Qur'an] is guidance. And those who have disbelieved in the verses of their Lord will have a painful punishment of foul nature."}, {"id": 241302, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4485, "text": "It is Allah who subjected to you the sea so that ships may sail upon it by His command and that you may seek of His bounty; and perhaps you will be grateful."}, {"id": 241303, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4486, "text": "And He has subjected to you whatever is in the heavens and whatever is on the earth - all from Him. Indeed in that are signs for a people who give thought."}, {"id": 241304, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4487, "text": "Say, [O Muhammad], to those who have believed that they [should] forgive those who expect not the days of Allah so that He may recompense a people for what they used to earn."}, {"id": 241305, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4488, "text": "Whoever does a good deed - it is for himself; and whoever does evil - it is against the self. Then to your Lord you will be returned."}, {"id": 241306, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4489, "text": "And We did certainly give the Children of Israel the Scripture and judgement and prophethood, and We provided them with good things and preferred them over the worlds."}, {"id": 241307, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4490, "text": "And We gave them clear proofs of the matter [of religion]. And they did not differ except after knowledge had come to them - out of jealous animosity between themselves. Indeed, your Lord will judge between them on the Day of Resurrection concerning that over which they used to differ."}, {"id": 241308, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4491, "text": "Then We put you, [O Muhammad], on an ordained way concerning the matter [of religion]; so follow it and do not follow the inclinations of those who do not know."}, {"id": 241309, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4492, "text": "Indeed, they will never avail you against Allah at all. And indeed, the wrongdoers are allies of one another; but Allah is the protector of the righteous."}, {"id": 241310, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4493, "text": "This [Qur'an] is enlightenment for mankind and guidance and mercy for a people who are certain [in faith]."}, {"id": 241311, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4494, "text": "Or do those who commit evils think We will make them like those who have believed and done righteous deeds - [make them] equal in their life and their death? Evil is that which they judge."}, {"id": 241312, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4495, "text": "And Allah created the heavens and earth in truth and so that every soul may be recompensed for what it has earned, and they will not be wronged."}, {"id": 241313, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4496, "text": "Have you seen he who has taken as his god his [own] desire, and Allah has sent him astray due to knowledge and has set a seal upon his hearing and his heart and put over his vision a veil? So who will guide him after Allah? Then will you not be reminded?"}, {"id": 241314, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4497, "text": "And they say, \"There is not but our worldly life; we die and live, and nothing destroys us except time.\" And they have of that no knowledge; they are only assuming."}, {"id": 241315, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4498, "text": "And when Our verses are recited to them as clear evidences, their argument is only that they say, \"Bring [back] our forefathers, if you should be truthful.\""}, {"id": 241316, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 436, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4499, "text": "Say, \"Allah causes you to live, then causes you to die; then He will assemble you for the Day of Resurrection, about which there is no doubt, but most of the people do not know.\""}, {"id": 241317, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4500, "text": "And to Allah belongs the dominion of the heavens and the earth. And the Day the Hour appears - that Day the falsifiers will lose."}, {"id": 241318, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4501, "text": "And you will see every nation kneeling [from fear]. Every nation will be called to its record [and told], \"Today you will be recompensed for what you used to do."}, {"id": 241319, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4502, "text": "This, Our record, speaks about you in truth. Indeed, We were having transcribed whatever you used to do.\""}, {"id": 241320, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4503, "text": "So as for those who believed and did righteous deeds, their Lord will admit them into His mercy. That is what is the clear attainment."}, {"id": 241321, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4504, "text": "But as for those who disbelieved, [it will be said], \"Were not Our verses recited to you, but you were arrogant and became a people of criminals?"}, {"id": 241322, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 501, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4505, "text": "And when it was said, 'Indeed, the promise of Allah is truth and the Hour [is coming] - no doubt about it,' you said, 'We know not what is the Hour. We assume only assumption, and we are not convinced.' \""}, {"id": 241323, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4506, "text": "And the evil consequences of what they did will appear to them, and they will be enveloped by what they used to ridicule."}, {"id": 241324, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4507, "text": "And it will be said, \"Today We will forget you as you forgot the meeting of this Day of yours, and your refuge is the Fire, and for you there are no helpers."}, {"id": 241325, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4508, "text": "That is because you took the verses of Allah in ridicule, and worldly life deluded you.\" So that Day they will not be removed from it, nor will they be asked to appease [Allah]."}, {"id": 241326, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4509, "text": "Then, to Allah belongs [all] praise - Lord of the heavens and Lord of the earth, Lord of the worlds."}, {"id": 241327, "identifier": "en.sahih", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 502, "ruku": 437, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 4510, "text": "And to Him belongs [all] grandeur within the heavens and the earth, and He is the Exalted in Might, the Wise."}]