[{"id": 242982, "identifier": "en.sahih", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6189, "text": "Have you not considered, [O Muhammad], how your Lord dealt with the companions of the elephant?"}, {"id": 242983, "identifier": "en.sahih", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6190, "text": "Did He not make their plan into misguidance?"}, {"id": 242984, "identifier": "en.sahih", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u064a\u0644", "surah_en_name": "Al-Fil", "surah_en_name_translation": "The Elephant", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 547, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6191, "text": "And He sent against them birds in flocks,"}, {"id": 242985, "identifier": "en.sahih", "surah_number": 105, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u06