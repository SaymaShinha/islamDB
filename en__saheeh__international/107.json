[{"id": 242991, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6198, "text": "Have you seen the one who denies the Recompense?"}, {"id": 242992, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6199, "text": "For that is the one who drives away the orphan"}, {"id": 242993, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6200, "text": "And does not encourage the feeding of the poor."}, {"id": 242994, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6201, "text": "So woe to those who pray"}, {"id": 242995, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6202, "text": "[But] who are heedless of their prayer -"}, {"id": 242996, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6203, "text": "Those who make show [of their deeds]"}, {"id": 242997, "identifier": "en.sahih", "surah_number": 107, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0627\u0639\u0648\u0646", "surah_en_name": "Al-Maa'un", "surah_en_name_translation": "Almsgiving", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6204, "text": "And withhold [simple] assistance."}]