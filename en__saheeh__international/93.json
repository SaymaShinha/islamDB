[{"id": 242874, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6080, "text": "By the morning brightness"}, {"id": 242875, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6081, "text": "And [by] the night when it covers with darkness,"}, {"id": 242876, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6082, "text": "Your Lord has not taken leave of you, [O Muhammad], nor has He detested [you]."}, {"id": 242877, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6083, "text": "And the Hereafter is better for you than the first [life]."}, {"id": 242878, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6084, "text": "And your Lord is going to give you, and you will be satisfied."}, {"id": 242879, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6085, "text": "Did He not find you an orphan and give [you] refuge?"}, {"id": 242880, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6086, "text": "And He found you lost and guided [you],"}, {"id": 242881, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6087, "text": "And He found you poor and made [you] self-sufficient."}, {"id": 242882, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6088, "text": "So as for the orphan, do not oppress [him]."}, {"id": 242883, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6089, "text": "And as for the petitioner, do not repel [him]."}, {"id": 242884, "identifier": "en.sahih", "surah_number": 93, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0636\u062d\u0649", "surah_en_name": "Ad-Dhuhaa", "surah_en_name_translation": "The Morning Hours", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 535, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6090, "text": "But as for the favor of your Lord, report [it]."}]