[{"id": 242855, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6059, "text": "By the night when it covers"}, {"id": 242856, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6060, "text": "And [by] the day when it appears"}, {"id": 242857, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6061, "text": "And [by] He who created the male and female,"}, {"id": 242858, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6062, "text": "Indeed, your efforts are diverse."}, {"id": 242859, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6063, "text": "As for he who gives and fears Allah"}, {"id": 242860, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6064, "text": "And believes in the best [reward],"}, {"id": 242861, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6065, "text": "We will ease him toward ease."}, {"id": 242862, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6066, "text": "But as for he who withholds and considers himself free of need"}, {"id": 242700, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6067, "text": "And denies the best [reward],"}, {"id": 242796, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6068, "text": "We will ease him toward difficulty."}, {"id": 242863, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6069, "text": "And what will his wealth avail him when he falls?"}, {"id": 242864, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6070, "text": "Indeed, [incumbent] upon Us is guidance."}, {"id": 242865, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6071, "text": "And indeed, to Us belongs the Hereafter and the first [life]."}, {"id": 242866, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 595, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6072, "text": "So I have warned you of a Fire which is blazing."}, {"id": 242867, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6073, "text": "None will [enter to] burn therein except the most wretched one."}, {"id": 242868, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6074, "text": "Who had denied and turned away."}, {"id": 242869, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6075, "text": "But the righteous one will avoid it -"}, {"id": 242870, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6076, "text": "[He] who gives [from] his wealth to purify himself"}, {"id": 242871, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6077, "text": "And not [giving] for anyone who has [done him] a favor to be rewarded"}, {"id": 242872, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6078, "text": "But only seeking the countenance of his Lord, Most High."}, {"id": 242873, "identifier": "en.sahih", "surah_number": 92, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0644\u064a\u0644", "surah_en_name": "Al-Lail", "surah_en_name_translation": "The Night", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 596, "ruku": 534, "hizb_quarter": 238, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "en", "name": "Saheeh International", "transelator_en_name": "Saheeh International", "format": "text", "type": "translation", "ayah_number": 6079, "text": "And he is going to be satisfied."}]