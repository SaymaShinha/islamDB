, "manzil": 7, "page": 602, "ruku": 549, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 6204, "text": "Ve yard\u0131m\u0131 da engellerler."}]