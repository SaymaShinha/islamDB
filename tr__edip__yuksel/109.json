[{"id": 579732, "identifier": "tr.yuksel", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 6208, "text": "De ki, \"Ey inkarc\u0131lar.\""}, {"id": 579733, "identifier": "tr.yuksel", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 6209, "text": "\"Ben, sizin tapt\u0131klar\u0131n\u0131za tapmam.\""}, {"id": 579734, "identifier": "tr.yuksel", "surah_number": 109, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0643\u0627\u0641\u0631\u0648\u0646", "surah_en_name": "Al-Kaafiroon", "surah_en_name_translation": "The Disbelievers", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 603, "ruku": 551, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 6210, "text": "\"Siz de benim tapt\u0131\u011f\u0131ma tapmazs\u0131n\u0131z.\""}, {"id": 579735, "identifier": "tr.yuksel", "surah_number": 109, "surah_ar_name": "\u063