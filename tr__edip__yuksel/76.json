[{"id": 579157, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5592, "text": "\u0130nsan\u0131n \u00fczerinden, kendisinin an\u0131lmaya de\u011fer bir \u015fey olmad\u0131\u011f\u0131 bir zaman periyodu ge\u00e7memi\u015f midir?"}, {"id": 579158, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5593, "text": "Biz insan\u0131 bir s\u0131v\u0131 kar\u0131\u015f\u0131mdan yaratt\u0131k ki onu s\u0131nayal\u0131m. Bu y\u00fczden onu i\u015fiten ve g\u00f6ren yapt\u0131k."}, {"id": 579159, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5594, "text": "Ona yolu g\u00f6sterdik; ya \u015f\u00fckredendir, ya da nank\u00f6r."}, {"id": 579160, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5595, "text": "\u0130nkarc\u0131lar i\u00e7in zincirler, prangalar ve alevli bir ate\u015f haz\u0131rlam\u0131\u015f\u0131zd\u0131r."}, {"id": 579161, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 578, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5596, "text": "\u0130yiler ise, kafur (CHO) kar\u0131\u015ft\u0131r\u0131lm\u0131\u015f bir kadehten i\u00e7erler."}, {"id": 579162, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5597, "text": "ALLAH'\u0131n kullar\u0131n\u0131n ta\u015f\u0131ra ta\u015f\u0131ra i\u00e7tikleri bir kaynak..."}, {"id": 579163, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5598, "text": "Onlar s\u00f6zlerini yerine getirirler ve alabildi\u011fine k\u00f6t\u00fc olan bir g\u00fcnden korkarlar."}, {"id": 579164, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5599, "text": "Yoksula, \u00f6ks\u00fcze ve tutsa\u011fa sevdikleri yiyecekleri yedirirler."}, {"id": 579165, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5600, "text": "\"Biz sizi ALLAH r\u0131zas\u0131 i\u00e7in yediriyoruz. Sizden ne bir kar\u015f\u0131l\u0131k, ne de bir te\u015fekk\u00fcr beklemiyoruz.\""}, {"id": 579166, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5601, "text": "\"Biz, surats\u0131z ve belal\u0131 bir g\u00fcnden \u00f6t\u00fcr\u00fc Rabbimizden \u00e7ekiniriz.\""}, {"id": 579167, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5602, "text": "Nitekim, ALLAH onlar\u0131 o g\u00fcn\u00fcn k\u00f6t\u00fcl\u00fc\u011f\u00fcnden korur ve onlara ne\u015fe ve sevin\u00e7 verir."}, {"id": 579168, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5603, "text": "Direndikleri i\u00e7in onlar\u0131 bah\u00e7e ve ipekle \u00f6d\u00fcllendirir."}, {"id": 579169, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5604, "text": "Orada koltuklara yaslan\u0131rlar; orada ne (yak\u0131c\u0131) g\u00fcne\u015f ne de dondurucu so\u011fuk g\u00f6r\u00fcrler."}, {"id": 579170, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5605, "text": "A\u011fa\u00e7lar\u0131n g\u00f6lgesi \u00fczerlerine sarkm\u0131\u015f ve meyveler yakla\u015ft\u0131r\u0131larak kopar\u0131lmalar\u0131 kolayla\u015ft\u0131r\u0131lm\u0131\u015ft\u0131r."}, {"id": 579171, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5606, "text": "Onlara g\u00fcm\u00fc\u015f tepsiler ve \u015feffaf bardaklarda sunulur."}, {"id": 579173, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5607, "text": "G\u00fcm\u00fc\u015ften yap\u0131lm\u0131\u015f \u015feffaf bardaklar... Onlar\u0131 tam olarak haketmi\u015flerdir."}, {"id": 579174, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5608, "text": "Orada zencefil (ginger) kar\u0131\u015ft\u0131r\u0131lm\u0131\u015f bir kadehten i\u00e7irilirler."}, {"id": 579175, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5609, "text": "Bir kaynak ki, ona \"Selsebil\" denir."}, {"id": 579176, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5610, "text": "Onlara \u00f6l\u00fcms\u00fcz gen\u00e7ler servis yapacakt\u0131r. Onlar\u0131 g\u00f6rsen, kendilerini sa\u00e7\u0131lm\u0131\u015f inci san\u0131rs\u0131n."}, {"id": 579177, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5611, "text": "Nereye baksan nimetler ve b\u00fcy\u00fck bir y\u00f6netim g\u00f6r\u00fcrs\u00fcn."}, {"id": 579178, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5612, "text": "\u00dcstlerinde ye\u015fil kadifeden elbiseler ve ipekler vard\u0131r. G\u00fcm\u00fc\u015f bilezikler tak\u0131nm\u0131\u015flard\u0131r. Rab'leri onlara temiz bir i\u00e7ecek i\u00e7irir."}, {"id": 579179, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 514, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5613, "text": "Bu, \u00f6d\u00fcl\u00fcn\u00fczd\u00fcr ve gayretinize te\u015fekk\u00fcr edilmi\u015ftir."}, {"id": 579180, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5614, "text": "Ku\u015fkusuz, Kuran'\u0131 sana bir indiri\u015fle biz indirdik."}, {"id": 579181, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5615, "text": "\u00d6yleyse Rabbinin h\u00fckm\u00fcn\u00fc izlemekte sabret; onlardan hi\u00e7 bir nank\u00f6r g\u00fcnahkara uyma."}, {"id": 579182, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 579, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5616, "text": "Sabah ak\u015fam Rabbinin ismini an."}, {"id": 579183, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5617, "text": "Geceleyin O'na secde et ve O'nu uzun gecelerde y\u00fccelt."}, {"id": 579184, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5618, "text": "Bunlar \u015fu ge\u00e7ici (d\u00fcnya hayat\u0131n\u0131) seviyorlar ve \u00f6nlerindeki a\u011f\u0131r bir g\u00fcn\u00fc ise \u00f6nemsemiyorlar."}, {"id": 579185, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5619, "text": "Onlar\u0131 biz yaratt\u0131k ve yerle\u015ftirdik. Diledi\u011fimiz zaman da onlar\u0131 benzerleriyle de\u011fi\u015ftiririz."}, {"id": 579186, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5620, "text": "Bu bir hat\u0131rlatmad\u0131r: Dileyen, Rabbine varan bir yol tutar."}, {"id": 579187, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5621, "text": "ALLAH dilemedik\u00e7e siz dileyemezsiniz. ALLAH Bilendir, Bilgedir."}, {"id": 579188, "identifier": "tr.yuksel", "surah_number": 76, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0627\u0646\u0633\u0627\u0646", "surah_en_name": "Al-Insaan", "surah_en_name_translation": "Man", "revelation_type": "Medinan", "juz": 29, "manzil": 7, "page": 580, "ruku": 515, "hizb_quarter": 232, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5622, "text": "O, diledi\u011fini ve/veya dileyeni Rahmetine sokar. Zalimlere ise ac\u0131 bir ceza haz\u0131rlam\u0131\u015ft\u0131r."}]