[{"id": 579495, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5968, "text": "Sana o bunaltan\u0131n haberi ula\u015ft\u0131 m\u0131?"}, {"id": 579496, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5969, "text": "O g\u00fcn y\u00fczler sayg\u0131 g\u00f6sterir."}, {"id": 579497, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5970, "text": "\u00c7al\u0131\u015fm\u0131\u015ft\u0131r, yorgun d\u00fc\u015fm\u00fc\u015ft\u00fcr."}, {"id": 579498, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5971, "text": "K\u0131zg\u0131n ate\u015fe girerler."}, {"id": 579499, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5972, "text": "Kaynar bir p\u0131nardan i\u00e7irilirler."}, {"id": 579500, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5973, "text": "De\u011fersiz bir yiyece\u011fin d\u0131\u015f\u0131nda bir \u015feyleri yoktur;"}, {"id": 579501, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5974, "text": "Ne besler, ne de a\u00e7l\u0131\u011f\u0131 giderir."}, {"id": 579502, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5975, "text": "O g\u00fcn ba\u015fka y\u00fczler de var ki mutludur."}, {"id": 579503, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5976, "text": "Yapt\u0131klar\u0131ndan \u00f6t\u00fcr\u00fc sevin\u00e7lidir."}, {"id": 579432, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5977, "text": "Y\u00fcksek bir cennettedir."}, {"id": 579504, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5978, "text": "Orada bir sa\u00e7mal\u0131k i\u015fitmezler."}, {"id": 579505, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5979, "text": "Orada akan bir kaynak var."}, {"id": 579506, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5980, "text": "Orada y\u00fcksek mobilyalar var."}, {"id": 579507, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5981, "text": "Kadehler konulmu\u015f,"}, {"id": 579508, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5982, "text": "Yast\u0131klar dizilmi\u015f,"}, {"id": 579509, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5983, "text": "Ve hal\u0131lar serilmi\u015ftir."}, {"id": 579510, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5984, "text": "Bakmazlar m\u0131 develere, nas\u0131l yarat\u0131ld\u0131?"}, {"id": 579511, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5985, "text": "Ve g\u00f6\u011fe, nas\u0131l y\u00fckseltildi?"}, {"id": 579512, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5986, "text": "Ve da\u011flara, nas\u0131l dikildi?"}, {"id": 579513, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5987, "text": "Ve yere, nas\u0131l d\u00f6\u015fendi?"}, {"id": 579514, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5988, "text": "Hat\u0131rlat, \u00e7\u00fcnk\u00fc sen hat\u0131rlat\u0131c\u0131s\u0131n."}, {"id": 579515, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5989, "text": "Sen onlar\u0131 zorlayacak de\u011filsin."}, {"id": 579516, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5990, "text": "Fakat kim y\u00fcz \u00e7evirir ve inkar ederse,"}, {"id": 579517, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5991, "text": "ALLAH onu en b\u00fcy\u00fck ceza ile cezaland\u0131r\u0131r."}, {"id": 579518, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5992, "text": "Onlar\u0131n d\u00f6n\u00fc\u015fleri bizedir."}, {"id": 579519, "identifier": "tr.yuksel", "surah_number": 88, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u063a\u0627\u0634\u064a\u0629", "surah_en_name": "Al-Ghaashiya", "surah_en_name_translation": "The Overwhelming", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 530, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5993, "text": "Hesaplar\u0131n\u0131 g\u00f6rmek de bize d\u00fc\u015fer."}]