[{"id": 578846, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5272, "text": "NuN, kaleme ve yazd\u0131klar\u0131na andolsun."}, {"id": 578847, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5273, "text": "Sen Rabbinin nimetiyle delirmi\u015f de\u011filsin."}, {"id": 578797, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5274, "text": "Senin i\u00e7in kesintisiz bir \u00f6d\u00fcl vard\u0131r."}, {"id": 578819, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5275, "text": "Ku\u015fkusuz sen g\u00fc\u00e7l\u00fc bir karaktere sahipsin."}, {"id": 578647, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5276, "text": "Sen de g\u00f6receksin, onlar da g\u00f6recekler;"}, {"id": 578688, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5277, "text": "Hanginizin \u015fa\u015fk\u0131n oldu\u011funu."}, {"id": 578848, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5278, "text": "Rabbin, kimin yolundan sapm\u0131\u015f oldu\u011funu da en iyi bilir, do\u011fru yolda olanlar\u0131 da en iyi bilir."}, {"id": 578849, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5279, "text": "\u00d6yleyse yalanlayanlara uyma."}, {"id": 578850, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5280, "text": "Kendilerinin \u00f6d\u00fcn verip uzla\u015fabilmesi i\u00e7in senin de \u00f6d\u00fcn verip uzla\u015fman\u0131 istediler."}, {"id": 578851, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5281, "text": "\u015eunlar\u0131n hi\u00e7birine uyma: yemin edip duran, a\u015fa\u011f\u0131l\u0131k,"}, {"id": 578852, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5282, "text": "\u0130ftirac\u0131, s\u00f6z g\u00f6t\u00fcr\u00fcp getiren,"}, {"id": 578853, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5283, "text": "\u0130yili\u011fe ve yard\u0131ma engel olan, sald\u0131rgan, g\u00fcnahkar."}, {"id": 578854, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5284, "text": "\u0130nsafs\u0131z ve sahtekar."}, {"id": 578855, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5285, "text": "Mal ve \u00e7ocuk sahibidir diye (onlara uyma)."}, {"id": 578856, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 564, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5286, "text": "Ayetlerimiz kendisine okundu\u011fu zaman, \"Efsane\" der."}, {"id": 578857, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5287, "text": "Onun burnunu i\u015faretliyece\u011fiz."}, {"id": 578858, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5288, "text": "Onlar\u0131 s\u0131nad\u0131k; t\u0131pk\u0131 bah\u00e7e sahiplerini s\u0131nad\u0131\u011f\u0131m\u0131z gibi. Sabahleyin dev\u015fireceklerine yemin etmi\u015flerdi."}, {"id": 578859, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5289, "text": "Bundan hi\u00e7 bir ku\u015fkular\u0131 yoktu."}, {"id": 578860, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5290, "text": "Onlar uykudayken Rabbin taraf\u0131ndan g\u00f6nderilen bir ziyaret\u00e7i (f\u0131rt\u0131na) bah\u00e7elerini ziyaret etti."}, {"id": 578861, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5291, "text": "Ve bah\u00e7e meyvesiz kalm\u0131\u015ft\u0131."}, {"id": 578862, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5292, "text": "Sabahleyin birbirlerine seslendiler:"}, {"id": 578863, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5293, "text": "\"Dev\u015firecekseniz, haydi ekinlerinize erken var\u0131n.\""}, {"id": 578864, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5294, "text": "Derken yola \u00e7\u0131kt\u0131lar, aralar\u0131nda konu\u015fuyorlard\u0131."}, {"id": 578865, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5295, "text": "\"Sak\u0131n, bug\u00fcn hi\u00e7bir yoksul oraya yan\u0131n\u0131za girmesin.\""}, {"id": 578866, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5296, "text": "Sonu\u00e7tan emin bir halde erken vard\u0131lar."}, {"id": 578660, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5297, "text": "Fakat bah\u00e7eyi g\u00f6r\u00fcnce, \"Biz yolu \u015fa\u015f\u0131rd\u0131k,\" dediler."}, {"id": 578680, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5298, "text": "\"Yok, do\u011frusu biz yoksun b\u0131rak\u0131ld\u0131k.\""}, {"id": 578867, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5299, "text": "Ortancalar\u0131 (erdemlileri), \"Ben size demedim mi? Rabbinizi y\u00fcceltmeniz gerekmez miydi?\" dedi."}, {"id": 578868, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5300, "text": "Dediler ki, \"Rabbimiz y\u00fccedir. Biz zalimler imi\u015fiz.\""}, {"id": 578869, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5301, "text": "Ve hemen birbirlerini su\u00e7lamaya ba\u015flad\u0131lar."}, {"id": 578870, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5302, "text": "Dediler ki, \"Yaz\u0131klar olsun bize. Azg\u0131nm\u0131\u015f\u0131z.\""}, {"id": 578871, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5303, "text": "\"Belki Rabbimiz bize daha iyisini verir. Biz Rabbimize d\u00f6n\u00fcyoruz.\""}, {"id": 578872, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 498, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5304, "text": "\u0130\u015fte azap b\u00f6yledir. Ahiret azab\u0131 ise daha b\u00fcy\u00fckt\u00fcr. Bir bilselerdi."}, {"id": 578873, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5305, "text": "Erdemliler, Rab'lerinden nimet bah\u00e7eleri (cennetleri) haketmi\u015flerdir."}, {"id": 578874, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5306, "text": "M\u00fcsl\u00fcmanlara su\u00e7lular gibi mi davranal\u0131m?"}, {"id": 578875, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5307, "text": "Neyiniz var, ne bi\u00e7im h\u00fck\u00fcm veriyorsunuz?"}, {"id": 578876, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5308, "text": "Yoksa bir kitab\u0131n\u0131z var da onu mu okuyup duruyorsunuz?"}, {"id": 578877, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5309, "text": "Ve i\u00e7inde her diledi\u011finizi bulabiliyorsunuz?"}, {"id": 578878, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5310, "text": "Yoksa, diledi\u011finiz h\u00fckm\u00fc verebilece\u011finize dair Dirili\u015f G\u00fcn\u00fcne kadar s\u00fcrecek bir g\u00fcvence mi ald\u0131n\u0131z bizden?"}, {"id": 578879, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5311, "text": "Sor onlara, \"Bunu kim garanti etmektedir?\""}, {"id": 578880, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5312, "text": "Yoksa onlar\u0131n ortaklar\u0131 m\u0131 var? Haydi ortaklar\u0131n\u0131 getirsinler, e\u011fer do\u011fru kimseler iseler?"}, {"id": 578881, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 565, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5313, "text": "G\u00fcn gelecek, onlar\u0131n i\u00e7y\u00fczleri a\u00e7\u0131\u011fa \u00e7\u0131kar\u0131lacak, secdeye \u00e7a\u011fr\u0131lacaklar; ancak buna g\u00fc\u00e7leri yetmeyecektir."}, {"id": 578882, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5314, "text": "G\u00f6zleri d\u00fc\u015fm\u00fc\u015f bir durumda, onlar\u0131 a\u015fa\u011f\u0131lanma kaplar. Oysa onlar sa\u011flam iken secdeye \u00e7a\u011fr\u0131lm\u0131\u015flard\u0131"}, {"id": 578883, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5315, "text": "Bu hadisi (s\u00f6z\u00fc) yalanlayanla Beni ba\u015fba\u015fa b\u0131rak. Onlar\u0131, bilmedikleri yerden yava\u015f yava\u015f yakla\u015ft\u0131raca\u011f\u0131z."}, {"id": 578884, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5316, "text": "Onlara m\u00fchlet veriyorum. Benim plan\u0131m sa\u011flamd\u0131r."}, {"id": 578885, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5317, "text": "Yoksa sen onlardan bir \u00fccret istiyorsun da onlar a\u011f\u0131r bir bor\u00e7 alt\u0131nda m\u0131 kal\u0131yorlar?"}, {"id": 578886, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5318, "text": "Yoksa ge\u00e7mi\u015fin ve gelece\u011fin bilgisi onlar\u0131n yan\u0131nda da onlar m\u0131 kaydediyorlar?"}, {"id": 578887, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5319, "text": "Rabbinin h\u00fckm\u00fcn\u00fcn ger\u00e7ekle\u015fmesi i\u00e7in sabret. Bal\u0131\u011f\u0131n arkada\u015f\u0131 (Yunus) gibi olma. Hani o, (bal\u0131k taraf\u0131ndan) yutulmu\u015f bir durumda iken seslenmi\u015fti."}, {"id": 578888, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5320, "text": "Rabbinden ona bir nimet ula\u015fmasayd\u0131, k\u0131nanm\u0131\u015f olarak \u00e7orak bir sahile at\u0131lacakt\u0131."}, {"id": 578889, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5321, "text": "Ancak Rabbi onu se\u00e7ip erdemlilerden k\u0131ld\u0131."}, {"id": 578890, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5322, "text": "Mesaj\u0131 i\u015fittikleri zaman, inkarc\u0131lar neredeyse seni g\u00f6zleriyle yiyeceklerdi. \"O, delidir!\" diyorlard\u0131."}, {"id": 578891, "identifier": "tr.yuksel", "surah_number": 68, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0644\u0645", "surah_en_name": "Al-Qalam", "surah_en_name_translation": "The Pen", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 499, "hizb_quarter": 226, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5323, "text": "Halbuki o, t\u00fcm evrene bir mesajd\u0131r."}]