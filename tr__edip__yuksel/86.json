, "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5938, "text": "Omurga ile kaburga kemikleri aras\u0131ndan \u00e7\u0131kar."}, {"id": 579465, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5939, "text": "Ku\u015fkusuz O, onu diriltme\u011fe g\u00fcc\u00fc yetendir."}, {"id": 579466, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5940, "text": "Gizemlerin ortaya \u00e7\u0131kar\u0131laca\u011f\u0131 g\u00fcn."}, {"id": 579467, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5941, "text": "Onun ne bir g\u00fcc\u00fc ne de bir yard\u0131mc\u0131s\u0131 olmayacakt\u0131r."}, {"id": 579468, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5942, "text": "Geri \u00e7eviren g\u00f6\u011fe andolsun,"}, {"id": 579469, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5943, "text": "Ve yar\u0131lan yery\u00fcz\u00fcne andolsun ki,"}, {"id": 579470, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5944, "text": "Bu, ay\u0131rdedici bir s\u00f6zd\u00fcr."}, {"id": 579471, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5945, "text": "\u015eakaya al\u0131namaz."}, {"id": 579472, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5946, "text": "Onlar bir plan kuruyor."}, {"id": 579473, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5947, "text": "Ben de bir plan kuruyorum."}, {"id": 579474, "identifier": "tr.yuksel", "surah_number": 86, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0637\u0627\u0631\u0642", "surah_en_name": "At-Taariq", "surah_en_name_translation": "The Morning Star", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 528, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5948, "text": "\u00d6yleyse inkarc\u0131lara az bir s\u00fcre daha tan\u0131."}]