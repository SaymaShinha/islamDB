[{"id": 579436, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5910, "text": "Andolsun galaksiler sahibi g\u00f6\u011fe."}, {"id": 579437, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5911, "text": "S\u00f6z verilen g\u00fcne,"}, {"id": 579438, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5912, "text": "Ve tan\u0131\u011fa da tan\u0131k olunana da andolsun."}, {"id": 579439, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5913, "text": "Kanyon halk\u0131na yaz\u0131klar olsun."}, {"id": 579440, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Tutu\u015fturulmu\u015f ate\u015fin-"}, {"id": 579441, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5915, "text": "Ba\u015f\u0131nda oturmu\u015flar,"}, {"id": 579442, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5916, "text": "Ve inananlara yapt\u0131klar\u0131 i\u015fkenceyi seyrediyorlar."}, {"id": 579443, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5917, "text": "\u0130nananlardan nefret ediyorlard\u0131. Sadece, onlar \u00dcst\u00fcn ve \u00d6vg\u00fcye lay\u0131k olan ALLAH'a inand\u0131klar\u0131 i\u00e7in..."}, {"id": 579444, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5918, "text": "G\u00f6klerin ve yerin y\u00f6netimi O'na aittir. Ve ALLAH her\u015feye Tan\u0131kt\u0131r."}, {"id": 579445, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5919, "text": "\u0130nanan erkeklere ve kad\u0131nlara zul\u00fcm ve i\u015fkencede bulunan ve daha sonra tevbe etmeyenler cehennem azab\u0131n\u0131 haketmi\u015flerdir. Onlar i\u00e7in yak\u0131c\u0131 bir azap vard\u0131r."}, {"id": 579446, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5920, "text": "\u0130nanan ve erdemli davrananlar ise i\u00e7lerinden \u0131rmaklar akan cennetleri haketmi\u015flerdir. B\u00fcy\u00fck ba\u015far\u0131 budur."}, {"id": 579447, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Do\u011frusu, Rabbinin yakalamas\u0131 pek \u00e7etindir."}, {"id": 579448, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Ba\u015flatan ve tekrarlayan O'dur."}, {"id": 579449, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5923, "text": "O Ba\u011f\u0131\u015flayand\u0131r, Sevendir."}, {"id": 579450, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5924, "text": "\u015eanl\u0131 y\u00f6netimin sahibidir."}, {"id": 579451, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Diledi\u011fini yapand\u0131r."}, {"id": 579452, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Ordular\u0131n tarihinden haberin var m\u0131?"}, {"id": 579453, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5927, "text": "Firavun'un, Semud'un?"}, {"id": 579454, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5928, "text": "\u0130nkarc\u0131lar, kronik yalanlay\u0131c\u0131lard\u0131r."}, {"id": 579455, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5929, "text": "ALLAH onlar\u0131 arkalar\u0131ndan ku\u015fatm\u0131\u015ft\u0131r."}, {"id": 579456, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5930, "text": "Ger\u00e7ekten, o \u015fanl\u0131 bir Kuran'd\u0131r."}, {"id": 579457, "identifier": "tr.yuksel", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5931, "text": "Korunmu\u015f bir ana levhadad\u0131r."}]