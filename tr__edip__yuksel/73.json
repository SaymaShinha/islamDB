[{"id": 579042, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5476, "text": "Ey b\u00fcr\u00fcnen,"}, {"id": 579043, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5477, "text": "Baz\u0131 geceler hari\u00e7, geceleyin kalk."}, {"id": 579044, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5478, "text": "Yar\u0131s\u0131nda veya yar\u0131s\u0131ndan biraz \u00f6nce."}, {"id": 579045, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5479, "text": "Yahut yar\u0131s\u0131ndan biraz sonra... Kuran'\u0131 da \u00f6zenle d\u00fc\u015f\u00fcne d\u00fc\u015f\u00fcne oku."}, {"id": 579046, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5480, "text": "Biz sana a\u011f\u0131r bir s\u00f6z b\u0131rakaca\u011f\u0131z."}, {"id": 579047, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5481, "text": "Ku\u015fkusuz gece kalk\u0131p meditasyonda bulunmak \u00e7ok daha etkili ve ifade a\u00e7\u0131s\u0131ndan daha uygundur."}, {"id": 579048, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5482, "text": "Seni g\u00fcnd\u00fcz\u00fcn epeyce u\u011fra\u015ft\u0131racak i\u015flerin vard\u0131r."}, {"id": 579049, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5483, "text": "Rabbinin ismini an ve t\u00fcm g\u00f6nl\u00fcnle O'na y\u00f6nel."}, {"id": 579050, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5484, "text": "Do\u011funun ve bat\u0131n\u0131n Rabbi; O'ndan ba\u015fka tanr\u0131 yoktur. Kendine avukat olarak O'nu se\u00e7."}, {"id": 579051, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5485, "text": "Onlar\u0131n s\u00f6zlerine kar\u015f\u0131 sab\u0131rl\u0131 ol ve onlardan g\u00fczellikle ayr\u0131l."}, {"id": 579052, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5486, "text": "Varl\u0131k sahibi olup da seni yalanlayanlar\u0131 bana b\u0131rak; onlara biraz s\u00fcre tan\u0131."}, {"id": 579053, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5487, "text": "Yan\u0131m\u0131zda zincirler ve cehennem vard\u0131r."}, {"id": 579054, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5488, "text": "Yutulmas\u0131 g\u00fc\u00e7 bir yiyecek ve ac\u0131 bir azap..."}, {"id": 579055, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5489, "text": "G\u00fcn gelir, yer ve da\u011flar titrer ve da\u011flar da\u011f\u0131lan kum y\u0131\u011f\u0131n\u0131na d\u00f6n\u00fc\u015f\u00fcr."}, {"id": 579056, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5490, "text": "Biz, size hakk\u0131n\u0131zda tan\u0131kl\u0131k edecek bir el\u00e7i g\u00f6nderdik; nas\u0131l ki Firavun'a da bir el\u00e7i g\u00f6ndermi\u015ftik."}, {"id": 579057, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5491, "text": "Ancak Firavun, el\u00e7iye kar\u015f\u0131 geldi; biz de onu feci bir bi\u00e7imde yakalad\u0131k."}, {"id": 579058, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5492, "text": "Peki, inkar ederseniz, \u00e7ocuklar\u0131n sa\u00e7lar\u0131n\u0131 a\u011fartan bir g\u00fcnden nas\u0131l korunursunuz?"}, {"id": 579059, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5493, "text": "G\u00f6k bile ondan dolay\u0131 yar\u0131l\u0131r. O'nun s\u00f6z\u00fc ger\u00e7ekle\u015fir."}, {"id": 579060, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5494, "text": "Bu bir hat\u0131rlatmad\u0131r; dileyen, Rabbine do\u011fru yol tutar."}, {"id": 579061, "identifier": "tr.yuksel", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 509, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5495, "text": "Rabbin, senin ve yolda\u015flar\u0131dan bir grubun, gecenin \u00fc\u00e7te ikisinden az, yar\u0131s\u0131nda ve \u00fc\u00e7te birinde kalkt\u0131\u011f\u0131n\u0131 bilir. Gecenin ve g\u00fcnd\u00fcz\u00fcn miktar\u0131n\u0131 ALLAH belirler. O, sizin bunu yapam\u0131yaca\u011f\u0131n\u0131z\u0131 bildi\u011fi i\u00e7in sizi affetmi\u015ftir. \u00d6yleyse Kuran'dan kolay\u0131n\u0131za geldi\u011fi kadar okuyun. Aran\u0131zda hastalar, yery\u00fcz\u00fcnde ALLAH'\u0131n l\u00fctfundan r\u0131z\u0131k arayanlar ve ALLAH yolunda sava\u015fanlar oldu\u011funu bilmektedir. Ondan kolay\u0131n\u0131za geldi\u011fi kadar okuyun. Namaz\u0131 g\u00f6zetin, zekat\u0131 verin ve g\u00fczel davranmak yoluyla ALLAH'a bir bor\u00e7sunun. Kendiniz i\u00e7in yapt\u0131\u011f\u0131n\u0131z her iyili\u011fi, ALLAH kat\u0131nda daha iyi ve daha b\u00fcy\u00fck bir \u00f6d\u00fcl olarak bulacaks\u0131n\u0131z. ALLAH'tan ba\u011f\u0131\u015flanma dileyin; ALLAH Ba\u011f\u0131\u015flayand\u0131r, Rahimdir."}]