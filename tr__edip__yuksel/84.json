[{"id": 579410, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5885, "text": "G\u00f6k \u00e7atlad\u0131\u011f\u0131 zaman."}, {"id": 579411, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5886, "text": "Ve do\u011fal yap\u0131s\u0131 gere\u011fince Rabbine kulak verdi\u011fi zaman."}, {"id": 579412, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5887, "text": "Yer d\u00fcmd\u00fcz edildi\u011fi,"}, {"id": 579413, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5888, "text": "\u0130\u00e7indekileri at\u0131p bo\u015faltt\u0131\u011f\u0131,"}, {"id": 579414, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5889, "text": "Ve do\u011fal yap\u0131s\u0131 gere\u011fince Rabbine kulak verdi\u011fi zaman."}, {"id": 579415, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5890, "text": "Ey insan, Rabbine do\u011fru \u00e7al\u0131\u015f\u0131p \u00e7abalars\u0131n ve sonunda O'nunla kar\u015f\u0131la\u015f\u0131rs\u0131n."}, {"id": 579416, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5891, "text": "Kitab\u0131 sa\u011f taraftan verilen,"}, {"id": 579417, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5892, "text": "Kolay bir hesaba \u00e7ekilecek,"}, {"id": 579418, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5893, "text": "Ve arkada\u015flar\u0131na sevin\u00e7 i\u00e7inde d\u00f6necektir."}, {"id": 579419, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5894, "text": "Kitab\u0131 arkas\u0131ndan verilen ise,"}, {"id": 579420, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5895, "text": "Yok olmay\u0131 arzulayacak,"}, {"id": 579421, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5896, "text": "Ve bir ate\u015fte yanacakt\u0131r."}, {"id": 579422, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5897, "text": "Oysa arkada\u015flar\u0131 aras\u0131nda sevin\u00e7liydi;"}, {"id": 579423, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5898, "text": "Bir daha (Rabbine) d\u00f6nmeyece\u011fini sanm\u0131\u015ft\u0131."}, {"id": 579424, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5899, "text": "Do\u011frusu, Rabbi onu g\u00f6rmektedir."}, {"id": 579425, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5900, "text": "Andolsun ak\u015fam\u0131n k\u0131z\u0131ll\u0131\u011f\u0131na,"}, {"id": 579426, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5901, "text": "Gecenin toplad\u0131\u011f\u0131na,"}, {"id": 579427, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5902, "text": "Dolunay halindeki aya,"}, {"id": 579428, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5903, "text": "Siz evreden evreye binip ge\u00e7eceksiniz."}, {"id": 579429, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5904, "text": "Peki onlar neden inanm\u0131yorlar?"}, {"id": 579430, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": true, "sajda_id": 14, "sajda_obligatory": false, "sajda_recommended": true, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5905, "text": "Onlara Kuran okundu\u011funda neden kabul etmiyorlar?"}, {"id": 579431, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5906, "text": "Aksine, inkarc\u0131lar yalanl\u0131yorlar."}, {"id": 579433, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5907, "text": "ALLAH onlar\u0131n saklad\u0131klar\u0131n\u0131 \u00e7ok iyi bilir."}, {"id": 579434, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5908, "text": "Onlar\u0131 ac\u0131 bir azapla m\u00fcjdele."}, {"id": 579435, "identifier": "tr.yuksel", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5909, "text": "\u0130nan\u0131p erdemli davrananlara ise kesilmez bir \u00f6d\u00fcl var."}]