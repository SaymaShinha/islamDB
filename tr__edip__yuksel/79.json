[{"id": 579271, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5713, "text": "Andolsun s\u00f6k\u00fcp \u00e7\u0131karanlara,"}, {"id": 579272, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5714, "text": "Rahat\u00e7a \u00e7ekenlere,"}, {"id": 579273, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5715, "text": "Y\u00fcz\u00fcp akanlara,"}, {"id": 579274, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5716, "text": "Yar\u0131\u015f\u0131p birbirlerini ge\u00e7enlere,"}, {"id": 579275, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5717, "text": "Ve b\u00f6ylece emirleri uygulayanlara..."}, {"id": 579276, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5718, "text": "O g\u00fcn o sars\u0131nt\u0131 sarsar."}, {"id": 579277, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5719, "text": "Ard\u0131ndan bir di\u011feri izler."}, {"id": 579278, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5720, "text": "O g\u00fcn y\u00fcrekler titrer."}, {"id": 579279, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5721, "text": "G\u00f6zleri ise al\u00e7al\u0131r."}, {"id": 579280, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5722, "text": "Derler ki, \"Daha \u00f6nceki halimize mi d\u00f6nd\u00fcr\u00fcld\u00fck?\""}, {"id": 579281, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5723, "text": "\"Biz \u00e7\u00fcr\u00fcm\u00fc\u015f kemikler olduktan sonra ha!?\""}, {"id": 579282, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5724, "text": "\"\u00d6yleyse bu zarar\u0131na bir d\u00f6n\u00fc\u015ft\u00fcr,\" derler."}, {"id": 579283, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5725, "text": "O bir tek d\u00fcrt\u00fc\u015ften ibarettir."}, {"id": 579284, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5726, "text": "Onlar uyan\u0131vermi\u015flerdir."}, {"id": 579285, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 583, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5727, "text": "Sana Musa'n\u0131n tarihi ula\u015ft\u0131 m\u0131?"}, {"id": 579286, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5728, "text": "Rabbi, kutsal Tuva vadisinde ona seslenmi\u015fti:"}, {"id": 579287, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5729, "text": "\"Firavun'a git; o azd\u0131.\""}, {"id": 579288, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5730, "text": "\"Ona de ki: Ar\u0131nmayacak m\u0131s\u0131n?\""}, {"id": 579289, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5731, "text": "\"Seni Rabbine ileteyim de sayg\u0131l\u0131 olas\u0131n.\""}, {"id": 579290, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5732, "text": "Ona b\u00fcy\u00fck mucizeyi g\u00f6sterdi."}, {"id": 579291, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5733, "text": "Fakat o yalanlad\u0131 ve kar\u015f\u0131 geldi."}, {"id": 579292, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5734, "text": "Sonra, s\u0131rt\u0131n\u0131 d\u00f6nd\u00fc, (aleyhte) \u00e7aba g\u00f6sterdi."}, {"id": 579293, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5735, "text": "Toplay\u0131p, ilan etti."}, {"id": 579294, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5736, "text": "\"Ben sizin en y\u00fcce rabbinizim,\" dedi."}, {"id": 579295, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5737, "text": "Sonunda, ALLAH onu ahiret ve d\u00fcnya cezas\u0131na \u00e7arpt\u0131."}, {"id": 579296, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 520, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5738, "text": "Ku\u015fkusuz, sayg\u0131 duyanlar i\u00e7in bunda bir ibret vard\u0131r."}, {"id": 579297, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5739, "text": "Siz mi, yoksa g\u00f6k m\u00fc yarat\u0131l\u0131\u015f a\u00e7\u0131s\u0131ndan daha zorludur? Onu O yapt\u0131."}, {"id": 579298, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5740, "text": "Onu alabildi\u011fine y\u00fckseltti ve d\u00fczenledi."}, {"id": 579299, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5741, "text": "Gecesini karartt\u0131, sabah\u0131n\u0131 ise ortaya \u00e7\u0131kard\u0131."}, {"id": 579300, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5742, "text": "Ve yeri de yumurta bi\u00e7imine soktu"}, {"id": 579301, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5743, "text": "Ondan suyunu ve otla\u011f\u0131n\u0131 \u00e7\u0131kard\u0131."}, {"id": 579302, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5744, "text": "Da\u011flar\u0131 da \u00e7akt\u0131."}, {"id": 579303, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5745, "text": "T\u00fcm bunlar sizin ve hayvanlar\u0131n\u0131z\u0131n ge\u00e7imi i\u00e7in."}, {"id": 579304, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5746, "text": "B\u00fcy\u00fck bask\u0131n geldi\u011fi zaman,"}, {"id": 579305, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5747, "text": "O g\u00fcn insan, neyin u\u011frunda \u00e7aba harcad\u0131\u011f\u0131n\u0131 anlar."}, {"id": 579306, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5748, "text": "Cehennem g\u00f6z \u00f6n\u00fcne \u00e7\u0131kar\u0131lacakt\u0131r."}, {"id": 579307, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5749, "text": "Azg\u0131nlara,"}, {"id": 579308, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5750, "text": "Ve d\u00fcnya hayat\u0131n\u0131 ye\u011fleyenlere gelince."}, {"id": 579309, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5751, "text": "Gidilecek yer cehennem olacakt\u0131r."}, {"id": 579310, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5752, "text": "Rabbinin makam\u0131na kar\u015f\u0131 sayg\u0131 duyan ve kendini k\u00f6t\u00fc arzulardan al\u0131koyanlara gelince"}, {"id": 579311, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5753, "text": "Gidilecek yer cennet olacakt\u0131r."}, {"id": 579312, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5754, "text": "Sana Saat (d\u00fcnyan\u0131n sonu) ne zaman ger\u00e7ekle\u015fecektir diye soruyorlar."}, {"id": 579313, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5755, "text": "Onu bildirmek, (ey Muhammed) senin g\u00f6revin de\u011fildir."}, {"id": 579314, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5756, "text": "Onun karar\u0131 Rabbine aittir."}, {"id": 579315, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Senin g\u00f6revin, sadece ondan korkanlar\u0131 uyarmakt\u0131r."}, {"id": 579316, "identifier": "tr.yuksel", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "tr", "name": "Edip Y\u00fcksel", "transelator_en_name": "Edip Y\u00fcksel", "format": "text", "type": "translation", "ayah_number": 5758, "text": "Onu g\u00f6rd\u00fckleri g\u00fcn, sanki (d\u00fcnyada) bir ak\u015fam veya ku\u015fluk vakti kadar kalm\u0131\u015flard\u0131r"}]