[{"id": 123883, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5552, "text": "Hle, p\u0159\u00edsah\u00e1m p\u0159i dni zmrtv\u00fdchvst\u00e1n\u00ed"}, {"id": 123884, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5553, "text": "a p\u0159i du\u0161i, je\u017e st\u00e1l\u00e9 v\u00fd\u010ditky si \u010din\u00ed!"}, {"id": 123885, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5554, "text": "Mysl\u00ed si \u010dlov\u011bk snad, \u017ee neshrom\u00e1\u017ed\u00edme jeho kosti?"}, {"id": 123886, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5555, "text": "V\u0161ak ano, a dokonce schopni jsme sestavit jeho prsty!"}, {"id": 123887, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5556, "text": "A naopak, \u010dlov\u011bk si sp\u00ed\u0161e p\u0159eje jednat i d\u00e1l h\u0159\u00ed\u0161n\u011b"}, {"id": 123888, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5557, "text": "a pt\u00e1 se: \"Kdy v\u016fbec ten den zmrtv\u00fdchvst\u00e1n\u00ed nastane?\""}, {"id": 118554, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5558, "text": "A\u017e zrak se oslep\u00ed,"}, {"id": 118553, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5559, "text": "a\u017e m\u011bs\u00edc se zatm\u00ed,"}, {"id": 118558, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5560, "text": "a\u017e slunce s m\u011bs\u00edcem se spoj\u00ed,"}, {"id": 123824, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5561, "text": "tehdy \u010dlov\u011bk vyk\u0159ikne: \"Kam prchnout m\u00e1m?\""}, {"id": 123856, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5562, "text": "V\u0161ak pozor! Nebude \u017e\u00e1dn\u00fd \u00fakryt zn\u00e1m,"}, {"id": 123889, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5563, "text": "u P\u00e1na tv\u00e9ho v ten den bude shroma\u017edi\u0161t\u011b jen!"}, {"id": 123890, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5564, "text": "V ten den bude \u010dlov\u011bku sd\u011bleno, co zav\u010das u\u010dinil a co zame\u0161kal."}, {"id": 123891, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5565, "text": "Ba co v\u00edc, \u010dlov\u011bk s\u00e1m sebe bude jasn\u011b z\u0159\u00edt,"}, {"id": 123892, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5566, "text": "a kdyby sv\u00e9 omluvy mohl p\u0159edlo\u017eit..."}, {"id": 123893, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5567, "text": "Neh\u00fdbej p\u0159\u00edli\u0161 jazykem sv\u00fdm, kdy\u017e je p\u0159edn\u00e1\u0161\u00ed\u0161, ve snaze je usp\u00ed\u0161it,"}, {"id": 123894, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5568, "text": "v\u017edy\u0165 N\u00e1m p\u0159\u00edslu\u0161\u00ed je shrom\u00e1\u017edit i \u010d\u00edst."}, {"id": 123895, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5569, "text": "A kdy\u017e ti je p\u0159ed\u010d\u00edt\u00e1me, sleduj jeho p\u0159edn\u00e1\u0161en\u00ed,"}, {"id": 123896, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 577, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5570, "text": "a potom na N\u00e1s je podat jeho objasn\u011bn\u00ed."}, {"id": 123897, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5571, "text": "V\u0161ak pozor! Vy naopak milujete rychle plynouc\u00ed"}, {"id": 123898, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5572, "text": "a zanedb\u00e1v\u00e1te \u017eivot budouc\u00ed."}, {"id": 123899, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5573, "text": "V ten den budou jedny tv\u00e1\u0159e z\u00e1\u0159\u00edc\u00ed"}, {"id": 123900, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5574, "text": "a ke sv\u00e9mu P\u00e1nu hled\u00edc\u00ed"}, {"id": 123901, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5575, "text": "a jin\u00e9 tv\u00e1\u0159e v ten den budou sinal\u00e9, tu\u0161\u00edc\u00ed,"}, {"id": 123902, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5576, "text": "\u017ee dovr\u0161\u00ed se na nich trest p\u00e1te\u0159 l\u00e1maj\u00edc\u00ed."}, {"id": 123903, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5577, "text": "V\u0161ak pozor! A\u017e du\u0161e vystoup\u00ed ke kl\u00ed\u010dn\u00ed kosti"}, {"id": 123904, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5578, "text": "a bude \u0159e\u010deno: \"Kdo je zakl\u00ednatel?\""}, {"id": 123905, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5579, "text": "a ona si pomysl\u00ed, \u017ee toto je se sv\u011btem rozluka,"}, {"id": 123906, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5580, "text": "a proplete se s nohou noha,"}, {"id": 123907, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 512, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5581, "text": "v ten den k tv\u00e9mu P\u00e1nu v\u0161ichni hn\u00e1ni budou."}, {"id": 123908, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5582, "text": "\"On almu\u017enu ned\u00e1val ani modlitbu nekonal,"}, {"id": 123909, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5583, "text": "v\u0161ak odvracel se a za le\u017e prohla\u0161oval,"}, {"id": 123910, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5584, "text": "a pak ke sv\u00e9 rodin\u011b py\u0161n\u011b kr\u00e1\u010deje se odebral!\""}, {"id": 123911, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5585, "text": "Ach b\u011bda ti, b\u011bda!"}, {"id": 123912, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5586, "text": "A znovu b\u011bda ti, b\u011bda!"}, {"id": 123913, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5587, "text": "Mysl\u00ed si \u010dlov\u011bk snad, \u017ee s\u00e1m sob\u011b bude z\u016fstaven?"}, {"id": 123914, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5588, "text": "Co\u017e nebyl kapkou semene vyst\u0159\u00edknut\u00e9ho jen?"}, {"id": 123915, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5589, "text": "Potom se kapkou p\u0159ilnavou stal a B\u016fh jej stvo\u0159il a vyrovnal"}, {"id": 123916, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5590, "text": "a dv\u011b pohlav\u00ed, mu\u017esk\u00e9 a \u017eensk\u00e9, z n\u011bho ud\u011blal."}, {"id": 123917, "identifier": "cs.hrbek", "surah_number": 75, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u064a\u0627\u0645\u0629", "surah_en_name": "Al-Qiyaama", "surah_en_name_translation": "The Resurrection", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 578, "ruku": 513, "hizb_quarter": 231, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5591, "text": "Co\u017e nen\u00ed v moci jeho, aby mrtv\u00fdm op\u011bt \u017eivot dal?"}]