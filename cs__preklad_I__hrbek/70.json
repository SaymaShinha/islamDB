[{"id": 123705, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5376, "text": "Ptal se t\u00e1zaj\u00edc\u00ed na trest, jen\u017e dopadne"}, {"id": 123706, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5377, "text": "na nev\u011b\u0159\u00edc\u00ed: nen\u00ed nikoho, kdo jej odraz\u00ed,"}, {"id": 123707, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5378, "text": "v\u017edy\u0165 od Boha, P\u00e1na stup\u0148\u016f, p\u0159ich\u00e1z\u00ed!"}, {"id": 123708, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5379, "text": "Stoupaj\u00ed k n\u011bmu po nich and\u011bl\u00e9 i duch v den, jeho\u017e d\u00e9lka let pades\u00e1t tis\u00edc m\u011b\u0159\u00ed."}, {"id": 123709, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5380, "text": "Neochv\u011bjn\u00fd bu\u010f a p\u011bkn\u011b jen vytrvej!"}, {"id": 123710, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0639\u0627\u0631\u062c", "surah_en_name": "Al-Ma'aarij", "surah_en_name_translation": "The Ascending Stairways", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 502, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5381, "text": "Nev\u011b\u0159\u00edc\u00ed daleko vid\u00ed ten den,"}, {"id": 123711, "identifier": "cs.hrbek", "surah_number": 70, "surah_ar_name": "\u0633\u