tifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5852, "text": "Co\u017e si mysl\u00ed, \u017ee nebudou vzk\u0159\u00ed\u0161eni"}, {"id": 124159, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5853, "text": "pro den nesm\u00edrn\u00fd,"}, {"id": 124160, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 587, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5854, "text": "pro den, kdy lid\u00e9 p\u0159ed P\u00e1na vesm\u00edru p\u0159edstoup\u00ed?"}, {"id": 124161, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5855, "text": "V\u0161ak pozor, v\u017edy\u0165 v\u011bru kniha h\u0159\u00ed\u0161n\u00edk\u016f je v Sid\u017ed\u017e\u00ednu!"}, {"id": 124162, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5856, "text": "V\u00ed\u0161 ty v\u016fbec, co je to Sid\u017ed\u017e\u00edn?"}, {"id": 124163, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5857, "text": "Kniha ozna\u010dkovan\u00e1!"}, {"id": 124164, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5858, "text": "B\u011bda v ten den t\u011bm, kdo\u017e za le\u017e prohla\u0161uj\u00ed"}, {"id": 124165, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5859, "text": "a za v\u00fdmysl den soudn\u00fd pokl\u00e1daj\u00ed!"}, {"id": 124166, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5860, "text": "Nepokl\u00e1d\u00e1 jej za v\u00fdmysl nikdo krom\u011b v\u0161ech protivn\u00edk\u016f h\u0159\u00ed\u0161n\u00fdch,"}, {"id": 124167, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5861, "text": "kte\u0159\u00ed - kdy\u017e jsou jim p\u0159edn\u00e1\u0161ena Na\u0161e znamen\u00ed - \u0159\u00edkaj\u00ed: \"To jsou pov\u00edda\u010dky star\u00fdch!\""}, {"id": 124168, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5862, "text": "V\u0161ak pozor! Naopak srdce jejich zreziv\u011bla od toho, co si vyslou\u017eila."}, {"id": 124169, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5863, "text": "V\u0161ak pozor! V\u011bru \u017ee budou v ten den od sv\u00e9ho P\u00e1na odd\u011bleni z\u00e1v\u011bsem"}, {"id": 124170, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5864, "text": "a potom budou ho\u0159et v ohni pekeln\u00e9m"}, {"id": 124171, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5865, "text": "a posl\u00e9ze jim bude \u0159e\u010deno: \"Hle, zde m\u00e1te, co naz\u00fdvali jste v\u00fdmyslem!\""}, {"id": 124172, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5866, "text": "V\u0161ak pozor! V\u017edy\u0165 v\u011bru kniha zbo\u017en\u00fdch je v \u00b4Illij\u00fan!"}, {"id": 124173, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5867, "text": "V\u00ed\u0161 ty v\u016fbec, co je to \u00b4Illij\u00fan?"}, {"id": 124174, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5868, "text": "Kniha ozna\u010dkovan\u00e1,"}, {"id": 124175, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5869, "text": "na vlastn\u00ed o\u010di ji spat\u0159\u00ed ten, kdo k P\u00e1nu bude p\u0159ibl\u00ed\u017een."}, {"id": 124176, "identifier": "cs.hrbek", "surah_number": 83, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0637\u0641\u0641\u064a\u0646", "surah_en_name": "Al-Mutaffifin", "surah_en_name_translation": "Defrauding", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 588, "ruku": 525, "hizb_quarter": 235, "sajda": false, "sajda_id": 0, "