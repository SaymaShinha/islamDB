, "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5741, "text": "temnotou noc jeho zahalil a jasu denn\u00edmu z n\u011bho vzej\u00edt dal"}, {"id": 124061, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5742, "text": "a zemi posl\u00e9ze rozprost\u0159el,"}, {"id": 124062, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5743, "text": "a aby voda i pastviny na n\u00ed vze\u0161ly, zp\u016fsobil"}, {"id": 124063, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5744, "text": "a tak\u00e9 hory tam zakotvil,"}, {"id": 124064, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5745, "text": "aby u\u017eitek v tom pro v\u00e1s i va\u0161e st\u00e1da byl."}, {"id": 124065, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5746, "text": "A a\u017e nadejde ten p\u0159evrat nesm\u00edrn\u00fd"}, {"id": 124066, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5747, "text": "v den, kdy \u010dlov\u011bk si vzpomene na v\u0161echno sv\u00e9 sna\u017een\u00ed,"}, {"id": 124067, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5748, "text": "tehdy p\u0159edvedeno bude peklo tomu, kdo uvid\u00ed."}, {"id": 124068, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5749, "text": "Ti, kdo\u017e vzpurn\u00ed byli"}, {"id": 124069, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5750, "text": "a \u017eivot pozemsk\u00fd si obl\u00edbili,"}, {"id": 124070, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5751, "text": "t\u011bm v\u011bru peklo za \u00fatulek p\u0159ipravili."}, {"id": 124071, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5752, "text": "V\u0161ak ten, jen\u017e postaven\u00ed P\u00e1na sv\u00e9ho se b\u00e1l a du\u0161i svou p\u0159ed v\u00e1\u0161n\u00ed spasil,"}, {"id": 124072, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5753, "text": "ten v\u011bru v zahrad\u011b rajsk\u00e9 sv\u016fj bude m\u00edt azyl."}, {"id": 124073, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5754, "text": "Budou se t\u011b na Hodinu, a kdy m\u00e1 nastat, pt\u00e1t;"}, {"id": 124074, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5755, "text": "pro\u010d ty m\u00e1\u0161 se vlastn\u011b o n\u00ed zmi\u0148ovat?"}, {"id": 124075, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5756, "text": "V\u017edy\u0165 P\u00e1nu tv\u00e9mu pat\u0159\u00ed lh\u016fta jej\u00ed kone\u010dn\u00e1!"}, {"id": 124076, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5757, "text": "Ty jen varovatelem jsi toho, jen\u017e obavu z n\u00ed m\u00e1."}, {"id": 124077, "identifier": "cs.hrbek", "surah_number": 79, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u0627\u0632\u0639\u0627\u062a", "surah_en_name": "An-Naazi'aat", "surah_en_name_translation": "Those who drag forth", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 584, "ruku": 521, "hizb_quarter": 233, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 5758, "text": "A v den, kdy spat\u0159\u00ed ji, bude se jim zd\u00e1t, jako by v hrobech sv\u00fdch v\u00edc ne\u017e ve\u010der \u010di jitro nestr\u00e1vili."}]