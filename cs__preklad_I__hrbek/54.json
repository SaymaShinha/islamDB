[{"id": 123190, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4847, "text": "Bl\u00ed\u017e\u00ed se hodina a m\u011bs\u00edc se rozpolt\u00ed."}, {"id": 123191, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4848, "text": "Kdy\u017e nev\u011b\u0159\u00edc\u00ed znamen\u00ed vid\u00ed, odvracej\u00ed se a \u0159\u00edkaj\u00ed: \"Kouzlo ustavi\u010dn\u00e9!\""}, {"id": 123192, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4849, "text": "A za le\u017e je prohla\u0161uj\u00ed a sv\u00e9 v\u00e1\u0161n\u011b n\u00e1sleduj\u00ed, v\u0161ak ustanoven\u00ed ka\u017ed\u00e9 je nem\u011bnn\u00e9."}, {"id": 123193, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4850, "text": "A v\u011bru dostalo se jim ji\u017e n\u011bkter\u00fdch zv\u011bst\u00ed, je\u017e v\u00fdstrahy obsahuj\u00ed"}, {"id": 123194, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4851, "text": "i moudrost dokonalou. V\u0161ak nic platn\u00e9 nen\u00ed varov\u00e1n\u00ed!"}, {"id": 123195, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4852, "text": "Odvra\u0165 se od nich tedy! V den, kdy svolavatel zavol\u00e1 k v\u011bci stra\u0161n\u00e9,"}, {"id": 123196, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4853, "text": "se sklopen\u00edmi zraky vystoup\u00ed z hrob\u016f sv\u00fdch jak kobylky rozpt\u00fdlen\u00e9,"}, {"id": 123197, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4854, "text": "krky ke svolavateli nata\u017een\u00e9! A \u0159eknou nev\u011b\u0159\u00edc\u00ed: \"Toto v\u011bru t\u011b\u017ek\u00fd je den!\""}, {"id": 123198, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4855, "text": "A ji\u017e p\u0159ed nimi lid Noem\u016fv znamen\u00ed za v\u00fdmysl pokl\u00e1dal a Na\u0161eho slu\u017eebn\u00edka za lh\u00e1\u0159e prohl\u00e1sil; i \u0159ekli oni: \"Bl\u00e1zen!\" a on byl jimi odstra\u0161en."}, {"id": 123199, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4856, "text": "P\u00e1na sv\u00e9ho vz\u00fdval: \"Pomoz mi, jsem p\u0159emo\u017een!\""}, {"id": 123200, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4857, "text": "A br\u00e1ny nebesk\u00e9 jsme otev\u0159eli pro vodu tekouc\u00ed proudem"}, {"id": 123201, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4858, "text": "a ze zem\u011b vytrysknout jsme dali pramen\u016fm, a\u017e setkaly se oba proudy dle rozkazu, jen\u017e byl ji\u017e p\u0159edur\u010den."}, {"id": 123202, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4859, "text": "A nalo\u017eili jsme Noeho na lo\u010f postavenou z prken a palmov\u00fdch vl\u00e1ken,"}, {"id": 123203, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4860, "text": "aby plula pod na\u0161\u00edm zrakem v odm\u011bnu tomu, jen\u017e byl tak odvr\u017een."}, {"id": 123204, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4861, "text": "A ponechali jsme ji jako znamen\u00ed. Co\u017e nen\u00ed nikoho, kdo p\u0159ipomn\u011bl by si to nyn\u00ed?"}, {"id": 123205, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4862, "text": "A jak\u00fd byl M\u016fj trest a M\u00e9 varov\u00e1n\u00ed?"}, {"id": 123206, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4863, "text": "A u\u010dinili jsme Kor\u00e1n snadn\u00fdm pro p\u0159ipom\u00edn\u00e1n\u00ed; co\u017e nen\u00ed nikoho, kdo p\u0159ipomn\u011bl by si to nyn\u00ed?"}, {"id": 123207, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4864, "text": "A tak\u00e9 \u00b4\u00c1dovci prohl\u00e1sili za le\u017e Na\u0161e znamen\u00ed a jak\u00fd byl M\u016fj trest a M\u00e9 varov\u00e1n\u00ed?"}, {"id": 123208, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4865, "text": "A vyslali jsme proti nim v\u00edtr svi\u0161tiv\u00fd v den neblah\u00fd, nekone\u010dn\u00fd,"}, {"id": 123209, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4866, "text": "jen\u017e lidi vytrh\u00e1val tak, \u017ee byli jak palem vyvr\u00e1cen\u00fdch kmeny."}, {"id": 123210, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4867, "text": "A jak\u00fd byl M\u016fj trest a M\u00e9 varov\u00e1n\u00ed?"}, {"id": 123211, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 463, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4868, "text": "A u\u010dinili jsme Kor\u00e1n snadn\u00fdm pro p\u0159ipom\u00edn\u00e1n\u00ed; co\u017e nen\u00ed nikoho, kdo p\u0159ipomn\u011bl by si to nyn\u00ed?"}, {"id": 123212, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4869, "text": "A rovn\u011b\u017e Tham\u00fadovci varov\u00e1n\u00ed za l\u017eiv\u00e9 pokl\u00e1dali"}, {"id": 123213, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4870, "text": "a \u0159ekli: \"M\u00e1me snad n\u00e1sledovat smrteln\u00edka z n\u00e1s vze\u0161l\u00e9ho? To bychom zbloudil\u00ed a po\u0161etil\u00ed byli!"}, {"id": 123214, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4871, "text": "Jak to, \u017ee zrovna jemu ze v\u0161ech n\u00e1s bylo p\u0159ipomenut\u00ed svr\u017eeno? Ba nikoliv, on lh\u00e1\u0159 je opov\u00e1\u017eliv\u00fd!\""}, {"id": 123215, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4872, "text": "V\u0161ak z\u00edtra se dozv\u011bd\u00ed, kdo lh\u00e1\u0159 byl opov\u00e1\u017eliv\u00fd!"}, {"id": 123216, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 529, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4873, "text": "My k nim v\u011bru po\u0161leme velbloudici jako poku\u0161en\u00ed jejich, a ty je trp\u011bliv\u011b pozoruj"}, {"id": 123217, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4874, "text": "a oznam jim, \u017ee voda je mezi n\u011b rozd\u011blena a napit\u00ed ka\u017ed\u00e9 m\u00e1 \u010das sv\u016fj."}, {"id": 123218, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4875, "text": "I zavolali sv\u00e9ho druha, ten ruku vzt\u00e1hl a velbloudici zahubil."}, {"id": 123219, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4876, "text": "A jak\u00fd byl M\u016fj trest a M\u00e9 varov\u00e1n\u00ed?"}, {"id": 123220, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4877, "text": "A poslali jsme na n\u011b jen v\u00fdk\u0159ik jedin\u00fd a stali se podobn\u00fdmi sl\u00e1m\u011b such\u00e9, z n\u00ed\u017e ohrada se stav\u00ed."}, {"id": 123221, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4878, "text": "A u\u010dinili jsme Kor\u00e1n snadn\u00fdm pro p\u0159ipom\u00edn\u00e1n\u00ed; co\u017e nen\u00ed nikoho, kdo p\u0159ipomn\u011bl by si to nyn\u00ed?"}, {"id": 123222, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4879, "text": "Za l\u017eiv\u00e9 soukmenovci Lotovi varov\u00e1n\u00ed prohl\u00e1sili,"}, {"id": 123223, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4880, "text": "a poslali jsme v\u00edtr kamen\u00ed nesouc\u00ed proti nim - krom\u011b rodu Lotova, jej\u017e za jitra jsme zachr\u00e1nili"}, {"id": 123224, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4881, "text": "z milosti Sv\u00e9. A takto odm\u011b\u0148ujeme ty, kdo\u017e vd\u011b\u010dnost projevili."}, {"id": 123225, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4882, "text": "A\u010dkoliv Lot je varoval p\u0159ed Na\u0161\u00ed p\u0159\u00edsnost\u00ed, oni pochybovali o varov\u00e1n\u00ed"}, {"id": 123226, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4883, "text": "a p\u0159\u00e1li si jeho hosty sv\u00e9st, v\u0161ak o\u010di jejich jsme oslepili: \"Okuste trestu M\u00e9ho a varov\u00e1n\u00ed!\""}, {"id": 123227, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4884, "text": "A naz\u00edt\u0159\u00ed zr\u00e1na je zas\u00e1hl trest trval\u00fd."}, {"id": 123228, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4885, "text": "\"Okuste trestu M\u00e9ho a varov\u00e1n\u00ed!\""}, {"id": 123229, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 464, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4886, "text": "A u\u010dinili jsme Kor\u00e1n snadn\u00fdm pro p\u0159ipom\u00edn\u00e1n\u00ed; co\u017e nen\u00ed nikoho, kdo p\u0159ipomn\u011bl by si to nyn\u00ed?"}, {"id": 123230, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4887, "text": "A v\u011bru dostalo se i rodu Fara\u00f3novu varov\u00e1n\u00ed,"}, {"id": 123231, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4888, "text": "v\u0161ak za le\u017e prohl\u00e1sili v\u0161echna Na\u0161e znamen\u00ed a uchv\u00e1tili jsme je tak, jak mocn\u00fd a v\u0161emohouc\u00ed \u010din\u00ed."}, {"id": 123232, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4889, "text": "Jsou snad va\u0161i nev\u011b\u0159\u00edc\u00ed siln\u011bj\u0161\u00ed, ne\u017e byli tamti \u010di bylo v\u00e1m v P\u00edsmech zaru\u010deno bezpe\u010d\u00ed?"}, {"id": 123233, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4890, "text": "Anebo \u0159eknou snad: \"My jsme shrom\u00e1\u017ed\u011bn\u00ed v\u00edt\u011bz\u00edc\u00ed.\""}, {"id": 123234, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4891, "text": "V\u0161ak toto shrom\u00e1\u017ed\u011bn\u00ed na \u00fat\u011bk bude obr\u00e1ceno a z\u00e1dy se oto\u010d\u00ed."}, {"id": 123235, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4892, "text": "Ba ano! Hodina bude \u010dasem sch\u016fzky jejich, Hodina velmi z\u00e1ludn\u00e1 a ho\u0159ce chutnaj\u00edc\u00ed."}, {"id": 123236, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4893, "text": "A h\u0159\u00ed\u0161n\u00edci v\u011bru budou v po\u0161etilosti a v bloud\u011bn\u00ed,"}, {"id": 123237, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4894, "text": "a v den, kdy budou tv\u00e1\u0159\u00ed k zemi vle\u010deni, zvol\u00e1no bude: \"Ochutnejte Saqaru pohlazen\u00ed!\""}, {"id": 123238, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 530, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4895, "text": "A ka\u017edou v\u011bc jsme vskutku v ur\u010den\u00fdch rozm\u011brech stvo\u0159ili"}, {"id": 123239, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4896, "text": "a rozkaz n\u00e1\u0161 je pouze slovo jedin\u00e9, rychl\u00e9 jak oka m\u017eiknut\u00ed."}, {"id": 123240, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4897, "text": "A vskutku jsme ji\u017e zahubili jin\u00e9 v\u00e1m podobn\u00e9. Co\u017e nen\u00ed nikoho, kdo p\u0159ipomn\u011bl by si to nyn\u00ed?"}, {"id": 123241, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4898, "text": "A v\u0161e, co konali, je v P\u00edsmech zaps\u00e1no"}, {"id": 123242, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4899, "text": "a v\u0161e, a\u0165 mal\u00e9 \u010di velk\u00e9, je v nich zaznamen\u00e1no."}, {"id": 123243, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4900, "text": "A bohabojn\u00ed v\u011bru budou mezi zahradami a \u0159ekami,"}, {"id": 118516, "identifier": "cs.hrbek", "surah_number": 54, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0642\u0645\u0631", "surah_en_name": "Al-Qamar", "surah_en_name_translation": "The Moon", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 531, "ruku": 465, "hizb_quarter": 212, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "cs", "name": "Hrbek", "transelator_en_name": "Preklad I. Hrbek", "format": "text", "type": "translation", "ayah_number": 4901, "text": "v p\u0159\u00edbytku pravdy, u vl\u00e1dce v\u0161emohouc\u00edho!"}]