[{"id": 385432, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5200, "text": "\ud558\ub298\uacfc \ub300\uc9c0\uc5d0 \uc788\ub294 \uac83\uc774 \ud558\ub098\ub2d8\uc758 \uc601\uad11\uc744 \ucc2c\ubbf8\ud558\ub098\ub2c8 \uc655\uad6d\uc640\ucc2c\ubbf8\ub294 \uadf8\ubd84\uc758 \uac83\uc774\ub85c\ub2e4 \uc2e4\ub85c \uadf8 \ubd84\uc740 \ubaa8\ub4e0 \uc77c\uc5d0 \uc804\uc9c0\uc804\ub2a5\ud558\uc2ec\uc774\ub77c"}, {"id": 385433, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5201, "text": "\ub108\ud76c\ub97c \ucc3d\uc870\ud558\uc2e0 \ubd84\ub3c4 \uadf8\ubd84\uc774\uc2dc\uac70\ub298 \uadf8\ub7ec\ub098 \ub108\ud76c\uc911\uc758 \ubb34\ub9ac\ub294 \ubd88\uc2e0\uc790\uc694 \ub2e4\ub978 \ubb34\ub9ac\ub294 \ubbff\ub294 \uc790\ub4e4 \uc774\ub77c \uc2e4\ub85c \ud558\ub098\ub2d8\uc740 \ub108\ud76c\uac00 \ud589\ud558 \ub294 \ubaa8\ub4e0 \uac83\uc744 \uc9c0\ucf1c\ubcf4\uace0 \uacc4\uc2dc\ub2c8\ub77c"}, {"id": 385434, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5202, "text": "\uadf8\ubd84\uaed8\uc11c \ud558\ub298\uacfc \ub300\uc9c0\ub97c \uc9c4\ub9ac\ub85c \ucc3d\uc870\ud558\uc2dc\uace0 \ub108\ud76c\uc758 \ud615\uc0c1\uc744 \ub9cc \ub4dc\uc0ac \uac00\uc7a5 \uc544\ub984\ub2e4\uc6b4 \ud615\uc0c1\uc73c\ub85c \ube5a \uc73c\uc168\uc73c\ub2c8 \ucd5c\ud6c4\uc758 \ubaa9\uc801\uc9c0\ub294 \uadf8\ubd84\uc774\uc2dc\ub77c"}, {"id": 385435, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5203, "text": "\uadf8\ubd84\uc740 \ud558\ub298\uacfc \ub300\uc9c0\uc5d0 \uc788\ub294 \ubaa8\ub4e0 \uac83\uc744 \uc544\uc2dc\uba70 \ub610\ud55c \ub108\ud76c\uac00 \uc228\uae30\ub294 \uac83\uacfc \ub4dc\ub7ec\ub0b4\ub294 \ubaa8\ub4e0 \uac83\ub3c4 \uc544\uc2dc\ub098\ub2c8 \ud558\ub098\ub2d8\uc740 \uc2ec\uc911\uc5d0 \uc788\ub294 \ubaa8 \ub4e0 \uac83\uc744 \uc54c\uace0 \uacc4\uc2dc\ub2c8\ub77c"}, {"id": 385436, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5204, "text": "\uc774\uc804\uc5d0 \ubbff\uc74c\uc744 \ubd88\uc2e0\ud588\ub358 \uc790\ub4e4\uc774 \uadf8\ub4e4\uc758 \ud589\uc704\ub85c \ub9d0\ubbf8\uc554\uc544 \uc0ac\uc545 \ud568\uc744 \ub9db\ubcf4\uace0 \uace0\ud1b5\uc2a4\ub7ec\uc6b4 \uc751\ubc8c\uc774 \uadf8\ub4e4\uc5d0\uac8c \uc788\uc5c8\ub358 \uadf8\ub4e4\uc758 \uc774\uc57c\uae30\uac00\ub108\ud76c\uc5d0\uac8c \uc774\ub974\uc9c0 \uc544\ub2c8 \ud588\ub290\ub1e8"}, {"id": 385437, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5205, "text": "\uadf8\uac83\uc740 \ubd84\uba85\ud55c \uc608\uc99d\uc73c\ub85c \uc120\uc9c0\uc790\ub4e4\uc774 \uadf8\ub4e4\uc5d0\uac8c \uc774\ub974\ub800\uc744 \ub54c \uc778 \uac04\uc774 \uc6b0\ub9ac\ub97c \uc778\ub3c4\ud55c\ub2e8 \ub9d0\uc774\ub1e8 \ub77c \uace0 \uadf8\ub4e4\uc740 \ub9d0\ud558\uba74\uc11c \ubd88\uc2e0\ud558\uace0 \uc678 \uba74\ud558\uc600\uae30 \ub54c\ubb38\uc774\ub77c \ud558\ub098\ub2d8\uc740 \ud48d\uc131\ud568\uacfc \uc601\uad11\uc73c\ub85c \ucda9\ub9cc\ud558\uc2dc\ub2c8\ub77c"}, {"id": 385438, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5206, "text": "\ubbff\uc74c\uc744 \ubd80\uc815\ud55c \uc790\ub4e4\uc740 \ubd80\ud65c\uc774\uc5c6\ub2e4\uace0 \uc8fc\uc7a5\ud558\ub098 \uc77c\ub7ec\uac00\ub85c\ub418 \uadf8\ub807\uc9c0 \uc544\ub2c8\ud558\uba70 \uc8fc\ub2d8\uc5d0 \uc758\ud558\uc5ec \ubd80\ud65c \ub418\uc5b4 \ub108\ud76c\uac00 \ud589\ud55c \ubaa8\ub4e0\uac83\uc5d0 \uad00\ud574 \uc9c8\ubb38\uc744 \ubc1b\uac8c \ub418\ub2c8\ub77c \uadf8\ub807\uac8c \ud568\uc774 \ud558\ub098\ub2d8\uaed8\ub294 \uc26c\uc6b4 \uc77c\uc774\ub77c"}, {"id": 385382, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5207, "text": "\uadf8\ub7ec\ubbc0\ub85c \ud558\ub098\ub2d8\uacfc \uadf8\ubd84\uc758 \uc120\uc9c0\uc790\uc640 \ud558\ub098\ub2d8\uc774 \uacc4\uc2dc\ud55c \ube5b\uc744 \ubbff \uc73c\ub77c \ud558\ub098\ub2d8\uc740 \ub108\ud76c\uac00 \ud589\ud558\ub294 \ubaa8 \ub4e0 \uac83\uc744 \uc54c\uace0 \uacc4\uc2dc\ub2c8\ub77c"}, {"id": 385439, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 556, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5208, "text": "\uad70\uc9d1\ud558\ub294 \uc5b4\ub290 \ub0a0\uc744 \uc704\ud558\uc5ec \ud558\ub098\ub2d8\uc774 \ub108\ud76c\ub97c \uc9d1\ud569\uc2dc\ud0a4\ub294 \uadf8\ub0a0\uc740 \ub108\ud76c \uac00\uc6b4\ub370 \uc783\uc740 \uc790\uc640 \uc5bb\uc740 \uc790\uac00 \uc788\uac8c \ub418\ub294 \ub0a0\ub85c \ud558\ub098\ub2d8\uc744 \ubbff\uc5b4 \uc758\ub85c\uc6c0\uc744 \ud589\ud55c \uc790\ub97c \uc704\ud574 \uadf8\ubd84\uc740 \uadf8\ub4e4\uc758 \uacfc\uc624\ub97c \uac70\ub450\uc5b4 \uc8fc\uc0ac \uac15\ubb3c\uc774 \ud750\ub974\ub294 \ucc9c\uad6d\uc73c\ub85c \uadf8\ub4e4\uc774 \ub4e4\uc5b4\uac00\ub294 \uac83\uc744 \ud5c8\ub77d\ud558\uc5ec \uc601\uc0dd\ucf00 \ud558\ub2c8 \uadf8\uac83\uc774 \uc2b9\ub9ac\ub77c"}, {"id": 385440, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 490, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5209, "text": "\uadf8\ub7ec\ub098 \ubbff\uc74c\uc744 \ubd88\uc2e0\ud558\uace0 \ud558\ub098\ub2d8\uc758 \uc608\uc99d\ub4e4\uc744 \uac70\uc5ed\ud55c \uc790\ub4e4\uc740 \ubd88\uc9c0\uc625\uc758 \uac70\uc8fc\uc790\uac00 \ub418\uc5b4 \uadf8\uacf3\uc5d0\uc11c\uc601\uc8fc\ud558\ub9ac\ub2c8 \uc0ac\uc545\ud55c \uc6b4\uba85\uc774\ub77c"}, {"id": 385441, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5210, "text": "\ud558\ub098\ub2d8\uc758 \ud5c8\ub77d\uc5c6\uc774 \uc5b4\ub5a4 \uc7ac\uc559\ub3c4 \uc788\uc744 \uc218 \uc5c6\ub098\ub2c8 \ud558\ub098\ub2d8\uc740 \uadf8\ubd84\uc744 \ubbff\ub294\uc790\uc758 \ub9c8\uc74c\uc744 \uc778\ub3c4\ud558\uc2e4 \uac83\uc774\ub77c \uc2e4\ub85c \ud558\ub098\ub2d8\uc740 \ubaa8\ub4e0 \uac83\uc744 \uc544\uc2ec\uc73c\ub85c \ucda9\ub9cc\ud558\uc2dc\ub2c8\ub77c"}, {"id": 385442, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5211, "text": "\uadf8\ub7ec\ubbc0\ub85c \ud558\ub098\ub2d8\uaed8 \ubcf5\uc885\ud558\uace0\uc120\uc9c0\uc790\uaed8 \uc21c\uc885\ud558\ub77c \ub108\ud76c\uac00 \uc678\uba74\ud55c \ub2e4 \ud558\uc5ec\ub3c4 \ud558\ub098\ub2d8\uc758 \uc120\uc9c0\uc790\ub294 \ubd84 \uba85\ud55c \uba54\uc138\uc9c0\ub97c \uc804\ud558\ub294 \uac83 \ubfd0\uc774\ub77c"}, {"id": 385443, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5212, "text": "\ud558\ub098\ub2d8 \uc678\uc5d0\ub294 \uc2e0\uc774 \uc5c6\ub098\ub2c8 \ubbff\ub294 \uc2e0\ub3c4\ub4e4\uc774\uc5ec \uadf8\ubd84\uaed8\ub9cc \uc758\ud0c1\ud574 \uc57c \ud558\ub2c8\ub77c"}, {"id": 385444, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5213, "text": "\ubbff\ub294 \uc0ac\ub78c\ub4e4\uc774\uc5ec \ub108\ud76c \uc544\ub0b4 \uc640 \uc790\uc190 \uc911\uc5d0\ub3c4 \ub108\ud76c \uc801\uc774 \uc788\uac70\ub298 \uadf8\ub4e4\ub3c4 \uc870\uc2ec\ud558\ub77c \uadf8\ub7ec\ub098 \ub108\ud76c\uac00 \uc0ac\ub791\uc744 \ubca0\ud480\uc5b4 \uc6a9\uc11c\ud558\uace0 \uad00\uc6a9\uc744 \ubca0\ud47c\ub2e4\uba74 \ud558\ub098\ub2d8\uc740 \uad00\uc6a9\uacfc \uc790\ube44\ub85c \ucda9\ub9cc\ud558\uc2dc\ub2c8\ub77c"}, {"id": 385445, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5214, "text": "\ub108\ud76c \uc7ac\uc0b0\uacfc \ub108\ud76c \uc790\uc190\uc740 \ud558\ub098\uc758 \uc2dc\ud5d8\uc774\ub098 \ud558\ub098\ub2d8 \uc548\uc5d0 \uc788\ub294 \uac83\uc740 \uac00\uc7a5 \uc704\ub300\ud55c \ubcf4\uc0c1\uc774\ub77c"}, {"id": 385446, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5215, "text": "\uadf8\ub7ec\ubbc0\ub85c \ucd5c\uc120\uc744 \ub2e4\ud558\uc5ec \ud558 \ub098\ub2d8\uc744 \ub450\ub824\uc6cc\ud558\uace0 \uadc0\ub97c \uae30\uc6b8\uc774 \uba70 \uc21c\uc885\ud558\uace0 \uc790\uc120\uc744 \ubca0\ud480\ub77c \uadf8\uac83 \uc774 \ub108\ud76c \uc2a4\uc2a4\ub85c\ub97c \uc704\ud55c \ubcf5\uc774 \ub418\uba70 \uc2a4\uc2a4\ub85c\uc758 \ud0d0\uc695\uc5d0\uc11c \ubc97\uc5b4\ub098\ub294 \uc790\ub9cc \uc774 \ubc88\uc131\ud568\uc744 \ub204\ub9ac\ub2c8\ub77c"}, {"id": 385447, "identifier": "ko.korean", "surah_number": 64, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062a\u063a\u0627\u0628\u0646", "surah_en_name": "At-Taghaabun", "surah_en_name_translation": "Mutual Disillusion", "revelation_type": "Medinan", "juz": 28, "manzil": 7, "page": 557, "ruku": 491, "hizb_quarter": 222, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5216, "text": "\ub108\ud76c\uac00 \ud558\ub098\ub2d8\uc744 \uc704\ud558\uc5ec \uc88b \uc740 \uac83\uc73c\ub85c \uc790\uc120\uc744 \ubca0\ud47c\ub2e4\uba74 \uadf8\ubd84 \uc740 \uadf8\uac83\uc744 \ub108\ud76c\uc5d0\uac8c \ub450\ubc30\ub85c \ud558\uc5ec \uc8fc\uc2dc\uba70 \