[{"id": 384702, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4474, "text": "\ud558 \ubc08"}, {"id": 384703, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4475, "text": "\uc774\uac83\uc740 \uad8c\ub2a5\uacfc \uc9c0\ud61c\ub85c \ucda9\ub9cc\ud558\uc2e0 \ud558\ub098\ub2d8\uc73c\ub85c\ubd80\ud130 \uacc4\uc2dc\ub41c \uc131\uc11c \ub77c"}, {"id": 384704, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4476, "text": "\uc2e4\ub85c \ud558\ub298\uacfc \ub300\uc9c0\uc5d0 \ubbff\ub294 \uc0ac \ub78c\ub4e4\uc744 \uc704\ud55c \uc608\uc99d\uc774 \uc788\uc73c\uba70"}, {"id": 384705, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4477, "text": "\ub108\ud76c\ub97c \ucc3d\uc870\ud55c \uac83\uacfc \ub300\uc9c0\uc704\uc5d0 \uc0b0\uc7ac\ud558\uc5ec \uc788\ub294 \uac83\ub3c4 \ubbff\ub294 \uc790\ub4e4\uc744 \uc704\ud55c \uc608\uc99d\uc774\ub77c"}, {"id": 384706, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4478, "text": "\ub0ae\uacfc \ubc24\uc774 \uad50\ucc28\ub418\ub294 \uac83\ub3c4 \ud558 \ub098\ub2d8\uaed8\uc11c \ud558\ub298\ub85c\ubd80\ud130 \uc77c\uc6a9\ud560 \uc591\uc2dd \uc744 \uc8fc\uc2dc\ub294 \uac83\ub3c4 \uadf8\ub9ac\uace0 \uc8fd\uc740 \ub300\uc9c0\ub97c \uc18c\uc0dd\ucf00\ud558\uba70 \ubc14\ub78c\uc758 \ubc29\ud5a5\uc744\ubcc0\ud654\uc2dc\ud0a4\ub294 \uac83\ub3c4 \uc774\ud574\ud558\ub294 \ubc31\uc131 \ub4e4\uc744 \uc704\ud55c \uc608\uc99d\uc774\ub77c"}, {"id": 384707, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4479, "text": "\uadf8\uac83\ub4e4\uc774 \ud558\ub098\ub2d8\uaed8\uc11c \uc9c4\ub9ac\ub85c \uc368 \uadf8\ub300\uc5d0\uac8c \ub0ad\uc1a1\ud55c \uadf8\ubd84\uc758 \uc608\uc99d \ub4e4\uc774\ub77c \uadf8\ub798\ub3c4 \ud558\ub098\ub2d8\uacfc \uadf8\ubd84\uc758 \uc608\uc99d\uc744 \uac70\uc5ed\ud55c\ub2e4\uba74 \uadf8\ub4e4\uc740 \uc5b4\ub5a4 \ub9d0\uc744 \ubbff\ub294\ub2e8 \ub9d0\uc774\ub1e8"}, {"id": 384708, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4480, "text": "\uac70\uc9d3\ud558\ub294 \ubaa8\ub4e0 \uc8c4\uc778\ub4e4\uc5d0\uac8c \uc7ac\uc559\uc774 \uc788\uc73c\ub9ac\ub77c"}, {"id": 384709, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4481, "text": "\uc554\uc1a1\ub41c \ud558\ub098\ub2d8\uc758 \ub9d0\uc500\uc744 \ub4e4\uc740\uadf8\uac00 \uadf8 \ub9d0\uc500\uc744 \ub4e3\uc9c0 \uc544\ub2c8\ud588\ub358 \uac83\ucc98\ub7fc \uac70\ub9cc\ud558\uace0 \uc624\ub9cc\ud574 \ud558\ub2c8 \uadf8\uc5d0 \uac8c \uace0\ud1b5\uc2a4\ub7ec\uc6b4 \uc751\ubc8c\uc774 \uc788\uc744 \uac83\uc774 \ub77c\uace0 \uc804\ud560"}, {"id": 384710, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4482, "text": "\ud558\ub098\ub2d8\uc758 \uc608\uc99d\uc744 \ub4e4\uc5c8\uc744 \ub54c \uadf8\uac83\ub4e4\uc744 \uc870\ub871\ud55c \uadf8\uc5d0\uac8c\ub3c4 \uace0\ud1b5\uc2a4\ub7ec\uc6b4 \ubc8c\uc774 \uc788\uc73c\ub9ac\ub77c"}, {"id": 384711, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4483, "text": "\uadf8\ub4e4 \uc55e\uc5d0\ub294 \uc9c0\uc625\ubfd0\uc774\uba70 \uadf8\ub4e4\uc774 \uc5bb\uc5c8\ub358 \uc5b4\ub5a4 \uac83\ub3c4 \uc720\uc6a9\ud558\uc9c0 \uc544\ub2c8\ud558\uba70 \ud558\ub098\ub2d8 \uc678\uc5d0\ub294 \uadf8\ub4e4\uc744 \uc704\ud574 \uc5b4\ub5a4 \ubcf4\ud638\uc790\ub3c4 \uc5c6\ub098\ub2c8 \uadf8\ub4e4 \uc5d0\uac8c\ub294 \ubb34\uc11c\uc6b4 \uc751\ubc8c \ubc16\uc5d4 \uc5c6\ub178\ub77c"}, {"id": 384712, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 434, "hizb_quarter": 199, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4484, "text": "\uc774\uac83\uc774 \ubc14\ub85c \uae38\uc774\ub77c \uadf8\ub7fc\uc73c\ub85c \uc8fc\ub2d8\uc758 \uc608\uc99d\uc744 \uac70\uc5ed\ud558\ub294 \uc790 \uadf8\ub4e4\uc5d0\uac8c\ub294 \uace0\ud1b5\uc2a4\ub7ec\uc6b4 \ubc8c\uc774 \uc788\uc744 \ubfd0\uc774\ub77c"}, {"id": 384713, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4485, "text": "\ubc14\ub2e4\ub85c \ud558\uc5ec\uae08 \ub108\ud76c\uc5d0\uac8c \uc21c \uc885\ucf00 \ud558\uc5ec \uadf8\ubd84\uc758 \uba85\ub839\uc73c\ub85c \ubc30\uac00 \ud56d\ud574\ud558\uac8c \ud558\uace0 \uadf8\uac83\uc73c\ub85c \uc778\ud558\uc5ec \uadf8\ubd84\uc758 \uc740\ud61c\ub97c \uad6c\ud558\uac8c \ud558\uc2e0 \ubd84\uc774 \ud558\ub098\ub2d8\uc774\uc2dc\ub77c \uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\ub294 \uac10 \uc0ac\ud558\ub77c"}, {"id": 384714, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 499, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4486, "text": "\uadf8\ubd84\uc740 \ud558\ub298\uacfc \ub300\uc9c0\uc704\uc5d0 \uc788 \ub294 \ubaa8\ub4e0 \uac83\uc774 \ub108\ud76c\uc5d0\uac8c \uc21c\uc885\ud1a0\ub85d \ud558\uc600\ub098\ub2c8 \uc2e4\ub85c \uadf8 \uc548\uc5d0\ub294 \uc0dd\uac01\ud558 \ub294 \uc0ac\ub78c\ub4e4\uc744 \uc704\ud55c \uc608\uc99d\ub4e4\uc774 \uc788\ub178 \ub77c"}, {"id": 384715, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4487, "text": "\ud558\ub098\ub2d8\uc758 \ub0a0\ub4e4\uc744 \ub450\ub824\uc6cc \ud558\uc9c0 \uc544\ub2c8\ud55c \uadf8\ub4e4\uc744 \uc6a9\uc11c\ud55c \ubbff\ub294 \uadf8\ub4e4\uc5d0\uac8c \uc77c\ub7ec \uac00\ub85c\ub418 \ud558\ub098\ub2d8\uc740 \uadf8 \ub4e4\uc774 \uc5bb\uc740 \uac83\uc5d0 \ub530\ub77c \ubc31\uc131\ub4e4\uc5d0\uac8c \ubcf4\uc0c1\ud558\uc2dc\ub2c8\ub77c"}, {"id": 384716, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4488, "text": "\uc758\ub85c\uc6c0\uc744 \uc2e4\ucc9c\ud55c \uc790 \uadf8\uac83\uc740\uc790\uae30\uc758 \uc601\ud63c\uc744 \uc704\ud574\uc11c\uc694 \uc545\uc744 \ud37c \ub728\ub9b0 \uc790 \uadf8\uac83\uc740 \uc2a4\uc2a4\ub85c \uc5ed\ud589\ub418\ub294 \uac83\uc73c\ub85c \ub108\ud76c \ubaa8\ub450\uac00 \ub108\ud76c \uc8fc\ub2d8\uaed8 \uadc0\uc758\ud558\ub178\ub77c"}, {"id": 384717, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4489, "text": "\uc2e4\ub85c \ud558\ub098\ub2d8\uc740 \uc774\uc2a4\ub77c\uc5d8 \uc790\uc190\ub4e4\uc5d0\uac8c \uc131\uc11c\uc640 \uc8fc\uad8c\uacfc \uc608\uc5b8 \uc790\ub4e4\uc744 \uc8fc\uc5c8\uc73c\uba70 \ub610\ud55c \ub2e4\ub978 \ubc31\uc131 \uc5d0 \uc6b0\uc120\ud558\uc5ec \uc88b\uc740 \uc77c\uc6a9\ud560 \uc591\uc2dd\uc744 \uc8fc\uc5c8\ub178\ub77c"}, {"id": 384718, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4490, "text": "\uadf8\ub9ac\uace0 \ud558\ub098\ub2d8\uc740 \uadf8\ub4e4\uc758 \uc885\uad50\uc5d0 \ubd84\uba85\ud55c \uc608\uc99d\uc744 \uc8fc\uc5c8\uc73c\ub418 \uadf8 \ub4e4 \uac00\uc6b4\ub370 \uc2dc\uae30\uc640 \uc9c8\ud22c\ub85c \uc778\ud558\uc5ec \uadf8\ub4e4\uc5d0\uac8c \uc9c0\uc2dd\uc774 \uc774\ub97c\ub54c \uae4c\uc9c0\ub294 \uc758\uacac\uc744 \ub2ec\ub9ac\ud558\uc9c0 \uc544\ub2c8\ud588\ub178\ub77c \uc2e4\ub85c\uc8fc\ub2d8\uaed8\uc11c \ubd80\ud65c\uc758 \ub0a0 \uadf8\ub4e4\uc774 \uc758\uacac \uc744 \ub2ec\ub9ac\ud588\ub358 \uac83\uc5d0 \uad00\ud558\uc5ec \uc2ec\ud310\uc744 \ud558\uc2dc\ub9ac\ub77c"}, {"id": 384719, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4491, "text": "\uadf8\ud6c4 \ud558\ub098\ub2d8\uc740 \uadf8\ub300\ub97c \ubc14\ub978 \uae38 \uc704\uc5d0 \ub450\uc5c8\uc73c\ub2c8 \uadf8\ub300\ub294 \uadf8 \uae38 \uc744 \ub530\ub974\ub418 \uc54c\uc9c0 \ubabb\ud558\ub294 \uc790\ub4e4\uc758 \uc720 \ud639\uc744 \ub530\ub974\uc9c0 \ub9d0\ub77c"}, {"id": 384720, "identifier": "ko.korean", "surah_number": 45, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062c\u0627\u062b\u064a\u0629", "surah_en_name": "Al-Jaathiya", "surah_en_name_translation": "Crouching", "revelation_type": "Meccan", "juz": 25, "manzil": 6, "page": 500, "ruku": 435, "hizb_quarter": 200, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4492, "text": "\uadf8\ub4e4\uc740 \ud558\ub098\ub2d8 \uc55e\uc5d0\uc11c \uadf8\ub300 \uc5d0\uac8c \uc720\uc6a9\uce58 \ubabb\ud558\ub2c8\ub77c \uc8c4\uc778\ub4e4\uc74