[{"id": 385474, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5242, "text": "\uc655\uad6d\uc744 \uc18c\uc720\ud558\uace0 \uacc4\uc2dc\ub294 \ud558 \ub098\ub2d8\uaed8 \ucd95\ubcf5\uc774 \uc788\uc73c\uc18c\uc11c \uc2e4\ub85c \uadf8 \ubd84\uc740 \ubaa8\ub4e0 \uc77c\uc5d0 \uc804\uc9c0\uc804\ub2a5\ud558\uc2ec\uc774\ub77c"}, {"id": 385475, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5243, "text": "\uc8fd\uc74c\uacfc \uc0dd\uba85\uc744 \uc81c\uc815\ud558\uc2dc\uc5b4 \uadf8\uac83\uc73c\ub85c \ub108\ud76c\uac00 \uc120\uc744 \ud589\ud558\ub294\uc9c0 \uc2dc \ud5d8\ud558\uace0\uc790 \ud558\uc2dc\ub2c8 \uadf8\ubd84\uc740 \uad8c\ub2a5\uacfc \uc9c0\ud61c\ub85c \ucda9\ub9cc\ud558\uc2ec\uc774\ub77c"}, {"id": 385476, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5244, "text": "\uadf8\ubd84\uc740 \uce60\ucc9c\uc744 \uce35\uc73c\ub85c \ud558\uc5ec \ucc28\ub840\ub85c \ucc3d\uc870\ud558\uc168\ub178\ub77c \ub108\ud76c\ub294 \uadf8\uac83\uc774 \uc644\uc804\ubb34\uacb0\ud558\uc5ec \ubb34\uc5c7\ud558\ub098 \uacb0\ud568\uc774\uc5c6\uc74c\uc744 \ubcfc \uac83\uc774\ub2c8 \ub2e4\uc2dc \ud55c\ubc88 \ub208\uc744\ub3cc\ub824\ubcf4\ub77c \uacb0\ud568\uc744 \ubcfc \uc218 \uc788\uaca0\ub290\ub1e8"}, {"id": 385477, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5245, "text": "\uadf8\ub7f0 \ud6c4 \ub2e4\uc2dc \ud55c\ubc88 \ub208\uc744 \ub4e4\uc5b4\ubcf4\ub77c \ud53c\uace4\ud558\uc5ec \uc788\ub358 \ub208\uc774 \uacb8\uc190\ud558 \uace0 \uc21c\uc885\ud55c \uc2dc\ub825\uc73c\ub85c \ub3cc\uc544\uc624\ub9ac\ub77c"}, {"id": 385478, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5246, "text": "\uc2e4\ub85c \ud558\ub098\ub2d8\uc740 \uac00\uc7a5 \ub0ae\uc740 \ud558 \ub298\uc744 \ub4f1\ubd88\ub85c\uc11c \uc7a5\uc2dd\ud558\uc600\uace0 \uadf8 \uac83\uc73c\ub85c \uc0ac\ud0c4\uc744 \ubb3c\ub9ac\uce68\uc5d0 \uc720\uc6a9\ud558\uac8c\ud558\uc600\uc73c\uba70 \uadf8\ub4e4\uc744 \uc704\ud574 \ud0c0\uc624\ub974\ub294 \ubd88\uc9c0\uc625\uc758 \uc751\ubc8c\uc744 \uc900\ube44\ud558\uc168\ub3c4\ub2e4"}, {"id": 385479, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5247, "text": "\uc8fc\ub2d8\uc744 \ubbff\uc9c0 \uc544\ub2c8\ud55c \uc790\ub4e4\uc744 \uc704\ud574\uc11c \uc0ac\uc545\ud55c \uc751\ubc8c\uc774 \uc788\ub098\ub2c8"}, {"id": 385480, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5248, "text": "\uadf8\ub4e4\uc774 \uadf8\uacf3\uc73c\ub85c \ub358\uc838\uc9c8 \ub54c \uadf8\ub4e4\uc740 \uc2e0\uc74c\uc18c\ub9ac \uac19\uc740 \ubb34\uc11c\uc6b4 \ube44 \ud0c4\uc758 \uc18c\ub9ac\ub97c \ub4e4\uc744 \uac83\uc774\ub77c"}, {"id": 385481, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5249, "text": "\uc9c0\uc625\uc740 \ubd84\ub178\ud558\uc5ec \ud3ed\ubc1c\ud560 \uac83\uc774\ub2c8 \ubd88\uc2e0\uc790\uc758 \ubb34\ub9ac\uac00 \ub358\uc838\uc9c8 \ub54c \ub9c8\ub2e4 \ub108\ud76c\uc5d0\uac8c \ud55c \uacbd\uace0\uc790\uac00 \uc774\ub974\uc9c0 \uc544\ub2c8 \ud588\uc5c8\ub290\ub1e8 \ub77c\uace0 \uc218\ud638\uc790\uac00 \uc9c8 \ubb38\ud560 \uac83\uc774\ub77c"}, {"id": 385482, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5250, "text": "\uc2e4\ub85c \uc800\ud76c\uc5d0\uac8c \ud55c \uacbd\uace0\uc790\uac00 \ub3c4\ub798\ud558\uc600\uc73c\ub098 \uc800\ud76c\uac00 \uadf8\ub97c \uac70\uc5ed \ud588\uc2b5\ub2c8\ub2e4 \ud558\ub098\ub2d8\uc740 \uc544\ubb34 \uac83\ub3c4 \uacc4 \uc2dc\ud558\uc9c0 \uc544\ub2c8\ud588\uc2b5\ub2c8\ub2e4 \ub2f9\uc2e0\ub4e4\uc740 \ud06c\uac8c \ubc29\ud669\ud558\uace0 \uc788\uc744 \ubfd0\uc785\ub2c8\ub2e4 \ub77c\uace0 \ub9d0\ud558\ub354\ub77c"}, {"id": 385483, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5251, "text": "\ub354\ud558\uc5ec \ub9d0\ud558\uae38 \uc6b0\ub9ac\uac00 \uadc0\ub97c\uae30\uc6b8\uc600\ub2e4\uba74 \ub610\ub294 \uc219\uace0 \ud588\ub354\ub77c\uba74 \uc6b0\ub9ac\ub294 \ud0c0\uc624\ub974\ub294 \ubd88\uc9c0\uc625\uc758 \ub3d9\ub8cc\uc911\uc5d0 \uc788\uc9c0 \uc544\ub2c8\ud588\uc744 \ud150\ub370 \ub77c\uace0 \ub9d0\ud558\ub354\ub77c"}, {"id": 385484, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5252, "text": "\uadf8\ub4e4\uc740 \uc8c4\uc545\ub4e4\uc744 \uace0\ubc31\ud558\ub098 \ud0c0\uc624\ub974\ub294 \ubd88\uc9c0\uc625\uc758 \ub3d9\ub8cc\ub4e4\ub85c\ubd80\ud130 \uc6a9\uc11c\uac00 \ub418\uc9c0 \uc54a\ub178\ub77c"}, {"id": 385485, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 562, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5253, "text": "\uadf8\ub7ec\ub098 \ubcf4\uc774\uc9c0 \uc544\ub2c8\ud55c \uc8fc\ub2d8\uc744 \ub450\ub824\uc6cc \ud55c \uc790\ub4e4\uc5d0\uac8c\ub294 \uad00\uc6a9\uacfc \ud070 \ubcf4\uc0c1\uc774 \uc788\ub178\ub77c"}, {"id": 385486, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5254, "text": "\ub108\ud76c\uac00 \uc228\uae30\uc5b4 \ub9d0\ud558\ub294 \uac83\uacfc \ub4dc\ub7ec\ub0b4\uc5b4 \ub9d0\ud558\ub294 \uac83\ub3c4 \ud558\ub098\ub2d8\uc740 \ub9c8\uc74c\uc18d\uc758 \ubaa8\ub4e0 \uac83\uc744 \uc544\uc2ec\uc73c\ub85c \ucda9 \ub9cc\ud558\uc2dc\ub2c8\ub77c"}, {"id": 385487, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 496, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5255, "text": "\uadf8\ubd84\uc774 \ucc3d\uc870\ud55c \uac83\uc744 \uadf8\ubd84\uc774 \ubaa8\ub97c\ub9ac \uc788\ub290\ub1e8 \uc2e4\ub85c \uadf8\ubd84\uc740 \uc2e0\ube44 \ud55c \ubaa8\ub4e0 \uac83\uc744 \uc774\ud574\ud558\uc2dc\uace0 \uc544\uc2dc\ub294 \ubd84\uc774\uc2dc\ub77c"}, {"id": 385466, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5256, "text": "\ud558\ub098\ub2d8\uc740 \ub108\ud76c\ub97c \uc704\ud574 \ub300\uc9c0 \ub97c \uc720\uc5f0\ud558\uac8c \ud558\uc168\ub098\ub2c8 \uacf3\uacf3\uc744 \ub2e4 \ub140 \ubcf4\ub77c \uadf8\ub9ac\uace0 \uadf8\ubd84\uc774 \ubca0\ud47c \uc591\uc2dd \uc744 \uc12d\ucde8\ud558\ub77c \uc8fd\uc740 \ud6c4\uc5d0\ub294 \uadf8\ubd84\uaed8 \ub85c \uadc0\uc758\ud558\ub178\ub77c"}, {"id": 385488, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5257, "text": "\ud558\ub298\uc5d0 \uacc4\uc2e0 \uadf8\ubd84\uaed8\uc11c \ub300\uc9c0 \ub97c \ud754\ub4e4\uc5b4 \uadf8\uac83\uc774 \ub108\ud76c\ub97c \uc0bc\ucf1c \ubc84 \ub9ac\uc9c0\ub294 \ubabb\ud558\uac8c \ud560 \uac83\uc774\ub77c \uc548\uc2ec\ud558 \ub290\ub1e8"}, {"id": 385489, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5258, "text": "\uc544\ub2c8\uba74 \ud558\ub298\uc5d0 \uacc4\uc2e0 \uadf8\ubd84\uaed8 \uc11c \ub108\ud76c \uc704\uc5d0 \uc751\ubc8c\uc744 \ub0b4\ub9ac\uc9c0 \uc544\ub2c8 \ud560 \uac83\uc774\ub77c \uc548\uc2ec\ud558\ub290\ub1e8 \uadf8\ub54c \ub108\ud76c \ub294 \ub098\uc758 \uc751\ubc8c\uc774 \uc5b4\ub5a0\ud55c \uac83\uc778\uac00\ub97c \uc54c\uac8c \ub418\ub9ac\ub77c"}, {"id": 385490, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5259, "text": "\uc2e4\ub85c \uadf8\ub4e4 \uc774\uc804 \ubc31\uc131\ub4e4\uc774 \ub098\uc758 \uacbd\uace0\ub97c \uac70\uc5ed\ud588\uc744 \ub54c \uadf8\ub4e4 \uc704\uc5d0 \ub0b4\ub824\uc9c4 \ub098\uc758 \ubc8c\uc774 \uc5bc\ub9c8\ub098 \ubb34\uc11c\uc6e0 \ub354\ub1e8"}, {"id": 385491, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5260, "text": "\uba38\ub9ac \uc704\uc5d0\uc11c \uc0c8\ub4e4\uc774 \ub0a0\uac1c\ub97c \ud3c8\ub2e4\uac00 \uc811\uc5c8\ub2e4 \ud558\uba74\uc11c \uba39\uc774\ub97c \ucc44 \ub824\uace0 \ub36e\uce58\ub294 \uc0c8\ub4e4\uc744 \ubcf4\uc9c0 \ubabb\ud588\ub354 \ub1e8 \uc790\ube44\ub85c\uc6b0\uc2e0 \ud558\ub098\ub2d8 \uc678\uc5d0\ub294 \uc5b4 \ub290 \ub204\uad6c\ub3c4 \uadf8\ub807\uac8c \uc720\uc9c0\ud560 \uc218 \uc5c6\ub098\ub2c8 \uc2e4\ub85c \uadf8\ubd84\uc740 \ubaa8\ub4e0 \uac83\uc744 \uc9c0\ucf1c\ubcf4\uace0 \uacc4\uc2ec\uc774\ub77c"}, {"id": 385492, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5261, "text": "\uc790\ube44\ub85c\uc6b0\uc2e0 \ud558\ub098\ub2d8 \uc678\uc5d0 \ub108 \ud76c\ub97c \ub3c4\uc6b8 \uc8fc\uc778\uc740 \ub204\uad6c\uc774\ub1e8 \uc2e4\ub85c \ubd88\uc2e0\uc790\ub4e4\uc740 \ud658\uc0c1\uc5d0 \ube60\uc838 \uc788\ub354\ub77c"}, {"id": 385493, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5262, "text": "\ud558\ub098\ub2d8\uc774 \uc591\uc2dd\uc744 \uac70\ub450\uc2e0\ub2e4\uba74\ub108\ud76c\uc5d0\uac8c \ub204\uac00 \ub108\ud76c\uc5d0\uac8c \uc77c\uc6a9\ud560 \uc591\uc2dd\uc744 \uc8fc\uaca0\ub290\ub1e8 \uadf8\ub7f0\ub370\ub3c4 \uadf8\ub4e4\uc740\uc9c4\ub9ac\uc5d0\uc11c \ubc97\uc5b4\ub098 \uc624\ub9cc\ud558\ub3c4\ub2e4"}, {"id": 385494, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5263, "text": "\uace0\uac1c \uc219\uc5ec \uac77\ub294 \uc790\uac00 \ub0ab\uaca0 \ub290\ub1e8 \uc544\ub2c8\uba74 \ubc14\ub978 \uae38\uc744 \uac77\ub294\uc790\uc758 \uc778\ub3c4\ub428\uc774 \ub0ab\uaca0\ub290\ub1e8"}, {"id": 385495, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5264, "text": "\uc77c\ub7ec\uac00\ub85c\ub418 \ub108\ud76c\ub97c \ucc3d\uc870\ud558\uc0ac\uc131\uc7a5\ucf00 \ud558\uace0 \ub4e3\ub294 \ub2a5\ub825\uacfc \ubcf4\ub294 \ub2a5\ub825\uacfc \uac10\uac01\uc758 \ub2a5\ub825\ub878 \uc774\ud574\uc758 \ub2a5\ub825 \uc744 \uc8fc\uc2e0 \ubd84\uc774 \ubc14\ub85c \ud558\ub098\ub2d8\uc774\uc2dc\ub77c \uadf8\ub7ec\ub098 \ub108\ud76c\ub294 \uac10\uc0ac\ud558\uc9c0 \uc54a\ub354\ub77c"}, {"id": 385496, "identifier": "ko.korean", "surah_number": 67, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0644\u0643", "surah_en_name": "Al-Mulk", "surah_en_name_translation": "The Sovereignty", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 563, "ruku": 497, "hizb_quarter": 225, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5265, "text": "\uc77c\ub7ec\uac00\ub85c\ub418 \ub300\uc9c0 \uc704\uc5d0 \ub108\ud76c