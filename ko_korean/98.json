[{"id": 386336, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6131, "text": "\uc131\uc11c\uc758 \ubc31\uc131\ub4e4 \uac00\uc6b4\ub370 \uc9c4\ub9ac \ub97c \uac70\uc5ed\ud55c \uc790\ub4e4\uacfc \ubd88\uc2e0\uc790\ub4e4\uc740 \ubd84 \uba85\ud55c \uc608\uc99d\uc774 \uadf8\ub4e4\uc5d0\uac8c \uc774\ub97c \ub54c \uae4c\uc9c0 \uadf8\ub4e4\uc740 \uae38\uc744 \ub5a0\ub098\ub824 \ud558\uc9c0 \uc544\ub2c8 \ud558\uc600\ub178\ub77c"}, {"id": 386337, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6132, "text": "\uc774 \uc608\uc99d\uc740 \uc131\uc2a4\ub7ec\uc6b4 \ub9d0\uc500\uc744 \ub0ad\uc1a1\ud560 \ud558\ub098\ub2d8\uc758 \uc120\uc9c0\uc790\ub85c"}, {"id": 386338, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6133, "text": "\uadf8 \uc548\uc5d0\ub294 \uc62c\ubc14\ub978 \uc728\ubc95\uc774 \uc788 \ub178\ub77c"}, {"id": 386339, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6134, "text": "\uc131\uc11c\uc758 \ubc31\uc131\ub4e4\uc740 \uadf8\ub4e4\uc5d0\uac8c \ubd84\uba85\ud55c \uc608\uc99d\uc774 \uc774\ub97c \ub54c \uae4c\uc9c0 \uc758\uacac\uc744\ub2ec\ub9ac\ud558\uc9c0 \uc544\ub2c8 \ud588\uc73c\ub2c8"}, {"id": 386340, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6135, "text": "\uadf8\ub4e4\uc5d0\uac8c \uc8fc\uc5b4\uc9c4 \uba85\ub839\uc740 \ud558 \ub098\ub2d8\uc744 \uacbd\ubc30\ud558\uace0 \uc9c4\uc2e4\ud55c \ubbff\uc74c\uc774 \ub418\ub3c4\ub85d \uadf8\ubd84\uaed8 \ud5cc\uc2e0\ud558\uba70 \uae30\ub3c4\ub97c \ub4dc\ub9ac\uace0 \uc774\uc2ac\ub78c\uc138\ub97c \ubc14\uce58\ub294 \uac83\ub4e4\uc774\ucc38\ub41c \uc2e0\uc559\uc774\ub77c"}, {"id": 386341, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6136, "text": "\uc2e4\ub85c \uc131\uc11c\uc758 \ubc31\uc131\ub4e4 \uc911\uc5d0 \uc9c4 \ub9ac\ub97c \uac70\uc5ed\ud55c \uc790\ub4e4\uacfc \ubd88\uc2e0\uc790\ub4e4\uc740 \ubd88\uc9c0\uc625\uc5d0 \uc788\uac8c \ub418\ub9ac\ub2c8 \uadf8\ub4e4\uc740 \uadf8 \uc548\uc5d0\uc11c \uc601\uc8fc\ud558\ub9e4 \uac00\uc7a5 \uc0ac\uc545\ud55c \ubb34 \ub9ac\ub4e4\uc774\ub77c"}, {"id": 386342, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 598, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6137, "text": "\uadf8\ub7ec\ub098 \ubbff\uc74c\uc73c\ub85c \uc120\uc744 \uc2e4\ucc9c\ud558\ub294 \uc790\ub4e4\uc740 \uac00\uc7a5 \uc758\ub85c\uc6b4 \uc790\ub4e4\ub85c"}, {"id": 386343, "identifier": "ko.korean", "surah_number": 98, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u064a\u0646\u0629", "surah_en_name": "Al-Bayyina", "surah_en_name_translation": "The Evidence", "revelation_type": "Medinan", "juz": 30, "manzil": 7, "page": 599, "ruku": 540, "hizb_quarter": 239, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 6138, "text": "\uadf8\ub4e4\uc744 \uc704\ud574 \uc8fc\ub2d8\uc758 \ubcf4\uc0c1\uc774 \uc788\ub098\ub2c8 \ubc11\uc73c\ub85c \uac15\ubb3c\uc774 \ud750\ub974\ub294 \uc601 \uc0dd\ud560 \uc5d0\ub374\uc758 \ucc9c\uad6d\uc774\ub77c \ud558\ub098\ub2d8\uc740 \uadf8\ub4e4\ub85c \uae30\ubed0\ud558\uc2dc\uace0 \uadf8\ub4e4\uc740 \uadf8\ubd84\uc73c\ub85c \uae30\ubed0\ud558\ub9ac\ub2c8 \uadf8\uac83\uc740 \uc8fc\ub2d8\uc744 \ub450 \ub824\uc6cc\ud558\ub294 \uc790\uc5d0 \ub300\ud55c \ubcf4\uc0c1\uc774\ub77c"}]