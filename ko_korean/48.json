[{"id": 384817, "identifier": "ko.korean", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4584, "text": "\ud558\ub098\ub2d8\uc774 \uadf8\ub300\uc5d0\uac8c \ubd84\uba85\ud55c \uc2b9\ub9ac\ub97c \ubca0\ud480\uc5c8\ub178\ub77c"}, {"id": 384818, "identifier": "ko.korean", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4585, "text": "\uadf8\uac83\uc740 \ud558\ub098\ub2d8\uaed8\uc11c \uc9c0\ub098\uac04 \uadf8\ub300\uc758 \uacfc\uc624\ub97c \uc6a9\uc11c\ud558\uace0 \uadf8\ub300\uc5d0\uac8c \uadf8\ubd84\uc758 \uc740\ud61c\ub97c \ucda9\ub9cc\ucf00 \ud558\uba70 \uadf8\ub300 \ub97c \uc62c\ubc14\ub978 \uae38\ub85c \uc778\ub3c4\ud558\uc2dc\uba70"}, {"id": 384819, "identifier": "ko.korean", "surah_number": 48, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062a\u062d", "surah_en_name": "Al-Fath", "surah_en_name_translation": "The Victory", "revelation_type": "Medinan", "juz": 26, "manzil": 6, "page": 511, "ruku": 446, "hizb_quarter": 204, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "ty