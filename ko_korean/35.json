[{"id": 383889, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3661, "text": "\ud558\ub098\ub2d8\uaed8 \ucc2c\ubbf8\ub97c \ub4dc\ub9b4\uc9c0\ub2c8 \uadf8\ubd84\uc740 \ubb34\uc5d0\uc11c \ucc9c\uc9c0\ub97c \ucc3d\uc870\ud558\uc168\uace0\ucc9c\uc0ac\ub4e4\uc744 \ub450\uc30d, \uc138 \uc30d, \ub137\uc30d\uc758 \ub0a0\uac1c\ub97c \uac00\uc9c4 \uc804\ub839\uc73c\ub85c \ub450\uc168\uc73c\uba70 \uadf8 \ubd84\uc758 \ub73b\ub300\ub85c \ucc3d\uc870\ub97c \ub354\ud574 \uac00\uc2dc\ub2c8 \uc2e4\ub85c \ud558\ub098\ub2d8\uc740 \ubaa8\ub4e0 \uc77c\uc5d0 \uc804\uc9c0\uc804 \ub2a5\ud558\uc2ec\uc774\ub77c"}, {"id": 383890, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3662, "text": "\ud558\ub098\ub2d8\uaed8\uc11c \uadf8\ubd84\uc758 \uc790\ube44\ub85c \uc778\ub958\ub97c \uc704\ud574 \ubcf4\ub0b4\uc2e0 \uadf8 \uc740\ud61c\ub97c \ub204 \uad6c\ub3c4 \uc81c\uc7ac\ud560 \uc218 \uc5c6\uc73c\uba70 \ub610\ud55c \uadf8\ubd84\uaed8\uc11c \uc81c\uc9c0\ud558\uc2e0 \uac83\uc744 \ub204\uad6c\ub3c4 \uc790\uc720 \ub86d\uac8c \ud560 \uc218 \uc5c6\ub098\ub2c8 \uadf8\ubd84\uc740 \uad8c\ub2a5\uacfc\uc9c0\ud61c\ub85c \ucda9\ub9cc\ud558\uc2ec\uc774\ub77c"}, {"id": 383891, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 434, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3663, "text": "\uc0ac\ub78c\ub4e4\uc774\uc5ec \ub108\ud76c\uc5d0\uac8c \ubca0\ud480\uc5b4 \uc900 \uc8fc\ub2d8\uc758 \uc740\ud61c\ub97c \uc0dd\uac01\ud560\uc9c0\ub2c8 \ud558 \ub098\ub2d8 \uc678\uc5d0 \ud558\ub298\uacfc \ub300\uc9c0\ub85c\ubd80\ud130 \ub108 \ud76c\uc5d0\uac8c \uc591\uc2dd\uc744 \uc8fc\ub294 \ub2e4\ub978 \ucc3d\uc870\uc8fc \uac00 \uc788\ub290\ub1e8 \uadf8\ubd84 \uc678\uc5d0\ub294 \uc2e0\uc774 \uc5c6\ub098\ub2c8 \uc5b4\ucc0c\ud558\uc5ec \ub108\ud76c\ub294 \uadf8\ubd84\uc758 \uc720\uc77c \uc131\uc744 \ubaa8\ub974\ub290\ub1e8"}, {"id": 383892, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3664, "text": "\uadf8\ub4e4\uc774 \uadf8\ub300\ub97c \ubd80\uc815\ud55c\ub2e4\uba74 \uadf8 \ub300 \uc774\uc804\uc758 \uc120\uc9c0\uc790\ub4e4\ub3c4 \ubd80\uc815\ub418\ub098\ub2c8 \ubaa8\ub4e0 \uac83\uc740 \ud558\ub098\ub2d8\uaed8\ub85c \uadc0\uc758\ud558\ub178\ub77c"}, {"id": 383893, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3665, "text": "\uc0ac\ub78c\ub4e4\uc774\uc5ec \uc2e4\ub85c \ud558\ub098\ub2d8\uc758 \uc57d\uc18d\uc740 \uc9c4\uc2e4\uc774\uac70\ub298 \ud604\uc138\uc758\uc0b6\uc774 \ub108\ud76c\ub97c \uc720\ud751\ucf00 \ud574\uc11c\ub294 \uc544\ub2c8\ub418\uba70 \uc720 \ud639\uc774 \ub108\ud76c\ub85c \ud558\uc5ec\uae08 \ud558\ub098\ub2d8\uc744 \uc18d \uc774\uac8c \ud574\uc11c\ub3c4 \uc544\ub2c8 \ub418\ub178\ub77c"}, {"id": 383894, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3666, "text": "\uc2e4\ub85c \uc0ac\ud0c4\uc740 \ub108\ud76c\uc758 \uc801\uc774\uac70\ub298 \uadf8\ub97c \uc801\uc73c\ub85c \ub300\ud558\ub77c \uadf8\ub294 \ud56d\uc0c1 \uadf8 \uc758 \ubb34\ub9ac\ub4e4\uc744 \ucd08\ub300\ud558\ub2c8 \uc774\ub4e4\uc740 \uc9c0\uc625\uc758 \ubc18\ub824\uc790\ub4e4 \uc911\uc5d0 \uc788\uac8c \ub418\ub9ac\ub77c"}, {"id": 383895, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 376, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3667, "text": "\ubd88\uc2e0\ud558\ub294 \uc790\ub4e4\uc5d0\uac8c\ub294 \uac00\ud639\ud55c \ubc8c\uc774 \uc788\uc73c\uba70 \ubbff\uc74c\uc73c\ub85c \uc120\uc744 \ud589\ud558 \ub294 \uc790\uc5d0\uac8c\ub294 \uad00\uc6a9\uacfc \ud070 \ubcf4\uc0c1\uc774 \uc788 \ub178\ub77c"}, {"id": 383896, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3668, "text": "\uadf8\uc758 \uc704\uc7a5\ub41c \uc0ac\uc545\ud55c \ud589\uc704\ub97c \uc120\uc774\ub77c \ud560 \uc218 \uc788\ub290\ub1e8 \ud558\ub098\ub2d8\uc740 \uadf8 \ubd84\uc758 \ub73b\uc5d0 \ub530\ub77c \ubc29\ud669\ucf00 \ud558\uace0 \uc633\uac8c \uc778\ub3c4\ud558\uae30\ub3c4 \ud558\uc2dc\ub2c8\ub77c \uadf8\ub7ec\ubbc0\ub85c \uadf8 \ub300\uc758 \uc601\ud754\uc774 \uadf8\ub4e4\ub85c \uc778\ud558\uc5ec \uc2ac\ud37c\ud558 \uc9c0 \uc54a\ub3c4\ub85d \ud558\ub77c \ud558\ub098\ub2d8\uc740 \uadf8\ub4e4\uc774 \ud589\ud558\ub294 \ubaa8\ub4e0 \uac83\uc744 \uc54c\uace0 \uacc4\uc2dc\ub2c8\ub77c"}, {"id": 383853, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3669, "text": "\ud558\ub098\ub2d8\uaed8\uc11c \ubc14\ub78c\uc744 \ubcf4\ub0b4\uc0ac \uadf8 \uac83\uc774 \uad6c\ub984\uc744 \uc77c\uc73c\ud0a4\uace0 \uadf8\uac83\uc744 \uc8fd \uc740 \ub300\uc9c0\ub85c \ubcf4\ub0b4\uc5b4 \uc8fd\uc740 \ub545\uc744 \ub2e4\uc2dc\uc18c\uc0dd\ucf00 \ud558\ub2c8 \ubd80\ud65c\uc774 \ubc14\ub85c \uadf8\uc640 \uac19\ub178\ub77c"}, {"id": 383897, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3670, "text": "\uc601\ud654\uc640 \uad8c\uc138\ub97c \uad6c\ud558\ub294 \uc790\uc5ec\ubaa8\ub4e0 \uc601\uad11\uacfc \uad8c\ub2a5\uc774 \ud558\ub098\ub2d8\uaed8 \uc788 \ub178\ub77c \uc88b\uc740 \uc5b8\ud589\ub3c4 \uadf8\ubd84\uaed8 \uc774\ub974\uba70 \uc88b\uc740 \ud589\uc704\ub294 \ud558\ub098\ub2d8\uaed8\uc11c \uadf8\uac83\uc744 \ub192\uc774 \uc744\ub9ac\uc2dc\ub418 \uc0ac\uc545\ud55c \uc74c\ubaa8\ub97c \uafb8 \ubbf8\ub294 \uc790\uc5d0\uac8c\ub294 \ubb34\uc11c\uc6b4 \ubc8c\uc774 \uc788\uc73c \uba70 \uadf8\ub7ec\ud55c \uc74c\ubaa8\ub294 \uc218\ud3ec\ub85c \ub05d\ub098\ub178 \ub77c"}, {"id": 383898, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 435, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3671, "text": "\ud558\ub098\ub2d8\uc774 \ud759\uc5d0\uc11c \ub108\ud76c\ub97c \ucc3d\uc870\ud558\uc0ac \ub2e4\uc2dc \uc815\uc561\uc5d0\uc11c \ub108\ud76c\ub97c \uc7ac \ucc3d\uc870 \ud558\uc168\uc73c\uba70 \ub108\ud76c\ub97c \uc30d\uc73c\ub85c \ub450 \uc168\ub178\ub77c \uc5b4\ub5a0\ud55c \uc5ec\uc131\ub3c4 \uadf8\ubd84\uc758 \uc544 \uc2ec\uc774 \uc5c6\uc774\ub294 \uc789\ud0dc\uc640 \ucd9c\uc0b0\uc744 \ud560 \uc218\uc5c6\uc73c\uba70 \uc5b4\ub5a0\ud55c \uc778\uac04\ub3c4 \uc0dd\uba85\uc744 \uc5f0 \uc7a5\ud558\uac70\ub098 \ub2e8\ucd95\ud560 \uc218 \uc5c6\ub098\ub2c8 \uc774 \ubaa8\ub4e0 \uac83\uc740 \uc131\uc11c \uc548\uc5d0 \uc788\ub178\ub77c \uc774\ub7ec \ud55c \uac83\uc740 \uc2e4\ub85c \ud558\ub098\ub2d8\uaed8 \uc26c\uc6b4 \uc77c\uc774\ub77c"}, {"id": 383899, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3672, "text": "\ub450 \uac08\ub798\ub85c \ud750\ub974\ub294 \ubb3c\uc774 \uac19 \uc9c0 \uc544\ub2c8\ud558\ub2c8 \uadf8 \ud558\ub098\ub294 \ub2ec\ucf64\ud558\uc5ec \ub9c8\uc2dc\uae30\uc5d0 \uc801\ud569\ud558\uace0 \uadf8 \ub2e4\ub978 \ud558\ub098 \ub294 \uc9dc\uace0 \uc4f0\ub354\ub77c \ub108\ud76c\ub294 \uadf8\ub4e4 \uac01\uac01 \uc73c\ub85c\ubd80\ud130 \uc2e0\uc120\ud55c \uc0dd\uc120\uc744 \uc12d\ucde8\ud558 \uba70 \ub610\ud55c \ub108\ud76c\uac00 \ubab8\uc5d0 \uc7a5\uc2dd\ud558\ub294 \ubcf4 \uc11d\uc744 \ucc44\uad74\ud558\ub9ac\ub77c \ub610\ud55c \ub108\ud76c\ub294 \ud30c\ub3c4\ub97c \uc77c\uad6c\uba70 \ud56d\ud574\ud558\ub294 \uc120\ubc15\uc744 \ubcf4\uba70 \ud558\ub098\ub2d8\uc758 \uc740\ud61c\ub97c \uad6c\ud560 \uc218 \uc788\uc73c\ub9ac\ub77c\uc774\uc5d0 \ub108\ud76c\ub294 \uac10\uc0ac\ud574\uc57c \ud558\ub178\ub77c"}, {"id": 383900, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3673, "text": "\ud558\ub098\ub2d8\uc740 \ubc24\uc744 \ub0ae\uc73c\ub85c \ud558\uc2dc \uace0 \ub0ae\uc744 \ubc24\uc73c\ub85c \ub450\uc2dc\uac8c \ud558\uc2dc\uba70 \ud574 \uc640 \ub2ec\uc774 \uadf8\ubd84\uc758 \ubc95\uce59\uc5d0 \uc21c\uc885\ud1a0\ub85d \ud558\uc168\uc73c\ub2c8 \uac01\uc790\ub294 \uc815\ud558\uc5ec\uc9c4 \uc2dc\uac04\uc5d0 \uc81c \uc6b4\ud589\uc744 \ud568\uc774\ub77c \uc774\ub807\uac8c \ud558\uc2dc\ub294 \ubd84\uc774 \ubc14\ub85c \uc8fc\ub2d8\uc774\uc2dc\uba70 \ubaa8\ub4e0 \uc601\uc5ed \uc774 \uadf8\ubd84\uaed8 \uc788\uc73c\ub9e4 \uadf8\ubd84 \uc678\uc5d0 \ub108 \ud76c\uac00 \uc22d\ubc30\ud558\ub294 \uac83\ub4e4\uc740 \uc9c0\ud478\ub77c\uae30 \ud558\ub098\ub3c4 \ub2e4\uc2a4\ub9b4 \ud798\uc774 \uc5c6\ub178\ub77c"}, {"id": 383901, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 377, "hizb_quarter": 174, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3674, "text": "\ub108\ud76c\uac00 \uadf8\ub4e4\uc5d0\uac8c \uad6c\ud558\ub098 \uadf8 \ub4e4\uc740 \ub4e3\uc9c0 \ubabb\ud560 \uac83\uc774\uba70 \uadf8\ub4e4\uc774 \ub4e4 \uc5c8\ub2e4 \ud558\ub354\ub77c\ub3c4 \ub108\ud76c\uc758 \uad6c\uc6d0\uc5d0 \uc751 \ud560 \uc218 \uc5c6\ub178\ub77c \uc2ec\ud310\uc758 \ub0a0 \uadf8\ub4e4\uc740 \uadf8\ub4e4\uc774 \ud558\ub098\ub2d8\uacfc \ube44\uc720\ud558\uc5ec \uc22d\ubc30\ud588 \ub358 \uac83\ub4e4\uc744 \uac70\uc5ed\ud560 \uac83\uc774\ub77c \ubaa8\ub4e0 \uac83 \uc744 \uc54c\uace0 \uacc4\uc2dc\ub294 \uadf8\ubd84\ucc98\ub7fc \uadf8\ub300\uc5d0\uac8c\uc9c4\ub9ac\ub97c \ub9d0\ud560 \uc790 \uc544\ubb34\ub3c4 \uc5c6\ub178\ub77c"}, {"id": 383902, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3675, "text": "\uc0ac\ub78c\ub4e4\uc774\uc5ec \ud558\ub098\ub2d8\uc744 \ud544\uc694\ub85c \ud55c \uc774\ub4e4\uc774 \ubc14\ub85c \ub108\ud76c\ub4e4\uc774\ub77c \ud558\ub098 \ub2d8\uc740 \ud48d\uc694\ud568\uc73c\ub85c \ucda9\ub9cc\ud558\uc2dc\ub2c8 \ubaa8\ub4e0 \ucc2c\ubbf8\ub97c \ud758\ub85c \ubc1b\uc73c\uc18c\uc11c"}, {"id": 383903, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3676, "text": "\uadf8\ubd84\uaed8\uc11c \ub73b\uc744 \ub450\uc2e4 \ub54c \ub108\ud76c \ub97c \uba78\ub9dd\ucf00 \ud558\uc0ac \uc0c8\ub85c\uc6b4 \uc138\ub300\ub97c \ub450 \uc2dc\ub9ac\ub2c8"}, {"id": 383904, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3677, "text": "\uadf8\ub807\uac8c \ud558\uc2ec\uc774 \ud558\ub098\ub2d8\uaed8\ub294 \uc5b4\ub824\uc6b4 \uc77c\uc774 \uc544\ub2c8\ub77c"}, {"id": 383905, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 436, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3678, "text": "\uc9d0\uc744 \uc9ca\uc5b4\uc9c4 \uc790\uac00 \ub2e4\uc2dc \ub2e4\ub978\uc0ac\ub78c\uc758 \uc9d0\uc744 \uc9ca\uc5b4\uc9c8 \uc218 \uc5c6\uc73c\ub9e4 \ubb34 \uac70\uc6b4 \uc9d0\uc9c4 \uc790\uac00 \ub2e4\ub978 \uc0ac\ub78c\uc744 \ubd88\ub7ec \uadf8\uc758 \uc9d0\uc744 \ub35c\uc5b4\ub2ec\ub77c \uad6c\ud558\ub098 \uac00\uc7a5 \uac00\uae4c\uc6b4 \uce5c\ucc99\uc774\ub77c\ub3c4 \uc870\uae08\ub3c4 \ub35c\uc5b4 \uc904 \uc218 \uc5c6\ub178\ub77c \uadf8\ub300\ub294 \ub2e8\uc9c0 \ubcf4\uc774 \uc9c0 \uc54a\ub294 \uc8fc\ub2d8\uc744 \ub450\ub824\uc6cc \ud558\uace0 \uc560\ubc30 \ud558\ub294 \uadf8\ub4e4\uc5d0\uac8c \uacbd\uace0\ud558\ub294 \uac83 \ubfd0\uc774 \ub77c \uc2a4\uc2a4\ub85c\ub97c \uc815\ud654\ud558\ub294 \uac83\uc740 \uc790\uae30 \uc601\ud63c\uc744 \uc704\ud574\uc11c\uc77c \ubd84 \ucd5c\ud6c4\uc758 \uc5ec\uc815 \uc740 \ud558\ub098\ub2d8\uaed8\ub85c \uac00\ub2c8\ub77c"}, {"id": 383906, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3679, "text": "\ubcf4\uc9c0 \ubabb\ud55c \uc790\uc640 \ubcf4\ub294 \uc790\uac00 \uac19\uc9c0 \uc544\ub2c8\ud558\uba70"}, {"id": 383907, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3680, "text": "\uc554\ud751\uacfc \uad11\uba85\uc774 \uac19\uc9c0 \uc544\ub2c8\ud558 \uba70"}, {"id": 383908, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3681, "text": "\uc11c\ub298\ud55c \uadf8\ub298\uacfc \uc791\ub82c\ud558\ub294 \uc5f4 \uc774 \uac19\uc9c0 \uc544\ub2c8\ud558\uba70"}, {"id": 383909, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3682, "text": "\uc0b4\uc544\uc788\ub294 \uac83\uacfc \uc8fd\uc740 \uac83\uc774 \uac19\uc9c0 \uc54a\ub178\ub77c \uc2e4\ub85c \ud558\ub098\ub2d8\uaed8\uc11c\ub294 \uadf8 \ubd84\uc758 \ub73b\uc774 \uc788\ub294 \uc790\ub85c \ud558\uc5ec \ub4e3\uac8c \ud558\uc2dc\ub178\ub77c"}, {"id": 383910, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3683, "text": "\uadf8\ub300\ub294 \ubb34\ub364\uc18d\uc5d0 \ubb3b\ud78c \uc790\ub97c \ub4e3\ub3c4\ub85d \ud560 \uc218 \uc5c6\uc73c\uba70 \uadf8\ub300\ub294 \ub2e8\uc9c0\uacbd\uace0\uc790\uc77c \ubfd0\uc774\ub77c"}, {"id": 383911, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3684, "text": "\ud558\ub098\ub2d8\uc740 \uadf8\ub300\ub97c \ubcf5\uc74c\uc758 \uc804 \ub2ec\uc790\uc640 \uacbd\uace0\uc790\ub85c\uc368 \uc9c4\ub9ac\uc640 \ud568\uaed8 \ubcf4\ub0c8\uc73c\ub418 \uc2e4\ub85c \uc5b4\ub5a0\ud55c \ubc31\uc131 \uc911\uc5d0 \uc11c\ub3c4 \uc815\uace0\uc790\uac00 \uc5c6\uc5c8\ub358 \uac83\uc740 \uc544\ub2c8 \uc5c8\ub178\ub77c"}, {"id": 383912, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3685, "text": "\uadf8\ub4e4\uc774 \uadf8\ub300\ub97c \ubd80\uc815\ud588\ub4ef\uc774 \uc120\uc9c0\uc790\ub4e4\uc774 \ubd84\uba85\ud55c \uc608\uc99d\uacfc \uc2dc\ud3b8\uacfc\ube5b\ub098\ub294 \uc131\uc11c\ub97c \uac00\uc9c0\uace0 \uadf8\ub4e4\uc5d0\uac8c \uc654\uc744 \ub54c \uadf8\ub4e4 \uc774\uc804\uc758 \uc120\uc870\ub4e4\ub3c4 \ubd80\uc815\ud588\ub178\ub77c"}, {"id": 383913, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 378, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3686, "text": "\ub098\ub294 \ubd88\uc2e0\ud55c \uadf8\ub4e4\uc744 \ubc8c\ud558\uc600 \ub178\ub77c \uadf8\ub4e4\uc5d0 \ub300\ud55c \ub098\uc758 \ubc8c\uc774 \uc5bc\ub9c8\ub098 \ubb34\uc11c\uc6e0\ub354\ub1e8"}, {"id": 383914, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 379, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3687, "text": "\ud558\ub098\ub2d8\uc774 \ud558\ub298\uc5d0\uc11c \ube44\ub97c \ub0b4 \ub9ac\uac8c \ud558\uc0ac \uadf8\uac83\uc73c\ub85c \uc5ec\ub7ec\uac00\uc9c0 \uc0c9 \uae54\uc758 \uc5f4\ub9e4\ub97c \ub9fa\uac8c \ud558\uc2ec\uc744 \ub108\ud76c\ub294 \ubcf4\uc9c0 \ubabb\ud588\ub290\ub1e8 \uc0b0\ub4e4\uc5d0\ub294 \uc5ec\ub7ec\uac00\uc9c0\uc0c9\uae54\uc758 \ud558\uc580\uce35 \ubd89\uc740\uc9c0\uc5ed \uadf8\ub9ac\uace0 \uac80\uc740 \uacf3\ub3c4 \uc788\uac8c \ud558\uc600\ub178\ub77c"}, {"id": 383915, "identifier": "ko.korean", "surah_number": 35, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0641\u0627\u0637\u0631", "surah_en_name": "Faatir", "surah_en_name_translation": "The Originator", "revelation_type": "Meccan", "juz": 22, "manzil": 5, "page": 437, "ruku": 379, "hizb_quarter": 175, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 3688, "text": "\uc0ac\ub78c\ub3c4 \ub3d9\ubb3c\ub3c4 \uac00\ucd95\ub3c4 \uadf8\uc640 \ub9c8\ucc2c\uac00\uc9c0\ub85c \uc5ec\ub7ec\uac00\uc9c0 \uc0c9\uae54\uc744 \ub450\uc168\ub178\ub77c \uadf8\ubd84\uc758 \uc885 \uac00\uc6b4\ub370\uc11c \ud558\ub098\ub2d8 \uc744 \ub450\ub824\uc6cc \ud558\ub294 \uc790\ub4e4\uc740 \u