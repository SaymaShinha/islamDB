ype": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4636, "text": "\uadf8\ub4e4\uc740 \uadf8\ub4e4\uc704\uc5d0 \ud558\ub298\uc744 \uccd0\ub2e4\ubcf4\uace0 \ud558\ub098\ub2d8\uc774 \uc5b4\ub5bb\uac8c \uadf8\uac83\uc744 \ucc3d \uc870\ud558\uace0 \uc7a5\uc2dd\ud558\ub418 \uadf8 \uc548\uc5d0 \ud55c\uc810\uc758 \uacb0\ud568\uc774 \uc5c6\uac8c \ud558\uc600\ub294\uac00\ub97c \uadf8\ub4e4\uc740 \uc219\uace0\ud558\uc9c0 \uc544\ub2c8 \ud558\ub290\ub1e8"}, {"id": 384874, "identifier": "ko.korean", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4637, "text": "\ub300\uc9c0\ub97c \ub113\uac8c \ub450\uc0ac \uadf8 \uc548\uc5d0 \ud655\uace0\ud55c \uc0b0\ub4e4\uc744 \ub450\uace0 \uadf8 \uc548\uc5d0\uc11c \uc544\ub984\ub2e4\uc6b4 \ucd08\ubaa9\uc774 \uc790\uc6c5\uc73c\ub85c \uc131\uc7a5\ucf00 \ud558 \uc2ec\uc744 \uc219\uace0\ud558\uc9c0 \uc544\ub2c8 \ud558\ub290\ub1e8"}, {"id": 384875, "identifier": "ko.korean", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4638, "text": "\uc774\ub85c\ud558\uc5ec \ud558\ub098\ub2d8\uaed8 \uadc0\uc758\ud558\ub294 \ubaa8\ub4e0 \uc885\ub4e4\ub85c \ud558\uc5ec\uae08 \uc9c0\ucf1c\ubcf4\uace0 \ucc2c \ubbf8\ud558\ub3c4\ub85d \ud568\uc774\ub77c"}, {"id": 384876, "identifier": "ko.korean", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4639, "text": "\ud558\ub098\ub2d8\uc740 \ud558\ub298\ub85c\ubd80\ud130 \ucd95\ubcf5\ubc1b \uc740 \ube44\ub97c \ub0b4\ub9ac\uac8c \ud558\uc0ac \uc774\ub85c \ud558\uc5ec \uacfc\uc218\uc6d0\uc744 \uac00\uafb8\uace0 \uace1\uc2dd\uc744 \uc218\ud655\ud558\ub3c4 \ub85d \ud568\uc774\ub77c"}, {"id": 384877, "identifier": "ko.korean", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 4640, "text": "\ub192\uc740 \uc885\ub824\ub098\ubb34\uc5d0 \uc8fc\ub801\uc8fc\ub801 \uacb9\uce58\uac8c \uc5f4\ub9e4\ub97c \uc8fc\uc2dc\uc5b4"}, {"id": 384878, "identifier": "ko.korean", "surah_number": 50, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0642", "surah_en_name": "Qaaf", "surah_en_name_translation": "The letter Qaaf", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 518, "ruku": 452, "hizb_quarter": 207, "sajda": false, "sajda_id": 0, "sajda_obligato