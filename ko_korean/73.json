[{"id": 385707, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5476, "text": "\ub9dd\ud22c \uc18d\uc5d0 \uc2f8\uc5ec\uc9c4 \uadf8\ub300\uc5ec"}, {"id": 385708, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5477, "text": "\ubc24\uc5d0 \uc77c\uc5b4\ub098 \uc608\ubc30\ud558\ub77c \uadf8\ub7ec\ub098\ubc24\uc911 \ub0b4\ub0b4\uac00 \uc544\ub2c8\uba70"}, {"id": 385709, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5478, "text": "\uadf8\uac83\uc758 \uc808\ubc18 \uc544\ub2c8\uba74 \uadf8 \uc774\ud558 \uba74 \ub418\uba70"}, {"id": 385710, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5479, "text": "\uadf8 \uc774\uc0c1\uc774\ub77c\ub3c4 \uc88b\uc73c\ub2c8\ub77c \uadf8 \ub9ac\uace0 \uafb8\ub780\uc744 \ub0ad\uc1a1\ud558\ub418 \ub290\ub9ac\uace0 \ub9ac \ub4ec\uc744 \ub450\uc5b4 \uc77d\uc73c\ub77c"}, {"id": 385711, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5480, "text": "\ub0b4\uac00 \uadf8\ub300\uc5d0\uac8c \uc911\uc694\ud55c \uba54\uc138\uc9c0 \ub97c \ubcf4\ub0b4\ub9ac\ub77c"}, {"id": 385712, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5481, "text": "\uc2e4\ub85c \ubc24\uc911\uc5d0 \uc77c\uc5b4\ub098\ub294 \uac83\uc740 \uc5b4\ub824\uc6b4 \uc77c\uc774\ub418 \uc608\ubc30\ud558\uace0 \ucc2c\ubbf8\ud558\ub294 \ub9d0\uc744 \ud558\uae30\uc5d0 \uac00\uc7a5 \uc801\ud569\ud55c \ub54c\ub77c"}, {"id": 385713, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5482, "text": "\uc2e4\ub85c \uadf8\ub300\uc5d0\uac8c\ub294 \ub0ae\uc5d0 \ud574\uc57c\ud560\uc784\ubb34\uac00 \uc788\ub098\ub2c8"}, {"id": 385714, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5483, "text": "\uadf8\ub300 \uc8fc\ub2d8\uc758 \uc774\ub984\uc744 \uc0c1\uae30\ud558\uace0 \uc628 \uc815\uc131\uc744 \ub2e4\ud558\uc5ec \uadf8\ubd84\ub9cc\uc744 \uc131\uae30\ub77c"}, {"id": 385715, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5484, "text": "\uadf8\ubd84\uc740 \ub3d9\ucabd\uacfc \uc11c\ucabd\uc758 \uc8fc\ub2d8\uc73c \ub85c \uadf8\ubd84 \uc678\uc5d0\ub294 \uc2e0\uc774 \uc5c6\ub098\ub2c8 \uadf8\ubd84 \ub9cc\uc744 \ubcf4\ud638\uc790\ub85c \ud0dd\ud558\ub77c"}, {"id": 385716, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5485, "text": "\uadf8\ub4e4\uc774 \ub9d0\ud558\ub294 \uac83\uc5d0 \uc778\ub0b4\ud558 \uace0 \uc815\uc911\ud55c \ud0dc\ub3c4\ub85c \uadf8\ub4e4\uc744 \uba40\ub9ac\ud558\ub77c"}, {"id": 385717, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5486, "text": "\uac70\uc9d3\ud558\ub294 \uc790\ub4e4\uc740 \ub0b4\uac8c \ub450\ub77c \uadf8\ub4e4\ub85c \ud558\uc5ec\uae08 \ud5a5\ub77d\ucf00 \ud558\uc5ec \uc7a0\uc2dc \ub9cc \uc720\uc608\ud558\ub9ac\ub77c"}, {"id": 385718, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5487, "text": "\ub098\uc5d0\uac8c\ub294 \uadf8\ub4e4\uc744 \ubb36\uc744 \uc871\uc1e0 \uc640 \uadf8\ub4e4\uc744 \ud0dc\uc6b8 \ubd88\uc774 \uc788\uc73c\uba70"}, {"id": 385719, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5488, "text": "\uc9c8\uc2dd\uc2dc\ud0ac \uc74c\uc2dd\uacfc \uace0\ud1b5\uc2a4\ub7ec\uc6b4\ubc8c\uc774 \uc788\ub098\ub2c8"}, {"id": 385720, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5489, "text": "\ub300\uc9c0\uc640 \uc0b0\ub4e4\uc774 \ud06c\uac8c \ub3d9\uc694\ud558 \ub294 \uadf8\ub0a0 \uc0b0\ub4e4\uc740 \ubaa8\ub798 \uc0b0\ub354\ubbf8\uac00 \ub418 \uc5b4 \ud758\ub7ec \ub0b4\ub9ac\ub2c8\ub77c"}, {"id": 385721, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5490, "text": "\uc2e4\ub85c \ub0b4\uac00 \uadf8\ub300\ub4e4\uc5d0\uac8c \ud55c \uc120\uc9c0\uc790\ub97c \ubcf4\ub0c4\uc740 \ub108\ud76c\ub97c \uc704\ud55c \uc99d \uc778\uc774 \ub418\ub3c4\ub85d \ud568\uc774\ub2c8 \uc774\ub294 \ub0b4\uac00 \ud30c \ub77c\uc624\uc5d0\uac8c \uc120\uc9c0\uc790\ub97c \ubcf4\ub0b8 \uac83\uacfc \uac19 \ub178\ub77c"}, {"id": 385722, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5491, "text": "\uadf8\ub7ec\ub098 \ud30c\ub77c\uc624\ub294 \uadf8 \uc120\uc9c0\uc790 \uc5d0\uac8c \ubcf5\uc885\ud558\uc9c0 \uc544\ub2c8\ud558\uc600\uc73c\ub9e4 \ub098\ub294 \uadf8\uc5d0\uac8c \ubb34\uac70\uc6b4 \uc751\ubc8c\uc744 \ub0b4\ub838\ub178\ub77c"}, {"id": 385723, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5492, "text": "\ub108\ud76c\uac00 \ud558\ub098\ub2d8\uc744 \ubd80\uc815\ud55c\ub2e4\uba74\uc5b4\ub9b0\uc774\uac00 \ud638\ud638\ubc31\ubc1c\uc774 \ub418\ub294 \uadf8\ub0a0 \ub108\ud76c\ub294 \ub108\ud76c \uc790\uc2e0\uc744 \uc5b4\ub5bb\uac8c \ubcf4\ud638 \ud558\ub824 \ud558\ub290\ub1e8"}, {"id": 385724, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5493, "text": "\uadf8\ub54c \ud558\ub298\uc740 \uac08\ub77c\uc9c0\uace0 \uadf8\ubd84 \uc758 \uc57d\uc18d\uc740 \uc774\ud589\ub418\ub2c8\ub77c"}, {"id": 385725, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 574, "ruku": 508, "hizb_quarter": 229, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5494, "text": "\uc2e4\ub85c \uc774\uac83\uc774 \ud558\ub098\uc758 \uad50\ud6c8\uc774 \ub2c8 \ub73b\uc774 \uc788\ub294 \uc790 \uc8fc\ub2d8\uc758 \uae38\uc744 \ub530 \ub974\uac8c \ud558\ub77c"}, {"id": 385728, "identifier": "ko.korean", "surah_number": 73, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0645\u0632\u0645\u0644", "surah_en_name": "Al-Muzzammil", "surah_en_name_translation": "The Enshrouded One", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 575, "ruku": 509, "hizb_quarter": 230, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ko", "name": "Korean", "transelator_en_name": "Unknown", "format": "text", "type": "translation", "ayah_number": 5495, "text": "\uc2e4\ub85c \uc8fc\ub2d8\uc740 \uadf8\ub300\uac00 \ubc24\uc758 \uc0bc \ubd84\uc758 \uc774 \ub610\ub294 \uadf8\uac83\uc758 \ubc18\uc744 \ub54c\ub85c\ub294 \uc0bc\ubd84\uc758 \uc77c\uc744 \uc608\ubc30\ud558\uba70 \uacbd\ubc30\ud558\ub294\uc9c0 \uc54c\uace0 \uacc4\uc2dc\uba70 \ub610\ud55c \uadf8\ub300\uc640 \ud568\uaed8\ud55c \ubb34\ub9ac\ub3c4 \uadf8\ub7ec\ud558\ub2c8\ub77c \ud558\ub098\ub2d8\ub9cc\uc774 \ubc24 \ub0ae\uc744 \uc6b4\uc6a9\ud558\uc2dc\uba70 \ub108\ud76c\uac00 \uadf8\ub807\uac8c \uacc4\uc0b0\ud560 \uc218 \uc5c6\uc74c\uc744 \uadf8\ubd84\uc740 \uc54c\uace0 \uacc4 \uc2dc\ub2c8\ub77c \uadf8\ub9ac\ud558\uc5ec \uadf8\ubd84\uc740 \ub108\ud76c\uc5d0\uac8c \uc790\ube44\ub97c \ubca0\ud478\uc168\uc73c\ub2c8 \uafb8\ub780\uc744 \ub9ce\uc774 \uc77d\uc73c\ub77c \ub108\ud76c\uc5d0\uac8c \uc26c\uc6cc\uc9c0\ub9ac\ub77c \ub610 \ud55c \uadf8\ubd84\uc740 \ub108\ud76c \uac00\uc6b4\ub370 \ubcd1\ub4e0\uc790 \uc788 \uace0 \ud558\ub098\ub2d8\uc758 \uc740\ud61c\ub97c \ucd94\uad6c\ud558\uba70 \uc9c0 \uc0c1\uc744 \uc5ec\ud589\ud558\ub294 \uc790 \uc788\uc73c\uba70 \ud558\ub098\ub2d8 \uc758 \uae38\uc5d0\uc11c \uc131\uc804\ud558\ub294 \uc790 \uc788\uc74c\uc744 \uc54c \uace0 \uacc4\uc2dc\ub098\ub2c8 \uafb8\ub780\uc744 \ub9ce\uc774 \uc77d\uc73c\ub77c \ub108\ud76c\uc5d0\uac8c \uc26c\uc6cc\uc9c0\ub9ac\ub77c \uadf8\ub9ac\uace0 \uc608\ubc30 \ub97c \ub4dc\ub9ac\uace0 \uc774\uc2ac\ub78c\uc138\ub97c \ub0b4\uba70 \ud558\ub098 \ub2d8\uaed8 \uc88b\uc740 \uac83\uc73c\ub85c \ub300\ubd80\ud558\ub77c \ub108\ud76c \uac00 \ub108\ud76c \uc601\ud63c\uc744 \uc704\ud574 \ubc14\uce5c \uac83\uc740 \ud558\ub098\ub2d8 \uc55e\uc5d0\uc11c \ub354 \uc88b\uc740 \uac83\uc73c\ub85c \ubc1c\uacac\ud558\ub9ac\ub2c8 \ud558\ub098\ub2d8\uc758 \uc740\ud61c\ub97c \ucd94\uad6c\ud558\ub77c \ud558\ub098\ub2d8\uc740 \uad00\uc6a9\uacfc \uc790\ube44\ub85c \ucda9\ub9cc \ud558\uc2dc\ub2c8\ub77c"}]