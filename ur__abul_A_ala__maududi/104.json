[{"id": 648489, "identifier": "ur.maududi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0627\u0628\u0648\u0627\u0644\u0627\u0639\u0644\u06cc \u0645\u0648\u062f\u0648\u062f\u06cc", "transelator_en_name": "Abul A'ala Maududi", "format": "text", "type": "translation", "ayah_number": 6180, "text": "\u062a\u0628\u0627\u06c1\u06cc \u06c1\u06d2 \u06c1\u0631 \u0627\u064f\u0633 \u0634\u062e\u0635 \u06a9\u06d2 \u0644\u06cc\u06d2 \u062c\u0648 (\u0645\u0646\u06c1 \u062f\u0631 \u0645\u0646\u06c1) \u0644\u0648\u06af\u0648\u06ba \u067e\u0631 \u0637\u0639\u0646 \u0627\u0648\u0631 (\u067e\u06cc\u0679\u06be \u067e\u06cc\u0686\u06be\u06d2) \u0628\u0631\u0627\u0626\u06cc\u0627\u06ba \u06a9\u0631\u0646\u06d2 \u06a9\u0627 \u062e\u0648\u06af\u0631 \u06c1\u06d2"}, {"id": 648490, "identifier": "ur.maududi", "surah_number": 104, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0647\u0645\u0632\u0629", "surah_en_name": "Al-Humaza", "surah_en_name_translation": "The Traducer", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 601, "ruku": 546, "hizb_quarter": 240, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "ur", "name": "\u0627\u0628\u0648\u0627\u0644\u0627\u0639\u0644\u06cc \u0645\u0648\u062f\u0648\u062f\u06cc", "transelator_en_name": "Abul A'ala Maududi", "format": "text", "type": "translation", "ayah_number": 6181, "text": "\u062c\u0633 \u0646\u06d2 \u0645\u0627\u0644 \u062c\u0645\u0639 \u06a9\u06cc\u0627 \u0627\u0648\u0631 \u0627\u064f\u0633\u06d2 \u06af\u0646 \u06af\u0646 \u06a9\u0631 \u0631\u06a9\u06be\u0627"}, {"id": 648491, "identifier": "ur.maududi", "surah_number": 104