[{"id": 510929, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5994, "text": "Betohem n\u00eb agimin,"}, {"id": 510930, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5995, "text": "dhe dhjet\u00eb net\u00ebt,"}, {"id": 510931, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5996, "text": "dhe n\u00eb t\u00eb gjitha q\u00eb jan\u00eb \u00e7ift e tek,"}, {"id": 510932, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5997, "text": "dhe nat\u00ebn q\u00eb shkon, -"}, {"id": 510933, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5998, "text": "vall\u00eb, a nuk \u00ebsht\u00eb ky betim p\u00ebr t\u00eb men\u00e7urin?"}, {"id": 510934, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5999, "text": "Vall\u00eb, a nuk di ti se \u00e7ka ka b\u00ebr\u00eb Zoti yt me Adin,"}, {"id": 510935, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6000, "text": "e me banor\u00ebt e qytetit Iremes, pronar\u00eb t\u00eb pallatee madh\u00ebshtore,"}, {"id": 510936, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6001, "text": "\u00e7far\u00eb nuk ka pasur askund tjet\u00ebr;"}, {"id": 510937, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6002, "text": "dhe si i d\u00ebnoi themudasit, t\u00eb cil\u00ebt kan\u00eb g\u00ebdhendur shk\u00ebmbinj n\u00eb lugin\u00eb,"}, {"id": 510938, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6003, "text": "dhe Faraonin \u2013 pronarin e shtyllave,"}, {"id": 510939, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6004, "text": "q\u00eb kan\u00eb b\u00ebr\u00eb zullum pa t\u00eb drejt\u00eb n\u00eb tok\u00eb"}, {"id": 510940, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6005, "text": "dhe shtonin ngat\u00ebrresat,"}, {"id": 510941, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6006, "text": "andaj Zoti yt, e l\u00ebshoi mbi ta kamxhikun e d\u00ebnimit (lloj-lloj d\u00ebnimesh),"}, {"id": 510942, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6007, "text": "sepse, me t\u00eb v\u00ebrtet\u00eb, Zoti yt i pret ata n\u00eb pusi (i p\u00ebrcjell\u00eb pun\u00ebt e tyre)."}, {"id": 510943, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6008, "text": "Por, Per\u00ebndia kur ta provoj\u00eb njeriun e ta dhuroj\u00eb me begati, (ai) thot\u00eb: \u201cZoti im \u00ebsht\u00eb sjell\u00eb n\u00eb m\u00ebnyr\u00eb bujar endaj meje!\u201d"}, {"id": 510944, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6009, "text": "E, kur ta provoj\u00eb at\u00eb, duke ia munguar furnizimin, at\u00ebher\u00eb ai thot\u00eb: \u201cZoti im m\u00eb ka posht\u00ebruar\u201d."}, {"id": 510945, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6010, "text": "Jo, jo! Por ju nuk po i respektoni bonjak\u00ebt,"}, {"id": 510946, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6011, "text": "dhe nuk po e nxitni nj\u00ebri-tjetrin p\u00ebr t\u2019i ushqyer t\u00eb varf\u00ebrit,"}, {"id": 509061, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6012, "text": "kurse trash\u00ebgimin\u00eb, e hani me lakmi t\u00eb pangopur,"}, {"id": 509478, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6013, "text": "dhe d\u00ebshironi pasuri t\u00eb pakufishme."}, {"id": 510947, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6014, "text": "Por kjo nuk \u00ebsht\u00eb mir\u00eb! At\u00ebher\u00eb, kur Toka do t\u00eb cop\u00ebtohet n\u00eb th\u00ebrmia,"}, {"id": 510948, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6015, "text": "dhe kur do t\u00eb arrij\u00eb urdh\u00ebri i Zotit t\u00ebnd, e engj\u00ebjt t\u00eb rreshtohen radh\u00eb pas radhe,"}, {"id": 510949, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 593, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6016, "text": "dhe at\u00eb Dit\u00eb kur afrohet (prezentohet) sk\u00ebterra; at\u00ebher\u00eb njeriu do t\u00eb kujtohet \u2013 por \u00e7\u2019dobi?!"}, {"id": 510950, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6017, "text": "dhe thot\u00eb: \u201cAh! Sikur t\u00eb kisha punuar gj\u00ebra t\u00eb mira n\u00eb jet\u00ebn time!\u201d"}, {"id": 510951, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6018, "text": "At\u00eb Dit\u00eb, askush nuk mund t\u00eb d\u00ebnoj\u00eb, si Ai (d\u00ebnimi i Tij),"}, {"id": 510952, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6019, "text": "dhe, at\u00eb dit\u00eb, askush nuk lidh\u00eb n\u00eb pranga si Ai!"}, {"id": 510953, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6020, "text": "E ti, o shpirt i besuar (k\u00ebmb\u00ebngul\u00ebs n\u00eb besim)"}, {"id": 510954, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6021, "text": "kthehu te Zoti yt i k\u00ebnaqur, sepse, edhe Ai \u00ebsht\u00eb i k\u00ebnaqur me ty,"}, {"id": 510955, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6022, "text": "prandaj, hynn\u00eb mes t\u00eb dashurve t\u00eb Mi,"}, {"id": 510956, "identifier": "sq.nahi", "surah_number": 89, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0641\u062c\u0631", "surah_en_name": "Al-Fajr", "surah_en_name_translation": "The Dawn", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 594, "ruku": 531, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6023, "text": "dhe hyn n\u00eb xhennetin Tim!"}]