[{"id": 505117, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 1, "text": "Me em\u00ebr t\u00eb Allahut, M\u00ebshirues dhe P\u00ebrd\u00ebllimtar i Madh."}, {"id": 511339, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 2, "text": "\u00c7do lavd\u00ebrim i p\u00ebrket Allahut (Per\u00ebndis\u00eb), Zotit t\u00eb Gjith\u00ebsis\u00eb"}, {"id": 511340, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 3, "text": "M\u00ebshirues dhe P\u00ebrd\u00ebllimtar i Madh."}, {"id": 511341, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4, "text": "Sunduesit t\u00eb dit\u00ebs s\u00eb gjukimit."}, {"id": 511342, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5, "text": "Vet\u00ebm Ty t\u00eb adhurojm\u00eb dhe vet\u00ebm prej Teje ndihm\u00eb k\u00ebrkojm\u00eb!"}, {"id": 511251, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 6, "text": "Udh\u00ebzona n\u00eb rrug\u00eb t\u00eb drejt\u00eb."}, {"id": 511286, "identifier": "sq.nahi", "surah_number": 1, "surah_ar_name": "\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0671\u0644\u0652\u0641\u064e\u0627\u062a\u0650\u062d\u064e\u0629\u0650", "surah_en_name": "Al-Faatiha", "surah_en_name_translation": "The Opening", "revelation_type": "Meccan", "juz": 1, "manzil": 1, "page": 1, "ruku": 1, "hizb_quarter": 1, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 7, "text": "N\u00eb rrug\u00ebn e atyre q\u00eb u ke dhuruar mir\u00ebsi. E jo n\u00eb (rrug\u00ebn) e atyre q\u00eb kan\u00eb shkaktuar pezmin (T\u00ebnd), as n\u00eb rrug\u00ebn e atyre q\u00eb jan\u00eb t\u00eb humbur!"}]