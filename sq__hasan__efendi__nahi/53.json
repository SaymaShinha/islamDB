[{"id": 509735, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Pasha yllin kur per\u00ebndon,"}, {"id": 509736, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4786, "text": "shoku juaj (Muhammedi) as nuk e ka humb\u00eb as nuk e ka shmang\u00eb rrug\u00ebn e drejt\u00eb!"}, {"id": 509737, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4787, "text": "Ai nuk flet sipas qejfit t\u00eb vet \u2013"}, {"id": 509738, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4788, "text": "Ajo (q\u00eb flet ai) \u00ebsht\u00eb vet\u00ebm Shpallja, q\u00eb i ka ardhur atij,"}, {"id": 509739, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4789, "text": "e m\u00ebsoi at\u00eb nj\u00eb (engj\u00ebll) me fuqi t\u00eb madhe,"}, {"id": 509740, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4790, "text": "me inteligjenc\u00eb t\u00eb lart\u00eb, e u paraqit (n\u00eb form\u00ebn e vet),"}, {"id": 509741, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4791, "text": "duek qen\u00eb n\u00eb sferat e larta (qiellore),"}, {"id": 509742, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4792, "text": "pastaj u afrua, e u l\u00ebshua, -"}, {"id": 509743, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4793, "text": "af\u00ebr (sa) dy harqe ose edhe m\u00eb af\u00ebr,"}, {"id": 509744, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4794, "text": "e ia shpalli robit t\u00eb Tij (Pejgamberit) at\u00eb q\u00eb ia shpalli."}, {"id": 509745, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Zemra e tij nuk e p\u00ebrg\u00ebnjeshtroi at\u00eb q\u00eb e pa,"}, {"id": 509554, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4796, "text": "vall\u00eb, a po polemizoni me te p\u00ebr at\u00eb q\u00eb e ka par\u00eb?"}, {"id": 509746, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Ai (Muhammedi), me t\u00eb v\u00ebrtet\u00eb, e ka par\u00eb at\u00eb (Xhebrailin) edhe nj\u00eb her\u00eb tjet\u00ebr,"}, {"id": 509747, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4798, "text": "te Sidretul muntehaja,"}, {"id": 509748, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4799, "text": "tek ajo (Sidrea) \u00ebsht\u00eb xhenneti \u2013 strehim,"}, {"id": 509749, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4800, "text": "kur Sidr\u00ebn e mbuloi ajo q\u00eb e mbulonte, (nj\u00eb mbules\u00eb e madhe),"}, {"id": 509750, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4801, "text": "shikimi i tij (Muhammedit) nuku shmang, as q\u00eb e kaloi (kufirin e caktuar)."}, {"id": 509751, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Ai, me t\u00eb v\u00ebrtet\u00eb, i pa argumentet m\u00eb madh\u00ebshtore t\u00eb Zotit t\u00eb tij."}, {"id": 509752, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4803, "text": "A e keni pa Latin dhe Uzzan (si jan\u00eb)?"}, {"id": 509753, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4804, "text": "Dhe Menatin, t\u00eb tretin, t\u00eb mbramin (sipas shkall\u00ebs)? \u2013"}, {"id": 509754, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Vall\u00eb, a p\u00ebr ju jan\u00eb djemt\u00eb, e p\u00ebr Ate (Per\u00ebndin\u00eb) vajzat?!"}, {"id": 509755, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Kjo ndarje \u00ebsht\u00eb e padrejt\u00eb. \u2013"}, {"id": 509756, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Ata (idhujt) jan\u00eb vet\u00ebm emra q\u00eb ua keni ngjitur ju dhe t\u00eb par\u00ebt tuaj, e Per\u00ebndia nuk ju ka d\u00ebrguar p\u00ebr ta \u2013 kurrfar\u00eb argumenti. Ata ndjekin vet\u00ebm dyshimin dhe at\u00eb q\u00eb jan\u00eb t\u00eb prirur; e, me t\u00eb v\u00ebrtet\u00eb, atyre u ka ardhur prej Zotit t\u00eb tyre udh\u00ebzimi n\u00eb rrug\u00ebn e drejt\u00eb."}, {"id": 505830, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Ose, a p\u00ebr njeriun jan\u00eb (t\u00eb gjitha) \u00e7ka d\u00ebshiron,"}, {"id": 509757, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4809, "text": "vet\u00ebm Per\u00ebndis\u00eb i p\u00ebrkasin (t\u00eb gjitha), ajo dhe kjo bot\u00eb!"}, {"id": 509758, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4810, "text": "E, sa engj\u00ebj ka n\u00eb qiej \u2013 nd\u00ebrmjet\u00ebsimi i t\u00eb cil\u00ebve nuk b\u00ebn dobi asgj\u00eb, p\u00ebrve\u00e7 pasi t\u00eb jap\u00eb leje Per\u00ebndia, p\u00ebr k\u00eb t\u00eb doj\u00eb Ai dhe kush \u00ebsht\u00eb i denj\u00eb p\u00ebr te."}, {"id": 509759, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Me t\u00eb v\u00ebrtet\u00eb, ata q\u00eb nuk besojn\u00eb n\u00eb jet\u00ebn tjet\u00ebr, i em\u00ebrojn\u00eb engj\u00ebjt me emra fem\u00ebror\u00eb (i konsiderojn\u00eb femra)"}, {"id": 509760, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4812, "text": "e, p\u00ebr k\u00ebt\u00eb, ata nuk kan\u00eb kurrfar\u00eb dijenie, ndjekin vet\u00ebm hamendjen (supozimin), e hamendja nuk sjell dobi asgj\u00eb p\u00ebr t\u00eb v\u00ebrtet\u00ebn."}, {"id": 509761, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4813, "text": "Andaj, ti (o Muhammed) largohu prej atij q\u00eb i ik\u00ebn Kur\u2019anit dhe q\u00eb d\u00ebshiron vet\u00ebm jet\u00ebn e k\u00ebsaj bote, -"}, {"id": 509762, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4814, "text": "kjo \u00ebsht\u00eb arritja e tyre n\u00eb dijeni, me t\u00eb v\u00ebrtet\u00eb, Zoti yt i njeh m\u00eb s\u00eb miri ata q\u00eb jan\u00eb shmangur nga rruga e Tij, dhe Ai i di m\u00eb s\u00eb miri ata q\u00eb ndjekin rrug\u00ebn e drejt\u00eb."}, {"id": 509763, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Per\u00ebndis\u00eb i p\u00ebrkasin t\u00eb gjitha (\u00e7ka ka) n\u00eb qiej dhe n\u00eb Tok\u00eb \u2013 e, sipas asaj q\u00eb kan\u00eb punuar, p\u00ebr t\u2019i d\u00ebnuar ata q\u00eb b\u00ebjn\u00eb keq, e p\u00ebr t\u2019i shp\u00ebrblyer ata q\u00eb b\u00ebjn\u00eb mir\u00eb me shp\u00ebrblim shum\u00eb t\u00eb mir\u00eb, t\u00eb madh,"}, {"id": 509764, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4816, "text": "ata q\u00eb ruhen prej m\u00ebkateve t\u00eb m\u00ebdha dhe veprave t\u00eb k\u00ebqia (imorale), pos m\u00ebkateve t\u00eb vogla. Me t\u00eb v\u00ebrtet\u00eb, Zoti yt \u00ebsht\u00eb i gjer\u00eb (i pakufish\u00ebm) n\u00eb falje. Ai, m\u00eb s\u00eb miri di p\u00ebr ju, qysh se u ka krijur prej dheut dhe qysh se keni qen\u00eb embion n\u00eb barkun e n\u00ebnave tuaja; andaj, mos u lavd\u00ebroni me pafajshm\u00ebrin\u00eb tuaj \u2013 Ai e njeh mir\u00eb at\u00eb q\u00eb ruhet nga m\u00ebkatet."}, {"id": 509765, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4817, "text": "A e ke par\u00eb ti at\u00eb q\u00eb \u00ebsht\u00eb shmangur (nga e v\u00ebrteta),"}, {"id": 509766, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4818, "text": "pak ka dh\u00ebn\u00eb, e pastaj e pengoi tjetr\u00ebn,"}, {"id": 509767, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4819, "text": "a i di ai gj\u00ebrat sekrete, dhe a i sheh ato?"}, {"id": 509768, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Vall\u00eb, a nuk \u00ebsht\u00eb i lajm\u00ebruar ai \u2013 p\u00ebr at\u00eb q\u00eb gjendet n\u00eb flet\u00ebt e Musait (n\u00eb Teurat)"}, {"id": 509769, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4821, "text": "dhe t\u00eb Ibrahimit: - \u201ci cili i plot\u00ebsoi (obligimet) \u2013"}, {"id": 509770, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4822, "text": "q\u00eb askush nuk do t\u2019i bart\u00eb m\u00ebkatet e personit tjet\u00ebr,"}, {"id": 509771, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4823, "text": "dhe q\u00eb njeriu do t\u00eb ket\u00eb vet\u00ebm at\u00eb q\u00eb \u00ebsht\u00eb marr\u00eb me te (punuar at\u00eb)"}, {"id": 509772, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4824, "text": "dhe q\u00eb p\u00ebrpjekjet e tij do t\u00eb shihen s\u00eb shpejti, padyshim"}, {"id": 509773, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4825, "text": "(dhe se) do t\u00eb shp\u00ebrblehet me shp\u00ebrblim t\u00eb plot\u00eb,"}, {"id": 509774, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4826, "text": "dhe, se, te Zoti yt do t\u00eb jet\u00eb kthimi,"}, {"id": 509573, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4827, "text": "Ai jep gazin dhe vajin (g\u00ebzimin dhe hidh\u00ebrimin),"}, {"id": 505981, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4828, "text": "Ai jep vdekjen dhe gjalles\u00ebn,"}, {"id": 506002, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4829, "text": "Ai krijon \u00e7iftet, mashkull e fem\u00ebr,"}, {"id": 509775, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4830, "text": "prej pik\u00ebs s\u00eb far\u00ebs (sperm\u00ebs) kur t\u00eb hedhet (injektohet),"}, {"id": 509776, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4831, "text": "dhe, me t\u00eb v\u00ebrtet\u00eb, tek Ai \u00ebsht\u00eb krijimi i dyt\u00eb (rindjallja),"}, {"id": 509777, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4832, "text": "dhe Ai jep pasuri dhe k\u00ebnaq\u00ebsi t\u00eb mjaftueshme,"}, {"id": 509778, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4833, "text": "dhe Ai \u00ebsht\u00eb Zot\u2019i Siriusit,"}, {"id": 509779, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4834, "text": "dhe Ai e ka shkat\u00ebrruar popullin e lasht\u00eb t\u00eb Adit,"}, {"id": 509780, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4835, "text": "dhe Themudin, dhe s\u2019ka l\u00ebn\u00eb ask\u00eb prej tyre,"}, {"id": 509781, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4836, "text": "e madje, para tyre \u2013 popullin e Nuhut, i cili ka qen\u00eb (edhe) m\u00eb zullumqar\u00eb dhe i zhytur edhe m\u00eb shum\u00eb (n\u00eb t\u00eb k\u00ebqia),"}, {"id": 509782, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4837, "text": "dhe Ai i p\u00ebrmbysi vendbanimet (e popullit t\u00eb Lutit),"}, {"id": 509783, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4838, "text": "dhe i mbuloi ata \u00e7ka i mbuloi (goditje e madhe),"}, {"id": 509784, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4839, "text": "e, n\u00eb \u00e7far\u00eb dhuntie t\u00eb Zotit t\u00ebnd dyshon ti (polemizon)?"}, {"id": 509785, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Ky paralajm\u00ebrues (pejgamber) \u00ebsht\u00eb si ata t\u00eb m\u00ebparshmit."}, {"id": 509786, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4841, "text": "U afrua ajo q\u00eb \u00ebsht\u00eb af\u00ebr (Dita e Kijametit),"}, {"id": 509787, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4842, "text": "nuk mund ta zbuloj\u00eb (at\u00eb) askush, pos Per\u00ebndis\u00eb!"}, {"id": 509788, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4843, "text": "E, vall\u00eb prej k\u00ebtyre fjal\u00ebve (Kur\u2019anit) po \u00e7uditeni,"}, {"id": 509789, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4844, "text": "dhe po qeshni, e nuk po qani? \u2013"}, {"id": 509790, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4845, "text": "e, adje, jeni edhe t\u00eb pakujdessh\u00ebm!"}, {"id": 509791, "identifier": "sq.nahi", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 4846, "text": "B\u00ebni sexhde p\u00ebr Per\u00ebndin\u00eb dhe adhuronie (Ate)!"}]