[{"id": 510269, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5324, "text": "\u00c7asti i t\u00eb v\u00ebrtet\u00ebs!"}, {"id": 510270, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5325, "text": "\u00c7ka \u00ebsht\u00eb \u00c7asti i t\u00eb v\u00ebrtet\u00ebs?"}, {"id": 510271, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5326, "text": "Kush ta shpjegon ty (plot\u00ebsisht) \u00c7astin e t\u00eb v\u00ebrtet\u00ebs (Kijametin)?"}, {"id": 510272, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5327, "text": "Themudi dhe Adi e kan\u00eb p\u00ebrg\u00ebnjeshtruar Kijametin,"}, {"id": 510273, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5328, "text": "e populli i Themudit \u00ebsht\u00eb shkat\u00ebrruar me nj\u00eb ulurim\u00eb (britm\u00eb) t\u00eb madhe temrruese,"}, {"id": 510274, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5329, "text": "e populli i Adit \u00ebsht\u00eb shkat\u00ebrruar me nj\u00eb furtun\u00eb t\u00eb hatashme,"}, {"id": 510275, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5330, "text": "t\u00eb cil\u00ebn Per\u00ebndia ua ka d\u00ebrguar atyre shtat\u00eb net\u00eb dhe tet\u00eb dit\u00eb \u2013 parreshtur, e ku mund t\u2019i v\u00ebrejshe njer\u00ebzit e p\u00ebrmbysur si trungjet e hurmeve t\u00eb kalbura,"}, {"id": 510276, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 566, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5331, "text": "e, a po sheh mos ka mbetur kush prej tyre?"}, {"id": 510277, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5332, "text": "E, ka ardhur Faraoni, dhe ata para tij, si dhe populli (i Lutit), me gabime t\u00eb m\u00ebdha"}, {"id": 510278, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5333, "text": "dhe e kund\u00ebrshtuan t\u00eb d\u00ebrguarin e Zotit t\u00eb tyre, andaj Per\u00ebndia i ka d\u00ebnuar ata me d\u00ebnim t\u00eb ashp\u00ebr."}, {"id": 510279, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5334, "text": "Ne, kur v\u00ebrshoi uji i madh, ju ngarkuam juve n\u00eb anije,"}, {"id": 510280, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5335, "text": "p\u00ebr t\u2019ju b\u00ebr\u00eb k\u00ebt\u00eb juve m\u00ebsim dhe q\u00eb t\u00eb mbajn\u00eb mend vesh\u00ebt e atyre njer\u00ebzve (t\u00eb dijsh\u00ebm),"}, {"id": 510281, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5336, "text": "e kur t\u00eb fryej\u00eb nj\u00ebher\u00eb buria,"}, {"id": 510282, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5337, "text": "e toka dhe malet t\u00eb bart\u00ebn e t\u00eb cop\u00ebtohen nga nj\u00eb shp\u00ebrthim (eksplodim)"}, {"id": 510283, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5338, "text": "at\u00eb dit\u00eb do t\u00eb ngjaj\u00eb Dita e Kijametit,"}, {"id": 510284, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5339, "text": "dhe do t\u00eb \u00e7ahet qielli, e at\u00eb dit\u00eb do t\u00eb jet\u00eb i thyesh\u00ebm,"}, {"id": 510285, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5340, "text": "dhe engj\u00ebjt do t\u00eb rrin\u00eb an\u00ebve t\u00eb qiellit, kurse at\u00eb dit\u00eb \u2013 tet\u00eb grupe engj\u00ebjsh do ta mbajn\u00eb Arshin (fronin) e Zotit t\u00ebnd."}, {"id": 510286, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5341, "text": "At\u00eb dit\u00eb do t\u00eb pyeteni (paraqiteni) ju, dhe asnj\u00eb fsheht\u00ebsi e juaj nuk do t\u00eb mbet\u00ebt e pazbuluar:"}, {"id": 510287, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5342, "text": "atij q\u00eb i jepet libri i veprave t\u00eb tij n\u00eb dor\u00ebn e djatht\u00eb, do t\u2019ju thot\u00eb (t\u00eb tjer\u00ebve): Qe, lexoni librin, tim;"}, {"id": 510288, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5343, "text": "un\u00eb kam qen\u00eb i bindur se do t\u00eb paraqes llogari."}, {"id": 510289, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5344, "text": "E, ai \u00ebsht\u00eb n\u00eb nj\u00eb jet\u00eb t\u00eb k\u00ebndshme,"}, {"id": 510290, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5345, "text": "n\u00eb nj\u00eb kopsht t\u00eb mrekulluesh\u00ebm,"}, {"id": 510291, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5346, "text": "frutet e t\u00eb cilit jan\u00eb t\u00eb ul\u00ebta (af\u00ebr duarve)."}, {"id": 510292, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5347, "text": "(Ju thuhet atyre): \u201cJu bafshin mir\u00eb ushqimi dhe pijet p\u00ebr veprat q\u00eb keni punuar, n\u00eb dit\u00ebt e kaluara!\u201d"}, {"id": 510293, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5348, "text": "Atij q\u00eb i jepet libri i veprave t\u00eb tij n\u00eb dor\u00ebn e majt\u00eb, do t\u00eb thot\u00eb: \u201cAh, mos t\u00eb m\u00eb ishte dh\u00ebn\u00eb libri im,"}, {"id": 510294, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5349, "text": "dhe mos t\u00eb kisha ditur p\u00ebr llogarin\u00eb time,"}, {"id": 510295, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5350, "text": "Ah, t\u00eb kisha qen\u00eb i vdekur (t\u00eb mos isha ringjallur)!"}, {"id": 510296, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5351, "text": "Pasuria ime nuk m\u00eb solli dobi,"}, {"id": 510297, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5352, "text": "dhe falimentoi fuqia ime!"}, {"id": 510298, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5353, "text": "(e atyre u thuhet): \u201cKapni ata dhe lidhni n\u00eb pranga,"}, {"id": 510299, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5354, "text": "pastaj n\u00eb sk\u00ebterr\u00eb f\u00ebrgoni,"}, {"id": 510300, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5355, "text": "e mandej lidhni n\u00eb zingjir\u00eb prej shtat\u00ebdhjet\u00eb kut\u00ebsh,"}, {"id": 510301, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5356, "text": "sepse, ai, me t\u00eb v\u00ebrtet\u00eb, nuk ka besuar Per\u00ebndin\u00eb e Madh\u00ebruesh\u00ebm,"}, {"id": 510302, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 567, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5357, "text": "dhe nuk ka nxitur t\u00eb ushqej\u00eb t\u00eb varf\u00ebrit e shkret\u00eb."}, {"id": 510303, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5358, "text": "Prandaj, ai, sot k\u00ebtu, nuk ka mik t\u00eb dashur,"}, {"id": 510304, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5359, "text": "as ushim tjet\u00ebr, p\u00ebrpos qelb\u00ebsir\u00ebs;"}, {"id": 510305, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 500, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5360, "text": "t\u00eb cilin e han\u00eb vet\u00ebm fajtor\u00ebt (jobesimtar\u00ebt)."}, {"id": 510306, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5361, "text": "E, Un\u00eb betohem me at\u00eb \u00e7ka shihni ju,"}, {"id": 510307, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5362, "text": "dhe me at\u00eb q\u00eb nuk shihni,"}, {"id": 510308, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5363, "text": "me t\u00eb v\u00ebrtet\u00eb, ajo (Kur\u2019ani) \u00ebsht\u00eb fjala e shpallur Profetit t\u00eb shenjt\u00eb,"}, {"id": 510309, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5364, "text": "e nuk \u00ebsht\u00eb fjal\u00eb e poetit. Pak ju besoni,"}, {"id": 510310, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5365, "text": "e as nuk \u00ebsht\u00eb fjal\u00eb e ndonj\u00eb magjistari (falltori), pak po mendoni ju!"}, {"id": 510311, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5366, "text": "(Kur\u2019ani) \u00ebsht\u00eb i zbritur nga Zotii gjith\u00ebsis\u00eb!"}, {"id": 510312, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5367, "text": "Sikur t\u00eb kishte trilluar (Muhammedi) di\u00e7ka prej fjal\u00ebve n\u00eb llogari Ton\u00eb,"}, {"id": 510313, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5368, "text": "Ne do ta kapnim at\u00eb \u00ebpr dor\u00ebn e djatht\u00eb,"}, {"id": 510314, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5369, "text": "dhe do t\u2019ia prenim arterien e zemr\u00ebs,"}, {"id": 510315, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5370, "text": "dhe askush prej jush nuk do t\u00eb mund ta mbronte prej k\u00ebtij (d\u00ebnimi)."}, {"id": 510316, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5371, "text": "Me t\u00eb v\u00ebrtet\u00eb, ai (Kur\u2019ani), \u00ebsht\u00eb, k\u00ebshill\u00eb p\u00ebr ata q\u00eb d\u00ebshirojn\u00eb zbatimin e urdh\u00ebrave dhe t\u00eb ruhen prej t\u00eb k\u00ebqiave,"}, {"id": 510317, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5372, "text": "por, Ne, me siguri, e dim\u00eb, q\u00eb ka prej jush p\u00ebrg\u00ebnjeshtrues."}, {"id": 510318, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5373, "text": "Me t\u00eb v\u00ebrtet\u00eb, ai (Kur\u2019ani), \u00ebsht\u00eb, fatkeq\u00ebsi p\u00ebr mohuesit,"}, {"id": 510158, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5374, "text": "por, ai (Kur\u2019ani), padyshim, \u00ebsht\u00eb e v\u00ebrtet\u00eb e pamohueshme."}, {"id": 510319, "identifier": "sq.nahi", "surah_number": 69, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u062d\u0627\u0642\u0629", "surah_en_name": "Al-Haaqqa", "surah_en_name_translation": "The Reality", "revelation_type": "Meccan", "juz": 29, "manzil": 7, "page": 568, "ruku": 501, "hizb_quarter": 227, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5375, "text": "Prandaj, falenderohu me emrin e Zotit t\u00ebnd t\u00eb Madh\u00ebruesh\u00ebm."}]