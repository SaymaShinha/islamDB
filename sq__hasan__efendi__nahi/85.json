[{"id": 510845, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5910, "text": "Betohem n\u00eb qiellin e stolisur me hyllsi,"}, {"id": 510846, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5911, "text": "dhe n\u00eb Dit\u00ebn e premtuar (Kijametit),"}, {"id": 510847, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5912, "text": "dhe n\u00eb d\u00ebshmitar\u00ebt, si dhe n\u00eb at\u00eb q\u00eb do t\u00eb d\u00ebshmohet (at\u00eb dit\u00eb),"}, {"id": 510848, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5913, "text": "mallkuar qofshin ata \u00eb kan\u00eb hapur hendeqe,"}, {"id": 510849, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5914, "text": "dhe q\u00eb i mbush\u00ebn me zjarr, me gjith\u00ebfar\u00eb l\u00ebn\u00ebd\u00ebsh djeg\u00ebse,"}, {"id": 510850, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5915, "text": "(mallkuar qofshin ata) kur u ul\u00ebn pran\u00eb tij"}, {"id": 510851, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5916, "text": "dhe ishin prezent t\u00eb asaj (q\u00eb u b\u00ebn\u00eb besimtar\u00ebve)!"}, {"id": 510852, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5917, "text": "E, (jobesimtar\u00ebt) i munduan ata vet\u00ebm pse i besuan Zotit t\u00eb Plot\u00ebfuqish\u00ebm dhe Falenderues,"}, {"id": 510853, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5918, "text": "pushteti i t\u00eb cilit shtrihet n\u00eb qiej dhe n\u00eb Tok\u00eb, - e Per\u00ebndia \u00ebsht\u00eb D\u00ebshmitar i \u00e7do gj\u00ebje."}, {"id": 510854, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Pa dyshim, ata q\u00eb i torturojn\u00eb besimtar\u00ebt dhe besimtaret e q\u00eb (pastaj) nuk pendohen \u2013 i pret d\u00ebnimi i sk\u00ebterr\u00ebs dhe (p\u00ebr ta ka) d\u00ebnim zjarri,"}, {"id": 510855, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5920, "text": "e, me t\u00eb v\u00ebrtet\u00eb, ata q\u00eb besojn\u00eb dhe punojn\u00eb vepra t\u00eb mira \u2013 p\u00ebr ta ka kopshte t\u00eb xhennetit, n\u00ebp\u00ebr t\u00eb cil\u00ebt rrjedhin lumenjt\u00eb; - e ky \u00ebsht\u00eb sukses i madh."}, {"id": 510856, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Me t\u00eb v\u00ebrtet\u00eb, nd\u00ebshkimi i Zotit t\u00ebnd, \u00ebsht\u00eb i ashp\u00ebr!"}, {"id": 510857, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Me t\u00eb v\u00ebrtet\u00eb, Ai krijon prej kurrgj\u00ebje dhe p\u00ebrs\u00ebri i kthen (ps vdekjes \u2013 i ringjall\u00eb);"}, {"id": 510858, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Ai \u00ebsht\u00eb fal\u00ebs dhe plot dashuri,"}, {"id": 510859, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5924, "text": "Zot i Arshit (fronit) t\u00eb Madh\u00ebruesh\u00ebm,"}, {"id": 510860, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Ai punon \u00e7ka t\u00eb doj\u00eb."}, {"id": 510861, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5926, "text": "A ka arritur te ti lajmi p\u00ebr ushtrit\u00eb"}, {"id": 510862, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5927, "text": "e Faraonit dhe Themudit?"}, {"id": 510863, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Por, jobesimtar\u00ebt \u2013 gjithmon\u00eb mohojn\u00eb,"}, {"id": 510864, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5929, "text": "e Per\u00ebndia (i Plot\u00ebfuqish\u00ebm), i p\u00ebrfshin\u00eb t\u00eb gjith\u00eb,"}, {"id": 510865, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5930, "text": "ky Kur\u2019an \u2013 (q\u00eb e mohonin ata), \u00ebsht\u00eb Kur\u2019ani i madh\u00ebruesh\u00ebm,"}, {"id": 510866, "identifier": "sq.nahi", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "sq", "name": "Efendi Nahi", "transelator_en_name": "Hasan Efendi Nahi", "format": "text", "type": "translation", "ayah_number": 5931, "text": "i shkruar n\u00eb Levhi Mahfudh (n\u00eb pllak\u00ebn e ruajtur me kujdes)."}]