[{"id": 149141, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5885, "text": "Wenn der Himmel zerrei\u00dft,"}, {"id": 149142, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5886, "text": "und seinem HERRN ganz Ohr wird, und ihm obliegt es,"}, {"id": 149143, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5887, "text": "und wenn die Erde ausgedehnt wird,"}, {"id": 149144, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5888, "text": "und herauswirft, was in ihr ist und darauf verzichtet,"}, {"id": 149145, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5889, "text": "und ihrem HERRN ganz Ohr wird, und ihr obliegt es,"}, {"id": 149146, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5890, "text": "Mensch! Gewi\u00df, du bist ein sich Anstrengender zu deinem HERRN mit Anstrengung, auf die du treffen wirst."}, {"id": 149147, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5891, "text": "Also hinsichtlich desjenigen, dem sein Register mit seiner Rechten gegeben wird,"}, {"id": 149148, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5892, "text": "so wird er einer leichten Abrechnung unterzogen,"}, {"id": 149149, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5893, "text": "und wird zu seiner Familie gl\u00fccklich zur\u00fcckkehren."}, {"id": 149150, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5894, "text": "Und hinsichtlich desjenigen, dem sein Register von hinter seinem R\u00fccken gegeben wird,"}, {"id": 149151, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5895, "text": "so wird er nach Vernichtung rufen,"}, {"id": 149152, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5896, "text": "und in Gluthitze hineingeworfen."}, {"id": 149153, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5897, "text": "Gewi\u00df, er war in seiner Familie gl\u00fccklich."}, {"id": 149121, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5898, "text": "Gewi\u00df, er dachte, da\u00df er nicht zur\u00fcckkehrt."}, {"id": 149154, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5899, "text": "Doch, mit Sicherheit! Gewi\u00df, sein HERR war \u00fcber ihn allsehend."}, {"id": 149155, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5900, "text": "Nein, ICH schw\u00f6re bei der Abendr\u00f6te"}, {"id": 149156, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5901, "text": "und bei der Nacht und dem, was sie zusammenbringt,"}, {"id": 149157, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5902, "text": "und bei dem Mond, wenn er voll wird,"}, {"id": 149158, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5903, "text": "ihr werdet doch von einem Zustand zum anderen aufsteigen."}, {"id": 149159, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5904, "text": "Weshalb verinnerlichen sie den Iman nicht?!"}, {"id": 149160, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": true, "sajda_id": 14, "sajda_obligatory": false, "sajda_recommended": true, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5905, "text": "Und wenn ihnen der Quran vorgetragen wird, vollziehen sie kein Sudschud."}, {"id": 149161, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5906, "text": "Nein, sondern diejenigen, die Kufr betrieben haben, leugnen ab,"}, {"id": 149162, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5907, "text": "und ALLAH wei\u00df besser Bescheid \u00fcber das, was ihnen bewu\u00dft ist."}, {"id": 149163, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5908, "text": "So \u00fcberbringe ihnen die \"frohe Botschaft\" \u00fcber eine qualvolle Peinigung,"}, {"id": 149164, "identifier": "de.zaidan", "surah_number": 84, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0625\u0646\u0634\u0642\u0627\u0642", "surah_en_name": "Al-Inshiqaaq", "surah_en_name_translation": "The Splitting Open", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 589, "ruku": 526, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5909, "text": "au\u00dfer denjenigen, die den Iman verinnerlichten und gottgef\u00e4llig guttaten, f\u00fcr die ist eine nicht endende Belohnung bestimmt."}]