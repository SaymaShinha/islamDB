[{"id": 149202, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5949, "text": "R\u00fchme den Namen deines allh\u00f6chsten HERRN,"}, {"id": 149203, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5950, "text": "Desjenigen, Der erschuf und dann zurechtmachte,"}, {"id": 149204, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5951, "text": "und Desjenigen, Der bestimmte, dann rechtleitete,"}, {"id": 149205, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5952, "text": "und Desjenigen, Der das Gegraste hervorbringen lie\u00df,"}, {"id": 149206, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5953, "text": "und ER es dann zum gr\u00fcnschwarzen Vertrockneten machte."}, {"id": 149207, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5954, "text": "WIR werden dich vortragen lassen, so vergi\u00df nicht"}, {"id": 149208, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5955, "text": "au\u00dfer dem, was ALLAH will. Gewi\u00df, ER kennt das Offenkundige und das, was verborgen ist."}, {"id": 149209, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5956, "text": "Und WIR f\u00fchren dich leicht zur Erleichterten."}, {"id": 149210, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5957, "text": "So ermahne, wenn die Ermahnung n\u00fctzt."}, {"id": 149211, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5958, "text": "Es wird sich ermahnen lassen derjenige, der Ehrfurcht hat,"}, {"id": 149212, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5959, "text": "und sie meidet der Unseligste."}, {"id": 149213, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5960, "text": "Derjenige, der in das gr\u00f6\u00dfte Feuer hineingeworfen wird,"}, {"id": 149214, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5961, "text": "dann darin weder stirbt, noch lebt."}, {"id": 149215, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5962, "text": "Bereits erfolgreich ist derjenige, der sich l\u00e4uterte"}, {"id": 149216, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 591, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5963, "text": "und des Namens seines HERRN gedachte, dann das rituelle Gebet verrichtete."}, {"id": 149217, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5964, "text": "Nein, sondern ihr bevorzugt das diesseitige Leben."}, {"id": 149218, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5965, "text": "Doch das Jenseits ist besser und bleibender."}, {"id": 149219, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5966, "text": "Gewi\u00df, dies ist doch in den ersten Schriftbl\u00e4ttern,"}, {"id": 149220, "identifier": "de.zaidan", "surah_number": 87, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0623\u0639\u0644\u0649", "surah_en_name": "Al-A'laa", "surah_en_name_translation": "The Most High", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 592, "ruku": 529, "hizb_quarter": 237, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5967, "text": "den Schriftbl\u00e4ttern von Ibrahim und Musa."}]