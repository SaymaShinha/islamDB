[{"id": 148073, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4785, "text": "Bei dem Stern, wenn er abst\u00fcrzt!"}, {"id": 148074, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4786, "text": "Euer Weggenosse ist weder irregegangen, noch folgte er einer Verf\u00fchrung."}, {"id": 148075, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4787, "text": "Und er spricht nicht aus eigenem Gutd\u00fcnken."}, {"id": 148076, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4788, "text": "Es ist nur ein Wahy, das als Wahy zuteil wird."}, {"id": 148077, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4789, "text": "Ihn lehrte einer von enormen Kr\u00e4ften,"}, {"id": 148078, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4790, "text": "von Intelligenz, dann stand er gerade,"}, {"id": 148079, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4791, "text": "w\u00e4hrend er am h\u00f6chsten Horizont war,"}, {"id": 143430, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4792, "text": "dann n\u00e4herte er sich, dann stieg er hinab,"}, {"id": 148080, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4793, "text": "so war er (so nahe) wie der Abstand beider Bogenenden oder noch n\u00e4her,"}, {"id": 148081, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4794, "text": "dann lie\u00df er Seinem Diener als Wahy das zuteil werden, was er als Wahy zuteil werden lie\u00df."}, {"id": 148082, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4795, "text": "Der Verstand erlog nicht, was er sah."}, {"id": 148083, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4796, "text": "Zweifelt ihr etwa an ihm das an, was er sieht?!"}, {"id": 148084, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4797, "text": "Und gewi\u00df, bereits sah er ihn gewi\u00df schon einmal"}, {"id": 148085, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4798, "text": "bei der \u00e4u\u00dfersten Sidra,"}, {"id": 148086, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4799, "text": "bei dieser ist die Dschanna der Unterkunft,"}, {"id": 148087, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4800, "text": "da zur Sidra die kommen, die kommen."}, {"id": 148088, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4801, "text": "Weder verfehlte der Blick, noch \u00fcberschritt er."}, {"id": 148089, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4802, "text": "Gewi\u00df, bereits sah er von den gro\u00dfen Ayat seines HERRN!"}, {"id": 148090, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4803, "text": "Wie seht ihr es mit Al-lat und Al-'uz-za"}, {"id": 148091, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4804, "text": "sowie die andere Dritte Manat?!\""}, {"id": 148092, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4805, "text": "Ist euch etwa das M\u00e4nnliche und Sein ist das Weibliche?!"}, {"id": 148093, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4806, "text": "Dies ist sicher eine ungerechte Teilung."}, {"id": 148094, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4807, "text": "Es sind nur Namen, die ihr und eure Ahnen gegeben habt, f\u00fcr die ALLAH keinen Beweis hinabsandte. Sie folgen doch nur dem Spekulieren und dem, was die Seelen begehren! Und gewi\u00df, bereits kam zu ihnen von ihrem HERRN die Rechtleitung."}, {"id": 148095, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4808, "text": "Oder geh\u00f6rt dem Menschen etwa das, was er sich w\u00fcnscht?!"}, {"id": 148096, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 460, "hizb_quarter": 210, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4809, "text": "So geh\u00f6rt ALLAH das Letzte und das Erste."}, {"id": 148097, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 526, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4810, "text": "Und wie viele Engel gibt es in den Himmeln, deren F\u00fcrbitte nichts n\u00fctzt, es sei denn, nachdem ALLAH zustimmt, wem ER will und an dem ER Wohlgefallen hat."}, {"id": 148098, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4811, "text": "Gewi\u00df, diejenigen, die den Iman an das Jenseits nicht verinnerlichen, benennen ja die Engel wie die Benennung des Weiblichen."}, {"id": 148099, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4812, "text": "Und sie verf\u00fcgen dar\u00fcber \u00fcber keinerlei Wissen. Sie folgen doch nur dem Spekulieren. Und das Spekulieren ersetzt die Wahrheit in Nichts."}, {"id": 148100, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4813, "text": "So wende dich ab von dem, der sich von Unserer Ermahnung abwendet und ausschlie\u00dflich das diesseitige Leben will."}, {"id": 148101, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4814, "text": "Dies ist ihr Erreichtes an Wissen. Gewi\u00df, dein HERR wei\u00df besser Bescheid \u00fcber denjenigen, der von Seinem Weg abirrte, und ER wei\u00df besser Bescheid \u00fcber denjenigen, der Rechtleitung fand."}, {"id": 148102, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4815, "text": "Und ALLAH geh\u00f6rt, was in den Himmeln und was auf Erden ist. ER wird denjenigen, die Schlechtes taten, doch das vergelten, was sie taten, und ER wird denjenigen, die Ihsan taten, mit dem Besseren vergelten."}, {"id": 148103, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 461, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4816, "text": "Es sind diejenigen, die das Schwere an Verfehlungen und die Abscheulichkeiten au\u00dfer den Kleinigkeiten meiden. Gewi\u00df, dein HERR ist von allumfassender Vergebung. ER wei\u00df besser Bescheid \u00fcber euch, als ER euch von der Erde hervorkommen lie\u00df, und als ihr Embryos in den B\u00e4uchen eurer M\u00fctter wart, so lobt euer Selbst nicht! ER wei\u00df besser Bescheid \u00fcber denjenigen, der Taqwa gem\u00e4\u00df handelt."}, {"id": 148104, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4817, "text": "Wie siehst du es mit demjenigen, der sich abwandte"}, {"id": 148105, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4818, "text": "und ein wenig gab, dann sich enthielt?!"}, {"id": 148106, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4819, "text": "Hat er etwa das Verborgene bei sich, so sieht er?!"}, {"id": 148107, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4820, "text": "Wurde ihm etwa nicht mitgeteilt \u00fcber das, was in den Schriften von Musa ist"}, {"id": 148108, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4821, "text": "sowie von Ibrahim, der (das ihm Gebotene) einhielt?!"}, {"id": 148109, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4822, "text": "Da\u00df keine belastete Seele die Last einer anderen tr\u00e4gt,"}, {"id": 148110, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4823, "text": "und da\u00df es dem Menschen nur das gibt, was er erstrebte,"}, {"id": 143433, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4824, "text": "und da\u00df sein Streben doch gesehen werden wird,"}, {"id": 148111, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4825, "text": "dann ihm daf\u00fcr die vollst\u00e4ndigste Vergeltung vergolten wird,"}, {"id": 148048, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4826, "text": "und da\u00df zu deinem HERRN doch das Endg\u00fcltige ist,"}, {"id": 148112, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4827, "text": "und da\u00df ER doch Derjenige ist, Der lachen und weinen lie\u00df,"}, {"id": 148113, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 527, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4828, "text": "und da\u00df ER doch Derjenige ist, Der sterben und beleben lie\u00df,"}, {"id": 148114, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4829, "text": "und da\u00df ER doch die Zweiheit, das M\u00e4nnliche und das Weibliche erschuf,"}, {"id": 148115, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4830, "text": "aus einer Nutfa, wenn sie ergossen wird,"}, {"id": 148116, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4831, "text": "und da\u00df Ihm doch die andere Erweckung obliegt,"}, {"id": 148117, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4832, "text": "und da\u00df ER doch Derjenige ist, Der reich werden und besitzen lie\u00df,"}, {"id": 148118, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4833, "text": "und da\u00df ER doch Derjenige ist, Der HERR von Asch-schi'ra ist,"}, {"id": 148119, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4834, "text": "und da\u00df ER doch das erste 'Aad zugrunde richtete"}, {"id": 148120, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4835, "text": "sowie Thamud, dann nichts \u00fcbriglie\u00df,"}, {"id": 148121, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4836, "text": "sowie die Leute von Nuh vorher, gewi\u00df, sie pflegten diejenigen zu sein, die noch mehr Unrecht und noch mehr \u00dcbertretungen begingen."}, {"id": 148122, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4837, "text": "Und das Umgewandte lie\u00df ER abst\u00fcrzen,"}, {"id": 148123, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4838, "text": "so bedeckte sie, was sie bedeckte."}, {"id": 148124, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4839, "text": "An welchen der Wohltaten deines HERRN zweifelst du denn?!"}, {"id": 148125, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4840, "text": "Dies ist eine Warnung von den ersten Warnungen."}, {"id": 148126, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4841, "text": "Die Nahende nahte sich,"}, {"id": 148127, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4842, "text": "f\u00fcr die es anstelle von ALLAH nichts Aufdeckendes gibt."}, {"id": 148128, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4843, "text": "Seid ihr etwa \u00fcber diesen Bericht erstaunt,"}, {"id": 148129, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4844, "text": "lacht und weint nicht,"}, {"id": 148130, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4845, "text": "w\u00e4hrend ihr in Arroganz sich Vergn\u00fcgende seid?!"}, {"id": 148131, "identifier": "de.zaidan", "surah_number": 53, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0646\u062c\u0645", "surah_en_name": "An-Najm", "surah_en_name_translation": "The Star", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 528, "ruku": 462, "hizb_quarter": 211, "sajda": true, "sajda_id": 13, "sajda_obligatory": true, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4846, "text": "So vollzieht Sudschud f\u00fcr ALLAH und dient Ihm!"}]