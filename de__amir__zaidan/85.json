[{"id": 149165, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5910, "text": "Bei dem Himmel von Sternbildern,"}, {"id": 149166, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5911, "text": "und bei dem angedrohten Tag,"}, {"id": 149167, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5912, "text": "und bei dem Zeugen und Bezeugten,"}, {"id": 149168, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5913, "text": "Tod sei den Weggenossen des Grabens."}, {"id": 149169, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5914, "text": "Das Feuer darin ist mit Brennstoff,"}, {"id": 149170, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5915, "text": "als sie um es herum sa\u00dfen,"}, {"id": 149171, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5916, "text": "und sie sind \u00fcber das, was sie mit den Mumin machen, Zeugen."}, {"id": 149172, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5917, "text": "Und sie mi\u00dfbilligten an ihnen nichts au\u00dfer, da\u00df sie den Iman verinnerlichten an ALLAH, Den Allw\u00fcrdigen, Den Alllobensw\u00fcrdigen,"}, {"id": 149173, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5918, "text": "Dem die Herrschaft der Himmel und der Erde geh\u00f6rt. Und ALLAH ist \u00fcber alles Zeuge."}, {"id": 149174, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5919, "text": "Gewi\u00df, f\u00fcr diejenigen, die die Mumin-M\u00e4nner und Mumin-Frauen der Fitna aussetzen, dann nicht bereuen, ist die Peinigung von Dschahannam und die Peinigung des Verbrennens bestimmt."}, {"id": 149175, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5920, "text": "Gewi\u00df, f\u00fcr diejenigen, die den Iman verinnerlicht und gottgef\u00e4llig Gutes getan haben, sind Dschannat bestimmt, die von Fl\u00fcssen durchflossen sind. Dies ist der gro\u00dfe Erfolg."}, {"id": 149176, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5921, "text": "Gewi\u00df, das Gewalt-Antun deines HERRN ist doch heftig."}, {"id": 149177, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5922, "text": "Gewi\u00df, ER ist derjenige, der beginnen und zur\u00fcckkehren l\u00e4\u00dft."}, {"id": 149178, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5923, "text": "Und ER ist Der Allvergebende, Der \u00c4u\u00dferst Liebende,"}, {"id": 149179, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5924, "text": "Der von dem herrlichen Al'ahrsch,"}, {"id": 149180, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5925, "text": "Der Allbewirkende dessen, was ER will."}, {"id": 149181, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5926, "text": "Wurde dir die Mitteilung \u00fcber die Soldaten zuteil,"}, {"id": 149182, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5927, "text": "\u00fcber Pharao und Thamud?!"}, {"id": 149183, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5928, "text": "Nein, sondern diejenigen, die Kufr betrieben haben, sind im Ableugnen."}, {"id": 149184, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5929, "text": "Und ALLAH ist vor ihnen allumfassend."}, {"id": 149185, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5930, "text": "Nein, sondern er ist ein herrlicher Quran,"}, {"id": 148207, "identifier": "de.zaidan", "surah_number": 85, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0628\u0631\u0648\u062c", "surah_en_name": "Al-Burooj", "surah_en_name_translation": "The Constellations", "revelation_type": "Meccan", "juz": 30, "manzil": 7, "page": 590, "ruku": 527, "hizb_quarter": 236, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 5931, "text": "auf einer wohlverwahrten Tafel."}]