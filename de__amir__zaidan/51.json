[{"id": 147966, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4676, "text": "Bei den das Streuen Streuenden,"}, {"id": 147967, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4677, "text": "dann den Schweres Tragenden,"}, {"id": 147968, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4678, "text": "dann den in Leichtigkeit sich Fortbewegenden,"}, {"id": 147969, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4679, "text": "dann den die Angelegenheit Einteilenden!"}, {"id": 147970, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4680, "text": "Was euch versprochen wird, ist doch wahrhaftig."}, {"id": 147971, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 520, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4681, "text": "Und gewi\u00df, der Din wird sicher geschehen."}, {"id": 147972, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4682, "text": "Bei dem Himmel von Kreisbahnen!"}, {"id": 147973, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4683, "text": "Gewi\u00df, ihr seid doch verschiedener Ansichten,"}, {"id": 147974, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4684, "text": "abgebracht wird davon, wer abgebracht wurde."}, {"id": 147975, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4685, "text": "Tod sei den L\u00fcgnern,"}, {"id": 147976, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4686, "text": "die in den Tiefen (der Unwissenheit) achtlos bleiben."}, {"id": 147977, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4687, "text": "Sie fragen: \"Wann geschieht der Tag des Din?\""}, {"id": 147978, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4688, "text": "\"Am Tag, wenn sie im Feuer der Fitna unterzogen werden.\""}, {"id": 147979, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4689, "text": "Erfahrt eure Fitna! Dies ist das, wozu ihr zur Eile aufzufordern pflegtet."}, {"id": 147980, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4690, "text": "Gewi\u00df, dieMuttaqi sind in Dschannat und an Quellen,"}, {"id": 147981, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4691, "text": "sie nehmen das, was ihr HERR ihnen zuteil werden lie\u00df. Gewi\u00df, sie pflegten vor diesem Muhsin zu sein."}, {"id": 147982, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4692, "text": "Sie pflegten ein wenig von der Nacht zu schlafen,"}, {"id": 147983, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4693, "text": "und vor der Morgend\u00e4mmerung baten sie um Vergebung."}, {"id": 147984, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4694, "text": "Und in ihren Verm\u00f6gensg\u00fctern ist ein Anrecht f\u00fcr den Bittenden und den Ausgeschlossenen."}, {"id": 147985, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4695, "text": "Und auf Erden sind Ayat f\u00fcr die Gewi\u00dfheit-Anstrebenden"}, {"id": 147986, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4696, "text": "sowie in euch selbst. Habt ihr etwa keinen Einblick?!"}, {"id": 147903, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4697, "text": "Und im Himmel ist euer Rizq und das, was euch versprochen wird."}, {"id": 147954, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 455, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4698, "text": "Beim HERRN des Himmels und der Erde! Gewi\u00df, es ist bestimmt wahr, genauso (wie es wahr ist), da\u00df ihr sprecht."}, {"id": 147955, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4699, "text": "Wurde dir der Bericht \u00fcber Ibrahims gew\u00fcrdigte G\u00e4ste zuteil?!"}, {"id": 147987, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4700, "text": "Als sie zu ihm eintraten und sagten: \"Salam (sei mit dir)!\" Er sagte: \"Salam (sei mit euch), ihr unbekannte Leute!\""}, {"id": 147988, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4701, "text": "Dann ging er unbemerkt zu seiner Familie, dann kam er mit einem wohlgen\u00e4hrten Kalb,"}, {"id": 147989, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4702, "text": "dann setzte er es ihnen vor, er sagte: \"Esst ihr etwa nicht?\""}, {"id": 147990, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4703, "text": "Dann verbarg er Furcht vor ihnen. Sie sagten: \"F\u00fcrchte dich nicht!\" Und sie \u00fcberbrachten ihm frohe Botschaft \u00fcber einen \u00e4u\u00dferst wissenden Jungen."}, {"id": 147991, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4704, "text": "Dann kam seine Frau in lautem \u00dcberschwang, schlug sich auf ihr Gesicht und sagte: \"Ich bin doch eine unfruchtbare Alte!\""}, {"id": 147992, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 26, "manzil": 7, "page": 521, "ruku": 456, "hizb_quarter": 208, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4705, "text": "Sie sagten: \"Solcherart sagte dein HERR. Gewi\u00df, ER ist Der Allweise, Der Allwissende.\""}, {"id": 147993, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4706, "text": "Er sagte: \"Was ist euer Bestreben, ihr Entsandte?\""}, {"id": 147994, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4707, "text": "Sie sagten: \"Gewi\u00df, wir wurden zu schwer verfehlenden Leuten entsandt,"}, {"id": 147995, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4708, "text": "damit wir \u00fcber sie Steine von Lehm schicken,"}, {"id": 147996, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4709, "text": "die bei deinem HERRN f\u00fcr die Ma\u00dflosen gekennzeichnet sind.\""}, {"id": 147997, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4710, "text": "Dann brachten WIR heraus, wer in ihr von den Mumin war."}, {"id": 147998, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4711, "text": "Dann fanden WIR in ihr nur ein Haus von Muslimen."}, {"id": 147999, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4712, "text": "Und WIR lie\u00dfen in ihr eine Aya f\u00fcr diejenigen, die sich vor der qualvollen Peinigung f\u00fcrchten."}, {"id": 148000, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4713, "text": "Und mit Musa (gab es eine Aya), als WIR ihn zu Pharao mit einem deutlichen Beweis entsandten."}, {"id": 148001, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4714, "text": "Dann wandte er sich zu seinen Protektoren und sagte: \"(Er ist) ein Magier oder geistesgest\u00f6rt.\""}, {"id": 148002, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4715, "text": "Dann bestraften WIR ihn mit seinen Soldaten, dann warfen WIR sie in den Flu\u00df, w\u00e4hrend er tadelnswert war."}, {"id": 148003, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4716, "text": "Und mit 'Aad (gab es eine Aya), als WIR \u00fcber sie den unfruchtbar machenden Wind schickten,"}, {"id": 148004, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4717, "text": "der nichts von dem l\u00e4\u00dft, wor\u00fcber er wehte, ohne da\u00df er es zu Zermalmtem machte."}, {"id": 148005, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4718, "text": "Und mit Thamud (gab es eine Aya), als ihnen gesagt wurde: \"Vergn\u00fcgt euch nur bis zu einer Zeit!\""}, {"id": 148006, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4719, "text": "Dann erhoben sie sich \u00fcber die Anweisung ihres HERRN, dann erschlug sie der Blitzschlag w\u00e4hrend sie zuschauten."}, {"id": 148007, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4720, "text": "Dann konnten sie weder aufstehen, noch wurde ihnen beigestanden."}, {"id": 148008, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 456, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4721, "text": "Ebenso die Leute von Nuh vorher, gewi\u00df, sie waren fisq-betreibende Leute."}, {"id": 148009, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4722, "text": "Und den Himmel errichteten WIR mit Kraft, und gewi\u00df, WIR sind doch Ausdehnende."}, {"id": 148010, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4723, "text": "Und die Erde breiteten wir aus. Wie sch\u00f6n sind die Ebnenden."}, {"id": 148011, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4724, "text": "Und von allen Dingen erschufen WIR Zweiheiten, damit ihr euch besinnt."}, {"id": 148012, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4725, "text": "\"Also entweicht zu ALLAH! Gewi\u00df, ich bin f\u00fcr euch vor Ihm ein deutlicher Warner."}, {"id": 148013, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 522, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4726, "text": "Und setzt neben ALLAH keinen anderen Gott ein! Gewi\u00df, ich bin f\u00fcr euch vor Ihm ein deutlicher Warner.\""}, {"id": 148014, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4727, "text": "Solcherart, zu denjenigen vor ihnen kam kein Gesandter, ohne da\u00df sie sagten: \"(Er ist) ein Magier oder geistesgest\u00f6rt.\""}, {"id": 148015, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4728, "text": "Vermachten sie es einander etwa?! Nein, sondern sie sind \u00fcbertretende Leute."}, {"id": 148016, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4729, "text": "So wende dich von ihnen ab, denn du bist nicht tadelnswert."}, {"id": 148017, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4730, "text": "Und erinnere, denn die Erinnerung nutzt doch den Mumin."}, {"id": 148018, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4731, "text": "Und ICH erschuf die Dschinn und die Menschen nur, um Mir zu dienen."}, {"id": 148019, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4732, "text": "ICH will von ihnen keinerlei Rizq, und ICH will nicht, da\u00df sie Mich speisen."}, {"id": 148020, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4733, "text": "Gewi\u00df, ALLAH ist Der wahre reichlich Rizq-Gew\u00e4hrende, Der mit der Allkraft, Der absolut Solide."}, {"id": 148021, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4734, "text": "Denn gewi\u00df, f\u00fcr diejenigen, die Unrecht begingen, ist ein Anteil, wie der Anteil ihrer Weggenossen, so sollen sie nicht zur Eile auffordern!"}, {"id": 148022, "identifier": "de.zaidan", "surah_number": 51, "surah_ar_name": "\u0633\u0648\u0631\u0629 \u0627\u0644\u0630\u0627\u0631\u064a\u0627\u062a", "surah_en_name": "Adh-Dhaariyat", "surah_en_name_translation": "The Winnowing Winds", "revelation_type": "Meccan", "juz": 27, "manzil": 7, "page": 523, "ruku": 457, "hizb_quarter": 209, "sajda": false, "sajda_id": 0, "sajda_obligatory": false, "sajda_recommended": false, "language": "de", "name": "Zaidan", "transelator_en_name": "Amir Zaidan", "format": "text", "type": "translation", "ayah_number": 4735, "text": "Also Niedergang sei denjenigen, die Kufr betrieben haben, an ihrem Tag, der ihnen angedroht wird."}]